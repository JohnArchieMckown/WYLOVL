* /*                                     IRARMWL3 AND IRARMWL4.      */ 02547900
* /*                                     AS A RESULT ENTRY POINT     */ 02548100
* /*                                     IRARMHIT NOW RESIDES IN     */ 02548300
* /*                                     IRARMWM4. UPDATE THE        */ 02548500
* /*                                     MODULE ID (MID) FOR ENTRY   */ 02548700
* /*                                     POINT IRARMHIT TO REFLECT   */ 02548900
* /*                                     THIS CHANGE.                */ 02549100
* /* $L1=LJOBM    JBB3313 880829 PD88AI: LARGE JOB MANAGEMENT        */ 02549200
* /*                                     UPDATE MODULE ID  (MID) FOR */ 02549300
* /*                                     ENTRY IRARMPR9 THAT WAS     */ 02549400
* /*                                     MOVED FROM IRARMSTM TO      */ 02549500
* /*                                     IRARMPRP                @L1A*/ 02549600
* /* $L2=WSMGT    HBB4420 900716 PD88AK: WORKING SET MANAGEMENT      */ 02549700
* /*                                     SUPPORT                 @L2A*/ 02549800
* /*                                                                 */ 02550002
* /* $PWA0185     JBB6607 980601 PD88KN: PLANFORM 1130 UPDATES       */ 02566600
* /*                                                                 */ 02583200
* /* *************************************************************** */ 02600002
* /*                                                                    02650002
.EPDTAL1 ANOP                                                           02700002
         SPACE 1                                                        02750002
         AIF   ('&DSECT' EQ 'NO').EPDTAL2                               02800002
