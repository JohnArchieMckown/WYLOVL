         MACRO ,                                                   @L1P 01650000
&LABEL   BLSUFANS &OPTION,&LV=,&A=,&SP= Model statement                 01700000
         SPACE 2                                                   @L1P 01750000
*********************************************************************** 01800000
* BLSUFANS--Free Automatic Storage                             #MD93075 01850000
*********************************************************************** 01900000
         SPACE ,                                                   @L1P 01950000
&LABEL   L     RF,SAR1(0,RD)           ->Parameter list            @L1P 02000000
         L     R1,0(0,RF)              ->ZZ2                       @L1P 02050000
         L     RF,ZZ2FANYP(0,R1)       ->BLSUFANY                       02100000
         BASR  RE,RF                   CALL BLSUFANY                    02150000
         MEND  ,                                                   @L1P 02200000
