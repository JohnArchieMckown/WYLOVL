*/* STATUS= HBB5510                                                  */ 00340200
*/*                                                                  */ 00340300
*/***END_OF_PROPRIETARY_STATEMENT*************************************/ 00340400
*/*                                                              @L9A*/ 00340500
*/* METHOD OF ACCESS                                             @L9A*/ 00340600
*/*   1. DECLARE MACRO CHARACTER VARIABLE CTMXV                  @L9A*/ 00340700
*/*   2. SET CTMXV EQUAL TO 'YES'                                @L9A*/ 00340800
*/*   3. DCL XVPTR                                               @L9A*/ 00340900
*/*   4. %INCLUDE SYSLIB(IHACTM)                                 @L9A*/ 00341000
*/*                                                              @L9A*/ 00341100
*/* FUNCTION                                                     @L9A*/ 00341200
*/*   THE XV IS USED AS AN SVC 35 (WTO) WORK AREA.               @L9A*/ 00341300
*/*                                                              @L9A*/ 00341400
*/* CREATED BY                                                   @L9A*/ 00341500
*/*   IEAVM600                                                   @L9A*/ 00341600
*/*                                                              @L9A*/ 00341700
*/* POINTED TO BY                                                @L9A*/ 00341800
*/*   THE XV IS BASED ON XVPTR WHICH MUST BE FILLED IN WITH      @L9A*/ 00341900
*/*   WITH THE ADDRESS OF THE XV IN ORDER TO ESTABLISH           @L9A*/ 00342000
*/*   ADDRESSABILITY CORRECTLY.  MODULES WHICH USE THE XV        @L9A*/ 00342100
*/*   MUST BE PASSED THE ADDRESS OF THE XV AND SET XVPTR         @L9A*/ 00342200
*/*   EQUAL TO THIS ADDRESS.                                     @L9A*/ 00342300
*/*                                                              @L9A*/ 00342400
*/* SERIALIZATION                                                @L9A*/ 00342500
*/*   NONE                                                       @L9A*/ 00342600
*/*                                                              @L9A*/ 00342700
*/* LOCATION                                                     @L9A*/ 00342800
*/*   THE STORAGE FOR THE XV IS OBTAINED FROM IEAVM600'S         @L9A*/ 00342900
*/*   DYNAMIC AREA, KEY 0 FROM SUBPOOL 229.                      @L9A*/ 00343000
*/*                                                              @L9A*/ 00343100
*/********************************************************************/ 00343200
*DECLARE                                                                00343300
* 1 XVSAV BASED(XVPTR) BDY(DWORD),                                      00343400
*                                 /* EXTENDED SAVEAREA           @L9C*/ 00343500
*   2 XVACRN       CHAR(3),       /* CONTROL BLOCK ID 'XV '      @L9A*/ 00343600
*   2 XVVRSN       FIXED(8),      /* VERSION LEVEL               @L9A*/ 00343700
*   2 XVA4         PTR(31),       /* REG AND D23 SAVEAREA    @ZA41986*/ 00343800
*     3 XVFNCT     CHAR(1),       /* D23 PROCESS CODE        @ZA41986*/ 00343900
*     3 XVAMOD     CHAR(1),       /* D23 MODULE ID           @ZA41986*/ 00344000
*     3 XVAABCD    FIXED(16),     /* ABEND CODE THAT CAUSED IEAVVWTO    00344100
*                                    IEAVMWTO TO FAIL            @01A*/ 00344200
*       4 *        CHAR(1),       /* RESERVED                    @01C*/ 00344300
*       4 XVREASON CHAR(1),       /* D23 REASON CODE             @01C*/ 00344400
*   2 XVA0         PTR(31),       /* REGISTER SAVEAREA ,                00344500
*                                    BYTE 1 - NUMBER OF WQES AND        00344600
*                                    BYTE 2-4 WPL PARM LENGTH    @L2C*/ 00344700
*   2 XVTIME       PTR(31),       /* TIME STORED BY IEAVM600     @L9C*/ 00344800
*   2 XVMSGLGH     FIXED(15),     /* MSGLENGTH, 4 + MSGTEXT LENGTH   */ 00344900
*   2 XVCHAR1      CHAR(1),       /* 1ST CHARACTER OF TEXT       @P7A*/ 00345000
*   2 XVMISC2      CHAR(1),       /* Miscellaneous flags         @01C*/ 00345100
*     3 XVGETENQ BIT(1),          /* INDICATE TO IEAVM604 TO OBTAIN     00345200
*                                    MPF/GENX TABLE ENQ          @01A*/ 00345300
*     3 XVFREENQ BIT(1),          /* INDICATE TO IEAVM604 TO RELEASE    00345400
*                                    MPF/GENX TABLE ENQ          @01A*/ 00345500
*     3 XVHADENQ BIT(1),          /* INDICATE CALLER HAD OBTAINED       00345610
*                                    THE MPF/GENX TABLE RESOURCE @0FA*/ 00345620
*     3 * BIT(5),                 /* Reserved                    @0FC*/ 00345630
*   2 XVPARMAD     PTR(31),       /* ADDR OF CALLER'S PARM LIST      */ 00345700
*   2 XVWQEAD      PTR(31),       /* ADDR OF WQE BUILT BY SVC 35     */ 00345800
*   2 XVCMAJOR     PTR(31),       /* ADDR OF MAJOR WQE           @L9A*/ 00345900
*   2 XVCMINOR     PTR(31),       /* ADDR OF MINOR WQE           @L9A*/ 00346000
*   2 XVTSRPTR     PTR(31),       /* ADDR OF TEXT SERVICE               00346100
*                                    PARAMETER LIST              @LHA*/ 00346200
*   2 XVTXTPTR     PTR(31),       /* ADDRESS OF WORKAREA CONTAINING WTO 00346300
*                                    TEXT WHEN TEXT PARAMETER USED ON   00346400
*                                    WTO                         @PCA*/ 00346500
    2 XVINTPTR     PTR(31),       /* INTERMEDIATE WORK POINTER FOR      00346600
                                     MLWTO'S                     @PCA*/ 00346700
*/***************************************************************@L9A*/ 00346800
*/*     XV FLAGS                                                 @L9A*/ 00346900
*/***************************************************************@L9A*/ 00347000
*   2 XVDFLAGS FIXED(32),         /* FLAGS WORD                      */ 00347100
*     3 XVD0    BIT(8),           /* FIRST FLAGS BYTE                */ 00347200
*       4 XVD0S250 BIT(1),        /* IF ON, USER'S EXIT PARM IN SP250   00347300
*                                    IF OFF, PARM LIST IS IN SP229   */ 00347400
*       4 XVD0RPFD BIT(1),        /* REPLY ID AVAIL & PUT IN ORE     */ 00347500
*       4 XVD0NWQE BIT(1),        /* NEED A WQE                      */ 00347600
*       4 XVD0NORE BIT(1),        /* NEED AN ORE                     */ 00347700
*       4 XVD0QID  BIT(1),        /* QID IS PRESENT IN THE WPL       */ 00347800
*       4 XVD0WWB  BIT(1),        /* A WWB HAS BEEN OBTAINED         */ 00347900
*       4 XVD0USER BIT(1),        /* RETURN TO USER IF SET ON BY THE    00348000
*                                    WTP ROUTINE.                @L9C*/ 00348100
*       4 XVD0HDCY BIT(1),        /* THIS MESSAGE IS A WTP TO BE SENT   00348200
*                                    TO HARDCOPY ONLY.               */ 00348300
*     3 XVD1     BIT(8),          /* FLAG BYTE WTO/WTOR/MLWTO        */ 00348400
*       4 XVD1PRIV BIT(1),        /* USER IS A PRIVILEGED TASK       */ 00348500
*       4 XVD1CFPL BIT(1),        /* STORAGE WAS FOUND FOR PARM LIST    00348600
*                                                                @L9C*/ 00348700
*       4 XVD1ALDN BIT(1),        /* ALL DONE GETTING WQE/ORE BLOCKS */ 00348800
*       4 XVD1OEXT BIT(1),        /* AN ORE EXISTS FOR THIS WQE      */ 00348900
*       4 XVRESV01 BIT(1),        /* RESERVED                        */ 00349000
*       4 XVD1PP   BIT(1),        /* USER IS A PROBLEM PROGRAM       */ 00349100
*       4 XVD1AUTH BIT(1),        /* USER IS AN AUTHORIZED PROGRAM   */ 00349200
*       4 XVD1PERR BIT(1),        /* PARM LIST FOUND TO BE IN ERROR  */ 00349300
*     3 XVD2       BIT(8),        /* MLWTO FLAGS BYTE                */ 00349400
*       4 XVD2CON  BIT(1),        /* CONNECTING MLWTO                */ 00349500
*       4 XVD2RDC  BIT(1),        /* ROUTE/DESCRIPTOR CODES EXIST    */ 00349600
*       4 XVD2MSGT BIT(1),        /* MSGTYPE FIELD EXISTS            */ 00349700
*       4 XVD2VALD BIT(1),        /* THE WPL IS VALID                */ 00349800
*       4 XVD2DELW BIT(1),        /* SEND THE WQE TO HARD COPY ONLY  */ 00349900
*       4 XVD2ZERO BIT(1),        /* MESSAGE HAS LENGTH OF ZERO      */ 00350000
*       4 XVD2WTOR BIT(1),        /* THIS IS A WTOR                  */ 00350100
*       4 XVD2QFHC BIT(1),        /* QUEUE THIS WQE FOR HARDCOPY     */ 00350200
*     3 XVD3       BIT(8),        /* FOURTH FLAGS BYTE           @L9C*/ 00350300
*       4 XVD3BLDJ BIT(1),        /* BUILD MAJOR WQE             @L9A*/ 00350400
*       4 XVD3BLD1 BIT(1),        /* BUILD LINE 1                @L9A*/ 00350500
*       4 XVD3BLD2 BIT(1),        /* BUILD LINE 2                @L9A*/ 00350600
*       4 XVD3TXT1 BIT(1),        /* TEXT LINE 1 BEING USED      @L9A*/ 00350700
*       4 XVD3TFXW BIT(1),        /* THE TCBTFX BIT WAS SET ON,         00350800
*                                    TURN IT OFF                 @L9A*/ 00350900
*       4 XVD3ENQY BIT(1),        /* ENQ WAS OBTAINED BY THIS SVC 35    00351000
*                                                                @01A*/ 00351100
*       4 XVD3XITR BIT(1),        /* CVXIT CHANGED ROUT CODE @ZA88861*/ 00351200
*       4 XVD3XITD BIT(1),        /* CVXIT CHANGED DESC CODE @ZA88861*/ 00351300
*   2 XVOREAD    PTR(31),         /* ADDR OF THE ORE                 */ 00351400
*/***************************************************************@L9A*/ 00351500
*/*     LINE TYPE FLAGS                                          @L9A*/ 00351600
*/***************************************************************@L9A*/ 00351700
*   2 XVX0       BIT(8),          /* LINE CONTROL FLAGS--MLWTO   @L9A*/ 00351800
*     3 XVX0FLCL BIT(1),          /* FIRST LINE IS CONTROL LINE  @L9A*/ 00351900
*     3 *        BIT(2),          /* RESERVED                    @L9A*/ 00352000
*     3 XVX0UDCL BIT(1),          /* USE DEFAULT CONTROL LINE    @L9A*/ 00352100
*     3 XVX0FLJE BIT(1),          /* FIRST LINE JUST 'E'         @L9A*/ 00352200
*     3 XVX0FEDE BIT(1),          /* FORCE END (LAST LINE TO BE         00352300
*                                    DE)                         @L9A*/ 00352400
*/***************************************************************@L9A*/ 00352500
*/*     MLWTO ERROR FLAGS                                        @L9A*/ 00352600
*/***************************************************************@L9A*/ 00352700
*   2 XVX1       BIT(8),          /* MLWTO ERROR FLAGS           @L9A*/ 00352800
*     3 XVX1STOP BIT(1),          /* ERROR IN PARM LIST.  IGNORE        00352900
*                                    MLWTO                       @L9A*/ 00353000
*     3 XVX1NOID BIT(1),          /* NO ID FOR CONNECTING MLWTO  @L9A*/ 00353100
*   2 XVX2       CHAR(1),         /* NUMBER OF LINES STILL TO DO @L9A*/ 00353200
*   2 XVMISC     CHAR(1),         /* MISCELLANEOUS FLAGS         @P5A*/ 00353300
