         MACRO                                                          00225000
         IHACTM  &MAP                                                   00225100
         AIF   ('&MAP' EQ 'XV').IHAXV        SVC 35 EXTENDED SAVE AREA  00225200
         AIF   ('&MAP' EQ 'CXSA').IHACXSA    SVC 72 EXTENDED SAVE AREA  00225300
         AIF   ('&MAP' EQ 'CQE').IHACQE      CONSOLE QUEUE ELEMENT      00225400
         AIF   ('&MAP' EQ 'DPL').IHADPL      DOM PARM LIST              00225500
         AIF   ('&MAP' EQ 'WWB').IHAWWB      WTO WAIT BLOCK MAP         00225600
         AIF   ('&MAP' EQ 'QR0').IHAQR0      IEAVMQR0 PARMLIST          00225700
         AIF   ('&MAP' EQ 'FTPT').IHAFTPT    FRR USERS PARM LIST MAP    00225800
         AIF   ('&MAP' EQ 'CQB').IHACQB      CMD QUEUE BUFFER  @G741PSS 00225900
         AIF   ('&MAP' EQ 'AMRQ').IHAAMRQ    ACTION MSG RET Q  @G741PSS 00226000
         AIF   ('&MAP' EQ 'UXIR').IHAUXIR    USER EXIT INTERFACE   @L6A 00226100
         AIF   ('&MAP' EQ 'CMDELEM').IHACMDE CONSOLE CMD Q ELEM    @LCA 00226200
         AIF   ('&MAP' EQ 'VRBMP').IEE3003 VERB TABLE              @LDA 00226300
         AIF   ('&MAP' EQ 'DOM0').IHADOM0    DOM REG 0 MAPPING     @LUA 00226400
         MNOTE *,'IMPROPER OPERAND'                                     00226500
         MEXIT                                                          00226600
.IHAXV   ANOP                                                           00226700
