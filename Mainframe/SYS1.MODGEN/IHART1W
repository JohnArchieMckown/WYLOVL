*%RTMTRAN: MACRO KEYS(PREFIX,PIC,SOPI);  /* Set TEAV,TEIV,TEPC   @LFA*/ 99914900
* ANS('DO; /* Begin RTMTRAN macro   */') SKIP NORESCAN; /*       @LFA*/ 99918200
* ANS(PREFIX(1)||'TEIV = OFF; /* Clear flags before select statement    99920400
* */') SKIP NORESCAN;          /*                                @LFA*/ 99924800
* ANS(PREFIX(1)||'TEAV = OFF; /* Clear flags before select statement    99929200
* */')  SKIP NORESCAN;         /*                                @LFA*/ 99933600
* ANS(PREFIX(1)||'TEPC = OFF; /* Clear flags before select statement    99935000
* */')  SKIP NORESCAN;         /*                                @LHA*/ 99936500
* ANS('SELECT(EVAL('||PIC(1)||'&''3F''X)); /* Select interrupt type */')99938000
*  SKIP  NORESCAN;                /*                            @LFA*/  99941500
* ANS('WHEN (''10''X,''11''X,''38'',''39'',''3A''X,''3B''X) '           99945000
*          ||'/* Segment translation or page translation */')           99948500
*       SKIP NORESCAN;         /*                                @LFA*/ 99952000
* ANS(PREFIX(1)||'TEAV = ON; /* SDWATRAN contains a valid address*/')   99955600
*  SKIP NORESCAN;              /*                                @LFA*/ 99960000
* ANS('WHEN (''1C''X,''20''X,''21''X,''24''X,''25''X) /* Space switch   99963500
* exception, AFX translation, ASX translation, primary authorization    99967000
* exception or secondary authorization exception */'                    99970500
*    ) SKIP NORESCAN;          /*                                @LFA*/ 99974000
* ANS(PREFIX(1)||'TEIV = ON; /* SDWATRAN contains a valid ASID */')     99977600
*  SKIP NORESCAN;              /*                                @LFA*/ 99982000
* ANS('WHEN (''22''X,''23''X)  /*  LX translation, EX translation */'   99982400
*    ) SKIP NORESCAN;          /*                                @LHA*/ 99982800
* ANS(PREFIX(1)||'TEPC = ON; /* SDWATRAN contains a valid PC# */')      99983200
*  SKIP NORESCAN;              /*                                @LHA*/ 99983600
* ANS('WHEN (''04''X)          /*  Protection exception           */'   99984000
*    ) SKIP NORESCAN;          /*                                @LHA*/ 99984400
* ANS('IF '||SOPI||' THEN    /* Suppress-on-protect indicated?   */')   99984800
*  SKIP NORESCAN;              /*                                @LHA*/ 99985200
* ANS(PREFIX(1)||'TEAV = ON; /* SDWATRAN contains a valid address*/')   99985600
*  SKIP NORESCAN;              /*                                @LHA*/ 99986000
* ANS('OTHER; /* SDWATRAN has no meaning */') SKIP NORESCAN; /*  @LFA*/ 99986400
* ANS('END; /* Select interrupt type */') SKIP NORESCAN; /*      @LFA*/ 99990800
* ANS('END; /* End RTMTRAN macro     */') SKIP NORESCAN; /*      @LFA*/ 99993000
*RETURN CODE(0);                   /*                            @LFA*/ 99995200
*%END;                             /* End of RTMTRAN Macro       @LFA*/ 99999600
