*%PDSL2  :  ;                                                           23000000
*                                                                       26600000
*DECLARE                                                                40600000
*  1 PDS2     BASED(PDS2PTR) BDY(HWORD),                                40700000
*                                                                       40800000
*   2 PDS2NAME    CHAR(8),               /* MEMBER NAME OR ALIAS NAME*/ 40850000
*   2 PDS2TTRP    CHAR(3),               /* TTR OF FIRST BLOCK OF       41100000
*                                           NAMED MEMBER             */ 41200000
*%IF PDSBLDL='NO'  %THEN                                                41250000
*   %GOTO PDSX2;                                                        41260000
*   2 PDS2CNCT    PTR(8),                /* CONCATENATION NUMBER OF     41300000
*                                           THE DATA SET             */ 41400000
*   2 PDS2LIBF    CHAR(1),               /* LIBRARY FLAG FIELD       */ 41500000
*%DCL PDS2LNRM CHAR;                                                    41600000
*%PDS2LNRM='''00''X';                    /* NORMAL CASE              */ 41700000
*%DCL PDS2LLNK CHAR;                                                    41800000
*%PDS2LLNK='''01''X';                    /* IF DCB OPERAND IN BLDL      41900000
*                                           MACRO INTRUCTION  WAS       42000000
*                                           SPECIFIED AS ZERO, NAME     42100000
*                                           WAS FOUND IN  LINK          42200000
*                                           LIBRARY                  */ 42300000
*%DCL PDS2LJOB CHAR;                                                    42400000
*%PDS2LJOB='''02''X';                    /* IF DCB OPERAND IN BLDL      42500000
*                                           MACRO INTRUCTION  WAS       42600000
*                                           SPECIFIED AS ZERO, NAME     42700000
*                                           WAS FOUND IN  JOB           42800000
*                                           LIBRARY                  */ 42900000
*%PDSX2 : ;                                                             42950000
*   2 PDS2INDC    CHAR(1),               /* INDICATOR BYTE           */ 43000000
*    3 PDS2ALIS BIT(1),                  /* NAME IN THE PDS2NAME FIELD  43100000
*                                           IS AN ALIAS              */ 43200000
*    3 PDS2NTTR BIT(2),                  /* NUMBER OF TTR'S IN THE      43300000
*                                           USER DATA FIELD          */ 43400000
*    3 PDS2LUSR BIT(5),                  /* LENGTH OF USER DATA         43500000
*                                           FIELD IN HALF WORDS      */ 43600000
*   2 PDS2TTRT    CHAR(3),               /* TTR OF FIRST BLOCK OF       43700000
*                                           TEXT                     */ 43800000
*    3 PDS2USRD    CHAR(1),              /* START OF VARIABLE LENGTH    43900000
*                                           USER DATA FIELD          */ 44000000
*   2 PDS2ZERO    CHAR(1),               /* ZERO                     */ 44100000
*   2 PDS2TTRN    CHAR(3),               /* TTR OF NOTE LIST OR         44200000
*                                           SCATTER/TRANSLATION         44300000
*                                           TABLE. USED FOR PROGRAMS    44400000
*                                           IN SCATTER LOAD  FORMAT     44500000
*                                           OR OVERLAY STRUCTURE        44600000
*                                           ONLY.                    */ 44700000
*   2 PDS2NL      PTR(8),                /* NUMBER OF ENTRIES IN        44800000
*                                           NOTE LIST FOR  PROGRAMS     44900000
*                                           IN OVERLAY STRUCTURE,       45000000
*                                           OTHERWISE ZERO           */ 45100000
*   2 PDS2ATR     CHAR(2),               /* TWO-BYTE PROGRAM            45200000
*                                           ATTRIBUTE FIELD          */ 45300000
*    3 PDS2ATR1    BIT(8),               /* FIRST BYTE OF PROGRAM       45400000
*                                           ATTRIBUTE FIELD          */ 45500000
*     4 PDS2RENT BIT(1),                 /* REENTERABLE              */ 45600000
*     4 PDS2REUS BIT(1),                 /* REUSABLE                 */ 45700000
*     4 PDS2OVLY BIT(1),                 /* IN OVERLAY STRUCTURE     */ 45800000
*     4 PDS2TEST BIT(1),                 /* PROGRAM TO BE TESTED -      45900000
*                                           TESTRAN                  */ 46000000
*     4 PDS2LOAD BIT(1),                 /* ONLY LOADABLE            */ 46100000
*     4 PDS2SCTR BIT(1),                 /* SCATTER FORMAT           */ 46200000
*     4 PDS2EXEC BIT(1),                 /* EXECUTABLE               */ 46300000
*     4 PDS21BLK BIT(1),                 /* IF ZERO, PROGRAM CONTAINS   46400000
*                                           MULTIPLE RECORDS WITH AT    46500000
*                                           LEAST ONE BLOCK OF TEXT.    46600000
*                                           IF ONE, PROGRAM             46700000
*                                           CONTAINS NO RLD ITEMS       46800000
*                                           AND  ONLY ONE BLOCK OF      46900000
*                                           TEXT.                    */ 47000000
*    3 PDS2ATR2    BIT(8),               /* SECOND BYTE OF PROGRAM      47100000
*                                           ATTRIBUTE FIELD          */ 47200000
*     4 PDS2FLVL BIT(1),                 /* IF ONE, THE PROGRAM CANNOT  47250000
*                                           BE PROCESSED BY THE E-LEVEL 47300000
*                                           OF THE LINKAGE EDITOR.      47350000
*                                           IF OFF, THE PROGRAM CAN BE  47400000
*                                           PROCESSED BY ANY LEVEL OF   47450000
*                                           THE LINKAGE EDITOR OR THE   47500000
*                                           BINDER.                  */ 47550000
*     4 PDS2ORG0 BIT(1),                 /* ORIGIN OF FIRST  BLOCK      47850000
*                                           OF TEXT IS ZERO.         */ 48100000
*     4 PDS2EP0  BIT(1),                 /* ENTRY POINT IS ZERO      */ 48150000
*     4 PDS2NRLD BIT(1),                 /* PROGRAM CONTAINS NO RLD     48200000
*                                           ITEMS                    */ 48500000
*     4 PDS2NREP BIT(1),                 /* PROGRAM CANNOT BE           48600000
*                                           REPROCESSED BY LINKAGE      48700000
*                                           EDITOR OR BINDER         */ 48800000
*     4 PDS2TSTN BIT(1),                 /* PROGRAM CONTAINS TESTRAN    48900000
*                                           SYMBOL CARDS             */ 49000000
*     4 PDS2LEF  BIT(1),                 /* PROGRAM CREATED BY          49100000
*                                           LINKAGE EDITOR F         */ 49200000
*     4 PDS2REFR BIT(1),                 /* REFRESHABLE PROGRAM      */ 49300000
*   2 PDS2STOR    PTR(24)  BDY(HWORD),   /* TOTAL CONTIGUOUS MAIN       49400000
*                                           STORAGE REQUIREMENT OF      49500000
*                                           PROGRAM                  */ 49600000
*   2 PDS2FTBL    FIXED(15) BDY(HWORD,2),/* LENGTH OF FIRST BLOCK OF    49700000
*                                           TEXT                     */ 49800000
