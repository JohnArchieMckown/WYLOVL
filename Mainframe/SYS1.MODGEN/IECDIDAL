*/*                                                                  */ 00104800
*/*01*   MACRO NAME - IECDIDAL                                       */ 00106000
*/*                                                                  */ 00107200
*/*01*   IDAL - TCCW Translator Indirect Address List                */ 00108400
*/*                                                                  */ 00109600
*/*01*   COPYRIGHT =                                                 */ 00110800
*/*         5740-XC6                                                 */ 00112000
*/*         THIS MACRO IS "RESTRICTED MATERIALS OF IBM"              */ 00113200
*/*         (C) COPYRIGHT IBM CORP. 1982, 1986                       */ 00114400
*/*         LICENSED MATERIALS - PROPERTY OF IBM                     */ 00115600
*/*                                                                  */ 00116800
*/*01*   STATUS = JBB2220                                            */ 00118000
*/*                                                                  */ 00119200
*/*01*   FUNCTION = This DSECT describes the IDAL list               */ 00120400
*/*                 which contains Indirect Address Words            */ 00121600
*/*                 generated by the TCCW translator in              */ 00122800
*/*                 translating the callers virtual channel          */ 00124000
*/*                 program to a real channel program.               */ 00125200
*/*                 This block supports 18 IDAWs.                    */ 00126400
*/*                                                                  */ 00127600
*/*01*   DEPENDENCIES = None                                         */ 00128800
*/*                                                                  */ 00130000
*/*01*   RESTRICTIONS = None                                         */ 00131200
*/*                                                                  */ 00132400
*/*01*   MODULE TYPE = Control Block                                 */ 00133600
*/*                                                                  */ 00134800
*/*01*   PROCESSOR =                                                 */ 00136000
*/*01*           PLS3 Compiler and assembler-H                       */ 00137200
*/*                                                                  */ 00138400
*/*01*   CHANGE ACTIVITY =                                           */ 00139600
*/*              NEW FOR VS2/020                                     */ 00140800
*/* $MAC(IECDIDAL),COMP(SC1C6):     TCCW IDAL block                  */ 00142000
*/* $D0=DCR099,JBB2110,820820,PDLE: Provide bilingual macro          */ 00143200
*/* $L1=VERS  ,HBB3310,860416,PDLC: Versioning macro into product    */ 00143800
*/*                                                                  */ 00144400
*/********************************************************************/ 00145600
*%GOTO IDALL2; /*                                                       00146800
IDAL     DSECT                                                          00150021
         SPACE 1                                                        00200021
IDALCHN  DS    A                   IDAL block chain pointer             00250000
         DS    A                   Reserved                             00300000
IDALHL   EQU   *-IDAL              IDAL header block length             00350000
IDALENTY DS    A                   Indirect address list entry          00400000
IDALEL   EQU   *-IDALENTY          IDAW length                          00450000
IDALNE   EQU   38                  number of IDAW entries- 160 byte     00500000
IDALNEL  EQU   60                  number of IDAW entries- 248 byte     00550000
IDALBL   EQU   IDALHL+IDALEL*IDALNE IDAL block length                   00600000
         MEND , */                                                      00650000
*%IDALL2 : ;                                                            00700000
* DECLARE                                                               00750000
*  1 IDAL BDY(DWORD) BASED(IDALPTR),  /* Indirect Address List block */ 00800000
*    2 IDALHL          BDY(DWORD), /* IDAL header area               */ 00850000
*      4 IDALCHN          PTR(31), /* IDAL block chain pointer       */ 00900000
*      4 *                PTR(31), /* Reserved                       */ 00950000
*    2 IDALEL(*)        BDY(WORD), /* IDAW entry area                */ 01000000
*      4 IDALENTY         PTR(31); /* IDAW entry                     */ 01050000
*/********************************************************************/ 01100000
*/*  IDAL Constants                                                  */ 01150000
*/********************************************************************/ 01200000
* DCL IDALNE  FIXED(8) CONSTANT(38); /* Number of IDAW entries-         01250000
*                                       160 byte block caller        */ 01300000
* DCL IDALNEL FIXED(8) CONSTANT(60); /* Number of IDAW entries-         01350000
*                                       248 byte block caller        */ 01400000
* DCL IDALBL  FIXED(8) CONSTANT(160);/* Block length for 38 entries  */ 01450000
* DCL IDALBLE FIXED(8) CONSTANT(248);/* Block length for 60 entries  */ 01500000
