*DECLARE                                                                15800000
*  1 Dgnb  Based(DgnbPtr) Bdy(DWord),/*                              */ 15850000
*   3 DgnbId      Char(4),           /* Control block id             */ 15900000
*   3 DgnbVer     Char(2),           /* Version number               */ 15950000
*   3 DgnbFlags   Char(1),           /* Flags                        */ 16000000
*     5 DgnbFlag1 Bit(8),            /* First flag byte              */ 16050000
*       7 DgnbDbNip Bit(1),          /* Diagnostic branch needs to be   16100000
*                                       enabled later in NIP         */ 16150000
*       7 DgnbSubpoolFilt Bit(1),    /* Subpool filtering is active  */ 16200000
*       7 DgnbKeyFilt Bit(1),        /* Key filtering is active      */ 16250000
*       7 *         Bit(5),          /* Reserved                     */ 16300000
*   3 DgnbResv1   Char(1),           /* Reserved                     */ 16350000
*   3 DgnbResv2   Char(4),           /* Reserved                     */ 16400000
*   3 DgnbFilterRoutineAddr Ptr(31), /* Address of filter routine, with 16450000
*                                       Amode bit set on             */ 16500000
*   3 DgnbZero Bdy(Word),           /* Initialize to zeros           */ 16550000
*     5 DgnbSyncCnt Fixed(15) Abnl,  /* Update synchronization count */ 16600000
*     5 * Char(2),                   /* Reserved                     */ 16650000
*     5 DgnbBits,                                                       16700000
*       7 DgnbWord1 Char(4) Bdy(Word),                                  16750000
*         9 DgnbByte1 Bit(8),                                           16800000
*           11 Dgnb_TempMC1 Bit(1), /* For temporary use             */ 16850000
