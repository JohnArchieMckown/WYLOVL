*%CBLSL2 : ;                                                            07050000
*                                                                       07100000
*  @LISTPUSH;                                                           07150000
*  %IF IHALIST='YES' %THEN                                              07200000
*     %GOTO CBLLIST;                                                    07250000
*  @LISTOFF;                                                            07300000
*  %CBLLIST:;                                                           07350000
*                                                                       07400000
* %IF CBLS_BASE = '' %THEN           /*                         @LKA*/  07416600
* %DO;                               /*                         @LKA*/  07433200
*   %CBLS_BASE = 'SVTCBLS';          /*                         @LKA*/  07449800
* %END;                              /*                         @LKA*/  07466400
* DCL 1 CBLS BASED(CBLS_BASE),       /*                         @LKC*/  07483000
*        2 CBLSCBLS CHAR(4),         /* CBLS ACRONYM                */  07500000
*        2 CBLR004  CHAR(2),         /* RESERVED                @LKC*/  07533300
*        2 CBLCBLSL FIXED(15),       /* Length of the CBLS      @LKC*/  07566600
*        2 CBLASCB  FIXED(15),       /* LENGTH OF THE ASCB          */  07600000
*        2 CBLASCBL FIXED(15),       /* ASCB LEVEL NUMBER           */  07650000
*        2 CBLTCB   FIXED(15),       /* TOTAL LENGTH OF THE TCB         07700000
*                                       (PREFIX + MAIN + EXTENSION) */  07750000
*        2 CBLTCBP  FIXED(15),       /* LENGTH OF THE TCB PREFIX    */  07800000
*        2 CBLTCBM  FIXED(15),       /* LENGTH OF MAIN SECTION OF       07850000
*                                       THE TCB                     */  07900000
*        2 CBLTCBL  FIXED(15),       /* TCB LEVEL NUMBER            */  07950000
*        2 CBLIHSA  FIXED(15),       /* LENGTH OF THE IHSA          */  08000000
*        2 CBLIHSAL FIXED(15),       /* IHSA LEVEL NUMBER           */  08050000
*        2 CBLWPRB  FIXED(15),       /* LENGTH OF THE WPRB          */  08100000
*        2 CBLWPRBL FIXED(15),       /* WPRB LEVEL NUMBER           */  08150000
*        2 CBLXSB   FIXED(15),       /* LENGTH OF THE XSB           */  08200000
*        2 CBLXSBL  FIXED(15),       /* XSB LEVEL NUMBER            */  08250000
*        2 CBLSTKH  FIXED(15),       /* LENGTH OF THE STACK HEADER  */  08300000
*        2 CBLSTKHL FIXED(15),       /* STKH LEVEL NUMBER           */  08350000
*        2 CBLASXB  FIXED(15),       /* LENGTH OF THE ASXB          */  08400000
*        2 CBLASXBL FIXED(15),       /* ASXB LEVEL NUMBER           */  08450000
*        2 CBLSTCB  FIXED(15),       /* LENGTH OF THE STCB          */  08500000
*        2 CBLSTCBL FIXED(15),       /* STCB LEVEL NUMBER           */  08550000
*        2 CBLVSSA  FIXED(15),       /* LENGTH OF STATIC PORTION        08600000
*                                       OF VSSA                     */  08650000
*        2 CBLVSSAL FIXED(15),       /* VSSA LEVEL NUMBER           */  08700000
*        2 CBLASSB  FIXED(15),       /* LENGTH OF THE ASSB      @D1A*/  08750000
*        2 CBLASSBL FIXED(15),       /* ASSB LEVEL NUMBER       @D1A*/  08800000
