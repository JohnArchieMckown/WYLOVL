         AGO   .USREND                                                  00242000
.USRERR1 MNOTE 8,'VALUE FOR USRCTL= PARAMETER IS TOO LONG'              00243000
.USREND  ANOP                                                           00244000
         SPACE 1                                              @E1402SC  00245000
         AIF   (K'&SPOOLCL GT 1).SPLERR1                      @E1402SC  00246000
         AIF   ('&SPOOLCL' EQ '').SPLCL1                      @E1402SC  00247000
         AIF   ('&SPOOLCL' EQ '*').SPLCL2                     @E1402SC  00248000
