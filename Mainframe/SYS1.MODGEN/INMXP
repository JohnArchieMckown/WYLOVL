         AGO   .LOGS2                                                   00206000
.LOGS1   ANOP                                                           00207000
&I       SETA  5                                                        00208000
         DC    CL10'.LOG.'         DEFAULT LOG SELECTOR                 00209000
.LOGS2   ANOP                                                           00210000
         DC    H'&I.'              LOG SELECTOR LENGTH                  00211000
         AGO   .LGSEND                                                  00212000
.LGSERR1 MNOTE 8,'VALUE FOR LOGSEL= PARAMETER IS TOO LONG'              00213000
.LGSEND  ANOP                                                           00214000
         SPACE                                                          00215000
         AIF   (K'&SYSCTL GT 44).SYSCER1                                00216000
