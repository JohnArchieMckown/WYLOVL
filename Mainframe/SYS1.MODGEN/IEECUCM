*                                                                       56994000
*DECLARE                                                                57021600
*  1 UCMPRFX BASED (UCMMCSPT) BDY(WORD), /* MCS UCM PREFIX START     */ 57049200
*   2 UCMMCENT    PTR(31),          /* ADDRESS OF MASTER CONSOLE UCM    57076800
*                                      ENTRY                         */ 57104400
*   2 UCMSAVE0    CHAR(72) BDY(WORD), /* RESIDENT REGISTER SAVE AREA    57132000
*                                      FOR IEACVTSK                  */ 57159600
