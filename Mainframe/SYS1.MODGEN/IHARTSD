*                       USE THE FIELDS.                          @P1A*/ 03783600
*          5 RTSDSRPL CHAR(40),   /* STOP/RESET PARAMETER LIST   @P1A*/ 03783700
*          5 RTSDSRRG CHAR(64),   /* AREA TO SAVE REGS           @P1A*/ 03783800
*          5 RTSDSPSW CHAR(8),    /* PSW FOR RESET               @P1A*/ 03784000
*          5 RTSDSRSA CHAR(72),   /* SAVE AREA FOR CALLING S/R   @P1A*/ 03784200
*          5 RTSDSROA CHAR(12),   /* STOP/RESET OUTPUT AREA      @P1A*/ 03784400
*          5 RTSDSRXM CHAR(8),    /* XM CONTROL REGS             @P1A*/ 03784600
*          5 RTSDSRFL BIT(32) ABNL, /* FLAGS                     @P1A*/ 03784800
*            7 RTSDSTOC BIT(1),   /* STOP DONE                   @P1A*/ 03785000
*            7 RTSDRSET BIT(1),   /* RESET DONE, C/S SERIALIZED  @P1A*/ 03785200
*            7 RTSDSTOS BIT(1),   /* STOP PROCESS STARTED        @P1A*/ 03785300
*            7 * BIT(29),         /* RESERVED                    @P1A*/ 03785400
*          5 RTSDSRLS CHAR(4),    /* SAVE CR15 - LINKAGE STACK   @P7A*/ 03785500
*          5 * CHAR(4),           /* RESERVED                    @P7C*/ 03785600
*        3 * CHAR(20),            /* RESERVED                    @LFC*/ 03788600
*        3 RTSDSLRP BDY(DWORD),   /* SLIP reg/PSW area. Must map same   03791600
*                                    as SCVADPLS                 @LFA*/ 03794600
*          5 RTSDSLPW CHAR(8),    /* SLIP TRAP PSW           @G860P31*/ 03797600
*          5 RTSDSLC3 FIXED(31),  /* SLIP TRAP CR3 (SASID)   @G860P31*/ 03800600
*          5 RTSDSLC4 FIXED(31),  /* SLIP TRAP CR4 (PASID)   @G860P31*/ 03803600
*          5 RTSDSLRG(16) CHAR(4),/* SLIP TRAP REGS          @G860P31*/ 03806600
*          5 RTSDSLAR(16) CHAR(4),/* SLIP TRAP ACCESS REGS       @L3A*/ 03809600
*          5 RTSDSLCR CHAR(64),   /* SLIP TRAP CONTROL REGS      @L3A*/ 03812600
*          5 RTSDSLG6 CHAR(64) BDY(WORD), /* SLIP G64H           @LFA*/ 03815600
*          5 RTSDC64S CHAR(128) BDY(DWORD); /* ESAME CRs         @LFA*/ 03819000
* DCL RTSDRSON BIT(32) CONSTANT('40000000'X);  /* TURN ON RTSDRSET      03822400
*                                                                @P1A*/ 03824700
* DCL RTSDRSOF BIT(32) CONSTANT('BFFFFFFF'X);  /* TURN OFF RTSDRSET     03827000
*                                                                @P1A*/ 03829300
*      /* SDUMP EXIT TABLE                                   @G386P31*/ 03831600
*  DCL 1  RTSDEXIT(*) BASED(RTSDEXTB), /* SDUMP EXIT TABLE   @G386P31*/ 03834000
*       2 RTSDEXFL CHAR(4),    /* EXIT FLAGS                 @G386P31*/ 03880800
*        3 RTSDEXMS BIT(16),   /* EXIT MASK USED TO IDENTIFY THE EXIT   03927600
*                                 WITH AN SDUMP PARAMETER    @G386P31*/ 03974400
*        3 * CHAR(1),          /* RESERVED                   @G386P31*/ 04021200
*        3 RTSDEXAT BIT(8),    /* EXIT ATTRIBUTES            @G386P31*/ 04068000
*         4 RTSDEXLC BIT(1),   /* ON FOR LOCAL EXIT          @G386P31*/ 04114800
*         4 RTSDEXGB BIT(1),   /* ON FOR GLOBAL EXIT         @G386P31*/ 04161600
*         4 RTSDEXSD BIT(1),   /* ON FOR SDUMP EXIT          @G38FP31*/ 04208400
*         4 RTSDEXSM BIT(1),   /* ON FOR SYSMDUMP EXIT       @G38FP31*/ 04239600
*         4 RTSDEXON BIT(1),   /* ON FOR ONE TIME ONLY EXIT  @ZMB0360*/ 04270800
*         4 RTSDXDFP BIT(1),   /* ON FOR DFP EXIT - CHECK THAT          04278600
*                                 SMSX IS INSTALLED              @L7A*/ 04286400
*         4 RTSDEXEG BIT(1),   /* ON FOR EARLY GLOBAL EXIT       @P6A*/ 04291600
*         4 RTSDEXNC BIT(1),   /* ON FOR NUCLEUS RESIDENT EXIT   @LDA*/ 04296800
*       2 RTSDEXAD PTR(31);    /* EXIT LOAD MODULE ADDRESS   @G386P31*/ 04302000
*      /* ADDRESS RANGE MAPPING FOR LISTA                            */ 04350000
*  DCL 1 RTSDRANG BASED(RTSDLSTN), /* ADDRESS RANGE FOR LISTA        */ 04400000
*      3  RTSDBADR  PTR(31),   /*BEGINNING ADDRESS                   */ 04450000
