*%GOTO UIP00;                 /* Bilingual mapping                      00010002
         MACRO                   Unit Information Parameters            00020002
         CBDZUIP &TYPE=GEN,&PREFIX=,&SIM=0,&DFLT=0,&SEL=0,&MLTS=1,     *00030002
               &VMSIM=0,&VMSEL=0,&VMDFLT=0,&SYSTEM=MVS             @LDA 00040002
.*                                                                      00041002
.*             Global Definitions                                       00042002
.*                                                                      00043002
         GBLB  &SKIP                   Prolog skip switch               00044002
.*                                                                      00045002
         AIF   (&SKIP).M000                                             00046002
         AIF   ('&TYPE' EQ 'GEN').M000                                  00047002
&SKIP    SETB  1                       Set switch indicating that we    00048002
.*                                     have printed the prolog          00049002
.*                                                                   */ 00049102
*%UIP00:;                                                               00049202
*/********************************************************************/ 00049302
*/* START OF SPECIFICATIONS                                          */ 00049402
*/*                                                                  */ 00049502
*/*  ACRONYM:  CBDZUIP                                               */ 00049602
*/*  EXTERNAL CLASSIFICATION:  PSPI                                  */ 00049702
*/*  END OF EXTERNAL CLASSIFICATION                                  */ 00049802
*/*  MACRO NAME:  CBDZUIP                                            */ 00049902
*/*  DSECT NAME:  UIP                                                */ 00050002
*/*  DESCRIPTIVE NAME:  Unit Information Parameters                  */ 00051002
*/*                                                                  */ 00052002
*/*  PROPRIETARY STATEMENT =                                         */ 00053002
*/*                                                                  */ 00054002
*/*     LICENSED MATERIALS - PROPERTY OF IBM                         */ 00055002
*/*     THIS MACRO IS "RESTRICTED MATERIALS OF IBM"                  */ 00056002
*/*     5647-A01 (C) COPYRIGHT IBM CORP. 1990, 2000                  */ 00057008
*/*                                                                  */ 00058002
*/*  STATUS = HCS6091                                                */ 00059008
*/*  COMPONENT: Hardware Configuration Definition (SC1XL)            */ 00059102
*/*  EYE CATCHER: UIP                                                */ 00059202
*/*  OFFSET: 0                                                       */ 00059302
*/*  LENGTH: 4                                                       */ 00059402
*/*  STORAGE ATTRIBUTES:                                             */ 00059502
*/*  MAIN STORAGE: Obtained by UIM                                   */ 00059602
*/*  RESIDENCY: Determined by caller                                 */ 00059702
*/*  SIZE: 232 bytes                                                 */ 00059802
*/*  CREATED BY: UIM                                                 */ 00059902
*/*  POINTED TO BY: N/A                                              */ 00060002
*/*  SERIALIZATION: NONE                                             */ 00061002
*/*  FUNCTION:  Maps the Unit Information Parameters.                */ 00062002
*/*                                                                  */ 00063002
*/*  MODULE TYPE = Bilingual Mapping Macro                           */ 00064002
*/*                                                                  */ 00065002
*/*  DEPENDENCIES = None                                             */ 00066002
*/*                                                                  */ 00067002
*/*  RESTRICTIONS = For HCD and UIM use only                         */ 00068002
*/*                                                                  */ 00069002
*/*  PROCESSOR = Assembler H and PL/AS Compiler                      */ 00069102
*/*                                                                  */ 00069202
*/*  INVOCATION:                                                     */ 00069302
*/*     PLAS:     %DCL UIPDEF CHAR                                   */ 00069402
*/*               %UIPDEF='value'                                    */ 00069502
*/*                   value = 'BDY(WORD)', 'BASED' or                */ 00069602
*/*                           'BASED(xxxxxxxx)'                      */ 00069702
*/*               %DCL UIPMVSDF CHAR                             @LDA*/ 00069802
*/*               %UIPMVSDF='value'                              @LDA*/ 00069902
*/*                   value = 'YES' or 'NO'                      @LDA*/ 00070002
*/*               If not specified, YES is assumed.              @LDA*/ 00071002
*/*                                                              @LDA*/ 00072002
*/*               %DCL UIPVMDEF CHAR                             @LDA*/ 00073002
*/*               %UIPVMDEF='value'                              @LDA*/ 00074002
*/*                   value = 'YES' or 'NO'                      @LDA*/ 00075002
*/*               If not specified, NO is assumed.               @LDA*/ 00076002
*/*                                                              @LDA*/ 00077002
*/*               %INCLUDE SYSLIB(CBDZUIP)                           */ 00078002
*/*                                                                  */ 00079002
*/*               If UIPDEF is not declared and initialized, the     */ 00079102
*/*               default is 'BDY(WORD)'.                            */ 00079202
*/*                                                                  */ 00079302
*/*     ASSEMBLER                                                    */ 00079402
*/*                CBDZUIP TYPE=GEN|DSECT                            */ 00079502
*/*                        ,PREFIX=nnn                               */ 00079602
*/*                        ,SIM=n                                    */ 00079702
*/*                        ,MLTS=1                                   */ 00079802
*/*                        ,SEL=n                                    */ 00079902
*/*                        ,DFLT=n                                   */ 00080002
*/*                        ,VMSIM=n                              @LDA*/ 00081002
*/*                        ,VMSEL=n                              @LDA*/ 00082002
*/*                        ,VMDFLT=n                             @LDA*/ 00083002
*/*                        ,SYSTEM=MVS|VM|ALL                    @LDA*/ 00084002
*/*                                                                  */ 00085002
*/*                 TYPE   specifies the type of request             */ 00086002
*/*                        GEN     causes the generation of the UIP  */ 00087002
*/*                                structure. This is the default    */ 00088002
*/*                                when the parameter is omitted.    */ 00089002
*/*                        DSECT   causes the inclusion of various   */ 00089102
*/*                                DSECTs defining the similar device*/ 00089202
*/*                                list, default parameter list and  */ 00089302
*/*                                parameter selection list          */ 00089402
*/*                 PREFIX  specifies an upto 3 character constant   */ 00089502
*/*                         used as prefix when generating the field */ 00089602
*/*                         labels. The default is UIP.              */ 00089702
*/*                 SIM     specifies the number of entries to be    */ 00089802
*/*                         generated in the similar device list.    */ 00089902
*/*                         This list indentifies devices which are  */ 00090002
*/*                         "lookalikes" of the named device. This   */ 00090102
*/*                         parameter is optional, the default is 0. */ 00090202
*/*                 MLTS    specifies the number of MLT list entries */ 00090302
*/*                         to be generated. The default is one.     */ 00090402
*/*                         Set to number required if different.     */ 00090502
*/*                 SEL     specifies the number of entries to be    */ 00090602
*/*                         generated in the parameter selection list*/ 00090702
*/*                         This parameter is optional, the default  */ 00090802
*/*                         is 0.                                    */ 00090902
*/*                 DFLT    specifies the number of entries to be    */ 00091002
*/*                         generated in the parameter default list. */ 00091102
*/*                         This parameter is optional, the default  */ 00091202
*/*                         is 0.                                    */ 00091302
*/*                 VMSIM   specifies the number of entries to be    */ 00091402
*/*                         generated in the similar device list (VM)*/ 00091502
*/*                         This list indentifies devices which are  */ 00091602
*/*                         "lookalikes" of the named device. This   */ 00091702
*/*                         parameter is optional, the default is 0. */ 00091802
*/*                 VMSEL   specifies the number of entries to be    */ 00091902
*/*                         generated in the parameter selection list*/ 00092002
*/*                         This parameter is optional, the default  */ 00092102
*/*                         is 0.                                    */ 00092202
*/*                 VMDFLT  specifies the number of entries to be    */ 00092302
*/*                         generated in the parameter default list. */ 00092402
*/*                         This parameter is optional, the default  */ 00092502
*/*                         is 0.                                    */ 00092602
*/*                 SYSTEM  specifies which sections of the UIP have */ 00092702
*/*                         to be generated.                         */ 00092802
*/*                         The possible values are MVS or VM.       */ 00092902
*/*                         This parameter is optional, the default  */ 00093002
*/*                         is MVS.                                  */ 00093102
*/*                                                                  */ 00093202
*/*                                                                  */ 00093302
*/*  CHANGE ACTIVITY =                                               */ 00093402
*/*    $L0=HCD,    HCSH101 900426 BOEB: Hardware Configuration Dialog*/ 00093502
*/*    $M1=HCD,    HCSH201 900921 BOEB: Support for device grouping  */ 00093602
*/*    $LD=HCD,    HCSH301 910423 BOEB: VM Device Support            */ 00093702
*/*    $LT=CF,     HCSH501 930323 BOEB: Support for Coupling Facility*/ 00093802
*/*    $L1=UCBVSCR,HCSH521 931020 BOEB: UCB VSCR                     */ 00093902
*/*    $H4=OSA,    HCSH501 940210 BOEB: Open Systems Adapter Support */ 00094002
*/*                                     - Include UIPGPFlg           */ 00094102
*/*    $L2=CSTAR,  HCSH521 940527 BMGU: Hardware Support             */ 00094202
*/*    $H1=SHARK,  HCS6051 971020 BKRE: SHARK Support                */ 00094305
*/*                                                                  */ 00094404
*/* END OF SPECIFICATIONS                                            */ 00094504
*/********************************************************************/ 00094604
*%GOTO UIP01;                         /*                                00094704
.*                                                                      00094804
.*             Local Definitions                                        00094904
.*                                                                      00095004
.M000    ANOP                                                           00095104
         LCLC  &P                  Label generation prefix              00095204
         LCLA  &NDX                Parameter list count            @LDA 00095304
         LCLB  &SYSVM              Indicator for VM section        @LDA 00095404
         LCLB  &SYSMVS             Indicator for MVS section       @LDA 00095504
.*                                                                      00095604
&P       SETC  'UIP'               Default prefix for UIP               00095704
         AIF   ('&PREFIX' EQ '').M005   PREFIX parameter coded ?        00095804
&P       SETC  '&PREFIX'                take user's specified prefix    00095904
         AIF   (K'&PREFIX LE 3).M005    If the user specified prefix is 00096004
&P       SETC  '&PREFIX'(1,3)           > 3 -> take only the first      00096104
.*                                      three characters                00096204
.M005    ANOP                                                           00096304
         SPACE 2                                                        00096404
.********************************************************************** 00096504
.*                                                                    * 00096604
.*    Check SYSTEM values                                         @LDA* 00096704
.*                                                                    * 00096804
.*******************************************************************FB* 00096904
         SPACE 1                                                        00097004
&NDX     SETA  1                   Start value for SYSTEM values   @LDA 00097104
.SYSLOOP ANOP                      Loop on SYSTEM values           @LDA 00097204
         AIF   ('&SYSTEM(&NDX)' EQ 'VM').SETVM Generate VM section @LDA 00097304
         AIF   ('&SYSTEM(&NDX)' EQ 'MVS').SETMVS  Gen MVS section  @LDA 00097404
         AIF   ('&SYSTEM(&NDX)' EQ 'ALL').SETALL  Gen all sections @LDA 00097504
         AGO   .SYSNEXT            Look for further values         @LDA 00097604
.SETVM   ANOP                      Request for VM section          @LDA 00097704
&SYSVM   SETB  1                   Indicate VM section             @LDA 00097804
         AGO   .SYSNEXT            Look for further values         @LDA 00097904
.SETMVS  ANOP                      Request for MVS section         @LDA 00098004
&SYSMVS  SETB  1                   Indicate MVS section            @LDA 00098104
         AGO   .SYSNEXT            Look for further values         @LDA 00098204
.SETALL  ANOP                      Request for ALL section         @LDA 00098304
&SYSMVS  SETB  1                   Indicate MVS section            @LDA 00098404
&SYSVM   SETB  1                   Indicate VM section             @LDA 00098504
         AGO   .SYSNEXT            Look for further values         @LDA 00098604
.SYSNEXT ANOP                      Proceed to next value           @LDA 00098704
         AIF   (&NDX GE N'&SYSTEM).SYSEND No further SYSTEM value  @LDA 00098804
&NDX     SETA  &NDX+1              Increment value count           @LDA 00098904
         AGO   .SYSLOOP            Process next SYSTEM value       @LDA 00099004
.SYSEND  ANOP                      SYSTEM parameter processed      @LDA 00099104
.*                                                                      00099204
         EJECT                                                          00099304
         AIF ('&TYPE' NE 'GEN').M100                                    00099404
         SPACE 2                                                        00099504
*********************************************************************** 00099604
*                                                                     * 00099704
*     Unit Information Parameters                                     * 00099804
*                                                                     * 00099904
********************************************************************WS* 00100004
         SPACE 1                                                        00100104
&P       DS    0F                  Unit Information Parameters          00100204
         SPACE 2                                                        00100304
*********************************************************************** 00100404
*                                                                     * 00100504
*         UIP Header                                                  * 00100604
*                                                                     * 00100704
*********************************************************************** 00100804
         SPACE 1                                                        00100904
&P.GHDS  DS    0F                  UIP Header Section                   00101004
         SPACE 1                                                        00101104
&P.GID   DC    CL4'UIP '           Control block identifier ("UIP ")    00101204
&P.GVER  DC    AL1(&P.GVER1)       UIP version number                   00101304
&P.GVER1 EQU   X'01'               .. Version 1 of UIP                  00101404
         DC    XL1'00'             Reserved                             00101504
&P.GELEN DC    AL2(&P.GELN1)       Entire Length of UIP                 00101604
         SPACE 2                                                        00101704
*********************************************************************** 00101804
*                                                                     * 00101904
*         General Section                                             * 00102004
*                                                                     * 00102104
*********************************************************************** 00102204
         SPACE 1                                                        00102304
&P.GENS  DS    0F                  UIP General Section                  00102404
         SPACE 2                                                        00102504
*********************************************************************** 00102604
*         Header of General Section                                   * 00102704
*********************************************************************** 00102804
         SPACE 1                                                        00102904
&P.GLEN  DC    AL2(&P.GLN1)        Length of general section            00103004
&P.GTYP  DC    AL1(&P.GEN)         Type of section                      00103104
&P.GEN   EQU   X'FF'               .. 255 for general section           00103204
         DC    XL1'00'             Reserved                             00103304
         SPACE 2                                                        00103404
*********************************************************************** 00103504
*         Body of General Section                                     * 00103604
*********************************************************************** 00103704
         SPACE 1                                                        00103804
&P.GUNMD DS    0CL12               Unit and model                       00103904
&P.GUNIT DC    CL8' '              Unit name                            00104004
&P.GMODL DC    XL4'00'             Model number (must be binary zero    00104104
*                                  if model is not specified)           00104204
         SPACE 1                                                        00104304
&P.GDESI DC    AL1(0)              Index to unit description in         00104404
*                                      associated UDT                   00104504
         SPACE 1                                                        00104604
&P.GGRP  DC    AL1(0)              Device type grouping                 00104704
&P.GDASD EQU   1                   .. DASD                              00104804
&P.GTAPE EQU   2                   .. Tape                              00104904
&P.GCLUS EQU   3                   .. Cluster Controller                00105004
&P.GCOMM EQU   4                   .. Communications                    00105104
&P.GMICR EQU   5                   .. MICR/OCR                          00105204
&P.GGRPH EQU   6                   .. Graphics                          00105304
&P.GPRT  EQU   7                   .. Printer                           00105404
&P.GCARD EQU   8                   .. Card reader/punch                 00105504
&P.GDISP EQU   9                   .. Display                           00105604
&P.GTPRT EQU   10                  .. Terminal printer                  00105704
&P.GOTHR EQU   255                 .. Other                             00105804
         DC    XL2'00'             Reserved                             00105904
         SPACE 1                                                        00106004
&P.GUSER DC    A(0)                UIM user value for device            00106104
         SPACE 1                                                        00106204
&P.GDFLT DS    0C                  Default values for replication       00106304
*                                     information                       00106404
&P.GDDRF DC    AL2(0)              Default replication factor           00106504
&P.GDLRF DC    AL2(0)              Minimum replication factor           00106604
&P.GDHRF DC    AL2(0)              Maximum replication factor           00106704
*                                  (if binary zero, the value enforced  00106804
*                                       by the dialog, is taken)        00106904
&P.GDNC  DC    AL2(0)              Count of device numbers to           00107004
*                                       generate for each device if     00107104
*                                       multiple-exposure device        00107204
&P.GDNI  DC    AL2(0)              Interval between device              00107304
*                                      numbers when multiple device     00107404
*                                      numbers are generated for the    00107504
*                                      same device (valid only when     00107604
*                                      the value of UIPDNC is greater   00107704
*                                      than one)                        00107804
&P.GRFLG DC    XL1'00'             Replication factor flags             00107904
&P.GFMEX EQU   X'80'               .. device is multi-exposure device   00108004
&P.GFGRP EQU   X'40'               .. device builds a group of devices *00108104
                                                                   @M1A 00108204
&P.GFPAV EQU   X'20'               .. If set, indicates that device     00108306
*                                     is a PAV device              @H1C 00108406
&P.GFBAS EQU   X'10'               .. If set, indicates that device     00108606
*                                     is a PAV-base device.             00108706
*                                     UIPGFPAV is also set.        @H1A 00108806
&P.GFALI EQU   X'08'               .. If set, indicates that device     00108906
*                                     is a PAV-alias device.            00109006
*                                     UIPGFPAV is also set.        @H1A 00109106
&P.GPFLG DC    XL1'00'             Processing flag                 @H4A 00109204
&P.GFIGN EQU   X'80'               .. device to be ignored by NOCHECK  *00109304
                                                                   @H4A 00109404
         SPACE 1                                                        00109504
&P.GDFLG DC    AL1(0)              Default Flag byte                    00109604
&P.GFTOU EQU   X'80'               .. TIMEOUT=NO is default             00109704
&P.GFSTA EQU   X'40'               .. STADET=NO  is default             00109804
&P.GFDMD EQU   X'01'               .. Device model is default           00109904
         DC    XL3'00'             Reserved                             00110004
         SPACE 1                                                        00110104
&P.GATT  DS    0CL8                Attachment information               00110204
&P.GMNCU DC    AL2(0)              maximum number of CUs a device can   00110304
*                                  be attached to                       00110404
*                                  (if binary zero, the value enforced  00110504
*                                  by the dialog, is taken)             00110604
         DC    XL6'00'             Reserved for attachment info    @LTC 00110704
&P.GPR   DC    XL1'00'             Processing Flags                @LTA 00110804
&P.GFINT EQU   X'80'               .. Internal UIT to be built     @LTA 00110904
         DC    XL15'00'            Reserved                        @LTC 00111004
         SPACE 1                                                        00111104
&P.GLN1  EQU   *-&P.GENS           Length of general section            00111204
         SPACE 2                                                        00111304
.MVSGEN  ANOP                                                      @LDA 00111404
         AIF   (NOT &SYSMVS).VMGEN No MVS section to be generated? @LDA 00111504
*********************************************************************** 00111604
*                                                                     * 00111704
*         MVS Section                                                 * 00111804
*                                                                     * 00111904
*********************************************************************** 00112004
         SPACE 1                                                        00112104
&P.MVSS  DS    0F                  UIP MVS Section                      00112204
         SPACE 2                                                        00112304
*********************************************************************** 00112404
*         Header of MVS Section                                       * 00112504
*********************************************************************** 00112604
         SPACE 1                                                        00112704
&P.MLEN  DC    AL2(&P.MLN1)        Length of MVS section                00112804
&P.MTYP  DC    AL1(&P.MVS)         Type of section                      00112904
&P.MVS   EQU   X'00'               .. 0 for MVS section                 00113004
         DC    XL1'00'             Reserved                             00113104
         SPACE 1                                                        00113204
*********************************************************************** 00113304
*         Body of MVS Section                                         * 00113404
*********************************************************************** 00113504
&P.MGNNM DC    CL8' '              Generic name of device               00113604
         SPACE 1                                                        00113704
&P.MSIMC DC    F'&SIM.'            Count of similar device list entries 00113804
&P.MSIMP DC    A(&P.SIMIL)         Pointer to similar device list       00113904
*                                  entries                              00114004
         SPACE 1                                                        00114104
&P.MRPRM DC    XL8'00'             Map of parameters that are           00114204
*                                      required (maps the parameters    00114304
*                                      in the same way as they are      00114404
*                                      mapped in the IODEVICE internal  00114504
*                                      text record field IODVPRMS)      00114604
&P.MOPRM DC    XL8'00'             Map of parameters that are           00114704
*                                      optional for this device (maps   00114804
*                                      the parameters in the same way   00114904
*                                      as they are mapped in the        00115004
*                                      internal text record field       00115104
*                                      IODVPRMS)                        00115204
&P.MSFEA DC    XL8'00'             Map of features that are             00115304
*                                      supported for this unit and      00115404
*                                      model (bits map into features    00115504
*                                      listed in UDTFEATL in associated 00115604
*                                      UDT - valid only if IODVFFEA     00115704
*                                      flag within UIPOPARM is set)     00115804
&P.MDFEA DC    XL8'00'             Map of features that are             00115904
*                                      default for this unit and        00116004
*                                      model (bits map into features    00116104
*                                      listed in UDTFEATL in associated 00116204
*                                      UDT - valid only if IODVFFEA     00116304
*                                      flag within UIPOPARM is set)     00116404
&P.MCFEA DC    XL8'00'             Map of features that are             00116504
*                                      recognized for migration         00116604
*                                      compatibility (bits map into     00116704
*                                      features listed in UDTCOMPL      00116804
*                                      in associated UDT - valid only   00116904
*                                      if IODVFFEA flag within          00117004
*                                      UIPOPARM is set)                 00117104
         DC    XL8'00'             Reserved                             00117204
         SPACE 1                                                        00117304
&P.MDFLG DC    AL1(0)              Default Flag byte                    00117404
&P.MFOFF EQU   X'80'               .. OFFLINE=YES is default            00117504
&P.MFGEN EQU   X'40'               .. UIM determines which generic      00117604
*                                     name is taken after feature       00117704
*                                     check (only for tapes)            00117804
&P.MFDMD EQU   X'20'               .. MODEL= is default ( value is      00117904
*                                     taken from MODL )                 00118004
&P.MFLG2 DC    AL1(0)              Flag byte 2                          00118104
&P.MFDYC EQU   X'80'               .. device supports dynamic           00118204
*                                     configuration                     00118304
&P.MFDVN EQU   X'40'               .. device supports device            00118404
*                                     number > 4095                     00118504
&P.MFDSE EQU   X'20'               .. device allows statistics          00118604
*                                     table entry to reside in          00118704
*                                     31 bit storage                    00118804
&P.MFDCX EQU   X'10'               .. device allows device              00118904
