*%PLSUFO:;                /* PLS VERSION */                             13550000
*DCL   ADRUFID          CHAR  (4)   /* IDENTIFIER                    */ 13600000
*      CONSTANT('UFO ');                                                13650000
*DCL   ADRUFO@          PTR   (31); /* ADRUFO BASING POINTER     @V21*/ 13700000
*DCL 1 ADRUFOB BASED(ADRUFO@) BDY(WORD),                                13750000
*      2 UFOHDR,                    /* HEADER OF UFO PARM LIST       */ 13800000
*        3 UFID         CHAR  (4),  /* IDENTIFIER EBCDIC "UFO "      */ 13850000
*        3 UFLEN        FIXED (15), /* LENGTH OF PARM LIST           */ 13900000
*        3 UFBDYOFF     FIXED (15), /* OFFSET TO UFOFUNCT OR UFOPARM */ 13950000
*        3 UFVOLI@      PTR   (31), /* ADDR OF INPUT VOL LIST  | 0   */ 14000000
*        3 UFVOLO@      PTR   (31), /* ADDR OF OUTPUT VOL LIST  | 0  */ 14050000
*        3 UFFUNCT      BIT  (16),  /* FUNCTION BEING PERFORMED      */ 14100000
*                                   /* '0000'X = PARM CHANGE ENTRY   */ 14150000
*                                   /* (THIS FIELD MUST BE KEPT IN   */ 14200000
*                                   /*  SYNCHRONIZATION WITH FBMAJOR */ 14250000
*                                   /*  IN ADRFUNCB)          @V23DTK*/ 14300000
*          4 UFFUNCT1   BIT   (8),  /* FUNCTION BYTE ONE             */ 14350000
*            5 UFFUDEF  BIT   (1),  /* 1 = DEFRAG OPERATION          */ 14400000
*            5 UFFUCOPY BIT   (1),  /* 1 = COPY OPERATION            */ 14450000
*            5 UFFUREST BIT   (1),  /* 1 = RESTORE OPERATION         */ 14500000
*            5 UFFUDUMP BIT   (1),  /* 1 = DUMP OPERATION            */ 14550000
*            5 UFFUPRT  BIT   (1),  /* 1 = PRINT OPERATION           */ 14600000
*            5 UFFUCPYD BIT   (1),  /* 1 = COPYDUMP OPERATION        */ 14650000
