* %DCL SC_BASE CHAR INTERNAL;                                           05300000
* %IF DAS_PL = 'N' %THEN %GOTO SK_SPL;                                  05316600
* %IF DAS_PL = 'NO' %THEN %GOTO SK_SPL;                                 05333200
* %SC_BASE = 'BASED(DASCRPTR)';                                         05350000
* %IF DAS_BASE = 'N' %THEN                                              05400000
*   %SC_BASE = '';                                                      05450000
* %IF DAS_BASE = 'NO' %THEN                                             05500000
*   %SC_BASE = '';                                                      05550000
* %IF DAS_PLL = 'N' %THEN %GOTO NOLIST_SPL;                             05556200
* %IF DAS_PLL = 'NO' %THEN %GOTO NOLIST_SPL;                            05562400
* %GOTO LIST_SPL;                                                       05568600
* %NOLIST_SPL: ;                                                        05574800
*@LIST PUSH;                                                            05581000
*@LIST OFF;                                                             05587200
* %LIST_SPL: ;                                                          05593400
*DECLARE                                                                05600000
* /*******************************************************************/ 05627100
* /*  COMMON SECTION                                                 */ 05654200
* /*******************************************************************/ 05681300
* 1 DASCR SC_BASE,           /* PARAMETER LIST MAPPING               */ 05708400
*  2 DASPLID  CHAR(8),       /* ID = 'IGGDASCR'                      */ 05735500
*  2 DASPVER FIXED(15),      /* VERSION OF PARAMETER LIST            */ 05762600
*  2 DASPLEN  FIXED(15),     /* LENGTH OF PARAMETER LIST             */ 05789700
*  2 DASPKEY  BIT(8),        /* KEY OF PARAMETER LIST                */ 05816800
*   3 DASPSKEY BIT(4),       /* STORAGE KEY OF PARAMETER LIST        */ 05843900
*   3 DASSKRSV BIT(4),       /* RESERVED                             */ 05871600
*  2 DASRSRV1 FIXED(8),      /* RESERVED                             */ 05919300
*  2 DASHRTCD FIXED(15),     /* HISTORIC SCRATCH RETURN CODE         */ 05967000
*  2 DASDIAGI FIXED(32),     /* DIAGNOSTIC INFORMATION               */ 06014700
*   /* REFER TO DADSM DIAGNOSIS REFERENCE FOR MORE INFORMATION       */ 06062400
*   3 DASERRCD FIXED(8),     /* DADSM SCRATCH RETURN CODE            */ 06110100
*   3 DASSFNID FIXED(8),     /* DADSM SCRATCH SUBFUNCTION ID         */ 06157800
*   3 DASSFRET FIXED(8),     /* SUBFUNCTION RETURN CODE              */ 06205500
*   3 DASSFREA FIXED(8),     /* SUBFUNCTION REASON CODE              */ 06253200
*  2 DASRSRV2 BIT(8),        /* RESERVED                             */ 06300900
*  2 DASFLAG2 BIT(8),        /* SMS INDICATOR FLAG                   */ 06348600
*   3 DASSMSMG BIT(1),       /* SMS MANAGED DATA SET                 */ 06396300
*   3 DASUNCAT BIT(1),       /* UNCATALOGED SMS MANAGED DATA SET     */ 06444000
*   3 DASCATCL BIT(1),       /* CATALOG IS THE CALLER            @P3A*/ 06459900
*   3 DASGDGRO BIT(1),       /* GDG ROLLOFF IN PROGRESS          @P3A*/ 06475800
*   3 DASGDGRN BIT(1),       /* GDG ROLLOFF NOSCRATCH            @P3A*/ 06491700
*   3 DASRSRV3 BIT(3),       /* RESERVED                         @P3C*/ 06507600
*  2 DASFLAG3 BIT(8),        /* FUNCTIONALLY AUTHORIZED REQUEST FLGS    06523500
*                               (PART 1)                             */ 06555300
*   3 DASFAUTH BIT(1),       /* FUNCTIONAL AUTHORIZATION OF REQUEST     06587100
*                               HAS BEEN GUARANTEED BY CALLER        */ 06634800
*   3 DASSAUTH BIT(1),       /* SECURITY AUTHORIZATION OF REQUEST       06682500
*                               HAS BEEN GUARANTEED BY CALLER        */ 06730200
*   3 DASPROFM BIT(1),       /* RACF PROFILE MANAGED BY CALLER       */ 06777900
*   3 DASBTIOT BIT(1),       /* BYPASS SYSZTIOT ENQUEUE              */ 06825600
*   3 DASBDSN  BIT(1),       /* BYPASS SYSDSN ENQUEUE                */ 06873300
*   3 DASBDEB  BIT(1),       /* BYPASS DEB CHECK (DSN NOT OPEN)      */ 06921000
*   3 DASNVALL BIT(1),       /* DO NOT DYNAMICALLY ALLOCATE             06968700
*                               VOLUMES                              */ 07016400
*   3 DASNERAS BIT(1),       /* DO NOT ERASE ANY EXTENTS             */ 07064100
*  2 DASFLAG4 BIT(8),        /* FUNCTIONALLY AUTHORIZED REQUEST FLGS    07080000
*                               (PART 2)                             */ 07095900
*   3 DASVOLMT BIT(1),       /* CALLER GUARANTEES VOLUME IS ALREADY     07111800
*                               MOUNTED                              */ 07127700
*   3 DASBSMS  BIT(1),       /* BYPASS SMS CALL FOR DESTROY PDSE @P9C*/ 07138300
*   3 DASBYSMS BIT(1),       /* BYPASS SMS CHECKING, FOR CLEANUP @02A*/ 07145300
*   3 DASRSRV4 BIT(5),       /* RESERVED                         @02C*/ 07152300
*  2 DASFLAG5 BIT(8),        /* AUTH NOT REQUIRED REQUEST FLAGS      */ 07159500
*   3 DASOVRPD BIT(1),       /* OVERRIDE PURGE DATE                  */ 07207200
*   3 DASVRFRD BIT(1),       /* VERIFY LAST REFERENCE DATE              07254900
*                               UNCHANGED (DFHSM)                    */ 07302600
*   3 DASERASE BIT(1),       /* ERASE ALL EXTENTS                    */ 07350300
*   3 DASRSRV5 BIT(5),       /* RESERVED                             */ 07400000
*  2 DASFLAG6 BIT(8),        /* PARAMETER FLAG BYTE                  */ 07414500
*   3 DASSVCEP BIT(1),       /* 1 = SCRATCH ENTERED AT SVC ENTRY POINT: 07429000
*                               0 = SCRATCH ENTERED AT BRANCH           07438600
*                               ENTRY                            @P1A*/ 07448200
*   3 DASRSRV6 BIT(7),       /* RESERVED                             */ 07458000
*  2 DASRSRV7 CHAR(2),       /* RESERVED                             */ 07472500
*  2 DASUCB PTR(31),         /* ADDRESS OF PRIMARY MOUNTABLE UCB     */ 07487400
*   3 DASVDSCB FIXED(32),    /* ADDRESS OF VIRTUAL DSCB, VIO DATA SET*/ 07531100
*  2 DASAVOLL PTR(31),       /* ADDRESS OF VOLUME LIST (IGGDAVLL)    */ 07574800
*  2 DASADSN  PTR(31),       /* ADDRESS OF DATA SET NAME             */ 07618500
*  2 DASREFDT CHAR(3),       /* REFERENCE DATE TO CHECK (DFHSM)      */ 07662200
*  2 DASRSRV8 CHAR(1);       /* RESERVED                             */ 07705900
*DECLARE                                                                07750000
*  /******************************************************************/ 07762500
*  /*  PARAMETER LIST CONSTANTS                                      */ 07775000
*  /******************************************************************/ 07787500
*  DASCRID CHAR(8) CONSTANT('IGGDASCR'), /*                             07800000
*                               PARAMETER LIST ID CONSTANT           */ 07850000
