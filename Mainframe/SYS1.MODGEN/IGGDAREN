* %DCL RN_BASE CHAR INTERNAL;                                           05300000
* %IF DAR_PL = 'N' %THEN %GOTO SK_RPL;                                  05316600
* %IF DAR_PL = 'NO' %THEN %GOTO SK_RPL;                                 05333200
* %RN_BASE = 'BASED(DARENPTR)';                                         05350000
* %IF DAR_BASE = 'N' %THEN                                              05400000
*   %RN_BASE = '';                                                      05450000
* %IF DAR_BASE = 'NO' %THEN                                             05500000
*   %RN_BASE = '';                                                      05550000
* %IF DAR_PLL = 'N' %THEN %GOTO NOLIST_RPL;                             05556200
* %IF DAR_PLL = 'NO' %THEN %GOTO NOLIST_RPL;                            05562400
* %GOTO LIST_RPL;                                                       05568600
* %NOLIST_RPL: ;                                                        05574800
*@LIST PUSH;                                                            05581000
*@LIST OFF;                                                             05587200
* %LIST_RPL: ;                                                          05593400
*DECLARE                                                                05600000
* /*******************************************************************/ 05620000
* /*  COMMON SECTION                                                 */ 05640000
* /*******************************************************************/ 05660000
* 1 DAREN RN_BASE,           /* PARAMETER LIST MAPPING               */ 05680000
*  2 DARPLID CHAR(8),        /* ID = 'IGGDAREN'                      */ 05700000
*  2 DARPVER  FIXED(15),     /* VERSION OF PARAMETER LIST            */ 05735700
*  2 DARPLEN  FIXED(15),     /* LENGTH OF PARAMETER LIST             */ 05771400
*  2 DARPKEY  BIT(8),        /* KEY OF PARAMETER LIST                */ 05807100
*   3 DARPSKEY BIT(4),       /* STORAGE KEY OF PARAMETER LIST        */ 05842800
*   3 DARSKRSV BIT(4),       /* RESERVED                             */ 05878500
*  2 DARRSRV1 CHAR(1),       /* RESERVED                             */ 05914200
*  2 DARHRTCD FIXED(15),     /* HISTORIC RENAME RETURN CODE          */ 05950000
*  2 DARDIAGI FIXED(32),     /* DIAGNOSTIC INFORMATION               */ 06000000
*   3 DARERRCD FIXED(8),     /* RENAME RETURN CODE                   */ 06050000
*   3 DARSFNID FIXED(8),     /* RENAME SUBFUNCTION ID                */ 06100000
*   3 DARSFRET FIXED(8),     /* SUBFUNCTION RETURN CODE              */ 06150000
*   3 DARSFREA FIXED(8),     /* SUBFUNCTION REASON CODE              */ 06200000
*  2 DARRSRV2 CHAR(1),       /* RESERVED                             */ 06250000
*  2 DARFLAG2 BIT(8),        /* SMS INDICATOR FLAG                   */ 06300000
*   3 DARSMSMG BIT(1),       /* SMS MANAGED DATA SET                 */ 06350000
*   3 DARUNCAT BIT(1),       /* UNCATALOGED SMS MANAGED DATA SET     */ 06387500
*   3 DARCATCL BIT(1),       /* CATALOG IS THE CALLER            @P3A*/ 06425000
*   3 DARRSRV3 BIT(5),       /* RESERVED                         @P3C*/ 06462500
*  2 DARFLAG3 BIT(8),        /* FUNCTIONALLY AUTHORIZED REQUEST FLGS */ 06500000
*   3 DARFAUTH BIT(1),       /* FUNCTIONAL AUTHORIZATION OF REQUEST     06550000
*                               HAS BEEN VERIFIED BY CALLER          */ 06600000
*   3 DARSAUTH BIT(1),       /* SECURITY AUTHORIZATION OF REQUEST       06650000
*                               HAS BEEN GUARANTEED BY CALLER        */ 06700000
*   3 DARPROFM BIT(1),       /* RACF PROFILE MANAGED BY CALLER       */ 06750000
*   3 DARDUPDS BIT(1),       /* ALLOW RENAME OF DUPLICATE DS     @L2A*/ 06772200
*   3 DARRSRV4 BIT(4),       /* RESERVED                         @L2C*/ 06794400
*  2 DARFLAG4 BIT(8),        /* PARAMETER FLAG BYTE                  */ 06816600
*   3 DARSVCEP BIT(1),       /* 1 = RENAME ENTERED AT SVC ENTRY POINT:  06833200
*                               0 = RENAME ENTERED AT BRANCH            06844200
*                               ENTRY                            @P1A*/ 06855200
*   3 DARBPDSC BIT(1),       /* DO NOT UPDATE DATA SET CHANGED   @01A   06862100
*                               BIT (DS1IND02) IN FMT1 DSCB      @01A*/ 06869000
*   3 DARRSRV5 BIT(6),       /* RESERVED                         @01C*/ 06875900
*  2 DARRSRV6 CHAR(4),       /* RESERVED                             */ 06883000
*  2 DARUCB PTR(31),         /* ADDRESS OF PRIMARY MOUNTABLE UCB     */ 06900000
*  2 DARAVOLL PTR(31),       /* ADDRESS OF VOLUME LIST (IGGDAVLL)    */ 06950000
*  2 DARADSN PTR(31),        /* ADDRESS OF OLD DATA SET NAME         */ 07000000
*  2 DARANDSN PTR(31);       /* ADDRESS OF NEW DATA SET NAME         */ 07050000
*DECLARE                                                                07100000
*  /******************************************************************/ 07112500
*  /*  PARAMETER LIST CONSTANTS                                      */ 07125000
*  /******************************************************************/ 07137500
*  DARENID CHAR(8) CONSTANT('IGGDAREN'), /*                             07150000
*                               PARAMETER LIST ID CONSTANT           */ 07200000
*  DARVER FIXED(15) CONSTANT(0), /*                                     07250000
*                               PARAMETER LIST VERSION               */ 07300000
*  DARLEN FIXED(15) CONSTANT(44); /*                                    07350000
*                               PARAMETER LIST LENGTH                */ 07365900
* %IF DAR_PLL = 'N' %THEN %GOTO RESETL_RPL;                             07381800
* %IF DAR_PLL = 'NO' %THEN %GOTO RESETL_RPL;                            07397700
* %GOTO SK_RPL;                                                         07413600
* %RESETL_RPL: ;                                                        07429500
*@LIST POP;                                                             07445400
* %SK_RPL: ;                                                            07461300
* %IF DAR_PL = '' %THEN %GOTO IN_RRC;                                   07477200
* %IF DAR_RC = '' %THEN %GOTO SK_RRC;                                   07493100
* %IF DAR_RC = 'N' %THEN %GOTO SK_RRC;                                  07509000
* %IF DAR_RC = 'NO' %THEN %GOTO SK_RRC;                                 07524900
* %IF DAR_RCL = 'N' %THEN %GOTO NOLIST_RRC;                             07540800
* %IF DAR_RCL = 'NO' %THEN %GOTO NOLIST_RRC;                            07556700
* %GOTO IN_RRC;                                                         07572600
* %NOLIST_RRC: ;                                                        07588500
*@LIST PUSH;                                                            07604400
*@LIST OFF;                                                             07620300
* %IN_RRC: ;                                                            07636200
*DECLARE                                                                07652100
*  /******************************************************************/ 07668000
*  /*  HISTORIC RETURN CODES                                         */ 07683900
*  /******************************************************************/ 07699800
*  DARHCD00 CONSTANT(000),          /* SUCCESSFUL DATA SET RENAMEED */  07715700
*                                                                       07731600
*  DARHCD04 CONSTANT(004),          /* NO VOLUME CONTAINING ANY PART    07747500
*                                      OF DATA SET WAS MOUNTED, NOR     07763400
*                                      WAS THERE A UNIT AVAILABLE FOR   07779300
*                                      MOUNTING                      */ 07795200
*                                                                       07811100
*  DARHCD08 CONSTANT(008),          /* AN UNUSUAL CONDITION WAS         07827000
*                                      ENCOUNTERED ON ONE OR MORE       07842900
*                                      VOLUMES                       */ 07858800
*                                                                       07874700
*  DARHCD0C CONSTANT(012);          /* THE PARAMETER OR VOLUME LIST     07890600
*                                      PASSED WAS INVALID            */ 07906500
*DECLARE                                                                07922400
*  /******************************************************************/ 07938300
*  /*  ERROR CODES                                                   */ 07954200
*  /******************************************************************/ 07970100
*  DARECD00 CONSTANT(000),          /* SUCCESSFUL COMPLETION         */ 07986000
*                                                                       08001900
*  DARECD04 CONSTANT(004),          /* CATAGORIZED ERROR CONDITION   */ 08017800
*                                                                       08033700
*  DARECD08 CONSTANT(008),          /* UNEXPECTED ERROR CONDITION    */ 08049600
*                                                                       08065500
*  DARECD0C CONSTANT(012),          /* INVALID PARAMETER OR REQUEST  */ 08081400
*                                                                       08097300
*  DARECD10 CONSTANT(016);          /* INVALID PARAMETER LIST        */ 08113200
* %IF DAR_RCL = 'N' %THEN %GOTO RESETL_RRC;                             08129100
* %IF DAR_RCL = 'NO' %THEN %GOTO RESETL_RRC;                            08145000
* %GOTO SK_RRC;                                                         08160900
* %RESETL_RRC: ;                                                        08176800
*@LIST POP;                                                             08192700
* %SK_RRC: ;                                                            08208600
* %IF DAR_PL = '' %THEN %GOTO IN_RSF;                                   08224500
* %IF DAR_SF = '' %THEN %GOTO SK_RSF;                                   08240400
* %IF DAR_SF = 'N' %THEN %GOTO SK_RSF;                                  08256300
