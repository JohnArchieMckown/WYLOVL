*********************************************************************** 00388300
*                 IECDVOID DSECT Mapping                              * 00389800
*                                                                     * 00391400
* NOTE - All VOID exits are called using pointer defined linkage      * 00393000
*********************************************************************** 00394600
.VOI001  ANOP                                                           00396621
VOID     DSECT                                                          00400021
VOIENTRY DS    0D                                                       00450000
VOIPRGE  DS    0D                  Purge routine address                00497200
VOIPRG   DS    A                   Purge routine address               X00544400
                                   (the high order byte will no longer X00591600
                                    be used as the purge use count)     00638800
VOIPRGCT DS    F                   Purge use count                      00686000
VOIRSTR  DS    A                   Restore rouitne address              00733200
VOIEXTNT DS    A                   Extent Check routine address         00780400
VOINOTE  DS    A                   SECP notification exit routine      X00827600
                                        address                         00874800
         DS    A                   Reserved                             00922000
         MEND                      */                                   00970000
*%VOIX2: ;                                                              01010000
*                                                                       01050021
* DCL 1 VOID     BDY(DWORD) BASED(VOIDPTR),                             01100000
*      2  VOIENTRY (*) BDY(DWORD), /* Entry name                     */ 01150000
*                                  /* VOID exits are called using       01200000
*                                     pointer defined linkage        */ 01250000
*         3 VOIPRGE,               /* Purge Routine address          */ 01300000
*           4 VOIPRG  PTR(31),     /* Purge routine address.            01350000
*                                     (the high order byte will no      01400000
*                                      longer be used as the use        01450000
*                                      count                         */ 01500000
*           4 VOIPRGCT FIXED(31),  /* PURGE use count                */ 01550000
*         3  VOIRSTR  PTR(31),     /* Restore routine address        */ 01600000
*         3  VOIEXTNT PTR(31),     /* Extent Check routine address   */ 01650000
*         3  VOINOTE  PTR(31),     /* SECP notification exit            01700000
*                                          routine address           */ 01750000
*         3  *        PTR(31),     /* Reserved - set to zero         */ 01800000
*         3  VOIEND   CHAR(0);     /* End of VOID                    */ 01850000
