*%GOTO PLSCMNT;                                                         00050000
*/*                                                                     00100000
         MACRO                                                          00150000
         IGGUCSIT &MF=LIST,&NAME=,&ALIAS=,&VLENGTH=,&DEFAULT=NO,&DESCR=X00200000
               ,&FOLD=NO,&DEVICE=                                 @P1A  00250000
         GBLA  &UCSIL                                                   00300000
         GBLB  &IGGUCSI,&IGGUCSC                                        00350000
         GBLC  &DEVTYPE                                        @42480JF 00400000
         LCLA  &NVLINES,&N,&TVL                                         00450000
         LCLB  &ERROR,&F1D,&F2D                                   @P1A  00500000
         LCLC  &NAME1,&FLAG1,&VLL,&VLO,&DESCRD,&DESCRO                  00550000
         LCLC  &IGGUCSB,&IGGUVL,&IGGUDS,&IGGUDD,&IGGUEND                00600000
         AIF   (&IGGUCSC).SKIPCMT  PRINT COMMENTS ONLY ONCE/ASSEMBLY    00650000
&IGGUCSC SETB  1                   SET COMMENT GLOBAL                   00700000
**/                                                                     00750000
*%PLSCMNT:;                                                             00800000
*/********************************************************************* 00850000
**                                                                   ** 00900000
**    $MAC (IGGUCSIT):                                       @42480JF** 00950000
**                                                                   */ 01000000
*/*PROPRIETARY V3 STATEMENT                                          */ 01031200
*/*LICENSED MATERIALS - PROPERTY OF IBM                              */ 01062400
*/*"RESTRICTED MATERIALS OF IBM"                                     */ 01093600
*/*5695-DF1                                                          */ 01124800
