*           3 JDVBSTR BIT(1),    /*JDT-DEFINED VERB STRING       @H1A*/ 08900000
*           3 LASTSTMT BIT(1),   /* LAST STMT FOR THIS STEP          */ 08950000
*           3 PROCSTR BIT(1),    /*PROC STATEMENT TEXT STRING        */ 09000000
*           3 DDSTR   BIT(1),    /*DD STATEMENT TEXT STRING          */ 09050000
*           3 EXECSTR BIT(1),    /*EXEC STATEMENT TEXT STRING        */ 09100000
*           3 JOBSTR   BIT(1),   /*JOB STATEMENT TEXT STRING         */ 09114200
*      2 STRINDCE      CHAR(2), /*EXTENDED STATEMENT TYPE FIELDS @L2A*/ 09128400
