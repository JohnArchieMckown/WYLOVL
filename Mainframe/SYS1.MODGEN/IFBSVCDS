*        %GOTO SVCDSPRO;          /* Bilingual Mapping Macro            00000100
         MACRO                                                          00000200
         IFBSVCDS &DSECT=YES                                            00000300
*                                                                       00000400
.*/      %SVCDSPRO: ;                                             /* */ 00000500
*/*START OF SPECIFICATIONS ******************************************** 00000600
*                                                                       00000700
*01* MACRO NAME: IFBSVCDS                                               00000800
