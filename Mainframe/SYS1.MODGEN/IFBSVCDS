*/* Created IFBSVCDS to map the special input from the SETLOGRC         00010200
*   command processor and SVC 76.                                       00010300
*                                                                       00010400
**********************************************************************/ 00010500
*        %GOTO SVCDSBEG;                                          /* *  00010600
*                                                                       00010700
         AIF   ('&DSECT' EQ 'YES').SVCDS10                              00010800
         AIF   ('&DSECT' EQ 'NO').SVCDS20                               00010900
         MNOTE 12,'AHA0010 IFBSVCDS:  DSECT MUST BE EITHER YES OR NO'   00011000
         AGO   .END                                                     00011100
.SVCDS10 ANOP                                                           00011200
IFBSVCDS DSECT ,                   IFBSVCDS data area mapping           00011300
         AGO   .SVCDS30                                                 00011400
.SVCDS20 ANOP                                                           00011500
IFBSVCDS DS    0F                  IFBSVCDS data area                   00011600
.SVCDS30 ANOP                                                           00011700
IFBSVCDS_ID         DS    CL8      Eye catcher 'IFBSVCDS'               00011800
IFBSVCDS_RECORD_LEN DS    F        Actual length of record to be        00011900
*                                  written                              00012000
IFBSVCDS_RECORD_PTR DS    A        Pointer to record to be written      00012100
*                                                                       00012200
IFBSVCDS_LEN        EQU   *-IFBSVCDS   Length of data area              00012300
*                                                                       00012400
.*                                                                      00012500
.END     ANOP                                                           00012600
         MEND  ,                                                        00012700
.*                                                                      00012800
.*/      %SVCDSBEG: ;                                                   00012900
                                                                @SPACE; 00013000
 %IF SVCDSBASE = '' %THEN %SVCDSBASE = ' ';                             00013100
                                                                @SPACE; 00013200
 DCL                                                                    00013300
  1 IFBSVCDS   BDY(WORD) SVCDSBASE, /* IFBSVCDS data area mapping    */ 00013400
    3 IFBSVCDS_ID            CHAR(8), /* Eye catcher                 */ 00013500
    3 IFBSVCDS_RECORD_LEN    FIXED(31), /* Actual length of record to   00013600
                                      be written                     */ 00013700
    3 IFBSVCDS_RECORD_PTR    PTR(31); /* Pointer to record to be        00013800
                                      written                        */ 00013900
 DCL                                                                    00014000
  IFBSVCDS_EYE_CATCHER CHAR(8)  CONSTANT('IFBSVCDS'), /* Eye catcher */ 00014100
  IFBSVCDS_R0_INPUT    BIT(32)  CONSTANT('FFFFFFFF'X); /* Input value   00014200
                                      for register 0                 */ 00014300
