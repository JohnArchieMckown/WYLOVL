.OUCBAL1 ANOP                                                           12550000
         SPACE 1                                                        12600000
         AIF   ('&DSECT' EQ 'NO').OUCBAL2                               12650000
&PF      DSECT ,                                                   @L2C 12700000
         AGO   .OUCBAL3                                                 12750000
.OUCBAL2 ANOP                                                           12800000
&PF.     DS    0D                                                  @L2C 12850000
.OUCBAL3 ANOP                                                      @L2A 12900000
         AIF   ('&LABEL' EQ '').OUCBAL4                            @L2C 12950000
&LABEL   DS    0D                                                  @L2C 13000000
.OUCBAL4 ANOP                                                      @L2C 13050000
         SPACE 1                                                        13100000
&PF.1BLK DS    0CL256  -           FIRST 256 BYTES OF OUCB         @P2A 13150000
&PF.CHE1 DS    0CL128  -           1st cache line of OUCB          @L9A 13200000
&PF.NAME DC    C'OUCB' -           BLOCK IDENTIFICATION            @L2C 13250000
&PF.FWD  DC    A(&PF.) -           SWAP CHAIN FORWARD POINTER      @L2C 13300000
&PF.BCK  DC    A(&PF.) -           SWAP CHAIN BCKWARD POINTER      @L2C 13350000
&PF.TMA  DC    F'0' -              TIME OF LAST ANALYSYS           @L2C 13400000
&PF.QFL  DC    B'00100000' -       SWAPPABILITY TRANSITION FLAGS   @L2C 13450000
