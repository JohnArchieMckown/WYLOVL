*%;/******************************************************************/ 05059000
*%;/* FIRST: FIGURE OUT THE BASED PART OF THE BASING EXPRESSION.     */ 05063100
*%;/******************************************************************/ 05067200
* %IF  ACBVPTR ~=''       %THEN %ACBBASD = ACBVPTR;            /*@LYA*/ 05071300
* %IF  ACBBASD   =''      %THEN %ACBBASD ='BASED';             /*@LYA*/ 05075400
* %IF  ACBBASD   ='NONE'  %THEN %ACBBASD ='';                  /*@LYA*/ 05079500
* %IF  ACBBASD ~='BASED' %THEN                                          05083600
*  %IF ACBBASD ~=''       %THEN                                         05087700
*     %ACBBASD   ='BASED(' || ACBBASD || ')';                  /*@LYA*/ 05091800
*                                                                       05095900
*%;/******************************************************************/ 05100000
*%;/* SECOND: FIGURE OUT THE VIA PART OF THE BASING EXPRESSION.      */ 05104100
*%;/******************************************************************/ 05108200
* %ACBVIA = '';                                                /*@LYA*/ 05112300
* %IF ACBALET   = '' %THEN %ACBALET = IDARLS_ALET;             /*@LYA*/ 05116400
* %IF ACBVPTR ~= '' %THEN %ACBALET = '';                       /*@LYA*/ 05120500
* %IF ACBALET ~= '' %THEN %ACBVIA = 'VIA('||ACBALET||')';      /*@LYA*/ 05124600
* %IF ACBBASD   = '' %THEN %ACBVIA = '';                       /*@LYA*/ 05128700
*                                                                       05132800
*%;/******************************************************************/ 05136900
*%;/* GENERATE THE CB BASING STATEMENT                               */ 05141000
*%;/******************************************************************/ 05145100
* DCL 1 IFGACB ACBBASD ACBVIA,                                 /*@LYA*/ 05149200
*          2 ACBCOMN,                       /* ACB COMMON SECTION       05154900
*                                                             X03004*/  05204900
