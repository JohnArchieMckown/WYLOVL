*%GOTO SXS1;                          /* BILINGUAL MAPPING              00001000
         MACRO                                                          00002000
         IRDSENSE                                                       00003000
*                                                                    */ 00004000
*%SXS1: ;                                                               00005000
*/* START OF SPECIFICATIONS ******************************************/ 00006000
*/*                                                                  */ 00007000
*/*01* Macro Name:  IRDSENSE                                         */ 00008000
*/*                                                                  */ 00009000
*/*01* Descriptive Name = Sense Data Map for a Dynamic Switch        */ 00010000
*/*                                                                  */ 00011000
*/* Copyright -                                                      */ 00012000
*/*      LICENSED MATERIALS - PROPERTY OF IBM                        */ 00013000
*/*      5647-A01 (C) COPYRIGHT IBM CORP. 1990, 2000                 */ 00014000
*/*                                                                  */ 00018000
*/*  Status =  HSWF100                                               */ 00019000
*/*                                                                  */ 00020000
*/*  Function = This macro maps the 32 byte sense data for a switch. */ 00021000
*/*                                                                  */ 00022000
*/*  Module Type = Bilingual Macro                                   */ 00023000
*/*                                                                  */ 00024000
*/*  Dependencies = None                                             */ 00025000
*/*                                                                  */ 00026000
*/*  Invocation =                                                    */ 00027000
*/*        PLAS = %INCLUDE SYSLIB(IRDSENSE)                          */ 00028000
*/*                                                                  */ 00029000
*/*        Assembler = IRDSENSE                                      */ 00030000
*/*                                                                  */ 00031000
*/*  Change Activity =                                               */ 00032000
*/*                                                                  */ 00033000
*/*  $MAC(IRDSENSE),COMP(00101): Switch Sense Data                   */ 00034000
*/*                                                                  */ 00035000
*/*  PN  REASON   RLSE     DATE   ORG   COMMENTS                     */ 00036000
*/*                                                                  */ 00037000
*/* $L0= SWITCH  HSWF100 900700     : Switch Device Support          */ 00038000
*/* $01= UY61105 HSWF100 910204    D: Dynamic I/O Support            */ 00039000
*/* $D2= OW08450 HSWF100 940627    D: 9032-3 support                 */ 00040000
*/* $02= UWxxxxx HSWF100 950913    D: 9033-4 support             @02A*/ 00041000
*/* $03= OW38551 HSWF100 980530 PDHC: Hermes - 9032-5 support.       */ 00041000
*/*                                   Map sense byte 29.         @03A*/ 00042000
*/* $04= OW44017 HSWF100 990601 PDLE: FICON 2032 SWITCH SUPPORT  @04A*/ 00041000
*/*                                                                  */ 00042000
*/* END OF SPECIFICATIONS ********************************************/ 00043000
*%GOTO SXS2;                                                            00044000
*/*                                                                     00045000
 TITLE   '&SYSECT  IRDSENSE - Sense Data Map for a Switch'              00046000
SXSENSE  DSECT ,         Switch sense data                              00047000
SXSENS0  DS    0BL.008   Sense byte 0 - architected for all devices     00048000
SXSCMDRJ EQU   X'80'     Command reject                                 00049000
SXSIR    EQU   X'40'     Intervention required                          00050000
SXSBOC   EQU   X'20'     Bus-Out check                                  00051000
SXSEQCK  EQU   X'10'     Equipment Check                                00052000
SXSDCK   EQU   X'08'     Data Check                                     00053000
SXSOVER  EQU   X'04'     Overrun                                        00054000
         ORG   SXSENS0+X'00000001'                                      00055000
SXSENS1  DS    0BL.008   Sense Byte 1 - Switch status                   00056000
SXSOSCF  EQU   X'10'     Operator console failure                       00057000
SXSSNSI  EQU   X'08'     Sense data incomplete                          00058000
SXSERP   EQU   X'02'     Error during recovery                          00059000
SXSDISK  EQU   X'01'     Diskette failure                               00060000
         ORG   SXSENS1+X'00000001'                                      00061000
SXSENS2  DS    0BL.008   Sense Byte 2 - Device support                  00062000
SXSMAINT EQU   X'80'     Maintenance record data                        00063000
SXSLOG   EQU   X'40'     EREP logging request                           00064000
SXSMSG   EQU   X'20'     Operator message request                       00065000
SXSINFO  EQU   X'10'     Information only record                        00066000
SXSINCID EQU   X'08'     Link Incident indicator                        00067000
SXSSPARE EQU   X'04'     Spare Port Available                           00068000
SXSASYNC EQU   X'02'     Asynchronous report to follow                  00069000
SXSINVCF EQU   X'01'     Invalid configuration detected                 00070000
         ORG   SXSENS2+X'00000001'                                      00071000
SXSENS3  DS    0BL.008   Sense Byte 3 - Device support                  00072000
SXSCHAN  EQU   X'80'     Channel initiated unit check                   00073000
SXSRESET EQU   X'40'     Resetting event indicator                      00074000
SXSALLEG EQU   X'20'     Reset Allegiance indicator                     00075000
SXSTKIEX EQU   X'10'     TKI was executed                               00076000
         ORG   SXSENS3+X'00000001'                                      00077000
SXSENS4  DS    1BL.008   Sense Byte 4 - CCW code of command rejected    00078000
SXSENS5  DS    1BL.008   Sense Byte 5 - command reject error reason    *00079000
                         code - see below                               00080000
SXSENS6  DS    1BL.008   Sense Byte 6                                   00081000
SXSENS7  DS    0BL.008   Sense Byte 7 - Type and Model                 *00082000
                                        identification            @04C  00082000
SXSDTYPC EQU   X'C0'     Director type code - see below           @04A  00083000
SXSRSV1  EQU   X'30'     Reserved and set to zeros                @04A  00083000
SXSDMDLC EQU   X'0F'     Director Model Code - see below          @04A  00083000
         ORG   SXSENS7+X'00000001'                                      00084000
SXSENS8  DS    0BL.008   Sense Byte 8                                   00085000
SXSLICV  EQU   X'F0'     LIC version or                           @02A  00086000
SXSLICV1 EQU   X'F0'     LIC version/Release character 1  OR      @04A  00086000
SXSEC5   EQU   X'F0'     EC level - 5th number                          00087000
SXSLICR  EQU   X'0F'     LIC release or                           @02A  00088000
SXSLICV2 EQU   X'0F'     LIC release/Release character 2  OR      @04A  00088000
SXSEC6   EQU   X'0F'     EC level - 6th number                          00089000
         ORG   SXSENS8+X'00000001'                                      00090000
SXSENS9  DS    0BL.008   Sense Byte 9                                   00091000
SXSLICP  DS    1CL0001   LIC patch in hex or                      @02A  00092000
SXSLICV3 EQU   X'F0'     LIC version/Release character 3  OR      @04A  00092000
SXSLICV4 EQU   X'0F'     LIC release/Release character 4  OR      @04A  00088000
SXSEC7   DS    1CL0001   EC level - 7th character                 @04C  00093000
SXSENS10 DS    0BL.008   Sense Byte 10                                  00094000
SXSSEV   EQU   X'F0'     Severity Code - see below                      00095000
         ORG   SXSENS10+X'00000001'                                     00096000
SXSENSPR DS    1CL0002   Port Numbers when byte 10 indicates port      *00097000
                         formatting                                     00098000
         ORG   SXSENS10+X'00000001'                                     00099000
SXSENS11 DS    1BL.008   Sense Byte 11                                  00100000
SXSENS12 DS    1BL.008   Sense Byte 12                                  00101000
SXSENS13 DS    1BL.008   Sense Byte 13                                  00102000
SXSENS14 DS    1BL.008   Sense Byte 14                                  00103000
SXSENS15 DS    1BL.008   Sense Byte 15                                  00104000
SXSENS16 DS    1BL.008   Sense Byte 16                                  00105000
SXSENS17 DS    1BL.008   Sense Byte 17                                  00106000
SXSENS18 DS    1BL.008   Sense Byte 18                                  00107000
SXSENS19 DS    1BL.008   Sense Byte 19                                  00108000
SXSENS20 DS    1BL.008   Sense Byte 20                                  00109000
SXSENS21 DS    1BL.008   Sense Byte 21                                  00110000
SXSENS22 DS    1BL.008   Sense Byte 22                                  00111000
SXSENS23 DS    1BL.008   Sense Byte 23                                  00112000
SXSENS24 DS    1BL.008   Sense Byte 24                                  00113000
SXSENS25 DS    1BL.008   Sense Byte 25                                  00114000
SXSENS26 DS    1BL.008   Sense Byte 26                                  00115000
SXSENS27 DS    1BL.008   Sense Byte 27                                  00116000
SXSENS28 DS    1BL.008   Sense Byte 28                                  00117000
SXSENS29 DS    0BL.008   Sense Byte 29                            @03C  00118000
SXSFRUM  EQU   X'0F'     FRU code modifier bits.                  @03A  00083000
         ORG   SXSENS29+X'00000001'                               @03A  00084000
SXSFRUS  DS    1BL.016   Sense Bytes 30-31 - FRU codes                  00119000
SXSFRU1  EQU   X'F0'     FRU code 1 mask                                00120000
SXSFRU2  EQU   X'0F'     FRU code 2 mask                                00121000
SXSFRU3  EQU   X'F0'     FRU code 3 mask                                00122000
SXSFRU4  EQU   X'0F'     FRU code 4 mask                                00123000
         ORG   SXSENSE+X'00000020'                                      00124000
SXSENSE_LEN EQU *-SXSENSE                                               00125000
*                                                                       00126000
