*********************************************************************** 00244000
*  Sense Byte 7  (SXSSEN7) - Type and Model identification              00245000
*    SXSDTYPC field  - bits 0-1 Director Type Code                      00247000
*    SXSDRSV1 field  - bits 2-3 Reserved and set to zeros               00247000
*    SXSDMDLC field  - bits 4-7 Director Model Code                     00247000
*                                                                       00247000
*********************************************************************** 00248000
*----SXSDTYPC field  - bits 0-1                                         00247000
SXSESCON EQU   B'00'     ESCON Director type code                  @04A 00251000
SXSFICON EQU   B'01'     FICON Director type code                  @04A 00252000
*                                                                       00247000
*                                                                       00249000
*----SXSDMDLC field  - bits 4-7                                         00247000
*                                                                       00247000
SXSESCMX EQU 4           Highest ESCON switch type index value     @04A 00249000
*                                                                       00250000
SXS9032  EQU   B'0000'   Machine type 9032-2                            00251000
SXS9033  EQU   B'0001'   Machine type 9033-1                            00252000
SXS90322 EQU   B'0010'   Machine type 9032-3                       @D2A 00253000
SXS90334 EQU   B'0011'   Machine type 9033-4                       @02A 00253000
SXS90325 EQU   B'0100'   Machine type 9032-5                       @03A 00253000
*                                                                       00250000
SXS20321 EQU   B'0001'   MACHINE TYPE 2032-1                       @04A 00251000
         MEND                                                        */ 00254000
*%SXS2: ;                                                               00255000
*   DECLARE                                                             00256000
*     1 SXSENSE   BASED BDY(WORD),  /* Switch sense data             */ 00257000
*      2 SXSENS0   BIT(8),          /* Sense byte 0 - architected    */ 00258000
*                                   /* for all devices               */ 00259000
*       3 SXSCMDRJ BIT(1),          /* Command reject                */ 00260000
*       3 SXSIR    BIT(1),          /* Intervention required         */ 00261000
*       3 SXSBOC   BIT(1),          /* Bus-Out check                 */ 00262000
*       3 SXSEQCK  BIT(1),          /* Equipment Check               */ 00263000
*       3 SXSDCK   BIT(1),          /* Data Check                    */ 00264000
*       3 SXSOVER  BIT(1),          /* Overrun                       */ 00265000
*       3 *        BIT(1),          /* Reserved                      */ 00266000
*       3 *        BIT(1),          /* Reserved                      */ 00267000
*      2 SXSENS1   BIT(8),          /* Sense Byte 1 - Switch status  */ 00268000
*       3 *        BIT(1),          /* Reserved                      */ 00269000
*       3 *        BIT(1),          /* Reserved                      */ 00270000
*       3 *        BIT(1),          /* Reserved                      */ 00271000
*       3 SXSOSCF  BIT(1),          /* Operator console failure      */ 00272000
*       3 SXSSNSI  BIT(1),          /* Sense data incomplete         */ 00273000
*       3 *        BIT(1),          /* Reserved                      */ 00274000
*       3 SXSERP   BIT(1),          /* Error during recovery         */ 00275000
