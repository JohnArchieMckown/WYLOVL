    DECLARE                                                             15150000
      1    ADSS     DEF(PRDSR) BDY(WORD), /* Beginning of symptom       15200000
                                       string 2048 into header   @L1P*/ 15250000
       2    SR       CHAR(2) BDY(WORD), /* 'SR' symptom area id         15300000
                                                                 @L1P*/ 15350000
       2    *        CHAR(40),      /* Reserved for future use          15400000
                                                                 @L1P*/ 15450000
       2    SRID     CHAR(9),       /* System identifier (derived       15500000
                                       from FESN system identifier)     15550000
                                                                 @L1P*/ 15600000
       2    *        CHAR(5),       /* Reserved for future use          15650000
                                                                 @L1P*/ 15700000
       2    SRDTYPE  CHAR(8),       /* Type of dump, such as 'SVC       15750000
                                       DUMP'                     @L1P*/ 15800000
       2    *        CHAR(8),       /* Reserved for future use          15850000
                                                                 @L1P*/ 15900000
       2    SRSLEN   FIXED(15),     /* Length of primary symptom        15950000
                                       string (ADSSRCRS)         @L1P*/ 16000000
       2    SRSOFF   FIXED(15),     /* Offset to primary symptom        16050000
                                       string (ADSSRCRS)         @L1P*/ 16100000
       2    SROLEN1  FIXED(15),     /* Length of secondary symptom      16150000
                                       string (ADSSRCSS) or 0    @L1P*/ 16200000
       2    SROFF1   FIXED(15),     /* Offset to secondary symptom      16250000
                                       string (ADSSRCSS) or 0    @L1P*/ 16300000
       2    SROLEN2  FIXED(15),     /* Length of optional symptom       16350000
                                       string data (ADSSRNSD) not in    16400000
                                       SDB format.               @L1P*/ 16450000
       2    SROFF2   FIXED(15),     /* Offset to optional symptom       16500000
