*%ITRH01: ;                                                             00018805
 @EJECT ;                                                               00018905
 DECLARE                                                                00019005
 %IF ITRHDEF='' %THEN                                                   00019105
   %GOTO ITRH100;                                                       00019205
  1 ITRH ITRHDEF,                   /* Internal text record header   */ 00019305
   %GOTO ITRH110;                                                       00019405
 %ITRH100: ;                                                            00019505
  1 ITRH BASED,                     /* Internal text record header   */ 00019605
 %ITRH110: ;                                                            00019705
   2 ITRHID CHAR(8),                /* Internal text record type        00019805
                                       (CHPID, CNTLUNIT, EDT,           00019905
                                       ID, IOCONFIG, IODEVICE, NIPCON,  00020005
                                       HCDEND, HCDSEP,                  00020105
                                       RDEVICE, RIOGEN,                 00020205
                                       or UNITNAME)              @LEC*/ 00020305
   2 ITRHSNBR CHAR(6),              /* statement number (right algnd)*/ 00020405
   2 *        CHAR(1),              /* Reserved                      */ 00020505
   2 ITRHFNBR CHAR(1),              /* File Number                   */ 00020605
   2 ITRHNEXT PTR(31),              /* Pointer to next internal text    00020705
                                       record, zero if last record   */ 00020805
   2 *        CHAR(4),              /* Reserved                      */ 00020905
   2 ITRHUSER CHAR(4),              /* For component use             */ 00021005
   2 ITRHTEXT CHAR(0);              /* Start of internal text        */ 00021105
 @EJECT;                                                                00021205
