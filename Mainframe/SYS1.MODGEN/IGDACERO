*       2  ACEROUSR CHAR(8),           /* USERID                     */ 05050000
*       2  ACEROGRP CHAR(8),           /* GROUPID                    */ 05100000
*       2  ACERODSG FIXED(31),         /* DATA SET ORGANIZATION      */ 05150000
*                                      /* (MAY BE ACEROPS, ACEROPO,  */ 05200000
*                                      /* ACEROVS, ACERODA, ACERONUL)*/ 05250000
*       2  ACERORCG FIXED(31),         /* DATA SET RECORD            */ 05300000
*                                      /* ORGANIZATION (MAY BE       */ 05350000
*                                      /* ACEROKS, ACEROES, ACERORR, */ 05400000
*                                      /* ACEROLS, ACERONUL)         */ 05450000
