*         5 RSTBID CHAR(4),        /* IDENTIFIER 'RSTB'              */ 04400000
*         5 RSTBVER FIXED(8),      /* VERSION NUMBER                 */ 04450000
*         5 RSTBRSRV CHAR(3),      /* RESERVED                       */ 04500000
