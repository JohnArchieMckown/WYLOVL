*           7 RSTBUSR    PTR(31),  /* USER CODES POINTER             */ 04950000
*           7 RSTBUSRN   PTR(31),  /* END OF USER CODES SECTION      */ 05000000
*           7 RSTBLAST   PTR(31);  /* END OF CODES POINTER           */ 05050000
*/********************************************************************/ 05100000
