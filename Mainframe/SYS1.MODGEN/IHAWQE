*%WQEL2: ;                                                              16050002
*@EJECT;                                                                16050200
* %WQELPUSH = '@LISTPUSH';          /*EQUATE LISTPUSH FOR MACRO  @LFA*/ 16050400
* %WQELPOP = '@LISTPOP';            /*EQUATE LISTPOP FOR MACRO   @LFA*/ 16050600
* %WQELOFF = '@LISTOFF';            /*EQUATE LISTOFF FOR MACRO   @LFA*/ 16050800
* WQELPUSH;                         /* @LISTPUSH                 @LFA*/ 16051000
* %IF WQEVIA = 'YES' %THEN              /* CHECK IF ALET SPECIFIED FOR  16051200
*                                       THE BASE WQE             @LFA*/ 16051400
*   %WQEVIA = 'VIA(WQEALET)';                                           16051600
* %ELSE                                                       /* @LFA*/ 16051800
*   %IF GLOBALARMODE = 'YES' %THEN      /* WQEVIA WAS NOT SPECIFIED     16052000
*                                       CHECK FOR IF GLOBAL DEFAULT     16052200
*                                       WAS SPECIFIED            @LFA*/ 16052400
*     %WQEVIA  = 'VIA(0)';              /* SET GLOBAL DEFAULT    @LFA*/ 16052600
*   %ELSE                               /* NO GLOBAL DESIRED     @LFA*/ 16052800
*     %WQEVIA  = '';                    /* SET TO NULL           @LFA*/ 16053000
* %IF WQESYVIA = 'YES' %THEN            /* CHECK IF ALET SPECIFIED FOR  16053200
*                                       THE SYSID ARRAY          @LFA*/ 16053400
*   %WQESYVIA = 'VIA(WQESYALET)';       /* SET SYSID ARRAY VIA TO ALET  16053600
*                                       SPECIFIED                @LFA*/ 16053800
* %ELSE                                 /* NO ALET SPECIFIED     @LFA*/ 16054000
*   %WQESYVIA = WQEVIA;                 /* VIA FOR SYSID ARRAY DEFAULTS 16054200
*                                       TO VALUE OF THE BASE WQE @LFA*/ 16054400
* %IF WMJMVIA = 'YES' %THEN             /* CHECK IF ALET SPECIFIED FOR  16054600
*                                       THE MAJOR WQE            @LFA*/ 16054800
*   %WMJMVIA = 'VIA(WMJMALET)';         /* SET MAJOR WQE VIA TO ALET    16055000
*                                       SPECIFIED                @LFA*/ 16055200
* %ELSE                                 /* NO ALET SPECIFIED     @LFA*/ 16055400
*   %WMJMVIA = WQEVIA;                  /* WMJMVIA NOT SPECIFIED-       16055600
*                                       MAJOR DEFAULTS TO BASE WQEVIA   16055800
*                                       VALUE                    @LFA*/ 16056000
* %IF WMNMVIA = 'YES' %THEN             /* CHECK IF ALET SPECIFIED FOR  16056200
*                                       THE MINOR WQE            @LFA*/ 16056400
*   %WMNMVIA = 'VIA(WMNMALET)';         /* SET MINOR WQE VIA TO ALET    16056600
*                                       SPECIFIED                @LFA*/ 16056800
* %ELSE                                 /* NO ALET SPECIFIED     @LFA*/ 16057000
*   %WMNMVIA = WMJMVIA;                 /* WMNMVIA NOT SPECIFIED-       16057200
*                                       MINOR DEFAULTS TO MAJOR VIA     16057400
*                                       VALUE                    @LFA*/ 16057600
*                                                                       16060002
