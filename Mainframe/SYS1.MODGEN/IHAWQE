*DECLARE                                                                16950002
*  1 WQE BASED(WQEPTR) WQEVIA BDY(WORD), /* START OF WQE         @LFC*/ 16960000
*   2 WQELKP      PTR(31),               /* LINKAGE POINTER      @L6C*/ 16970000
*   2 WQENBR      FIXED(31),             /* MESSAGE LENGTH (CCW         17250002
*                                           COUNT FIELD)             */ 17300002
*    3 *           FIXED(15),            /* NOT TO BE USED       @L5A*/ 17305800
*    3 WQETXTLN    FIXED(15),            /* ACTUAL TEXT LENGTH   @L5A*/ 17311600
*   2 WQERTCT     PTR(15) BDY(WORD),     /* ROUTED WQE COUNT     @L5C*/ 17317400
*   2 WQEUSE      PTR(15),               /* WQE USE COUNT        @L6C*/ 17323200
*   2 WQEPAD      CHAR(1),               /* BLANK              Y02710*/ 17329000
*   2 WQETS       CHAR(8) BDY(WORD,2),   /* TIME STAMP         Y02710*/ 17334800
*     3 WQETSHH   CHAR(2),               /* HOURS PART OF TIME Y02710*/ 17340600
*     3 WQEPER1   CHAR(1),               /* DECIMAL POINT      Y02710*/ 17346400
*     3 WQETSMM   CHAR(2),               /* MINUTES PART OF TS Y02710*/ 17352200
*     3 WQEPER2   CHAR(1),               /* DECIMAL POINT      Y02710*/ 17358000
*     3 WQETSSS   CHAR(2),               /* SECONDS PART OF TS Y02710*/ 17363800
*   2 WQEPAD1     CHAR(1),               /* BLANK                    */ 17369600
