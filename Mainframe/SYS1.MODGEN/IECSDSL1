*                                       is on, this volume will appear  85225200
*                                       to MVS to have all space        85231500
*                                       allocated in one or more        85237800
*                                       FMT1 DSCBs.              @06A*/ 85244100
*          4 DS4DSF      BIT(1),        /* Set by ICKDSF         @06C*/ 85250400
*          4 DS4DEVAV    BIT(1),        /* Value in DS4DEVAC, number of 85257000
*                                          alternate cylinders, is      85285500
*                                          valid.  Otherwise the number 85314000
*                                          is not available in this     85342500
*                                          DSCB.                 @04A*/ 85371000
*         3 DS4DEVTL    CHAR(2),        /* DEVICE TOLERANCE          */ 85400000
*         3 DS4DEVDT    CHAR(1),        /* NUMBER OF DSCB'S PER TRACK*/ 85600000
*         3 DS4DEVDB    CHAR(1),        /* NUMBER OF DIRECTORY          86000000
*                                          BLOCKS PER TRACK          */ 86200000
*        2 DS4AMTIM    CHAR(8),         /* VSAM TIME STAMP           */ 86250000
*        2 DS4AMCAT    CHAR(3),         /* VSAM CATALOG INDICATOR    */ 86300000
*         3 DS4VSIND    BIT(8),         /* VSAM INDICATORS           */ 86350000
*          4 DS4VSREF    BIT(1),        /* A VSAM CATALOG REFERENCES    86360000
*                                          THIS VOLUME               */ 86370000
*          4 DS4VSBAD    BIT(1),        /* VSAM DATA SETS ON VOLUME     86380000
*                                          ARE UNUSABLE              */ 86390000
*          4 *           BIT(6),        /* RESERVED                  */ 86392000
*         3 DS4VSCRA    PTR(16) BDY(BYTE),/* RELATIVE TRACK LOCATION    86394000
*                                          OF THE CRA                */ 86396000
*        2 DS4R2TIM    CHAR(8),         /* VSAM VOLUME/CATALOG MATCH    86446000
*                                          TIME STAMP                */ 86496000
*        2 *           CHAR(5),         /* RESERVED                  */ 86546000
*        2 DS4F6PTR    CHAR(5),         /* POINTER TO FIRST FORMAT      86600000
*                                          6 DSCB                    */ 86800000
*        2 DS4VTOCE    CHAR(10),        /* VTOC EXTENT DESCRIPTION   */ 87000000
*        2 *           CHAR(10),        /* RESERVED                  */ 87022200
*        2 DS4EFLVL    FIXED(8),/* EXTENDED FREE-SPACE MANAGEMENT@L6A*/ 87044400
*                               /* LEVEL.                        @L6A*/ 87066600
*                               /* X'00' MEANS EXTENDED FREE-    @L6A*/ 87088800
*                               /* SPACE MANAGEMENT WILL NOT BE  @L6A*/ 87111000
*                               /* USED FOR THIS VOLUME.         @L6A*/ 87133200
*                               /* X'07' MEANS WHEN IN NON-INDEX @02C*/ 87155400
*                               /* FORMAT, FREE-SPACE MANAGED IN @L6A*/ 87177600
*                               /* F7 DSCB'S.                    @L6A*/ 87199800
*        2 DS4EFPTR    CHAR(5), /* POINTER TO EXTENDED FREE-SPACE@L6A*/ 87222000
*                               /* INFORMATION:                  @L6A*/ 87244200
*                               /* FOR DS4EFLVL=X'00' THIS IS 0. @L6A*/ 87266400
*                               /* FOR DS4EFLVL=X'07' THIS IS THE@L6A*/ 87288600
*                               /* CCHHR OF THE FIRST F7 DSCB.   @L6A*/ 87310800
*        2 *           CHAR(9), /* RESERVED                      @L6A*/ 87377400
*        2 DS4END      CHAR(0);         /* END OF THE FORMAT 4 DSCB  */ 87400000
*DCL DS4SMS   BIT(2) CONSTANT('11'B);   /* SYSTEM MANAGED VOLUME @P3C*/ 87433300
*DCL DS4SMSCV BIT(2) CONSTANT('01'B);   /* SYSTEM MANAGED VOLUME IN     87466600
*                                          INITIAL STATUS        @P3C*/ 87499900
