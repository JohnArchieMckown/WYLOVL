*/*                                                                  */ 41400000
*/* **************************************************************** */ 41600000
*/*                                                                  */ 41800000
*/*                     PL/S VERSION OF IECSDSL1                     */ 41850000
*/*                                                                  */ 42200000
*/*      ******************************************************      */ 42400000
*/*                                                                  */ 42600000
*/*      BEFORE THE INCLUDE STATEMENT FOR THIS MACRO, THE            */ 42800000
*/*      VARIABLES CORRESPONDING TO THE DSCB'S TO BE MAPPED          */ 43000000
*/*      MUST BE INITIALIZED IN THE FOLLOWING MANNER -               */ 43200000
*/*                                                                  */ 43400000
*/*      FOR A FORMAT 1 DSCB -                                       */ 43600000
*/*      FORMAT1 MUST BE A MACRO VARIABLE SET TO 'YES'.              */ 43800000
*/*      F1PTR MUST BE A POINTER VARIABLE.                           */ 44000000
*/*                                                                  */ 44200000
*/*      FOR A FORMAT 2 DSCB -                                       */ 44400000
*/*      FORMAT2 MUST BE A MACRO VARIABLE SET TO 'YES'.              */ 44600000
*/*      F2PTR MUST BE A POINTER VARIABLE.                           */ 44800000
*/*                                                                  */ 45000000
*/*      FOR A FORMAT 3 DSCB -                                       */ 45200000
*/*      FORMAT3 MUST BE A MACRO VARIABLE SET TO 'YES'.              */ 45400000
*/*      F3PTR MUST BE A POINTER VARIABLE.                           */ 45600000
*/*                                                                  */ 45800000
*/*      FOR A FORMAT 4 DSCB -                                       */ 46000000
*/*      FORMAT4 MUST BE A MACRO VARIABLE SET TO 'YES'.              */ 46200000
*/*      F4PTR MUST BE A POINTER VARIABLE.                           */ 46400000
*/*                                                                  */ 46600000
*/*      FOR A FORMAT 5 DSCB -                                       */ 46800000
*/*      FORMAT5 MUST BE A MACRO VARIABLE SET TO 'YES'.              */ 47000000
*/*      F5PTR MUST BE A POINTER VARIABLE.                           */ 47200000
*/*                                                                  */ 47400000
*/*      FOR A FORMAT 6 DSCB -                                       */ 47600000
*/*      FORMAT6 MUST BE A MACRO VARIABLE SET TO 'YES'.              */ 47800000
*/*      F6PTR MUST BE A POINTER VARIABLE.                           */ 48000000
*/*                                                                  */ 48200000
*/*      NOTE THAT MORE THAN ONE DSCB CAN BE MAPPED PER INCLUDE      */ 48400000
*/*      STATEMENT BY SETTING MORE THAN ONE MACRO VARIABLE TO        */ 48600000
*/*      'YES'.  HOWEVER, THE DSCB'S WILL BE MAPPED IN ASCENDING     */ 48800000
*/*      NUMERIC SEQUENCE.  TO MAP THE DSCB'S IN NON-ASCENDING       */ 49000000
*/*      NUMERIC ORDER, SEPARATE POINTER VARIABLES MUST BE USED.     */ 49050000
*/*                                                                  */ 49200000
*/*      ******************************************************      */ 49400000
*/*                                                                  */ 49600000
*%PLSDSCB: ;                                                            49650000
*%IF FORMAT1 ~= 'YES'                   /* IF FORMAT1 = YES, THEN    */ 49800000
*    %THEN %GOTO F2;                    /* BUILD A FORMAT 1 DSCB     */ 50000000
*/*                    FORMAT 1 DSCB                                 */ 50200000
*DCL 1 IECSDSF1 BASED(F1PTR),           /* FORMAT 1 DSCB - DESCRIBED */ 50400000
*        2 *           CHAR(140);       /* IN THE FOLLOWING STRUCTURE*/ 50600000
*DCL 1 IECSDSL1 BASED(ADDR(IECSDSF1)),  /* START OF THE FORMAT 1 DSCB*/ 50800000
*        2 DS1DSNAM    CHAR(44),        /* DATA SET NAME             */ 51000000
*        2 DS1FMTID    CHAR(1),         /* FORMAT IDENTIFIER         */ 51200000
*        2 DS1DSSN     CHAR(6),         /* DATA SET SERIAL NUMBER    */ 51400000
*        2 DS1VOLSQ    CHAR(2),         /* VOLUME SEQUENCE NUMBER    */ 51600000
*        2 DS1CREDT    CHAR(3),         /* CREATION DATE             */ 51800000
*        2 DS1EXPDT    CHAR(3),         /* EXPIRATION DATE           */ 52000000
*        2 DS1NOEPV    CHAR(1),         /* NUMBER OF EXTENTS ON         52200000
*                                          VOLUME                    */ 52400000
*        2 DS1NOBDB    CHAR(1),         /* NUMBER OF BYTES USED IN      52600000
*                                          LAST DIRECTORY BLOCK (PDS    52650000
*                                          ONLY)                     */ 52700000
*        2 DS1FLAG1    CHAR(1),         /* FLAG 1                @LAA*/ 52750000
*          3 DS1COMPR BIT(1),           /* COMPRESSABLE EXTENDED        52800000
*                                            FORMAT (DS1STRP MUST       52850000
*                                            ALSO BE SET)        @LAA*/ 52900000
*          3 DS1CPOIT  BIT(1),          /* CHECKPOINTED DATASET  @03A*/ 52950000
*          3 *         BIT(6),          /* RESERVED              @03A*/ 53000000
*        2 DS1SYSCD    CHAR(13),        /* SYSTEM CODE               */ 53200000
*        2 DS1REFD     CHAR(3),         /* DATE LAST                    53250013
*                                          REFERENCED            @01C*/ 53300013
*        2 DS1SMSFG    CHAR(1),         /* SYSTEM MANAGED STORAGE       53327200
*                                          INDICATORS            @L4A*/ 53354400
