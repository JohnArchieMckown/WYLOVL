*/*                                                                  */ 00950000
*/*  STATUS: DFSMS/MVS 1.5.0                                         */ 01000000
*/*                                                                  */ 01200000
*/********************************************************************/ 01216613
*/*                                                                  */ 01233213
*/*              ASSEMBLY LANGUAGE VERSION OF IECSDSL1               */ 01250000
*/*                                                                  */ 01600000
*/*      ******************************************************      */ 01800000
*/*                                                                  */ 02000000
*/*      THE FORMAT OF EACH OF THE DATA SET CONTROL                  */ 02200000
*/*      BLOCKS (DSCB'S) CAN BE GENERATED BY CODING                  */ 02400000
*/*                    IECSDSL1 (N)                                  */ 02600000
*/*      WHERE N IS A NUMBER 1 - 6 CORRESPONDING TO THE              */ 02800000
*/*      FORMAT NUMBER OF THE DSCB TO BE MAPPED.  MORE               */ 03000000
*/*      THAN ONE DSCB CAN BE MAPPED PER MACRO CALL BY               */ 03200000
*/*      SEPARATING THE FORMAT NUMBERS BY COMMAS.  THE               */ 03400000
*/*      DSCB'S THUS GENERATED WILL BE MAPPED IN THE                 */ 03600000
*/*      ORDER IN WHICH THE FORMAT NUMBERS ARE CODED.                */ 03800000
*/*                                                                  */ 03822200
*/*      ******************************************************      */ 03844400
*/*                                                                  */ 03866600
*/*      NOTE: THIS MACRO DOES NOT GENERATE A DSECT STATEMENT.   @02A*/ 03888800
*/*      IT CONTINUES THE PREVIOUS DSECT OR CSECT.  FOR ONE      @02A*/ 03911000
*/*      INVOCATION, THE DSCB'S ARE CONTIGUOUS, NOT OVERLAID ON  @02A*/ 03933200
*/*      SAME AREA.  THIS APPLIES ONLY TO THE ASSEMBLER; IT      @02A*/ 03955400
*/*      MEANS NOTHING IN PL/X.                                  @02A*/ 03977600
*/*                                                                  */ 04000000
*/*      ******************************************************      */ 04200000
*/*                                                                  */ 04400000
         MACRO                                                          04600000
         IECSDSL1 &PARA                                                 04800000
.********************************************************************** 04810013
.*                                                                      04820000
.*MODULE-TYPE = MACRO                                                   04820500
.*                                                                      04821000
.*PROCESSOR = ASSEMBLER OR PLS                                          04821500
.*                                                                      04822000
.*CHANGE ACTIVITY =                                                     04822500
.*$L1=IXVTOC,JDM1134,T790227,STLBS: INDEXED VTOC SUPPORT           @L1A 04823313
.*$L2=ICFCAT,JDM1113,T790225,STLMD: ICF CATALOG SUPPORT            @L2A 04826613
.*$01=OZ52996,JDM1113,81110,STLRRB: DATE LAST REFERENCED           @01A 04829913
.*$L3=MVS/XA,JDP1111,,STLBS:        BIT DESCRIPTIONS               @L3A 04831000
.*$P1=KKC0088,HDP2210,,STLAM:       REMOVE SEMICOLON FROM ASM REM  @P1A 04832100
.*$L4=MVS/XA,HDP3310,T86304,STLMC:  DADSM SUPPORT FOR SMS          @L4A 04832600
.*$P2=KPQ0028,HDP2230,T86111,STLAM: ADD INDEX DISABLE BIT IN F4    @P2A 04832900
.*                                  (FROM APAR OZ97209 FOR HDP2210)@P2A 04833000
.*$P3=KJA0756,HDP3310,T86293,STLKAA: COMPACT SAVED SECONDARY QTY   @P3A 04833100
.*$P4=KJA2615,HDP3310,T87118,STLKAA: CLARIFY FIELD DESCRIPTIONS    @P4A 04875400
.*$P5=KJA2737,HDP3310,T87159,STLKAA: ADD SYSTEM DETERMINED         @P5A 04889500
.*                                   BLOCKSIZE FLAG IN FMT1        @P5A 04903600
.*$L5=PDSE,HDP3320,T87201,STLKAA:  PDSE DATA SET SUPPORT           @L5A 04910600
.*$P6=KJB3529,HDP3310,T87251,STLKAA: CLARIFY UNCATALOGED BIT       @P6A 04914100
.*$P7=KET1795,HDP3320,T89044,STLKSC: CHANGE TO DS1PDSE.            @P7A 04915900
.*$L6=64KTRACK,JDZ1110,T920103,SJPLKSC: DADSM SUPPORT FOR DASD WITH@L6A 04916200
.*                                      MORE THAN 64K TRACKS       @L6A 04916500
.*$L7=STRIPE,JDZ1110,911003,SJPLRPS:  EXTENDED SEQUENTIAL SUPPORT  @L7A 04916600
.*$L8=PDSEX,JDZ1110,920115,SJPLRPS:     HFS DATA SET SUPPORT       @L8A 04916700
.*$L9=EXTATTR,JDZ1110,920910,SJPLRPS:   EXTENDED ATTRIBUTE SUPPORT @L9A 04916800
.*$02=OY_____,HDP3330,930122,SJPLRPS:   ASSORTED CLEANUP           @02A 04916900
.*$LA=COMPRESS,HDZ11B0,930122,SJPLRPS:  EXTENDED FORMAT SUPPORT    @LAA 04917000
.*$03=OW00735,HDZ11B0,93AUG09,SJPLRG: ADD BIT DS1CPOIT TO FMT1     @03A 04917300
.*$04=OW22041,HDZ11C0,96Aug09,SJPLRPS alternate cylinder           @04A 04917500
.*$LB=SERVICE,HDZ11D0,960201,SJPLGEC: O/C/EOV SERVICEABILITY       @LBA 04917600
.*$05=OW32541,HDZ11D0,98Mar27,SJPLRPS define Amberjack bit         @05A 04931700
.*                                                                      04945800
.**** END OF SPECIFICATIONS ******************************************* 04960000
         LCLA  &CTR                                                     05000000
&CTR     SETA  1                                                        05200000
.LOOP    AIF   (&CTR GT N'&PARA).ENDOFIT                                05600000
         AIF   ('&PARA(&CTR)' EQ '1').FIRSTON                           05800000
         AIF   ('&PARA(&CTR)' EQ '2').SECNDON                           06000000
         AIF   ('&PARA(&CTR)' EQ '3').THIRDON                           06200000
         AIF   ('&PARA(&CTR)' EQ '4').FORTHON                           06400000
         AIF   ('&PARA(&CTR)' EQ '5').FIFTHON                           06600000
         AIF   ('&PARA(&CTR)' EQ '6').SIXTHON                           06800000
         AGO   .ERROR                                                   07000000
.FIRSTON ANOP                                                           07200000
