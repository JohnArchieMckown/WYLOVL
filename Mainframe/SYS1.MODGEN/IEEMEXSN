*%GOTO  MEXSN0;                    /*  BILINGUAL MAPPING                00050000
         MACRO                                                          00100000
         IEEMEXSN ,                                                  */ 00150000
*%MEXSN0: ;                                                             00200000
*/********************************************************************/ 00250000
*/*                                                                  */ 00300000
*/*     Excessive Spin Notification Routine Parameter List (EXSN)    */ 00350000
*/*                                                                  */ 00400000
*/*  COMPONENT: SCLWT (Loadwait/Restart)                             */ 00450000
*/*                                                                  */ 00500000
*/*  COPYRIGHT =                                                     */ 00550000
*/*                                                                  */ 00575000
*/*     5695-047                                                     */ 00600000
*/*     THIS MODULE IS "RESTRICTED MATERIALS OF IBM"                 */ 00650000
*/*     (C) COPYRIGHT IBM CORP. 1986, 1990                           */ 00700000
*/*     LICENSED MATERIALS - PROPERTY OF IBM                         */ 00750000
*/*                                                                  */ 00900000
*/*  STATUS:  HBB4410                                                */ 00950000
*/*                                                                  */ 01000000
*/*  FUNCTION:                                                       */ 01050000
*/*                                                                  */ 01200000
*/*     Maps a parameter list for interfacing with                   */ 01233300
*/*     Excessive Spin Notification Routine (IEEVEXSN).              */ 01266600
*/*                                                                  */ 01300000
*/*  METHOD OF ACCESS:                                               */ 01350000
*/*                                                                  */ 01450000
*/*     BAL  -  Include the IEEMEXSN mapping macro.                  */ 01500000
*/*             Produces DSECT EXSN.                                 */ 01550000
*/*                                                                  */ 01600000
*/*     PL/S -  Include IEEMEXSN mapping macro.                      */ 01650000
*/*             Produces structure EXSN.                             */ 01700000
*/*                                                                  */ 01750000
*/*          -  Set macro variable %EXSNBASE for an alternate        */ 01800000
*/*             base for EXSN structure.                             */ 01850000
*/*                                                                  */ 01900000
*/*  CREATED BY:                                                     */ 01950000
*/*                                                                  */ 01975000
*/*     Callers of IEEVEXSN.                                         */ 02000000
*/*                                                                  */ 02050000
*/*  DELETED BY:                                                     */ 02100000
*/*                                                                  */ 02125000
*/*     Callers of IEEVEXSN.                                         */ 02150000
*/*                                                                  */ 02200000
*/*  POINTED TO BY:                                                  */ 02225000
*/*                                                                  */ 02237500
*/*     On entry to IEEVEXSN, Register 1 must point to               */ 02250000
*/*     this parameter list.                                         */ 02275000
*/*                                                                  */ 02300000
*/*  SERIALIZATION: N/A.                                             */ 02350000
*/*                                                                  */ 02450000
*/*  SUBPOOL AND KEY:                                                */ 02500000
*/*                                                                  */ 02550000
*/*     Caller's area  - Caller's subpool and storage key.           */ 02600000
*/*                                                                  */ 02650000
*/*  CHANGE ACTIVITY:                                                */ 02662500
*/*                                                                  */ 02675000
*/*  Flag Reason  Release Date   Origin  Description                 */ 02677200
*/*  ------------ ------- ------ ------- --------------------------- */ 02679400
*/*  $D0=DCRR0026 JBB2220 840907 PDR8:   IEEVEXSN INTERFACE CHANGE   */ 02681600
*/*                                                              @D0A*/ 02683800
*/*  $L1=EXSPIN   HBB3310 861029 PD1X:   AUTOMATE SPIN LOOP RECOVERY */ 02686000
*/*                                                              @L1A*/ 02688200
*/*  $H1=CNITG    HBB4410 890918 PDEH:   Update Prolog.              */ 02690400
*/*                                                              @H1A*/ 02692600
*/*  $P1=PEO4071  HBB4410 900330 PDEH:   Copyright fix.          @P1A*/ 02693700
*/*                                                                  */ 02694800
*/*                                                                  */ 02697000
*/********************************************************************/ 02700000
*%GOTO MEXSN1; /*                                                       02750000
*                                                                       02800000
EXSN     DSECT               EXCESSIVE SPIN ROUTINE PARAMETER LIST.     02850000
EXSNCPUA DS    XL1           LOGICAL ADDRESS OF CPU CAUSING EXCESSIVE   02900000
*                            SPIN DETECTION.                            02950000
EXSNREAS DS    XL1           REASON FOR EXCESSIVE SPIN DETECTION.       03000000
*                            SEE CONSTANTS BELOW.                       03050000
EXSNCCNT DS    XL1           COUNT OF NUMBER OF TIMES IEEVEXSN HAS      03100000
*                            PROCESSED CALLS FROM THE SAME SPIN LOOP.   03150000
EXSNRV03 DS    CL13          RESERVED.                                  03200000
*                                                                       03250000
*                                                                       03300000
EXSNLEN  EQU   16            LENGTH OF EXCESSIVE SPIN ROUTINE           03350000
*                            PARAMETER LIST IN BYTES.                   03400000
*                                                                       03450000
*        THE FOLLOWING CONSTANTS REPRESENT REASONS FOR EXCESSIVE SPIN   03500000
*        DETECTION. THEY INDICATE THE TEXT TO BE INSERTED IN MESSAGE    03550000
*        IEA490A, IEE331A OR IEE601E. THEY ARE ALSO USED AS THE         03600000
*        NUMBER TO BE ADDED TO '090'X TO PRODUCE A WAIT STATE CODE.     03650000
*                                                                       03700000
EXSNRIRS EQU   1             EXCESSIVE SPIN WAITING FOR RISGNL          03750000
*                            RESPONSE CONDITION. USED BY IEAVERI.       03800000
EXSNLKRL EQU   2             EXCESSIVE SPIN WAITING FOR LOCK RELEASE    03850000
*                            CONDITION. USED BY IEAVELK.                03900000
EXSNNTOP EQU   3             EXCESSIVE SPIN WAITING FOR SIGP NOT        03950000
*                            OPERATIONAL CONDITION. USED BY IEAVESGP.   04000000
EXSNRSTR EQU   4             EXCESSIVE SPIN WAITING FOR RESTART         04050000
*                            RESOURCE. USED BY LOADWAIT SERVICE.   @H1A 04100000
EXSNASQU EQU   5             EXCESSIVE SPIN WAITING FOR ADDRESS SPACE   04150000
*                            TO QUIESCE CONDITION. USED BY IEAVTMTC.    04200000
EXSNSTOP EQU   6             EXCESSIVE SPIN WAITING DUE TO A       @L1A 04216600
*                            STOPPED CONDITION. USED BY IEEVEXSN.  @L1A 04233200
EXSNINRL EQU   7             EXCESSIVE SPIN WAITING FOR INTERSECT       04250000
*                            RELEASE CONDITION. USED BY IEAVEINT.       04300000
EXSNEQCK EQU   8             EXCESSIVE SPIN WAITING FOR SIGP EQUIPMENT  04350000
*                            CHECK CONDITION. USED BY IEEVESGP.         04400000
EXSNOPIN EQU   9             EXCESSIVE SPIN WAITING FOR SIGP OPERATOR   04450000
*                            INTERVENING CONDITION. USED BY IEAVESGP.   04500000
EXSNCHSP EQU   10            EXCESSIVE SPIN WAITING FOR SIGP CHECK      04550000
*                            STOP CONDITION. USED BY IEAVESGP.          04600000
EXSNNTRY EQU   11            EXCESSIVE SPIN WAITING FOR SIGP NOT        04650000
*                            READY CONDITION. USED BY IEAVESGP.         04700000
EXSNBUSY EQU   12            EXCESSIVE SPIN WAITING FOR SIGP BUSY       04750000
*                            CONDITION. USED BY IEAVESGP.               04800000
EXSNRCCK EQU   13            EXCESSIVE SPIN WAITING FOR SIGP RECEIVER   04850000
*                            CHECK CONDITION. USED BY IEAVESGP.         04900000
EXSNBBRL EQU   14            EXCESSIVE SPIN WAITING FOR SUCCESSFUL      04950000
*                            BIND BREAK CONDITION. USED BY IEAVEBBR.    05000000
         MEND                                                           05050000
*              */                                                       05100000
*%MEXSN1: ;                                                             05150000
*@EJECT;                                                                05200000
*%ACTIVATE EXSNBASE;                                                    05250000
*   DCL 1 EXSN BDY(WORD) EXSNBASE, /* EXCESSIVE SPIN ROUTINE            05300000
*                                     PARAMETER LIST.                */ 05350000
*        3 EXSNCPUA FIXED(8),      /* LOGICAL ADDRESS OF CPU CAUSING    05400000
*                                     EXCESSIVE SPIN DETECTION.      */ 05450000
*        3 EXSNREAS FIXED(8),      /* REASON FOR EXCESSIVE SPIN         05500000
*                                     DETECTION. SEE CONSTANTS BELOW.*/ 05550000
*        3 EXSNCCNT FIXED(8),      /* COUNT OF NUMBER OF TIMES          05600000
*                                     IEEVEXSN HAS PROCESSED CALLS      05650000
*                                     FROM THE SAME SPIN LOOP.       */ 05700000
*        3 EXSNRV03 CHAR(13);      /* RESERVED.                      */ 05750000
*                                                                       05800000
*     /*                                                                05850000
*       THE FOLLOWING CONSTANTS REPRESENT REASONS FOR EXCESSIVE SPIN    05900000
*       DETECTION. THEY INDICATE THE TEXT TO BE INSERTED IN MESSAGE     05950000
*       IEA490A, IEE331A OR IEE601E. THEY ARE ALSO USED AS THE          06000000
*       NUMBER TO BE ADDED TO '090'X TO PRODUCE A WAIT STATE CODE.      06050000
*                                                                    */ 06100000
*   DCL EXSNRIRS FIXED(8)                                               06150000
*          CONSTANT(1),            /* EXCESSIVE SPIN WAITING FOR        06200000
*                                     RISGNL RESPONSE CONDITION.        06250000
*                                     USED BY IEAVERI.               */ 06300000
*       EXSNLKRL FIXED(8)                                               06350000
*          CONSTANT(2),            /* EXCESSIVE SPIN WAITING FOR        06400000
*                                     LOCK RELEASE CONDITION.           06450000
*                                     USED BY IEAVELK.               */ 06500000
*       EXSNNTOP FIXED(8)                                               06550000
*          CONSTANT(3),            /* EXCESSIVE SPIN WAITING FOR        06600000
*                                     SIGP NOT OPERATIONAL CONDITION.   06650000
*                                     USED BY IEAVESGP.              */ 06700000
*       EXSNRSTR FIXED(8)                                               06750000
*          CONSTANT(4),            /* EXCESSIVE SPIN WAITING FOR        06800000
*                                     RESTART RESOURCE.                 06850000
*                                     USED BY LOADWAIT SERVICE.  @H1A*/ 06900000
*       EXSNASQU FIXED(8)                                               06950000
*          CONSTANT(5),            /* EXCESSIVE SPIN WAITING FOR        07000000
*                                     ADDRESS SPACE TO QUIESCE          07050000
*                                     CONDITION.                        07100000
*                                     USED BY IEAVTMTC.              */ 07150000
*       EXSNSTOP FIXED(8)                                               07160000
*          CONSTANT(6),            /* EXCESSIVE SPIN WAITING DUE TO     07170000
*                                     A STOPPED CONDITION.              07180000
*                                     USED BY IEEVEXSN.          @L1A*/ 07190000
*       EXSNINRL FIXED(8)                                               07200000
*          CONSTANT(7),            /* EXCESSIVE SPIN WAITING FOR        07250000
*                                     INTERSECT RELEASE CONDITION.      07300000
*                                     USED BY IEAVEINT.              */ 07350000
*       EXSNEQCK FIXED(8)                                               07400000
*          CONSTANT(8),            /* EXCESSIVE SPIN WAITING FOR        07450000
*                                     SIGP EQUIPMENT CHECK CONDITION.   07500000
*                                     USED BY IEAVESGP.              */ 07550000
*       EXSNOPIN FIXED(8)                                               07600000
*          CONSTANT(9),            /* EXCESSIVE SPIN WAITING FOR        07650000
*                                     SIGP OPERATOR INTERVENING         07700000
*                                     CONDITION.                        07750000
*                                     USED BY IEAVESGP.              */ 07800000
*       EXSNCHSP FIXED(8)                                               07850000
*          CONSTANT(10),           /* EXCESSIVE SPIN WAITING FOR        07900000
*                                     SIGP CHECK STOP CONDITION.        07950000
*                                     USED BY IEAVESGP.              */ 08000000
*       EXSNNTRY FIXED(8)                                               08050000
*          CONSTANT(11),           /* EXCESSIVE SPIN WAITING FOR        08100000
*                                     SIGP NOT READY CONDITION.         08150000
*                                     USED BY IEAVESGP.              */ 08200000
*       EXSNBUSY FIXED(8)                                               08250000
*          CONSTANT(12),           /* EXCESSIVE SPIN WAITING FOR        08300000
*                                     SIGP BUSY CONDITION.              08350000
*                                     USED BY IEAVESGP.              */ 08400000
*       EXSNRCCK FIXED(8)                                               08450000
*          CONSTANT(13),           /* EXCESSIVE SPIN WAITING FOR        08500000
*                                     SIGP RECEIVER CHECK CONDITION.    08550000
*                                     USED BY IEAVESGP.              */ 08600000
*       EXSNBBRL FIXED(8)                                               08650000
*          CONSTANT(14);           /* EXCESSIVE SPIN WAITING FOR        08700000
*                                     SUCCESSFUL BIND BREAK RELEASE     08750000
*                                     CONDITION.                        08800000
*                                     USED BY IEAVEBBR.              */ 08850000
*%DEACTIVATE EXSNBASE;                                                  08900000
