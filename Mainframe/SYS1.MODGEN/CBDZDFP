*   3 *        CHAR(3),             /* Reserved, must be zero        */ 00051613
*   3 DFPRELP  PTR(31),             /* Pointer to array containing UCB  00051713
*                                      relocation information        */ 00051813
*   3 DFPRELCT FIXED(31),           /* Number of entries in array of    00051913
*                                      UCB relocation information    */ 00052013
*   3 DFPDFLTM CHAR(4),             /* Default model number          */ 00052113
*   3 DFPIOSUS CHAR(4),             /* For IOS use               @H4A*/ 00052213
*   3 *        CHAR(20),            /* Reserved, must be zero    @H4C*/ 00052313
*   3 DFPEND   CHAR(0);             /* End of DFP.                   */ 00052413
*@SPACE (2);                                                            00052513
*/********************************************************************/ 00052613
*/*                                                                  */ 00052713
*/*  The following maps the array of UCB relocation information.     */ 00052813
*/*                                                                  */ 00052913
*/********************************************************************/ 00053013
*@SPACE (2);                                                            00053113
*DECLARE                                                                00053213
* 1 DFPRELAR(*) BASED(DFPRELP),     /* Array of UCB relocation          00053313
*                                      information                   */ 00053413
*   3 DFPPSEG  FIXED(8),            /* UCB segment type where the ACON  00053513
*                                      is located                    */ 00053613
*   3 DFPPALEN FIXED(8),            /* ACON length                      00053713
                                       if not specified, defaults to    00053813
*                                      4 bytes                       */ 00053913
*   3 DFPPOFF  FIXED(15),           /* Offset into segment where the    00054013
*                                      ACON is located               */ 00054113
*   3 DFPRSEG  FIXED(8),            /* UCB segment type where the ACON  00054213
*                                      points                        */ 00054313
*   3 *        CHAR(3),             /* Reserved, must be zero        */ 00054413
*   3 DFPRDNBR FIXED(32),           /* Device number of the UCB where   00054513
*                                      the ACON points               */ 00054613
*   3 DFPREND  CHAR(0);             /* End of DFPRELAR               */ 00054713
*DECLARE DFPRLEN FIXED(31) CONSTANT(LENGTH(DFPRELAR)); /* Length of     00054813
*                                                        DFPRELAR    */ 00054913
*@SPACE (2);                                                            00055013
*/********************************************************************/ 00055113
*/*                                                                  */ 00055213
*/*  The following constants are used to fill in the DFPPSEG         */ 00055313
*/*  and DFPRSEG fields.  Note - DFPCMSEG and DFPPREFX can only      */ 00055413
*/*  be used to fill in the DFPRSEG field.                           */ 00055513
*/*                                                                  */ 00055613
*/********************************************************************/ 00055713
*@SPACE (2);                                                            00055813
*DECLARE DFPDDSEG FIXED(8) CONSTANT(1); /* Device dependent segment  */ 00055913
*DECLARE DFPDDEXT FIXED(8) CONSTANT(2); /* Device dependent             00056013
*                                      extension                     */ 00056113
*DECLARE DFPDCEXT FIXED(8) CONSTANT(3); /* Device class extension    */ 00056213
*DECLARE DFPCMSEG FIXED(8) CONSTANT(4); /* Common segment            */ 00056313
*DECLARE DFPPREFX FIXED(8) CONSTANT(5); /* Prefix                    */ 00056413
*@EJECT;                                                                00056513
*/********************************************************************/ 00056613
*/*                                                                  */ 00056713
*/*  The following constants are used to place an identifier and     */ 00056813
*/*  version number in the DFP.                                      */ 00056913
*/*                                                                  */ 00057013
*/********************************************************************/ 00057113
*@SPACE (2);                                                            00057213
*DECLARE DFPCBID CHAR(4) CONSTANT('DFP '); /* DFP identifier         */ 00057313
*DECLARE DFPVERN BIT(8)  CONSTANT('01'X);  /* DFP version number     */ 00057413
*@SPACE (2);                                                            00057513
*%DEACT DFPDEF;                                                         00058010
