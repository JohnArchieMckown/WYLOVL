*/*                                                                  */ 01050000
*/* FUNCTION = O/C/E MACRO USED TO PROVIDE ADDRESSABILITY TO         */ 01100000
*/*            O/C/E WORK AREA EXTENSION                             */ 01150000
*/*                                                                  */ 01200000
*/* NOTES = NONE.                                                    */ 01250000
*/*                                                                  */ 01300000
*/*     DEPENDENCIES = NONE                                          */ 01350000
*/*                                                                  */ 01400000
*/*     RESTRICTIONS = CHANGES TO THIS MACRO MUST BE COORDINATED     */ 01450000
*/*                    WITH FUNCTIONS WHICH INTERFACE INTERNALLY     */ 01500000
*/*                    WITH O/C/E ROUTINES. THIS MACRO WAS DESIGNED  */ 01550000
*/*                    TO BE USED IN A DSECT.                        */ 01600000
*/*                                                                  */ 01700000
*/*     REGISTER CONVENTIONS = NOT APPLICABLE.                       */ 01750000
*/*                                                                  */ 01800000
*/* PATCH LABEL = NOT APPLICABLE.                                    */ 01850000
*/*                                                                  */ 01900000
*/* MODULE TYPE = MACRO.                                             */ 01950000
*/*                                                                  */ 02000000
*/*     PROCESSOR = ASSEMBLER LANGUAGE.                              */ 02050000
*/*                                                                  */ 02100000
*/*     MODULE SIZE = DESCRIBED BY THE LABEL WAXLEN.                 */ 02150000
*/*                                                                  */ 02200000
*/*     ATTRIBUTES = DSECT FIELDS.                                   */ 02250000
*/*                                                                  */ 02300000
*/* ENTRY POINT = WAXTN IN BAL AND PLS VERSIONS                      */ 02350000
*/*                                                                  */ 02400000
*/*     PURPOSE = THIS MACRO IS TO BE USED IN CONJUNCTION WITH       */ 02450000
*/*               THE IECDSECS MACRO TO DEFINE AND MAP INTERNAL      */ 02500000
*/*               WORK AREAS USED BY O/C/E AND FUNCTIONS WHICH       */ 02550000
*/*               INTERNALLY INTERFACE WITH O/C/E. THIS MACRO IS     */ 02600000
*/*               CALLED BY O/C/E CSECTS AT ASSEMBLY TIME. IT WILL   */ 02650000
*/*               BE AUTOMATICALLY INCLUDED ANY TIME THE O/C/E       */ 02700000
*/*               WORK AREA IS MAPPED. THE EXTENSION WILL BE         */ 02750000
*/*               OBTAINED IN STORAGE ABOVE THE 16M LINE.            */ 02800000
*/*                                                                  */ 02850000
*/*     LINKAGE = IFGWAX FOR BAL CALL                                */ 02900000
*/*               % INCLUDE SYSLIB(IFGWAX) IN PLS CALL               */ 02950000
*/*                                                                  */ 03000000
*/*     INPUT = NONE                                                 */ 03050000
*/*                                                                  */ 03100000
*/*                                                                  */ 03150000
*/*     OUTPUT = MAPPING OF COMMON O/C/E WORK AREA EXTENSION.        */ 03200000
*/*                                                                  */ 03250000
*/* EXIT NORMAL = MEND STATEMENT                                     */ 03300000
*/*                                                                  */ 03350000
*/* EXIT ERROR = NONE                                                */ 03400000
*/*                                                                  */ 03450000
*/* EXTERNAL REFERENCES = NONE.                                      */ 03500000
*/*                                                                  */ 03550000
*/* TABLES = NONE.                                                   */ 03600000
*/*                                                                  */ 03650000
*/* MACROS = NONE                                                    */ 03700000
*/*                                                                  */ 03750000
*/* CHANGE ACTIVITY =                                                */ 03800000
*/*                                                                  */ 03850000
*/*$00=SERVICE,HDZ11D0,960822,SJPLGEC: INITIAL DISTRIBUTION          */ 03900000
*/*$L1=K140587,HDZ11D0,970228,SJPLJAL: DETECT DUPLICATE VOLSERS  @L1A*/ 03925000
*/*$L2=K140652,HDZ11D0,970228,SJPLJAL: MISSING PLX CLOSING CMT   @L2A*/ 03937500
*/*$L3=ANSIV4,HDZ11E0,981231,SJPLJAL: ANSI V4 SUPPORT            @L3A*/ 03940600
*/*$L4=MODIDTR,HDZ11D0,981231,SJPLJAL: SERVICEABILITY: MODID TRC @L4A*/ 03943700
*/*$L5=ANSIV4,HDZ11E0.981231,SJPLJAL: ANSIV V4 CCSID SUPPORT     @L5A*/ 03946800
*/*$L6=GT32K,HDZ11F0.981109,SJPLGEC: LBI LARGE BLKSIZE SUPPORT   @L6A*/ 03948400
*/*$L7=OW38717,HDZ11E0,990630,SJPLJAL: BLYTHE2 support           @L7A*/ 03949600
*/*$L8=KFI0091,HDZ11F0,990930,SJPLJAL: SAVE PSEUDO LBL BLOCKIDS  @L8A*/ 03949800
*/*$01=OW42881,HDZ11E0,000216,SJPLGEC: TASK OWNS ENQ FLAG        @01A*/ 03949900
*/*$P1=K161198,HDZ11F0,000328,SJPLGEC: CORRECT DOUBLY DEFINED FLG@P1A*/ 03966600
*/*$P2=K161213,HDZ11F0,000331,SJPLGEC: DEFINE NEW FLAG           @P2A*/ 03974900
*/*$02=OW44466,HDZ11F0,000306,SJPLGEC: BLKSZLIM SUPPORT          @02A*/ 03978890
*/*$03=OW44532,HDZ11F0,000525,SJPLGEC: DEFINE NEW FLAG FOR BCB   @03A*/ 03979880
*/*$L9=OW45309,HDZ11F0,000730,SJPLJAL: MTL SUPPORT               @L9A*/ 03980270
*/*                                                                  */ 03980870
*/*                                                                  */ 03983300
*/* **************************************************************** */ 04000000
*/*                                                                  */ 04050000
*/*                                                                  */ 04100000
*/*      THIS MACRO IS USED TO DEFINE AND MAP THE EXTENSION TO THE   */ 04150000
*/*      O/C/E WORK AREA. THE EXTENSION RESIDES ABOVE THE 16M LINE.  */ 04200000
*/*                                                                  */ 04250000
*/*      O/C/E = OPEN, OPEN TYPE=J, CLOSE, CLOSE TYPE=T,             */ 04300000
*/*              EOV, FEOV, & RDJFCB.                                */ 04350000
*/*                                                                  */ 04400000
*/* **************************************************************** */ 04450000
*/*                                                                  */ 05150000
*/* **************************************************************** */ 05200000
*/*                                                                  */ 05250000
*%GOTO WAXL2;                                                           05300000
WAXTN    DSECT                                                          05350000
         SPACE 1                                                        05400000
WAXMODID DS    CL8                 ID OF MODULE WHO DETECTED ERROR(TO   05450000
*                                   BE USED IN DETERMINATE ABEND        05500000
*                                   MESSAGES).                          05550000
         SPACE 1                                                        05600000
*********************************************************************** 05650000
*                                                                       05700000
*        O/C/E INTERNAL USE WORK AREA                                   05750000
*                                                                       05800000
*        WAXWORKN CAN BE USED BY ANY O/C/E MODULE TO DEFINE FLAGS       05850000
*        OR FIELDS WHICH IT WANTS TO USE FOR INTERNAL USE ONLY. IF      05900000
*        ANOTHER O/C/E MODULE IS GIVEN CONTROL THEN THOSE FLAGS OR      05950000
*        FIELDS ARE NO LONGER VALID AND COULD BE DEFINED FOR SOME       06000000
*        OTHER USE BY THE CURRENT O/C/E MODULE IN CONTROL. WAXWORKN     06050000
*        SHOULD NOT BE USED TO PASS INFORMATION FROM ONE MODULE TO      06100000
*        ANOTHER.                                                       06150000
*                                                                       06200000
*********************************************************************** 06250000
         SPACE 1                                                        06300000
WAXWORK0 DS    CL4                      WORK AREA 0                     06350000
WAXWORK1 DS    CL4                      WORK AREA 1                     06400000
WAXWORK2 DS    CL4                      WORK AREA 2                     06450000
WAXWORK3 DS    CL4                      WORK AREA 3                     06500000
WAXWORK4 DS    CL4                      WORK AREA 4                     06550000
WAXWORK5 DS    CL4                      WORK AREA 5                     06600000
WAXWORK6 DS    CL4                      WORK AREA 6                     06650000
WAXWORK7 DS    CL4                      WORK AREA 7                     06700000
WAXWORK8 DS    CL4                      WORK AREA 8                     06750000
WAXWORK9 DS    CL4                      WORK AREA 9                     06800000
         SPACE 1                                                        06850000
WAXNXTVL DS    CL6                      NEXT VOLSER DURING EOV     @L1A 06862500
WAXFLG1  DS    BL1                      MISC FLAGS                 @L1A 06875000
WAX1HRTN EQU   X'80'                    RETURN TO IFG0551H AFTER   @L1A 06887500
*                                       IEC708I                    @L1A 06900000
WAXLBIOK EQU   X'40'                    DEVICE(S) SUPPORT LBI      @L6A 06906200
WAXNLMOD EQU   X'20'                    NL TAPE DS OPENED MOD      @L7A 06907400
WAXNINDX EQU   X'10'                    VOLM/FILEV REQUESTS TO     @L7A 06908600
WAXNOFMT EQU   X'08'                    BLYTHE CART NOT FORMATTED  @L7A 06909800
*                                       DISABLE LOADER INDEXING    @L7A 06911000
WAXNQHLD EQU   X'04'                    THIS TASK OWNS VOLSER ENQ  @P1C 06911700
WAXFSLBI EQU   X'02'                    RECFM IS STANDARD WITH     @P2A 06911900
*                                        DIFFERENT BLKSIZES        @P2A 06912100
WAXCBLNF EQU   X'01'                    BCBCBLNF FLAG ON-MUST GET  @03A 06912190
*                                        BCB BELOW LINE AND BUFFERS@03A 06912280
*                                        ABOVE                     @03A 06912370
         DS    CL1                      RESERVED                   @L1A 06912500
WAXREG14 DS    F                        REG14                      @L1A 06925000
WAXDFVER DS    CL1                      ANSI VERSION SYS DEFAULT   @L3A 06928300
WAXALVER DS    CL1                      PREVAILING ANSI VERSION    @L3A 06931600
         DS    H                        RESERVED                   @L3A 06934900
WAXJBCCS DS    F                        CCSID ON JCL JOB STMT      @L5A 06935800
WAXSPCCS DS    F                        CCSID ON JCL STEP STMT     @L5A 06936700
WAXDDCCS DS    F                        CCSID ON JCL DD  STMT      @L5A 06937600
WAXLBCCS DS    F                        CCSID ON LABEL FOR INPUT   @L5A 06938500
*                                       OR MOD                     @L5A 06939200
WAXCSERR DS    F                        INTERNAL PROB DET ERROR    @L5C 06939900
*                                       CODE FOR CCSID FAILURE     @L5A 06940600
WAXMODTR DS    F                        ADDRESS OF MODID TRACE     @L4A 06941600
WAXLBKS8 DS    0XL8                     SIOTX BLOCK SIZE IN 8 BYTES@L6A 06942500
         DS    F                        HI-ORDER HALF OF BLOCKSIZE @L6A 06943400
WAXLBKSZ DS    CL4                      SIOTX BLOCKSIZE VALUE      @L6A 06944300
WAXMXBLK DS    CL8                      MAXIMUM BLKSIZE SUPPORTED  @L6A 06945200
*                                       (RETURNED BY DEVTYPE)      @L6A 06946100
WAXRBLK  DS    CL8                      RECOMMENDED BLKSIZE        @L6A 06947000
*                                       (RETURNED BY DEVTYPE)      @L6A 06947900
WAXBLKID DS    F                        BLOCKID OF TM AFTER DATA   @L8A 06950500
WAXMTLVL DS    CL6                      MTL VOLSER FROM VOL1, OR   @L9A 06955190
*                                       VOLM OR IEC519A OR VOLNSNS @L9A 06957180
*                                       LABEL                      @L7A 06961000
WAXRECTK DS    XL1                      NTA TAPE TRACK RECTECH -   @L7A 06964900
*                                         TRUE/UNDERLYING VALUE    @L7A 06968800
WAXMEDTY DS    XL1                      NTA TAPE MEDIA TYPE -      @L7A 06972700
*                                         TRUE/UNDERLYING VALUE    @L7A 06976600
WAXCAPM  DS    F                        NTA TAPE MEDIA CAPACITY    @L7A 06980500
WAXCAPP  DS    F                        NTA TAPE PARTITION CAPACITY@L7A 06984400
WAXSVLA  DS    AL4                      SERVL ADDRESS              @L7A 06988300
WAXBKLIM DS    CL8                      BLKSZLIM VALUE             @02A 06993190
WAXSTGRP DS    CL8                      SMS STORAGE GRP ASSOCIATED @L9A 06994370
*                                       WITH CURRENT UNIT          @L9A 06994560
WAXMTDIS DS    CL1                      MEDIA TYPE FOR MSGDISP     @L9A 06994750
WAXSTGTY DS    XL1                      SG TYPE (GENERAL/SPECIFIC) @L9A 06994940
         DS    CL2                      RESERVED FOR FUTURE USE    @L9A 06995130
         DS    15F                      RESERVED FOR FUTURE USE    @L9C 06995320
         SPACE 1                                                        06996100
*                                                                       07000000
****************************************************************        07050000
*                                                                       07100000
WAXLEN   EQU   *-WAXTN                  WORKAREA EXTENSION SIZE         07150000
*                                                                       07200000
****************************************************************        07250000
*                                                                       07300000
*             END OF SYMBOLS GENERATED BY IFGWAX                        07350000
*                                                                       07400000
****************************************************************        07450000
         MEND                                                           07500000
