*                                          AFTER IEC708I         @L1A*/ 09194400
*        3 WAXLBIOK   BIT(1),           /* DEVICE(S) SUPPORT LBI @L6A*/ 09197500
*        3 WAXNLMOD   BIT(1),           /* NL TP DS OPENED MOD   @L7A*/ 09200600
*        3 WAXNINDX   BIT(1),           /* VOLM/FILEV REQUESTS TO@L7A*/ 09203700
*                                       /* DISABLE LOADER INDEX  @L7A*/ 09206800
*        3 WAXNOFMT   BIT(1),           /* BLY CART NOT FORMATTED@L7A*/ 09209900
*        3 WAXNQHLD   BIT(1),           /* THIS TASK OWNS VOLSER        09213900
*                                            ENQ                 @P1C*/ 09214800
*        3 WAXFSLBI   BIT(1),           /* RECFM IS STANDARD WITH       09215200
*                                            DIFFERENT BLKSIZES  @P2A*/ 09215600
*        3 WAXCBLNF   BIT(1),           /* BCBCBLNF FLAG ON-MUST GET    09216090
*                                          BCB BELOW LINE AND BUFFERS   09216180
*                                          ABOVE                 @03C*/ 09216270
*      2 *            CHAR(1),          /* RESERVED              @L1A*/ 09216600
*      2 WAXREG14     CHAR(4),          /* REG14                 @L2C*/ 09227700
*      2 WAXDFVER     CHAR(1),          /* ANSI VER SYS DEFAULT  @L3A*/ 09230600
*      2 WAXALVER     CHAR(1),          /* PREVAILING AL VERSION @L3A*/ 09233500
*      2 *            CHAR(2),          /* RESERVED              @L3A*/ 09236400
*      2 WAXJBCCS     FIXED(32),        /* CCSID ON JCL JOB STMT @L5A*/ 09237700
*      2 WAXSPCCS     FIXED(32),        /* CCSID ON JCL STEP STMT@L5A*/ 09239000
*      2 WAXDDCCS     FIXED(32),        /* CCSID ON JCL DD STMT  @L5A*/ 09240300
*      2 WAXLBCCS     FIXED(32),        /* CCSID ON LBL FOR INPUT       09241600
*                                          OR MODE               @L5A*/ 09242900
*      2 WAXCSERR     FIXED(32),        /* INTERNAL PROB DET ERROR      09244200
*                                          CODE FOR CCSID FAILURE@L5C*/ 09245500
*      2 WAXMODTR     CHAR(4),          /* ADDRESS OF MODID TRACE@L5C*/ 09246800
*      2 WAXLBKS8     CHAR(8),          /* 8 BYTE SIOT BLKSIZE   @L6A*/ 09247100
*        3 *          CHAR(4),          /*                       @L6A*/ 09247400
*        3 WAXLBKSZ   CHAR(4),          /* SIOTX BLKSIZE VALUE   @L6A*/ 09247700
*      2 WAXMXBLK     CHAR(8),          /* MAXIMUM BLKSIZE SUPPORTED    09248000
*                                          (RETURNED BY DEVTYPE) @L6A*/ 09248300
*      2 WAXRBLK      CHAR(8),          /* RECOMMENDED BLKSIZE          09248600
*                                          (RETURNED BY DEVTYPE) @L6A*/ 09248900
*      2 WAXBLKID     FIXED(32),        /* BLOCKID TM AFTER DATA @L8A*/ 09251100
*      2 WAXMTLVL     CHAR(6),          /* MTL VOLSER FROM VOL1, OR     09255290
*                                          VOLM OR IEC519A OR           09257280
*                                          VOLNSNS               @L9A*/ 09259270
*      2 WAXRECTK     FIXED(8),         /* NTA TAPE TRACK RECTECH@L7A   09264700
*                                           TRUE/UNDERLYING VALUE@L7A*/ 09268500
