         SPACE 1                                                        07300000
*********************************************************************** 07350000
*        PREFIX, EXTENDED PREFIX, AND WTG TABLE AREA                    07400000
*********************************************************************** 07450000
         SPACE 1                                                        07500000
PRLPREFX DS    8F                       WORK AREA PREFIX                07550000
PRLSEWKA DS    20F                      EXTENDED PREFIX                 07600000
FIRST    EQU   PRLSEWKA                                                 07650000
PWTGTBL  DS    6F                       DADSM WTG TABLE            @P5C 07700000
         AIF   ('&EP' EQ 'EP').PRL0010                                  07750000
         AIF   ('&EP' EQ '').PRL0020                                    07800000
         IHBERMAC 1001,EP,&EP                                           07850000
         AGO   .PRL0020                                                 07900000
.PRL0010 ANOP                                                           07950000
         ORG   PRLPREFX                                                 08000000
         MNOTE '         IECDSECS PREFX,EXPAND=YES'                     08050000
         IECDSECS PREFX,EXPAND=YES                                      08100000
.PRL0020 ANOP                                                           08150000
         SPACE 1                                                        08200000
*********************************************************************** 08250000
*        FORMAT 4 DSCB AREA                                             08300000
*********************************************************************** 08350000
         SPACE 1                                                        08400000
VTOCDSCB DS    XL96                     FORMAT 4 DSCB                   08450000
         AIF   ('&F4' EQ 'F4').PRL0030                                  08500000
         AIF   ('&F4' EQ '').PRL0040                                    08550000
         IHBERMAC 1001,F4,&F4                                           08600000
         AGO   .PRL0040                                                 08650000
.PRL0030 ANOP                                                           08700000
         ORG   VTOCDSCB                                                 08750000
         MNOTE '         IECSDSL1 (4)'                                  08800000
         IECSDSL1 (4)                                                   08850000
.PRL0040 ANOP                                                           08900000
         SPACE 1                                                        08950000
*********************************************************************** 09000000
*        FIRST 140-BYTE DSCB AREA                                       09050000
*********************************************************************** 09100000
         SPACE 1                                                        09150000
INDSCB   DS    XL140                    140-BYTE DSCB AREA              09200000
&C       SETA  1                                                        09250000
.PRL0050 AIF   (&C GT N'&D1).PRL0060                                    09300000
         AIF   (&D1(&C) EQ 0 OR &D1(&C) EQ 4).PRL0055                   09350000
         ORG   INDSCB                                                   09400000
         MNOTE '         IECSDSL1 (&D1(&C))'                            09450000
         IECSDSL1 (&D1(&C))                                             09500000
.PRL0055 ANOP                                                           09550000
&C       SETA  &C+1                                                     09600000
         AGO   .PRL0050                                                 09650000
.PRL0060 ANOP                                                           09700000
         SPACE 1                                                        09750000
