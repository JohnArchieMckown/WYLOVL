*/*END PROPRIETARY V3 STATEMENT                                      */ 00102600
*/*                                                                  */ 00102800
*/*STATUS = DFSMS/MVS 1.6.0                                          */ 00102900
*/*                                                                  */ 00103000
*/*FUNCTION = DADSM PARTIAL RELEASE MACRO WHICH PROVIDES             */ 00103200
*/*           ADDRESSABILITY TO THE 1608 DECIMAL BYTE WORK AREA  @L3C*/ 00103400
*/*           (NOT INCLUDING THE 8 WORD PREFIX OR THE 6 WORD RECOVERY*/ 00103600
*/*           ROUTINE PARMETER LIST) USED BY PARTIAL RELEASE.        */ 00103800
*/*                                                                  */ 00104000
*%/*NOTES = THIS MACRO IS AN INTERNAL MACRO USED IN THE FOLLOWING   */; 00104200
*%/*        DADSM FUNCTIONS:                                        */; 00104400
*%/*                                                                */; 00104600
*%/*            PARTIAL RELEASE                                     */; 00104800
*%/*                                                                */; 00105000
*%/*        TO REFERENCE THIS WORK AREA WITHIN THE FUNCTIONS LISTED */; 00105200
*%/*        ABOVE, CODE A USING STATEMENT ON THE LABEL 'PRLSEWKA'.  */; 00105400
*%/*                                                                */; 00105600
*%/*     DEPENDENCIES = THIS MACRO IS DESIGNED TO BE USED OPTIONALLY*/; 00105800
*%/*                    WITH THE IECSDSL1 DSCB MAPPING MACRO, THE   */; 00106000
*%/*                    PREFX MACRO (DEFINED IN THE IECDSECS MACRO) */; 00106200
*%/*                    WHICH DEFINES THE 8 WORD WORK AREA PREFIX,  */; 00106400
*%/*                    THE 20 WORD EXTENDED WORK AREA PREFIX, AND  */; 00106600
*%/*                    THE 8 WORD DADSM WTG TABLE, AND THE     @P5C*/; 00106700
*%/*                    DSMADT MACRO (ALSO DEFINED IN THE IECDSECS  */; 00106800
*%/*                    MACRO) WHICH                            @05C*/; 00106900
*%/*                    WHICH DEFINES THE 10 WORD DADSM AUDIT   @05C*/; 00107000
*%/*                    TRAIL.                                  @05C*/; 00107100
*%/*                                                            @L3A*/; 00107400
*%/*                THE SAF PARAMETER LIST LENGTH DEFINED BELOW @L3A*/; 00107600
*%/*                ('SAFPLIST') IS DEPENDENT ON THE LENGTHS OF @L3A*/; 00107800
*%/*                CERTAIN RACF MACROS - SEE THE DETAILED NOTES@L3A*/; 00108000
*%/*                FOLLOWING 'SAFPLIST' FOR A FULL DESCRIPTION.@L3A*/; 00108200
*%/*                                                            @L3A*/; 00108400
*%/*                                                                */; 00108600
*%/*     RESTRICTIONS = NONE                                        */; 00108800
*%/*                                                                */; 00109000
*%/*     REGISTER CONVENTIONS = NOT APPLICABLE                      */; 00109200
*%/*                                                                */; 00109400
*%/*PATCH LABEL = NOT APPLICABLE                                    */; 00109600
*%/*                                                                */; 00109800
*%/*MODULE TYPE = MACRO                                             */; 00110000
*%/*                                                                */; 00110200
*%/*     PROCESSOR = ASSEMBLER LANGUAGE OR PLS COMPILER         @L5C*/; 00110400
*%/*                                                                */; 00110600
*%/*     MODULE SIZE = 1632 DECIMAL BYTES (NOT INCLUDING PREFIX)@L3C*/; 00110800
*%/*                                                                */; 00111000
*%/*     ATTRIBUTES = NOT APPLICABLE                                */; 00111200
*%/*                                                                */; 00111400
*%/*ENTRY POINT = PRLPREFX                                          */; 00111600
*%/*                                                                */; 00111800
*%/*     PURPOSE = THIS MACRO IS CALLED AT ASSEMBLY TIME BY THE     */; 00112000
*%/*               FUNCTIONS LISTED UNDER 'NOTES' TO EXPAND THE     */; 00112200
*%/*               PARTIAL RELEASE WORK AREA.                       */; 00112400
*%/*                                                                */; 00112600
*%/*     LINKAGE =                                                  */; 00112800
*%/*            ASSEMBLER:                                      @L5A*/; 00113000
*%/*                                                                */; 00113200
*%/*               LABEL IECPRLWA EP,F4,D1=(N),D2=(N),ADT=YES       */; 00113400
*%/*                                                                */; 00113600
*%/*            PLS:                                            @L5A*/; 00113800
*%/*                                                            @L5A*/; 00114000
*%/*               %INCLUDE SYSLIB(IECPRLWA)                    @L5A*/; 00114200
*%/*                                                                */; 00114400
*%/*   INPUT =                                                      */; 00114600
*%/*                                                                */; 00114800
*%/*        ASSEMBLER:                                          @L5A*/; 00115000
*%/*                                                                */; 00115200
*%/*           OPTIONAL LABEL SPECIFIED AS AN ACCEPTABLE ASSEMBLER  */; 00115400
*%/*           LABEL WHICH CAN BE USED BY THE CALLING CSECT TO      */; 00115600
*%/*           ESTABLISH ADDRESSABILITY TO THE ENTIRE WORK AREA.    */; 00115800
*%/*                                                                */; 00116000
*%/*           EP IS THE FIRST POSITIONAL PARAMETER, SPECIFYING     */; 00116200
*%/*           THAT THE LABELS FOR EACH OF THE FIELDS IN THE 8      */; 00116400
*%/*           WORD PREFIX, 20 WORD EXTENDED PREFIX, AND THE 4      */; 00116600
*%/*           WORD DADSM WTG TABLE ARE TO BE EXPANDED. IF THESE    */; 00116800
*%/*           FIELDS ARE NOT TO BE DEFINED, A COMMA MUST BE        */; 00117000
*%/*           CODED TO INDICATE THE ABSENCE OF THIS PARAMETER.     */; 00117200
*%/*                                                                */; 00117400
*%/*           F4 IS THE SECOND POSITIONAL PARAMETER, SPECIFYING    */; 00117600
*%/*           THAT THE FIELDS OF THE FORMAT 4 DSCB ARE TO BE       */; 00117800
*%/*           EXPANDED USING THE IECSDSL1 DSCB MAPPING MACRO.      */; 00118000
*%/*           IF THESE FIELDS ARE NOT TO BE DEFINED AND A          */; 00118200
*%/*           KEYWORD PARAMETER FOLLOWS, A COMMA MUST BE           */; 00118400
*%/*           CODED TO INDICATE THE ABSENCE OF THIS PARAMETER.     */; 00118600
*%/*                                                                */; 00118800
*%/*           D1=(N) IS A KEYWORD PARAMETER THAT SPECIFIES THE     */; 00119000
*%/*           FORMAT(S) OF THE DSCB(S) TO BE EXPANDED IN THE       */; 00119200
*%/*           FIRST 140 BYTE DSCB AREA (AT LABEL INDSCB)       @L1A*/; 00119400
*%/*           USING THE IECSDSL1 MACRO.                        @L1A*/; 00119600
*%/*           N CAN BE A NUMBER FROM 0 - 6.  IF N IS 0 OR 4 OR     */; 00119800
*%/*           IF THIS KEYWORD IS OMITTED, NO LABELS ARE EXPANDED.  */; 00120000
*%/*           IF N IS 1, 2, 3, 5, OR 6, THE CORRESPONDING FORMAT   */; 00120200
*%/*           OF THE DSCB WILL BE EXPANDED.  MORE THAN ONE DSCB    */; 00120400
*%/*           FORMAT CAN BE MAPPED IN THE FIRST 140 BYTE DSCB      */; 00120600
*%/*           AREA BY SEPARATING THE FORMAT NUMBERS BY COMMAS.     */; 00120800
*%/*                                                                */; 00121000
*%/*           D2=(N) IS A KEYWORD PARAMETER THAT SPECIFIES         */; 00121200
*%/*           THE FORMAT(S) OF THE DSCB(S) TO BE EXPANDED IN THE   */; 00121400
*%/*           SECOND 140 BYTE DSCB AREA (AT LABEL OUTDSCB),    @L1A*/; 00121600
*%/*           USING THE IECSDSL1 MACRO.                        @L1A*/; 00121800
*%/*           N CAN BE A NUMBER FROM 0 - 6.  IF N IS 0 OR 4 OR     */; 00122000
*%/*           IF THIS KEYWORD IS OMITTED, NO LABELS ARE EXPANDED.  */; 00122200
*%/*           IF N IS 1, 2, 3, 5, OR 6, THE CORRESPONDING FORMAT   */; 00122400
*%/*           OF THE DSCB WILL BE EXPANDED.  MORE THAN ONE DSCB    */; 00122600
*%/*           FORMAT CAN BE MAPPED IN THE SECOND 140 BYTE DSCB     */; 00122800
*%/*           AREA BY SEPARATING THE FORMAT NUMBERS BY COMMAS.     */; 00123000
*%/*                                                                */; 00123200
*%/*           ADT=YES IS A KEYWORD PARAMETER WHICH SPECIFIES THAT  */; 00123400
*%/*           THE LABELS FOR EACH OF THE FIELDS WITHIN THE 10  @05C*/; 00123600
*%/*           WORD DADSM AUDIT TRAIL ARE TO BE EXPANDED.       @05C*/; 00123800
*%/*                                                            @L5A*/; 00124000
*%/*      PLS:                                                  @L5A*/; 00124200
*%/*                                                            @L5A*/; 00124400
*%/*       %PRLBASE -- IF NOT BLANK CONTAINS BASING EXPRESSION  @L5A*/; 00124600
*%/*         FOR THE PRLWA.                                     @L5A*/; 00124800
*%/*                                                            @L5P*/; 00125000
*%/*       %PRLF4 -- IF EQUAL TO 'YES' SIGNIFIES:               @L5A*/; 00125200
*%/*         THAT THE FIELDS OF THE FORMAT 4 DSCB ARE TO BE     @L5P*/; 00125400
*%/*         EXPANDED USING THE IECSDSL1 DSCB MAPPING MACRO.    @L5P*/; 00125600
*%/*         IF THESE FIELDS ARE NOT TO BE DEFINED AND A        @L5P*/; 00125800
*%/*         KEYWORD PARAMETER FOLLOWS, A COMMA MUST BE         @L5P*/; 00126000
*%/*         CODED TO INDICATE THE ABSENCE OF THIS PARAMETER.   @L5P*/; 00126200
*%/*                                                            @L5P*/; 00126400
*%/*       %PRLD1 -- IF EQUAL TO A NUMBER N SIGNIFIES:          @L5A*/; 00126600
*%/*         FORMAT(S) OF THE DSCB(S) TO BE EXPANDED IN THE     @L5P*/; 00126800
*%/*         FIRST 140 BYTE DSCB AREA (AT LABEL INDSCB)         @L5P*/; 00127000
*%/*         USING THE IECSDSL1 MACRO.                          @L5P*/; 00127200
*%/*         N CAN BE A NUMBER FROM 0 - 6.  IF N IS 0 OR 4 OR   @L5P*/; 00127400
*%/*         IF THIS KEYWORD IS OMITTED, NO LABELS ARE EXPANDED @L5P*/; 00127600
*%/*         IF N IS 1, 2, 3, 5, OR 6, THE CORRESPONDING FORMAT @L5P*/; 00127800
*%/*         OF THE DSCB WILL BE EXPANDED.  MORE THAN ONE DSCB  @L5P*/; 00128000
*%/*         FORMAT CAN BE MAPPED IN THE FIRST 140 BYTE DSCB    @L5P*/; 00128200
*%/*         AREA BY SEPARATING THE FORMAT NUMBERS BY COMMAS.   @L5P*/; 00128400
*%/*                                                            @L5P*/; 00128600
*%/*       %PRLD2 -- IF EQUAL TO A NUMBER N SIGNIFIES:          @L5A*/; 00128800
*%/*         THE FORMAT(S) OF THE DSCB(S) TO BE EXPANDED IN THE @L5P*/; 00129000
*%/*         SECOND 140 BYTE DSCB AREA (AT LABEL OUTDSCB),      @L5P*/; 00129200
*%/*         USING THE IECSDSL1 MACRO.                          @L5P*/; 00129400
*%/*         N CAN BE A NUMBER FROM 0 - 6.  IF N IS 0 OR 4 OR   @L5P*/; 00129600
*%/*         IF THIS KEYWORD IS OMITTED, NO LABELS ARE EXPANDE  @L5P*/; 00129800
*%/*         IF N IS 1, 2, 3, 5, OR 6, THE CORRESPONDING FORMA  @L5P*/; 00130000
*%/*         OF THE DSCB WILL BE EXPANDED.  MORE THAN ONE DSCB  @L5P*/; 00130200
*%/*         FORMAT CAN BE MAPPED IN THE SECOND 140 BYTE DSCB   @L5P*/; 00130400
*%/*         AREA BY SEPARATING THE FORMAT NUMBERS BY COMMAS.   @L5P*/; 00130600
*%/*                                                                */; 00130800
*%/*   OUTPUT = DADSM PARTIAL RELEASE WORK AREA EXPANSION           */; 00131000
*%/*                                                                */; 00131200
*%/*EXIT-NORMAL = MEND STATEMENT                                    */; 00131400
*%/*                                                                */; 00131600
*%/*EXIT-ERROR = ERROR MESSAGES:                                    */; 00131800
*%/*                                                                */; 00132000
*%/*    MNOTE 12,***  IHB002 INVALID EP OPERAND SPECIFIED-&EP       */; 00132200
*%/*    MNOTE 12,***  IHB002 INVALID F4 OPERAND SPECIFIED-&F4       */; 00132400
*%/*    MNOTE 12,***  IHB002 INVALID ADT OPERAND SPECIFIED-&ADT     */; 00132600
*%/*                                                                */; 00132800
*%/*    THE MACRO EXPANSION CONTINUES AS IF THE INVALID PARAMETER   */; 00133000
*%/*    WERE NOT SPECIFIED.                                         */; 00133200
*%/*                                                                */; 00133400
*%/*EXTERNAL REFERENCES = NONE                                      */; 00133600
*%/*                                                                */; 00133800
*%/*    ROUTINES = NONE                                             */; 00134000
*%/*                                                                */; 00134200
*%/*    DATA AREAS = PARTIAL RELEASE WORK AREA                      */; 00134400
*%/*                                                                */; 00134600
