*-                                                                              
*-       List of special symbols, used in both Wylbur (UTIL) and                
*-         MILTEN (SIFT).                                                       
*-                                                                              
*-         Revision 1, 10/29/90, JNK                                            
*-                                                                              
         FCHAR ':',TEXT=''                       Small circumflex             
         FCHAR ':...',TEXT='...',ALT='I'           Ellipsis                     
         FCHAR ':+-',TEXT='+/-',ALT='1'            Plus-or-minus                
         FCHAR ':!^',TEXT='!',ALT='A'              Upside-down !                
         FCHAR ':^^',TEXT='^^',ALT='E'             Approximately                
         FCHAR ':^',TEXT='^'                       Small tilde                  
         FCHAR ':^=',TEXT='^=',ALT='-'             Not-equal sign               
         FCHAR ':%%',TEXT='<per 1000>'             Per-thousand                 
         FCHAR ':?^',TEXT='?',ALT='@'              Upside-down ?                
         FCHAR ':''',TEXT='''',ALT='+'             Acute accent                 
         FCHAR ':a',TEXT='a',CASE=LOWER           a-circumflex                 
         FCHAR ':a^',TEXT='a',CASE=LOWER           a-tilde                      
         FCHAR ':a_',TEXT='a',ALT=';'              a superscript,ul             
         FCHAR ':a`',TEXT='a',CASE=LOWER           a-grave                      
         FCHAR ':a:',TEXT='a',CASE=LOWER           a-umlaut                     
         FCHAR ':a''',TEXT='a',CASE=LOWER          a-acute                      
         FCHAR ':ae',TEXT='ae',ALT='>',CASE=LOWER  ae ligature                  
         FCHAR ':ao',TEXT='a',CASE=LOWER           a-ring                       
         FCHAR ':base''',TEXT=','                  Base '                       
         FCHAR ':base"',TEXT=',,'                  Base "                       
         FCHAR ':blotch',TEXT='X',ALT='a'          Checkerboard pattern         
         FCHAR ':breve',TEXT='<breve>'             Breve                        
         FCHAR ':bullet',TEXT='*',ALT='%'          Bullet                       
         FCHAR ':c,',TEXT='c',CASE=LOWER           C cedilla                    
         FCHAR ':caron',TEXT='<caron>'             Caron (inv. accent)          
         FCHAR ':cedilla',TEXT=','                 Cedilla                      
         FCHAR ':cent',TEXT='<cent>',ALT='"'       Cent sign                    
         FCHAR ':copyright',TEXT='(c)',ALT=')'     Copyright                    
         FCHAR ':cr',TEXT='<CR>',ALT='d'           CR=carriage return           
         FCHAR ':curly_d',TEXT='<curly_d>',ALT='6' Curly d                      
         FCHAR ':currency',TEXT='<currency>'       Currency                     
         FCHAR ':dagger',TEXT='<dagger>'           Dagger                       
         FCHAR ':dbl_dagger',TEXT='<dbl_dagger>'   Dbl dagger                   
         FCHAR ':deg',ALT='!',TEXT='<degree>'      Degree sign                  
         FCHAR ':div',TEXT='/',ALT='V'             Division sign                
         FCHAR ':dot',TEXT='<dot>'                 Dot accent                   
         FCHAR ':dotless_i',TEXT='i'               Dotless-i                    
         FCHAR ':e',TEXT='e',CASE=LOWER           e-circumflex                 
         FCHAR ':e`',TEXT='e',CASE=LOWER           e-grave                      
         FCHAR ':e:',TEXT='e',CASE=LOWER           e-umlaut                     
         FCHAR ':e''',TEXT='e',CASE=LOWER          e-acute                      
         FCHAR ':em_dash',TEXT='--',ALT='Q'        Em dash                      
         FCHAR ':en_dash',TEXT='-',ALT='P'         En dash                      
         FCHAR ':f',TEXT='f',ALT='D'               Florin / Function            
         FCHAR ':ff',TEXT='<FF>',ALT='c'           FF  form feed                
         FCHAR ':fi',TEXT='fi'                     fi ligature                  
         FCHAR ':fl',TEXT='fl'                     fl ligature                  
         FCHAR ':fraction',TEXT='/'                Fraction                     
         FCHAR ':ge',TEXT='>=',ALT='3'             > or =                       
         FCHAR ':h_umlaut',TEXT='<>'               h_umlaut                     
         FCHAR ':ht',TEXT='<HT>',ALT='b'           HT=horizontal tab            
         FCHAR ':i',TEXT='i',CASE=LOWER           i-circumflex                 
         FCHAR ':i`',TEXT='i',CASE=LOWER           i-grave                      
         FCHAR ':i:',TEXT='i',CASE=LOWER           i-umlaut                     
         FCHAR ':i''',TEXT='i',CASE=LOWER          i-acute                      
         FCHAR ':infinity',TEXT='<infinity>',ALT='0'  Infinity                  
         FCHAR ':integral',TEXT='<Integral>',ALT=':'  Integration               
         FCHAR ':l''',TEXT='''',ALT='T'            Left '                       
         FCHAR ':l''euro',TEXT='<'                 Left European '              
         FCHAR ':l"',TEXT='"',ALT='R'              Left "                       
         FCHAR ':l"euro',TEXT='<<',ALT='G'         Left European "              
         FCHAR ':le',TEXT='<=',ALT='2'             < or =                       
         FCHAR ':lf',TEXT='<LF>',ALT='e'           LF  line feed                
         FCHAR ':lozenge',TEXT='<>',ALT='W'        Diamond                      
         FCHAR ':macron',TEXT='-'                  Macron (horiz bar)           
         FCHAR ':mu',TEXT='<mu>',ALT='5',CASE=LOWER   mu                        
         FCHAR ':n^',TEXT='n',CASE=LOWER           n-tilde                      
         FCHAR ':nl',TEXT='<NL>',ALT='h'           NL=new line                  
         FCHAR ':not',TEXT='^',ALT='B'             Not                          
         FCHAR ':o',TEXT='o',CASE=LOWER           o-circumflex                 
         FCHAR ':o^',TEXT='o',CASE=LOWER           o-tilde                      
         FCHAR ':o/',TEXT='o',ALT='?',CASE=LOWER   o-thorn                      
         FCHAR ':o_',TEXT='o',ALT='<'              o superscript,ul             
         FCHAR ':o`',TEXT='o',CASE=LOWER           o-grave                      
         FCHAR ':o:',TEXT='o',CASE=LOWER           o-umlaut                     
         FCHAR ':o''',TEXT='o',CASE=LOWER          o-acute                      
         FCHAR ':oe',TEXT='oe',ALT='O',CASE=LOWER  oe ligature                  
         FCHAR ':ogonek',TEXT='?'                  ogonek                       
         FCHAR ':para',TEXT='<paragraph>',ALT='&&'  Paragraph                   
         FCHAR ':pi',TEXT='<pi>',ALT='9',CASE=LOWER  pi                         
         FCHAR ':r''',TEXT='''',ALT='U'            Right '                      
         FCHAR ':r''euro',TEXT='>'                 Right European '             
         FCHAR ':r"',TEXT='"',ALT='S'              Right "                      
         FCHAR ':r"euro',TEXT='>>',ALT='H'         Right European "             
         FCHAR ':radical',TEXT='<radical>',ALT='C' Radical                      
         FCHAR ':regmark',TEXT='(R)',ALT='('       Registered tm                
         FCHAR ':ring',TEXT='<ring>'               angstrom ring                
         FCHAR ':section',TEXT='<section>',ALT='$' Section                      
         FCHAR ':sm_bullet',TEXT='-',ALT='^'       Small bullet                 
         FCHAR ':sp',TEXT=' '                      Non-trivial space            
         FCHAR ':ss',TEXT='ss',ALT=''''            German ss                    
         FCHAR ':tm',TEXT='(tm)',ALT='*'           Trademark                    
         FCHAR ':u',TEXT='u',CASE=LOWER           u-circumflex                 
         FCHAR ':u`',TEXT='u',CASE=LOWER           u-grave                      
         FCHAR ':u:',TEXT='u',CASE=LOWER           u-umlaut                     
         FCHAR ':u''',TEXT='u',CASE=LOWER          u-acute                      
         FCHAR ':ukpound',TEXT='<UKpound>',ALT='#' British pound sign           
         FCHAR ':umlaut',TEXT='<umlaut>',ALT=','   Umlaut                       
         FCHAR ':vt',TEXT='<VT>',ALT='i'           VT=vertical tab              
         FCHAR ':y:',TEXT='y',ALT='X',CASE=LOWER   y-umlaut                     
         FCHAR ':yen',TEXT='<yen>',ALT='4'         Yen sign                     
         FCHAR ':A',TEXT='A',CASE=UPPER           A-circumflex                 
         FCHAR ':A^',TEXT='A',ALT='L',CASE=UPPER   A-tilde                      
         FCHAR ':A`',TEXT='A',ALT='K',CASE=UPPER   A-grave                      
         FCHAR ':A:',TEXT='A',CASE=UPPER           A-umlaut                     
         FCHAR ':A''',TEXT='A',CASE=UPPER          A-acute                      
         FCHAR ':Ao',TEXT='A',CASE=UPPER           A-ring                       
         FCHAR ':Apple',TEXT='<Apple>'             Apple logo                   
         FCHAR ':AE',TEXT='AE',ALT='.',CASE=UPPER  AE ligature                  
         FCHAR ':C,',TEXT='C',CASE=UPPER           C cedilla                    
         FCHAR ':Delta',TEXT='<Delta>',ALT='F'     Delta                        
         FCHAR ':E',TEXT='E',CASE=UPPER           E-circumflex                 
         FCHAR ':E`',TEXT='E',CASE=UPPER           E-grave                      
         FCHAR ':E:',TEXT='E',CASE=UPPER           E-umlaut                     
         FCHAR ':E''',TEXT='E',CASE=UPPER          E-acute                      
         FCHAR ':I',TEXT='I',CASE=UPPER           I-circumflex                 
         FCHAR ':I`',TEXT='I',CASE=UPPER           I-grave                      
         FCHAR ':I:',TEXT='I',CASE=UPPER           I-umlaut                     
         FCHAR ':I''',TEXT='I',CASE=UPPER          I-acute                      
         FCHAR ':N^',TEXT='N',CASE=UPPER           N-tilde                      
         FCHAR ':O',TEXT='O',CASE=UPPER           O-circumflex                 
         FCHAR ':O^',TEXT='O',ALT='M',CASE=UPPER   O-tilde                      
         FCHAR ':O/',TEXT='O',ALT='/',CASE=UPPER   O-thorn                      
         FCHAR ':O`',TEXT='O',CASE=UPPER           O-grave                      
         FCHAR ':O:',TEXT='O',CASE=UPPER           O-umlaut                     
         FCHAR ':O''',TEXT='O',CASE=UPPER          O-acute                      
         FCHAR ':Omega',TEXT='<Omega>',ALT='='     Omega                        
         FCHAR ':OE',TEXT='OE',ALT='N',CASE=UPPER  OE ligature                  
         FCHAR ':Pi',TEXT='<Pi>',ALT='8',CASE=UPPER  Pi                         
         FCHAR ':Sigma',TEXT='<Sigma>',ALT='7'     Sigma                        
         FCHAR ':U',TEXT='U',CASE=UPPER           U-circumflex                 
         FCHAR ':U`',TEXT='U',CASE=UPPER           U-grave                      
         FCHAR ':U:',TEXT='U',CASE=UPPER           U-umlaut                     
         FCHAR ':U''',TEXT='U',CASE=UPPER          U-acute                      
         FCHAR ':Y:',TEXT='Y',CASE=UPPER           Y-umlaut                     
         FCHAR ':1/2',TEXT='1/2'                   1/2 half                     
         FCHAR ':1/4',TEXT='1/4'                   1/4 quarter                  
