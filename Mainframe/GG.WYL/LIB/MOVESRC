SET EXE NOL TER; 6/83, Niz
COMM *** WYLBUR ***  (Remember to update MACLIB and HIBAL)
SET AUTH WYL$GG
SET ESC &
IF (LINES NE 0) REA PRO 'ENTER SAVE OR CR TO CLEAR? '
THEN COMM
SET VAL N6 0
REA STR S0 PRO 'Auto (no verify) mode? ' UPP
IF (S0 EQ '') EXE 4.1
IF (IND(S0,'Y')+IND(S0,'OK') NE 0) SET VAL N6 1
COMM
SHO DIR IN WYL.GG.WYL.SOURCE ON CAT COL CLR
DEL F(2)
CH 9/235 to '' n
SET VAL W0 LAST
SHO CAT USE WYL GRO GG COL 5000
SET VAL W9 F
IF (W9 GT W0) EXE 17
IF (LIN(W9) EQ 'MACLIB') EXE 13
P '&(LIN(W9)) ' (1) IN 5000/L N
IF (* LT 0)
THEN DEL &W9
SET * &W9
SET VAL W9 NEX
EXE 10
DEL 5000/L
NUM 1 BY 1
CH 50 TO '''' N
CH ' ' TO '' N
CH 1 TO 'EXEC 9999 SAV PARM ''' N
END CLR EXEC
COP 9998/L EXE COM
IF (N6 EQ 1) THEN 10003 WRITE PARM
10004,
10004.1,
EXE ACT CLR
CLR EXE
USE WYL.GG.WYL.SOURCE#&(PAR) ON CAT CLR
COP FRO WYL.GG.WYL.&(PAR) TO 10000 ON CAT
COM F 10000 N
IF (* EQ -1) EXE RET
REA STR S0 PRO '&(PARM)? '
IF (S0 EQ '') EXE 10003
IF (IND(S0,'Y')+IND(S0,'OK') EQ 0) EXE RET
USE &WYL.&(PAR) ON CAT CLR
SAV &WYL.SOURCE#&(PAR) REP ON CAT QUIET
EXE RET
EXE RET
