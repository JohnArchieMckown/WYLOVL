; this is xprocs#xoncond public
;
XPROC () BEGIN
  ON ATTN CMD 'PCALL ATTN'
  ON ERROR CMD 'PCALL ERROR'
  COMM XPROC 1
  COMM XPROC 2
  COMM XPROC 3
  COMM XPROC 4
  COMM XPROC 5
  COMM XPROC 6
  COMM XPROC 7
  COMM XPROC 8
  COMM XPROC 9
  COMM XPROC 10
  COMM XPROC 11
  COMM XPROC 12
  COMM XPROC 13
  COMM XPROC 14
  COMM XPROC 15
END

PROC ATTN () BEGIN
  COMM 0001 ATTN HIT
  COMM 0002 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0003 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0004 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0005 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0006 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0007 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0008 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0009 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0010 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0011 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0012 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0013 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0014 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  COMM 0015 ATTN=&(ATTN) ATTNID=&(ATTNID)  ATTNINFO='&(ATTNINFO)'
  ;ON ATTN RESET
END

PROC ERROR () BEGIN
  COMM 0001 ERROR HIT
  COMM 0002 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0003 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0004 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0005 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0006 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0007 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0008 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0009 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0010 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0011 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0012 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0013 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0014 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  COMM 0015 ERROR=&(ERROR) ERRINFO='&(ERRINFO)'
  ;ON ERROR RESET
END
