; TOMULT: EXECFILE TO HELP PEOPLE USE THE MULTIPLE (M---) CHARACTER
; SETS -- MAINTAINED BY Lynn McRae, SCIP User Services, 7-2429
SET EXEC NOLOG TERSE
CLEAR VARIABLES
SET VAL S9=ESCAPE
SET ESCAPE @
READ STRING S0 PROMPT 'LINE RANGE FOR BOLD? '
IF (S0 EQ 'NONE') EXEC 77
IF (S0 EQ '') EXEC 77
CHANGE 'a' to '@(S'8A')' IN @S0 NOLIST
CHANGE 'b' to '@(S'9A')' IN @S0 NOLIST
CHANGE 'c' to '@(S'AA')' IN @S0 NOLIST
CHANGE 'd' to '@(S'BA')' IN @S0 NOLIST
CHANGE 'e' to '@(S'CA')' IN @S0 NOLIST
CHANGE 'f' to '@(S'CB')' IN @S0 NOLIST
CHANGE 'g' to '@(S'CD')' IN @S0 NOLIST
CHANGE 'h' to '@(S'CE')' IN @S0 NOLIST
CHANGE 'i' to '@(S'CF')' IN @S0 NOLIST
CHANGE 'j' to '@(S'DA')' IN @S0 NOLIST
CHANGE 'k' to '@(S'DB')' IN @S0 NOLIST
CHANGE 'l' to '@(S'DC')' IN @S0 NOLIST
CHANGE 'm' to '@(S'DD')' IN @S0 NOLIST
CHANGE 'n' to '@(S'DE')' IN @S0 NOLIST
CHANGE 'o' to '@(S'DF')' IN @S0 NOLIST
CHANGE 'p' to '@(S'EA')' IN @S0 NOLIST
CHANGE 'q' to '@(S'EB')' IN @S0 NOLIST
CHANGE 'r' to '@(S'EC')' IN @S0 NOLIST
CHANGE 's' to '@(S'ED')' IN @S0 NOLIST
CHANGE 't' to '@(S'EE')' IN @S0 NOLIST
CHANGE 'u' to '@(S'EF')' IN @S0 NOLIST
CHANGE 'v' to '@(S'FA')' IN @S0 NOLIST
CHANGE 'w' to '@(S'FB')' IN @S0 NOLIST
CHANGE 'x' to '@(S'FC')' IN @S0 NOLIST
CHANGE 'y' to '@(S'FD')' IN @S0 NOLIST
CHANGE 'z' to '@(S'FE')' IN @S0 NOLIST
CHANGE 'A' to '@(S'41')' IN @S0 NOLIST
CHANGE 'B' to '@(S'42')' IN @S0 NOLIST
CHANGE 'C' to '@(S'43')' IN @S0 NOLIST
CHANGE 'D' to '@(S'44')' IN @S0 NOLIST
CHANGE 'E' to '@(S'45')' IN @S0 NOLIST
CHANGE 'F' to '@(S'46')' IN @S0 NOLIST
CHANGE 'G' to '@(S'47')' IN @S0 NOLIST
CHANGE 'H' to '@(S'48')' IN @S0 NOLIST
CHANGE 'I' to '@(S'49')' IN @S0 NOLIST
CHANGE 'J' to '@(S'51')' IN @S0 NOLIST
CHANGE 'K' to '@(S'52')' IN @S0 NOLIST
CHANGE 'L' to '@(S'53')' IN @S0 NOLIST
CHANGE 'M' to '@(S'54')' IN @S0 NOLIST
CHANGE 'N' to '@(S'55')' IN @S0 NOLIST
CHANGE 'O' to '@(S'56')' IN @S0 NOLIST
CHANGE 'P' to '@(S'57')' IN @S0 NOLIST
CHANGE 'Q' to '@(S'58')' IN @S0 NOLIST
CHANGE 'R' to '@(S'59')' IN @S0 NOLIST
CHANGE 'S' to '@(S'62')' IN @S0 NOLIST
CHANGE 'T' to '@(S'63')' IN @S0 NOLIST
CHANGE 'U' to '@(S'64')' IN @S0 NOLIST
CHANGE 'V' to '@(S'65')' IN @S0 NOLIST
CHANGE 'W' to '@(S'66')' IN @S0 NOLIST
CHANGE 'X' to '@(S'67')' IN @S0 NOLIST
CHANGE 'Y' to '@(S'68')' IN @S0 NOLIST
CHANGE 'Z' to '@(S'69')' IN @S0 NOLIST
CHANGE '^' TO '@(S'D0')' IN @S0 NOLIST
CHANGE '!' TO '@(S'90')' IN @S0 NOLIST
CHANGE '1' TO '@(S'71')' IN @S0 NOLIST
CHANGE '2' TO '@(S'72')' IN @S0 NOLIST
CHANGE '3' TO '@(S'73')' IN @S0 NOLIST
CHANGE '4' TO '@(S'74')' IN @S0 NOLIST
CHANGE '5' TO '@(S'75')' IN @S0 NOLIST
CHANGE '6' TO '@(S'76')' IN @S0 NOLIST
CHANGE '7' TO '@(S'77')' IN @S0 NOLIST
CHANGE '8' TO '@(S'78')' IN @S0 NOLIST
CHANGE '9' TO '@(S'79')' IN @S0 NOLIST
CHANGE '0' TO '@(S'70')' IN @S0 NOLIST
CHANGE '$' TO '@(S'C0')' IN @S0 NOLIST
CHANGE '?' TO '@(S'E1')' IN @S0 NOLIST
CHANGE '&' TO '@(S'80')' IN @S0 NOLIST
READ STRING S0 PROMPT 'LINE RANGE FOR ITALIC? '
IF (S0 EQ 'NONE') EXEC 147
IF (S0 EQ '') EXEC 147
CHANGE 'a' to '@(S'0A')' IN @S0 NOLIST
CHANGE 'b' to '@(S'0B')' IN @S0 NOLIST
CHANGE 'c' to '@(S'0C')' IN @S0 NOLIST
CHANGE 'd' to '@(S'0D')' IN @S0 NOLIST
CHANGE 'e' to '@(S'0E')' IN @S0 NOLIST
CHANGE 'f' to '@(S'0F')' IN @S0 NOLIST
CHANGE 'g' to '@(S'1A')' IN @S0 NOLIST
CHANGE 'h' to '@(S'1B')' IN @S0 NOLIST
CHANGE 'i' to '@(S'1C')' IN @S0 NOLIST
CHANGE 'j' to '@(S'1D')' IN @S0 NOLIST
CHANGE 'k' to '@(S'1E')' IN @S0 NOLIST
CHANGE 'l' to '@(S'1F')' IN @S0 NOLIST
CHANGE 'm' to '@(S'2A')' IN @S0 NOLIST
CHANGE 'n' to '@(S'2B')' IN @S0 NOLIST
CHANGE 'o' to '@(S'2C')' IN @S0 NOLIST
CHANGE 'p' to '@(S'2D')' IN @S0 NOLIST
CHANGE 'q' to '@(S'2E')' IN @S0 NOLIST
CHANGE 'r' to '@(S'2F')' IN @S0 NOLIST
CHANGE 's' to '@(S'38')' IN @S0 NOLIST
CHANGE 't' to '@(S'39')' IN @S0 NOLIST
CHANGE 'u' to '@(S'3A')' IN @S0 NOLIST
CHANGE 'v' to '@(S'3B')' IN @S0 NOLIST
CHANGE 'w' to '@(S'3C')' IN @S0 NOLIST
CHANGE 'x' to '@(S'3D')' IN @S0 NOLIST
CHANGE 'y' to '@(S'3E')' IN @S0 NOLIST
CHANGE 'z' to '@(S'3F')' IN @S0 NOLIST
CHANGE 'A' to '@(S'01')' IN @S0 NOLIST
CHANGE 'B' to '@(S'02')' IN @S0 NOLIST
CHANGE 'C' to '@(S'03')' IN @S0 NOLIST
CHANGE 'D' to '@(S'04')' IN @S0 NOLIST
CHANGE 'E' to '@(S'05')' IN @S0 NOLIST
CHANGE 'F' to '@(S'06')' IN @S0 NOLIST
CHANGE 'G' to '@(S'07')' IN @S0 NOLIST
CHANGE 'H' to '@(S'08')' IN @S0 NOLIST
CHANGE 'I' to '@(S'09')' IN @S0 NOLIST
CHANGE 'J' to '@(S'11')' IN @S0 NOLIST
CHANGE 'K' to '@(S'12')' IN @S0 NOLIST
CHANGE 'L' to '@(S'13')' IN @S0 NOLIST
CHANGE 'M' to '@(S'14')' IN @S0 NOLIST
CHANGE 'N' to '@(S'15')' IN @S0 NOLIST
CHANGE 'O' to '@(S'6A')' IN @S0 NOLIST
CHANGE 'P' to '@(S'17')' IN @S0 NOLIST
CHANGE 'Q' to '@(S'18')' IN @S0 NOLIST
CHANGE 'R' to '@(S'19')' IN @S0 NOLIST
CHANGE 'S' to '@(S'22')' IN @S0 NOLIST
CHANGE 'T' to '@(S'23')' IN @S0 NOLIST
CHANGE 'U' to '@(S'24')' IN @S0 NOLIST
CHANGE 'V' to '@(S'25')' IN @S0 NOLIST
CHANGE 'W' to '@(S'26')' IN @S0 NOLIST
CHANGE 'X' to '@(S'27')' IN @S0 NOLIST
CHANGE 'Y' to '@(S'28')' IN @S0 NOLIST
CHANGE 'Z' to '@(S'29')' IN @S0 NOLIST
CHANGE '^' TO '@(S'9D')' IN @S0 NOLIST
CHANGE '!' TO '@(S'10')' IN @S0 NOLIST
CHANGE '1' TO '@(S'31')' IN @S0 NOLIST
CHANGE '2' TO '@(S'32')' IN @S0 NOLIST
CHANGE '3' TO '@(S'33')' IN @S0 NOLIST
CHANGE '4' TO '@(S'34')' IN @S0 NOLIST
CHANGE '5' TO '@(S'35')' IN @S0 NOLIST
CHANGE '6' TO '@(S'36')' IN @S0 NOLIST
CHANGE '7' TO '@(S'37')' IN @S0 NOLIST
CHANGE '8' TO '@(S'20')' IN @S0 NOLIST
CHANGE '9' TO '@(S'21')' IN @S0 NOLIST
CHANGE '0' TO '@(S'30')' IN @S0 NOLIST
CHANGE '$' TO '@(S'8D')' IN @S0 NOLIST
CHANGE '?' TO '@(S'00')' IN @S0 NOLIST
CHANGE '&' TO '@(S'9C')' IN @S0 NOLIST
SET ESCAPE @S9
CLR EXEC
