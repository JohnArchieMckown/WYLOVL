SET EXEC NOLOG TERSE
; THIS EXECFILE IS MAINTAINED BY JOHN SACK & ROBIN SPRING.
USE WYL.GA.PUB.LIBDATA#FILEDEF CLR PUB
CALL SPIRES(BW)
SELECT FILEDEF
/CHANGE ' FILE =       ' TO ' FILE = $ACCOUNT.MAIL.LIST;' NOLIST
/CHANGE 'ACCOUNTS =      ' TO 'ACCOUNTS = $ACCOUNT;' NOLIST
ADD
COMMENT YOUR FILE DEFINITION HAS BEEN SUCCESSFULLY ADDED
CALL SPICOMP(W)
COMPILE MAIL.LIST
EXEC FROM LIB#CREFORM PUBLIC CLEAR
