EXAMPLES OF JCL TO ASSEMBLE BASIC
EXAMPLE 1
//  JOB  (LNG$GG,721),'BASIC @@@@@@@@'
/*JOBPARM LINES=99
//  EXEC PGM=IEFBR14
//DELETE DD DSN=WYL.GG.LNG.@@@@@@@@,VOL=SER=SCR002,UNIT=DASD,
//  DISP=(OLD,DELETE)
//ASM     EXEC ASMC,REGION=192K,LEVEL=G,
//    PARM='SY=(BAS)'
//SYSLIB DD  DSN=SYS3.LG.BASMAC,DISP=SHR,DCB=BLKSIZE=7280
//       DD  DSN=SYS3.ORVYL.UMACLIB,DISP=SHR,DCB=BLKSIZE=7280
//       DD  DSN=SYS1.MACLIB,DISP=SHR,DCB=BLKSIZE=7280
//SYSLIN DD  DSN=WYL.GG.LNG.@@@@@@@@,VOL=SER=SCR002,UNIT=DASD,
//  DISP=(NEW,KEEP),DCB=(LRECL=80,BLKSIZE=4000,RECFM=FB),
//   SPACE=(TRK,(10,10))
//SYSIN     DD *
EXAMPLE 2
//  JOB  (LNG$GG,721),'CREATE @@@@@@@@'
//SCRATCH EXEC PGM=IEHPROGM
//SYSPRINT DD DUMMY
//ENC003 DD VOL=SER=ENC003,UNIT=DISK,DISP=OLD
//SYSIN DD *
       SCRATCH DSNAME=SYS3.LG.BASMAC,VOL=DISK=ENC003
//SOURCE  EXEC PGM=IEBUPDTE,PARM=NEW
//SYSPRINT DD DUMMY
//SYSUT2 DD DSN=SYS3.LG.BASMAC,DISP=(NEW,KEEP),UNIT=DISK,
//   VOL=SER=ENC003,SPACE=(TRK,(12,5,10)),
//   DCB=(DSORG=PO,LRECL=80,BLKSIZE=3520,RECFM=FB)
//SYSIN DD *
