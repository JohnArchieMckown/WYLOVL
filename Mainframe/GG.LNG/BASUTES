...................................CUNY INPURT OF BASIC
//   JOB  (LNG$GG,721)
/*JOBPARM T=10,BIN=721
/*SETUP        'MOUNT FORIGN TAPE CU0374, READ ONLY, 1600 BPI'
/*SETUP        'CALLED FOR AS CU0374'
//MOVESTEP EXEC PGM=IEHMOVE
//SYSPRINT DD SYSOUT=A
//SYSUT1 DD UNIT=DASD,VOL=SER=SCR002,DISP=OLD
//SCR002 DD UNIT=DASD,VOL=SER=SCR002,DISP=OLD
//TAPE9 DD UNIT=(TAPE9,1,DEFER),DCB=DEN=3,DISP=(OLD,PASS),
//   VOL=SER=CU0374,LABEL=(1,SL)
//SYSIN DD *
         COPY  FROM=TAPE9=(CU0374,1),TO=DASD=SCR002,FROMDD=TAPE9,      X
               PDS=WYL.CU.ORV.BASIC.EXPORT.SOURCE,                     *
               RENAME=WYL.GG.LNG.BASIC.SOURCE
         COPY  FROM=TAPE9=(CU0374,2),TO=DASD=SCR002,FROMDD=TAPE9,      X
               PDS=WYL.CU.ORV.BASIC.EXPORT.UPDATES.FLOOR,              *
               RENAME=WYL.GG.LNG.BASIC.UPDATES
         COPY  FROM=TAPE9=(CU0374,3),TO=DASD=SCR002,FROMDD=TAPE9,      X
               PDS=WYL.CU.ORV.BASIC.EXPORT.MACLIB,                     *
               RENAME=WYL.GG.LNG.BASIC.MACLIB
         COPY  FROM=TAPE9=(CU0374,4),TO=DASD=SCR002,FROMDD=TAPE9,      X
               PDS=WYL.CU.ORV.BASIC.EXPORT.CUNYMAC,                    *
               RENAME=WYL.GG.LNG.BASIC.CUNYMAC
         COPY  FROM=TAPE9=(CU0374,5),TO=DASD=SCR002,FROMDD=TAPE9,      X
               PDS=WYL.CU.ORV.BASIC.EXPORT.OBJLIB,                     *
               RENAME=WYL.GG.LNG.BASIC.OBJLIB
         COPY  FROM=TAPE9=(CU0374,6),TO=DASD=SCR002,FROMDD=TAPE9,      X
               PDS=WYL.CU.ORV.BASIC.EXPORT.OBJTOT,                     *
               RENAME=WYL.GG.LNG.BASIC.OBJTOT
         COPY  FROM=TAPE9=(CU0374,7),TO=DASD=SCR002,FROMDD=TAPE9,      X
               PDS=WYL.CU.ORV.BASIC.STARLIB,                           *
               RENAME=WYL.GG.LNG.BASIC.STARLIB
