         MACRO                                                          00001000
&L       SCTYPE &T,&SCANWA                                              00002000
&L       SYSLR 15,&SCANWA,NULL=                                         00003000
         SYSLR 1,&T                                                     00004000
         XCALL SCTYPE                                                   00005000
         MEND                                                           00006000
