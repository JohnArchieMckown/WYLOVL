         MACRO                                                          00001000
&L       HEXSEG &LOC,&LEN,&SEGCB                                        00002000
&L       SETMSG &LOC,&LEN                                               00003000
         SEGLD &SEGCB                                                   00004000
         VCALL HEXSEG                                                   00005000
         MEND                                                           00006000
