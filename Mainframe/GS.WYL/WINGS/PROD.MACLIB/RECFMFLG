         MACRO                                                          00001000
&A       RECFMFLG &PRF                                                  00002000
.* Record format flags for WINGS                                        00003000
         AIF   ('&A' EQ '').NOF                                         00004000
&A       FLAG  ,                   Format                               00005000
.NOF     ANOP                                                           00006000
.* Format flag values:                                                  00007000
         FLAG  (&PRF.EDIT,1,EQ)        - Edit format                    00008000
         FLAG  (&PRF.FIX,2,EQ)          - Fixed blocked format          00009000
         FLAG  (&PRF.VAR,3,EQ)          - Variable blocked format       00010000
         FLAG  (&PRF.VBS,4,EQ)         - Note: vbs not supported.       00011000
         MEND                                                           00012000
