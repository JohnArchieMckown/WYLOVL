TERMINAL XTERM66 BEGIN             ;X windows terminal (66 line display)
  TCLASS VT220                     ;generic terminal type class
  MODEL                            ;model terminal supported
  QSCROLL                          ;quick scrolling
  WIDTH 80,132                     ;screen width
  HEIGHT 66                        ;screen height
  BACKGROUND '0'                   ;normal
  NORMAL '0'                       ;normal
  LNORMAL '0'                      ;normal
  BOLD '0;1;7'                     ;bold inverse
  LCOL '0;1'                       ;bold
  DIM '0'                          ;normal
  LDIM '0'                         ;normal
  NEW '0'                          ;normal
  LNEW '0'                         ;normal
  MARKED '4'                       ;underlined
  LMARKED '0'                      ;normal
