TERMINAL CXTERM40 BEGIN            ;X windows color terminal (40 line display)
  TCLASS ISC2400                   ;generic terminal type class
  MODEL                            ;model terminal supported
  COLOR                            ;this is a color terminal
  QSCROLL                          ;quick scrolling
  WIDTH 80,132                     ;screen width
  HEIGHT 40                        ;screen height
  BACKGROUND '0'                   ;normal
  NORMAL '0;37'                    ;white
  LNORMAL '0;36'                   ;cyan
  BOLD '0;33'                      ;yellow
  LCOL '0;37'                      ;white
  DIM '0;34'                       ;blue
  LDIM '0;34'                      ;blue
  NEW '0;32'                       ;green
  LNEW '0;32'                      ;green
  MARKED '0;7;36'                  ;reverse, blue
  LMARKED '0;7;36'                 ;reverse, reverse blue
  SPELL '31'                       ;red
  COLUMN '0;36'                    ;cyan
  COLTAB '0;33'                    ;yellow
  COLMARGIN '0;31'                 ;red
  MSG '0;33'                       ;yellow
  STATUS '0;7;34'                  ;inverse blue
  PF     '0;7;34'                  ;inverse blue
  PFHIGH '0;7;34'                  ;inverse blue
  COMMAND '0;36'                   ;cyan
  ERROR '0;7'                      ;inverse
  OPTION '7;37'                    ;inverse white
END
;
