;; *****************************************************************
;; *                                                               *
;; *  WYL.GS.WYL.TERMINAL                            [Production]  *
;; *                                                               *
;; *  WYLBUR Terminal Type Table                                   *
;; *                                                               *
;; *  WYLBUR/370 - Release VI - Class I - Stanford Proprietary     *
;; *                                                               *
;; *  Created on 08/24/82 at 18:00:20 by GG.JDN                    *
;; *  Updated on 06/12/97 at 09:24:16 by GG.MPD                    *
;; *                                                               *
;; *****************************************************************
;
;;Terminal Type Table Format:
;;TERMINAL termname[,synonyms...] BEGIN  ;terminal name
;;  [TCLASS termclass]             ;generic terminal type class
;;  MODEL|NOMODEL                  ;supports the model terminal
;;  VTAM                           ;VTAM      supported by VTAM (3270 type terminal)
;;  SAMSON                         ;micro ftp supported
;;  SCROLL|QSCROLL|NOSCROLL        ;SCROLL    single line scrolling
;;                                 ;QSCROLL   multiple line scrolling
;;                                 ;NOSCROLL  no scrolling (default)
;;  WIDTH nnn                      ;screen width
;;  HEIGHT nnn                     ;screen height
;;  BACKGROUND 'aaaaaaaa'          ;background rendition
;;  NORMAL     'aaaaaaaa'          ;normal rendition
;;  LNORMAL    'aaaaaaaa'          ;normal line-number rendition
;;  BOLD       'aaaaaaaa'          ;bold rendition
;;  LCOL       'aaaaaaaa'          ;collect line-number rendition
;;  DIM        'aaaaaaaa'          ;dim rendition
;;  LDIM       'aaaaaaaa'          ;dim line-number rendition
;;  NEW        'aaaaaaaa'          ;new line rendition
;;  LNEW       'aaaaaaaa'          ;new line-number rendition
;;  MARKED     'aaaaaaaa'          ;marked line rendition
;;  LMARKED    'aaaaaaaa'          ;marked line-number rendition
;;  SPELL      'aaaaaaaa'          ;misspelled word rendition
;;  COLUMN     'aaaaaaaa'          ;column line rendition
;;  COLTAB     'aaaaaaaa'          ;column tab indicator rendition
;;  COLMARGIN  'aaaaaaaa'          ;column margin indicator rendition
;;  [FMARKED]                      ;even blanks get marked rendition
;;  MSG        'aaaaaaaa'          ;information line rendition
;;  STATUS     'aaaaaaaa'          ;status line rendition
;;  PF         'aaaaaaaa'          ;pf key line rendition (NET_VIEW)
;;  PFHIGH     'aaaaaaaa'          ;pf key highlight rend (NET_VIEW)
;;  COMMAND    'aaaaaaaa'          ;command line rendition
;;  ERROR      'aaaaaaaa'          ;error/message line rendition
;;  OPTION     'aaaaaaaa'          ;pf key menu rendition
;;END
;
;;The terminal types are sorted by manufacturer/model.
;;
;;Rendition attributes:
;;
;; a = 0   No rendition [reset]
;;     1   Bold or increased intensity
;;     2   Faint, decreased intensity, or secondary color
;;     3   Italic
;;     4   Underscore
;;     5   Slow blink (slower than 150 per minute)
;;     6   Rapid blink (faster than 150 per minute)
;;     7   Negative (reverse) image
;;     8   Blanked
;;
;;    30   Black color
;;    31   Red
;;    32   Green
;;    33   Yellow
;;    34   Blue
;;    35   Magenta
;;    36   Cyan
;;    37   White
;;
;
TERMINAL TTY BEGIN                 ;Default terminal type, basic hardcopy devices
  NOMODEL                          ;model terminal not supported
  WIDTH 80                         ;screen width
  HEIGHT 24                        ;screen height
END
;
TERMINAL XTTY BEGIN                ;Like TTY, but without clear-screen even [ltm]
  NOMODEL                          ;model terminal not supported
  WIDTH 80                         ;screen width
  HEIGHT 24                        ;screen height
END
;
TERMINAL VIRTUAL BEGIN             ;MILTEN Virtual terminal
  NOMODEL                          ;model terminal not supported
  WIDTH 0                          ;screen width
  HEIGHT 0                         ;screen height
END
;
;TERMINAL MAC,MACINTOSH BEGIN       ;Apple Macintosh
;  MODEL                            ;model terminal supported
;  SCROLL                           ;single line scrolling
;  WIDTH 80,132                     ;screen width
;  HEIGHT 24                        ;screen height
;  BACKGROUND '0'                   ;normal
;  NORMAL '0'                       ;normal
;  LNORMAL '0'                      ;normal
;  BOLD '0;7'                       ;inverse bold
;  LCOL '0;1'                       ;bold
;  DIM '0'                          ;normal
;  LDIM '0'                         ;normal
;  NEW '0'                          ;normal
;  LNEW '0'                         ;normal
;  MARKED '4'                       ;underlined
;  LMARKED '0'                      ;normal
;  SPELL '0;1'                      ;bold
;  COLUMN '0'                       ;normal
;  COLTAB '0'                       ;normal
;  COLMARGIN '7'                    ;inverse
;  MSG '0'                          ;normal
;  STATUS '0;7'                     ;inverse
;  PF     '0;7'                     ;inverse
;  PFHIGH '0;7'                     ;inverse
;  COMMAND '0'                      ;normal
;  ERROR '0;7'                      ;inverse
;  OPTION '0;7'                     ;inverse
;END
;
TERMINAL MACSAM,MACSAMSON BEGIN    ;Apple Macintosh (Samson)
  TCLASS SAMSON                    ;generic terminal type class
  MODEL                            ;model terminal supported
  SAMSON                           ;Samson micro
  QSCROLL                          ;quick scrolling
  WIDTH 80,120                     ;screen width
  HEIGHT 24                        ;screen height
  BACKGROUND '0'                   ;normal
  NORMAL '0'                       ;normal
  LNORMAL '0'                      ;normal
  BOLD '0;7'                       ;inverse bold
  LCOL '0;1'                       ;bold
  DIM '0'                          ;normal
  LDIM '0'                         ;normal
  NEW '0'                          ;normal
  LNEW '0'                         ;normal
  MARKED '4'                       ;underlined
  LMARKED '0'                      ;normal
  SPELL '0;1'                      ;bold
  COLUMN '0'                       ;normal
  COLTAB '0'                       ;normal
  COLMARGIN '7'                    ;inverse
  MSG '0'                          ;normal
  STATUS '0;7'                     ;inverse
  PF     '0;7'                     ;inverse
  PFHIGH '0;7'                     ;inverse
  COMMAND '0'                      ;normal
  ERROR '0;7'                      ;inverse
  OPTION '0;7'                     ;inverse
END
;
TERMINAL MACSAMC BEGIN             ;Apple Color Macintosh (Samson)
  TCLASS SAMSON                    ;generic terminal type class
  MODEL                            ;model terminal supported
  SAMSON                           ;Samson micro
  COLOR                            ;this is a color terminal (VTAM)
  QSCROLL                          ;quick scrolling
  WIDTH 80,120                     ;screen width
  HEIGHT 24                        ;screen height
  BACKGROUND '0'                   ;normal
  NORMAL '0;37'                    ;white
  LNORMAL '0;36'                   ;cyan
  BOLD '0;33'                      ;yellow
  LCOL '0;37'                      ;white
  DIM '0;34'                       ;blue
  LDIM '0;34'                      ;blue
  NEW '0;32'                       ;green
  LNEW '0;32'                      ;green
  MARKED '0;7;36'                  ;reverse, blue
  LMARKED '0;7;36'                 ;reverse, reverse blue
  SPELL '31'                       ;red
  COLUMN '0;36'                    ;cyan
  COLTAB '0;33'                    ;yellow
  COLMARGIN '0;31'                 ;red
  MSG '0;33'                       ;yellow
  STATUS '0;7;34'                  ;inverse blue
  PF     '0;7;34'                  ;inverse blue
  PFHIGH '0;7;34'                  ;inverse blue
  COMMAND '0;36'                   ;cyan
  ERROR '0;7'                      ;inverse
  OPTION '7;37'                    ;inverse white
END
;
