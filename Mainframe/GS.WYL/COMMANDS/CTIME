;
;;  ctime -- Command timing exec.
;;
;;     Written, 2/90, Jim Nisbet
;

xproc () begin
   orig_orvcpu = orvyl_cpu
   orig_wylcpu = wylbur_cpu
   orig_timebin = timebin

   try &(parm_string)
   try write 'Timing information: '\
      || timebin-orig_timebin || ' seconds ('\
      || wylbur_cpu-orig_wylcpu || ' WYLBUR CPU, '\
      || orvyl_cpu-orig_orvcpu || ' ORVYL CPU)'
end
