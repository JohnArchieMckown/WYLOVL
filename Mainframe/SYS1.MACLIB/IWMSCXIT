.*                                                                      00123100
.******************************************************************     00123200
.***AI.L01 IWMSCXIT FMT=LIST FILL IN PARAMETER AREA                     00123300
.******************************************************************     00123400
                      ACTR  10000                                       00123500
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00123600
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00123700
&ZCVAL                SETC  'LAE'          XAX MODE                     00123800
.ZIMFXA               ANOP ,                                            00123900
.*                                                                      00124000
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00124100
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00124200
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00124300
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00124400
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00124500
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00124600
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00124700
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00124800
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00124900
&ZOPN                 SETA  &ZOPN+1                                     00125000
&ZOPL(&ZOPN)          SETC  ' '                                         00125100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00125200
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00125300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00125400
                      AGO   .ZIMFNX                                     00125500
.*                                                                      00125600
.ZIMFN2               ANOP ,                                            00125700
&ZOPN                 SETA  &ZOPN+1                                     00125800
&ZOPL(&ZOPN)          SETC  ' '                                         00125900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00126000
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00126100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00126200
.ZIMFNX               ANOP ,                                            00126300
.********************************************************************   00126400
.***AI.L02 IWMSCXIT FMT=LIST INITIALIZE PARAMETER AREA                  00126500
.********************************************************************   00126600
.***AI.L03 MF=(SLE)                                                     00126700
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00126800
.*                                                                      00126900
&ZOPN                 SETA  &ZOPN+1                                     00127000
