&ZOPV(&ZOPN)          SETC  '0(&ZPLLEN,1),0(1)'                         00127300
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00127400
.ZIMFLX               ANOP  ,                                           00127500
.*-------------------------------------------------------------------   00127600
.***AI.L04 XPLISTLEN XL2                                                00127700
&ZOPN                 SETA  &ZOPN+1                                     00127800
&ZOPL(&ZOPN)          SETC  ' '                                         00127900
&ZOPO(&ZOPN)          SETC  'MVC'                                       00128000
&ZOPV(&ZOPN)          SETC  '2(2,1),=AL2(&ZPLLEN)'                      00128100
&ZOPD(&ZOPN)          SETC  '++ INPUT XPLISTLEN XL2'                    00128200
.*-------------------------------------------------------------------   00128300
.***AI.L05 XACTION BL1                                                  00128400
&ZW                   SETC  ''                                          00128500
&ZCLNG                SETC  ''             RESET BIT ON FLAG            00128600
.*-------------------------------------------------------------------   00128700
.***AI.L08 XACTION "QUIESCE"                                            00128800
.***      BL1'1XXXXXXX'                                                 00128900
         AIF   ('&XACTION' NE 'QUIESCE').ZI5A                           00129000
&ZW                   SETC  '&ZW.1'                                     00129100
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00129200
                      AGO   .ZI5C                                       00129300
.*                                                                      00129400
.ZI5A                 ANOP  ,                                           00129500
&ZW                   SETC  '&ZW.0'                                     00129600
.ZI5C                 ANOP  ,                                           00129700
.*-------------------------------------------------------------------   00129800
.***AI.L08 XACTION "RESUME"                                             00129900
.***      BL1'X1XXXXXX'                                                 00130000
         AIF   ('&XACTION' NE 'RESUME').ZI6A                            00130100
&ZW                   SETC  '&ZW.1000000'                               00130200
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00130300
                      AGO   .ZI6C                                       00130400
.*                                                                      00130500
.ZI6A                 ANOP  ,                                           00130600
&ZW                   SETC  '&ZW.0000000'                               00130700
.ZI6C                 ANOP  ,                                           00130800
.*-------------------------------------------------------------------   00130900
.***AI.L11 XACTION BL1                                                  00131000
                      AIF   ('&ZCLNG' NE 'YES').ZI6E  ALL ZERO BITS     00131100
&ZOPN                 SETA  &ZOPN+1                                     00131200
&ZOPL(&ZOPN)          SETC  ' '                                         00131300
&ZOPO(&ZOPN)          SETC  'OI'                                        00131400
&ZOPV(&ZOPN)          SETC  '4(1),B''&ZW'''                             00131500
&ZOPD(&ZOPN)          SETC  '++ INPUT BL1'                              00131600
.ZI6E                 ANOP  ,                                           00131700
.*-------------------------------------------------------------------   00131800
.***AI.L25  XCONNTKN XL4                                                00131900
                      AIF   ('&ZXCONNTKN' EQ '').ZI8E                   00132000
                      AIF   ('&ZXCONNTKN' EQ 'OUTPUT').ZI8E             00132100
&ZCGPR                SETC  '&XCONNTKN'                                 00132200
.*                                                                      00132300
                      AIF   ('&XCONNTKN'(1,1) NE '(').ZI8B              00132400
&ZCGPR   SETC  '0('.'&XCONNTKN'(2,K'&XCONNTKN-2).')'                    00132500
                      AGO   .ZI8D                                       00132600
.ZI8B                 ANOP  ,                                           00132700
.*                                                                      00132800
&ZC                   SETC  '&XCONNTKN'(1,1)                            00132900
                      AIF   (T'&ZC NE 'N').ZI8D                         00133000
&ZC                   SETC  '&XCONNTKN'                                 00133100
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI8D                00133200
&ZCGPR                SETC  '=BL4''&XCONNTKN'''                         00133300
.*                                                                      00133400
.ZI8D                 ANOP  ,                                           00133500
.*                                                                      00133600
&ZOPN                 SETA  &ZOPN+1                                     00133700
&ZOPL(&ZOPN)          SETC  ' '                                         00133800
&ZOPO(&ZOPN)          SETC  'MVC'                                       00133900
&ZOPV(&ZOPN)          SETC  '8(4,1),&ZCGPR'                             00134000
&ZOPD(&ZOPN)          SETC  '++ &ZXCONNTKN XCONNTKN'                    00134100
.*                                                                      00134200
.ZI8E                 ANOP  ,                                           00134300
.*-------------------------------------------------------------------   00134400
&ZOPN1                SETA &ZOPN                                        00134500
.*******************************************************************    00134600
.***  PROCESS IWMSCXIT OUTPUT PARAMETERS                                00134700
.*******************************************************************    00134800
                      ACTR  10000                                       00134900
.*******************************************************************    00135000
.***AO.X02  RETURN CODE PROCESSING                                      00135100
.*******************************************************************    00135200
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00135300
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00135400
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00135500
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00135600
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00135700
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00135800
&ZOPN                 SETA  &ZOPN+1                                     00135900
&ZOPL(&ZOPN)          SETC  ' '                                         00136000
&ZOPO(&ZOPN)          SETC  'ST'                                        00136100
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00136200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00136300
                      AGO   .ZMRETX                                     00136400
.*                                                                      00136500
.ZMRET2               ANOP  ,                                           00136600
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00136700
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00136800
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00136900
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00137000
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00137100
&ZOPN                 SETA  &ZOPN+1                                     00137200
&ZOPL(&ZOPN)          SETC  ' '                                         00137300
&ZOPO(&ZOPN)          SETC  'LR'                                        00137400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00137500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00137600
.ZMRETX               ANOP  ,                                           00137700
.*******************************************************************    00137800
.***AO.X03  REASON CODE PROCESSING                                      00137900
.*******************************************************************    00138000
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00138100
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00138200
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00138300
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00138400
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00138500
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00138600
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00138700
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00138800
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00138900
&ZOPN                 SETA  &ZOPN+1                                     00139000
&ZOPL(&ZOPN)          SETC  ' '                                         00139100
&ZOPO(&ZOPN)          SETC  'ST'                                        00139200
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00139300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00139400
                      AGO   .ZMRSNX                                     00139500
.*                                                                      00139600
.ZMRSN2               ANOP  ,                                           00139700
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00139800
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00139900
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00140000
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00140100
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00140200
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00140300
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00140400
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00140500
&ZOPN                 SETA  &ZOPN+1                                     00140600
&ZOPL(&ZOPN)          SETC  ' '                                         00140700
&ZOPO(&ZOPN)          SETC  'LR'                                        00140800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00140900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00141000
.ZMRSNX               ANOP  ,                                           00141100
.*******************************************************************    00141200
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00141300
.*******************************************************************    00141400
.ZMFLX                ANOP  ,              MF=L PROCESSING              00141500
.*******************************************************************    00141600
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00141700
.*******************************************************************    00141800
                      ACTR  10000                                       00141900
.*                                                                      00142000
                      LCLA  &ZLC           COLUMN POSITION              00142100
                      LCLA  &ZOPX          LIST TABLE INDEX             00142200
                      LCLC  &ZLL           LABEL                        00142300
                      LCLC  &ZLOP          OPERATION                    00142400
                      LCLC  &ZLV           VALUE                        00142500
                      LCLC  &ZLD           DESCRIPTION                  00142600
.*                                                                      00142700
&ZOPX                 SETA  1                                           00142800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00142900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00143000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00143100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00143200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00143300
                      AIF   (K'&ZLD LE 0).ZL5                           00143400
&ZLC                  SETA  35             COMMENT COLUMN               00143500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00143600
&ZLC                  SETA  33             COMMMENT COLUMN              00143700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00143800
&ZLV                  SETC  '&ZLV'.' '                                  00143900
                      AGO   .ZL3                                        00144000
.*                                                                      00144100
.ZL4                  ANOP  ,                                           00144200
&ZLV                  SETC  '&ZLV'.' '                                  00144300
.ZL5                  ANOP  ,                                           00144400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00144500
.*                                                                      00144600
&ZLL     &ZLOP &ZLV                                                     00144700
.*                                                                      00144800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00144900
                      AGO   .ZL2           CONTINUE LOOPING             00145000
.*                                                                      00145100
.ZLX                  ANOP  ,                                           00145200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00145300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00145400
         LCLC  &T1                                                      00145500
         AIF   ('&XACTION' EQ 'NULL').FINIS                             00145600
.*                                                                      00145700
&T1      SETC  '&XSRV_MGR_EXIT@'                                        00145800
         AIF   ('&XSRV_MGR_EXIT@'(1,1) NE '(').EXITRS                   00145900
&T1      SETC  '&XSRV_MGR_EXIT@'(2,K'&XSRV_MGR_EXIT@-2)                 00146000
&T1      SETC  '0(&T1.)'                                                00146100
.EXITRS    ANOP                                                         00146200
.* &T1 IS NOW A VALID BASE-DISPLACEMENT EXPRESSION                      00146300
.*                                                                      00146400
         L     15,&T1                  FETCH CONNECT EXIT @             00146500
         BALR  14,15                                                    00146600
.*                                                                      00146700
.FINIS   ANOP                                                           00146800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00146900
.ZLINKX               ANOP  ,                                           00147000
.*******************************************************************    00147100
.***AP.X01  FORMAT GENERATED CODE                                       00147200
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00147300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00147400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00147500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00147600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00147700
                      AIF   (K'&ZLD LE 0).ZL51                          00147800
&ZLC                  SETA  35             COMMENT COLUMN               00147900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00148000
&ZLC                  SETA  33             COMMENT COLUMN               00148100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00148200
&ZLV                  SETC  '&ZLV'.' '                                  00148300
                      AGO   .ZL31                                       00148400
.*                                                                      00148500
.ZL41                 ANOP  ,                                           00148600
&ZLV                  SETC  '&ZLV'.' '                                  00148700
.ZL51                 ANOP  ,                                           00148800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00148900
.*                                                                      00149000
&ZLL     &ZLOP &ZLV                                                     00149100
.*                                                                      00149200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00149300
                      AGO   .ZL21          CONTINUE LOOPING             00149400
.*                                                                      00149500
.ZLX1                 ANOP  ,                                           00149600
.*******************************************************************    00149700
