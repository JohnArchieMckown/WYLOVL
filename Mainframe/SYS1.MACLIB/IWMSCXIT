                   GBLC  &ZTEST      YES=TEST NO=NO TEST                00044200
                   GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX        00044300
.*                                                                      00044400
                   GBLC  &ZIWMSCXIT  YES=MACRO USED AT LEAST ONCE       00044500
.*******************************************************************    00044600
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00044700
.*******************************************************************    00044800
                   LCLC  &ZMS        ERROR MESSAGE WORK AREA            00044900
                   LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS       00045000
                   LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED         00045100
                   LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR        00045200
                   LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE        00045300
                   LCLA  &ZAWRK      USED AS A WORK AREA                00045400
                   LCLA  &ZASIZ      USED FOR SIZE OF VALUE             00045500
                   LCLA  &ZN         NUMERIC WORK VARIABLE              00045600
                   LCLC  &ZC         CHARACTER WORK VARIABLE            00045700
                   LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE        00045800
                   LCLA  &ZTLA       VALUE TOO LONG VARIABLE            00045900
                   LCLA  &ZMACRET    MACRO RETURN CODE                  00046000
&ZMACRET           SETA  0           ASSUME NO ERRORS                   00046100
.*                                                                      00046200
                   LCLC  &ZCSYSLIST(255)        NONXKEY, POS ARRAY      00046300
                   LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()         00046400
                   LCLA  &ZPLLEN     PARAMETER LIST LENGTH              00046500
                   LCLC  &ZENDBASEPL            End of base PL          00046600
                   LCLA  &ZPLWORKA   PARAMETER LIST LENGTH              00046700
.*******************************************************************    00046800
.***AB.04  MACRO VARIABLES FOR RANGES AND SETS OF ARGUMENTS             00046900
.*******************************************************************    00047000
                   LCLA  &ZI         INDEX FOR ARRAY                    00047100
                   LCLA  &ZJ         INDEX FOR PARMS                    00047200
                   LCLA  &ZK         INDEX FOR PARSE                    00047300
                   LCLA  &ZLO        LOW RANGE VALUE                    00047400
                   LCLA  &ZHI        HIGH RANGE VALUE                   00047500
                   LCLC  &ZW         WORK AREA FOR PARM                 00047600
.********************************************************************   00047700
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00047800
.********************************************************************   00047900
                   GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL            00048000
                   GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE     00048100
.*                                                                      00048200
                   AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED     00048300
                   AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED      00048400
                   SPLEVEL  TEST                                        00048500
.ZSYS2             ANOP  ,                                              00048600
                   AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1          00048700
                   AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1          00048800
                   SYSSTATE TEST                                        00048900
                   AGO   .ZSYS4                                         00049000
.ZSYS3             ANOP  ,                                              00049100
&SYSASCE           SETC  'P'         ASSUME SYSSTATE PRIMARY            00049200
.ZSYS4             ANOP  ,                                              00049300
.*******************************************************************    00049400
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00049500
.*******************************************************************    00049600
                   LCLC  &XCONNTKN   ++ NAME                            00049700
                   LCLC  &XSRV_MGR_EXIT@        ++ NAME                 00049800
                   LCLC  &XACTION    ++ VALUE                           00049900
                   LCLC  &XRETCODE   ++ NAME                            00050000
                   LCLC  &XRSNCODE   ++ NAME                            00050100
                   LCLA  &XPLISTVER  ++ VALUE                           00050200
                   LCLC  &XMF        ++ VALUE                           00050300
                   LCLC  &XMFCTRL    ++ NAME                            00050400
                   LCLC  &XMFATTR    ++ VALUE                           00050500
                   LCLC  &XMFSCK     ++ VALUE                           00050600
.*******************************************************************    00050700
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00050800
.*******************************************************************    00050900
                   LCLC  &ZXCONNTKN  ++ INPUT NAME                      00051000
                   LCLC  &ZXSRV_MGR_EXIT@       ++ INPUT NAME           00051100
                   LCLC  &ZXACTION   ++ INPUT VALUE                     00051200
                   LCLC  &ZXRETCODE  ++ OUTPUT NAME                     00051300
                   LCLC  &ZXRSNCODE  ++ OUTPUT NAME                     00051400
                   LCLC  &ZXPLISTVER            ++ INPUT VALUE          00051500
                   LCLC  &ZXMF       ++ INPUT VALUE                     00051600
                   LCLC  &ZXMFCTRL   ++ INPUT NAME                      00051700
                   LCLC  &ZXMFATTR   ++ INPUT VALUE                     00051800
                   LCLC  &ZXMFSCK    ++ INPUT VALUE                     00051900
.********************************************************************   00052000
.***AC.PLV01  PROCESS PLISTVER                                      *   00052100
.********************************************************************   00052200
&XPLISTVER      SETA  0                                                 00052300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00052400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00052500
&XPLISTVER      SETA  0                                                 00052600
.ZPLV0          ANOP  ,                                                 00052700
.*                                                                      00052800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00052900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00053000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00053100
.*-------------------------------------------------------------------   00053200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00053300
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00053400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00053500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00053600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00053700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00053800
                AGO   .ZPVCK2                                           00053900
.ZPVCK3         ANOP  ,                                                 00054000
.*-------------------------------------------------------------------   00054100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00054200
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00054300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00054400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00054500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00054600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054700
                AGO   .ZPVCKE                                           00054800
.ZPVCK2         ANOP  ,                                                 00054900
.*-------------------------------------------------------------------   00055000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00055100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00055200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00055300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00055400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00055500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00055600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00055700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00055800
                AGO   .ZPVCK5                                           00055900
.ZPVCK4         ANOP  ,                                                 00056000
&XPLISTVER      SETA  &PLISTVER(1)                                      00056100
.ZPVCK5         ANOP  ,                                                 00056200
.*-------------------------------------------------------------------   00056300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00056400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00056500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00056600
                AIF   (&XPLISTVER LT 0).ZPVCK6  TOO LOW                 00056700
                AIF   (&XPLISTVER LE 0).ZPVCKE  NOT HIGH                00056800
.ZPVCK6         ANOP   ,                                                00056900
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00057000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057100
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00057200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00057300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057400
.ZPVCKE         ANOP   ,                                                00057500
.********************************************************************   00057600
