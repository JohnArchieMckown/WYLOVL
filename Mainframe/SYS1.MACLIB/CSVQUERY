.***AC.PLV01  PROCESS PLISTVER                                      *   00090700
.********************************************************************   00090800
&XPLISTVER      SETA  0                                                 00090900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00091000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00091100
&XPLISTVER      SETA  4                                                 00091200
.ZPLV0          ANOP  ,                                                 00091300
.*                                                                      00091400
                AIF   ('&INEPTKN' EQ '').ZPLV1E                         00091500
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00091600
&XPLISTVER      SETA  2                                                 00091700
.ZPLV1E         ANOP  ,                                                 00091800
.*                                                                      00091900
                AIF   ('&OUTXTLST' EQ '').ZPLV2E                        00092000
                AIF   (&XPLISTVER GE 3).ZPLV2E                          00092100
&XPLISTVER      SETA  3                                                 00092200
.ZPLV2E         ANOP  ,                                                 00092300
.*                                                                      00092400
                AIF   ('&OUTLOADPT' EQ '').ZPLV3E                       00092500
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00092600
&XPLISTVER      SETA  1                                                 00092700
.ZPLV3E         ANOP  ,                                                 00092800
.*                                                                      00092900
                AIF   ('&OUTPDATA' EQ '').ZPLV4E                        00093000
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00093100
&XPLISTVER      SETA  2                                                 00093200
.ZPLV4E         ANOP  ,                                                 00093300
.*                                                                      00093400
                AIF   ('&OUTPID' EQ '').ZPLV5E                          00093500
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00093600
&XPLISTVER      SETA  2                                                 00093700
.ZPLV5E         ANOP  ,                                                 00093800
.*                                                                      00093900
                AIF   ('&OUTEPTKN' EQ '').ZPLV6E                        00094000
                AIF   (&XPLISTVER GE 2).ZPLV6E                          00094100
&XPLISTVER      SETA  2                                                 00094200
.ZPLV6E         ANOP  ,                                                 00094300
.*                                                                      00094400
                AIF   ('&OUTDIAG' EQ '').ZPLV7E                         00094500
                AIF   (&XPLISTVER GE 3).ZPLV7E                          00094600
&XPLISTVER      SETA  3                                                 00094700
.ZPLV7E         ANOP  ,                                                 00094800
.*                                                                      00094900
                AIF   ('&OUTRTID' EQ '').ZPLV8E                         00095000
                AIF   (&XPLISTVER GE 3).ZPLV8E                          00095100
&XPLISTVER      SETA  3                                                 00095200
.ZPLV8E         ANOP  ,                                                 00095300
.*                                                                      00095400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00095500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00095600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00095700
.*-------------------------------------------------------------------   00095800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00095900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00096000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00096100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00096200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00096300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00096400
                AGO   .ZPVCK2                                           00096500
.ZPVCK3         ANOP  ,                                                 00096600
.*-------------------------------------------------------------------   00096700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00096800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00096900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00097000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00097100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00097200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00097300
                AGO   .ZPVCKE                                           00097400
.ZPVCK2         ANOP  ,                                                 00097500
.*-------------------------------------------------------------------   00097600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00097700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00097800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00097900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00098000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00098100
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00098200
.ZPVCK6         ANOP  ,                                                 00098300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00098400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00098500
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00098600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00098700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00098800
&XPLISTVER      SETA  4                                                 00098900
                AGO   .ZPVCKE                                           00099000
.*-------------------------------------------------------------------   00099100
.ZPVCK4A        ANOP  ,                                                 00099200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00099300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00099400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00099500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00099600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00099700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00099800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00099900
                AGO   .ZPVCK5                                           00100000
.ZPVCK4         ANOP  ,                                                 00100100
&XPLISTVER      SETA  &PLISTVER(1)                                      00100200
.ZPVCK5         ANOP  ,                                                 00100300
.ZPVCKE         ANOP  ,                                                 00100400
