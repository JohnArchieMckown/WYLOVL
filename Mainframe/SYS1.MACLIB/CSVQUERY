.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00237500
.******************************************************************     00237600
                 ACTR  10000                                            00237700
.*----------------------------------------------------------------***   00237800
.***AH.P02 EXTRA POSITIONALS                                            00237900
                 AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS             00238000
                 AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL             00238100
                 AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL         00238200
                 AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL          00238300
.ZGLISTX1        ANOP  ,                                                00238400
&ZCVAL           SETC  ''          ERROR FIELD                          00238500
&ZAWRK           SETA  0           POSITIONAL NUMBER                    00238600
.ZGLIST2         ANOP  ,                                                00238700
&ZAWRK           SETA  &ZAWRK+1    NEXT POSITIONAL                      00238800
                 AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE             00238900
&ZCVAL           SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                  00239000
                 AGO   .ZGLIST2                                         00239100
.*                                                                      00239200
.ZGLIST3         ANOP  ,                                                00239300
&ZCVAL           SETC '&ZCVAL'(2,K'&ZCVAL-1)    STRIP COMMA             00239400
&ZMS             SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE         00239500
&ZMS             SETC  '&ZMS "&ZCVAL"'                                  00239600
&ZMS             SETC  '&ZMS CANNOT BE USED.'                           00239700
&ZMS             SETC  '&ZMS NO POSITIONAL ARGUMENTS'                   00239800
                 MNOTE 8,' &ZMS ARE ALLOWED.'                           00239900
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00240000
.ZGLISTX         ANOP  ,                                                00240100
&ZASYSLNDX       SETA  0                                                00240200
