.*                                                                      00389800
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00389900
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00390000
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00390100
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00390200
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00390300
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00390400
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00390500
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00390600
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00390700
&ZOPN                 SETA  &ZOPN+1                                     00390800
&ZOPL(&ZOPN)          SETC  ' '                                         00390900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00391000
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00391100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00391200
                      AGO   .ZIMFNX                                     00391300
.*                                                                      00391400
.ZIMFN2               ANOP ,                                            00391500
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00391600
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00391700
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00391800
&ZOPN                 SETA  &ZOPN+1                                     00391900
&ZOPL(&ZOPN)          SETC  ' '                                         00392000
&ZOPO(&ZOPN)          SETC  'LAE'                                       00392100
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00392200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00392300
                      AGO   .ZIMFNX                                     00392400
.ZIMFNX1              ANOP ,                                            00392500
