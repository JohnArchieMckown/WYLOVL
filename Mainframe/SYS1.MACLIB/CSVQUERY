&ZOPD(&ZOPN)          SETC '++ FIELD_LABEL'                             00343800
.*                                                                      00343900
&ZOPN                 SETA  &ZOPN+1                                     00344000
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._KEYUSED_OUTLENGTH'               00344100
&ZOPO(&ZOPN)          SETC  'EQU'                                       00344200
&ZOPV(&ZOPN)          SETC  'B''10000000'''                             00344300
&ZOPD(&ZOPN)          SETC  '++ KEYUSED.'                               00344400
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).OUTLENGTH'                    00344500
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00344600
.*                                                                      00344700
&ZOPN                 SETA  &ZOPN+1                                     00344800
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._KEYUSED_OUTEPA'                  00344900
&ZOPO(&ZOPN)          SETC  'EQU'                                       00345000
&ZOPV(&ZOPN)          SETC  'B''01000000'''                             00345100
&ZOPD(&ZOPN)          SETC  '++ KEYUSED.'                               00345200
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).OUTEPA'                       00345300
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00345400
.*                                                                      00345500
&ZOPN                 SETA  &ZOPN+1                                     00345600
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._KEYUSED_OUTEPNM'                 00345700
