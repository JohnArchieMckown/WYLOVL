&ZOPN                 SETA  &ZOPN+1                                     00500600
&ZOPL(&ZOPN)          SETC  ' '                                         00500700
&ZOPO(&ZOPN)          SETC  'ST'                                        00500800
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00500900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00501000
                      AGO   .ZMRETX                                     00501100
.*                                                                      00501200
.ZMRET2               ANOP  ,                                           00501300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00501400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00501500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00501600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00501700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00501800
&ZOPN                 SETA  &ZOPN+1                                     00501900
&ZOPL(&ZOPN)          SETC  ' '                                         00502000
&ZOPO(&ZOPN)          SETC  'LR'                                        00502100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00502200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00502300
.ZMRETX               ANOP  ,                                           00502400
.*******************************************************************    00502500
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00502600
.*******************************************************************    00502700
.ZMFLX                ANOP  ,              MF=L PROCESSING              00502800
.*******************************************************************    00502900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00503000
.*******************************************************************    00503100
                      ACTR  10000                                       00503200
.*                                                                      00503300
                      LCLA  &ZLC           COLUMN POSITION              00503400
                      LCLA  &ZOPX          LIST TABLE INDEX             00503500
                      LCLC  &ZLL           LABEL                        00503600
                      LCLC  &ZLOP          OPERATION                    00503700
                      LCLC  &ZLV           VALUE                        00503800
                      LCLC  &ZLD           DESCRIPTION                  00503900
.*                                                                      00504000
&ZOPX                 SETA  1                                           00504100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00504200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00504300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00504400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00504500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00504600
                      AIF   (K'&ZLD LE 0).ZL5                           00504700
&ZLC                  SETA  35             COMMENT COLUMN               00504800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00504900
&ZLC                  SETA  33             COMMMENT COLUMN              00505000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00505100
&ZLV                  SETC  '&ZLV'.' '                                  00505200
                      AGO   .ZL3                                        00505300
.*                                                                      00505400
.ZL4                  ANOP  ,                                           00505500
&ZLV                  SETC  '&ZLV'.' '                                  00505600
.ZL5                  ANOP  ,                                           00505700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00505800
.*                                                                      00505900
&ZLL     &ZLOP &ZLV                                                     00506000
.*                                                                      00506100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00506200
                      AGO   .ZL2           CONTINUE LOOPING             00506300
.*                                                                      00506400
.ZLX                  ANOP  ,                                           00506500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00506600
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00506700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00506800
&NDXNUM  SETC '&SYSNDX'            GENERATE UNIQUE LABEL FOR BRANCH     00506900
         LA   15,20                ASSUME BAD RET CODE                  00507000
         L    14,16(0,0)           GET CVT ADDRESS                      00507100
         TM   1264(14),B'00010000' CHECK RELEASE LEVEL BIT              00507200
         BNO  @&NDXNUM             TEST FAILED - PASS BACK RC 20        00507300
         L    14,772(14,0)         GET SFT ADDRESS                      00507400
         L    14,232(14,0)         GET LX,EX (PC NUMBER)                00507500
         PC   0(14)                PC TO CSVQSER                        00507600
@&NDXNUM DS  0H                                                         00507700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00507800
.ZLINKX               ANOP  ,                                           00507900
.*******************************************************************    00508000
.***AP.X01  FORMAT GENERATED CODE                                       00508100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00508200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00508300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00508400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00508500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00508600
                      AIF   (K'&ZLD LE 0).ZL51                          00508700
&ZLC                  SETA  35             COMMENT COLUMN               00508800
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00508900
&ZLC                  SETA  33             COMMENT COLUMN               00509000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00509100
&ZLV                  SETC  '&ZLV'.' '                                  00509200
                      AGO   .ZL31                                       00509300
.*                                                                      00509400
.ZL41                 ANOP  ,                                           00509500
&ZLV                  SETC  '&ZLV'.' '                                  00509600
.ZL51                 ANOP  ,                                           00509700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00509800
.*                                                                      00509900
&ZLL     &ZLOP &ZLV                                                     00510000
.*                                                                      00510100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00510200
                      AGO   .ZL21          CONTINUE LOOPING             00510300
.*                                                                      00510400
.ZLX1                 ANOP  ,                                           00510500
.*******************************************************************    00510600
.***AP.X02  END OF CSVQUERY MACRO                                       00510700
