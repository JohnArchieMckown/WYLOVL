.*******************************************************************    00334400
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00334500
.*******************************************************************    00334600
                 AIF   (&ZMACRET NE 0).ZLX2     ERROR DETECTED          00334700
.*******************************************************************    00334800
.***AE.01  START OF CSVQUERY MACRO CODE                                 00334900
.*******************************************************************    00335000
                 LCLC  &M0M0001                 NAME                    00335100
&M0M0001         SETC  '&XLABEL'                SET NAME                00335200
                 AIF   (K'&XLABEL NE 0).XLAB2                           00335300
&M0M0001         SETC  'M00M&SYSNDX'            DEFAULT NAME            00335400
.*                                                                      00335500
.XLAB2           ANOP  ,                                                00335600
                 AIF   ('&XMF' EQ 'L').XLAB3                            00335700
&M0M0001 DS    0H                             CSVQUERY-4                00335800
.XLAB3           ANOP  ,                                                00335900
.*******************************************************************    00336000
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00336100
.*******************************************************************    00336200
              PUSH  PRINT                                               00336300
                 AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON       00336400
                 PRINT OFF         ++ PR01                              00336500
                 AGO   .ZPRNTXX                                         00336600
.ZPRINTX         PRINT ON                                               00336700
.ZPRNTXX         ANOP  ,                                                00336800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00336900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00337000
.********************************************************************   00337100
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00337200
.********************************************************************   00337300
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00337400
                      LCLA  &ZOPN1         INVOCATION INDEX             00337500
                      LCLC  &ZOPL(4095)    LABEL                        00337600
                      LCLC  &ZOPO(4095)    OPERATION                    00337700
                      LCLC  &ZOPV(4095)    VALUE                        00337800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00337900
.*                                                                      00338000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00338100
.******************************************************************     00338200
.***AL.L10 CSVQUERY FMT=LIST MF=S|L PARAMETERS                          00338300
.******************************************************************     00338400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00338500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00338600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00338700
&XMFATTR              SETC  '0D'                                        00338800
.*                                                                      00338900
&ZOPN                 SETA  &ZOPN+1                                     00339000
&ZOPL(&ZOPN)          SETC  ' '                                         00339100
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00339200
&ZOPO(&ZOPN)          SETC  'B'                                         00339300
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00339400
                      AGO   .ZALVL6B                                    00339500
.ZALVL6A              ANOP                                              00339600
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00339700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00339800
.ZALVL6B              ANOP                                              00339900
&ZOPD(&ZOPN)          SETC  '++ CSVQUERY ARG PARMS'                     00340000
.*                                                                      00340100
&ZOPN                 SETA  &ZOPN+1                                     00340200
&ZOPL(&ZOPN)          SETC  ' '                                         00340300
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00340400
&ZOPV(&ZOPN)          SETC  '1'                                         00340500
&ZOPD(&ZOPN)          SETC  ' '                                         00340600
.*                                                                      00340700
.ZLMFSL2              ANOP  ,                                           00340800
.*-------------------------------------------------------------------   00340900
.***AM.L00  CSVQUERY FMT=LIST name                                      00341000
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00341100
&ZOPN                 SETA &ZOPN+1                                      00341200
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00341300
&ZOPO(&ZOPN)          SETC 'EQU'                                        00341400
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00341500
&ZOPD(&ZOPN)          SETC '++ CSVQUERY NAME'                           00341600
.ZLMFSL2M             ANOP  ,                                           00341700
.*-------------------------------------------------------------------   00341800
.***AM.L01  CSVQUERY FMT=LIST PARAMETERS                                00341900
&ZOPN                 SETA &ZOPN+1                                      00342000
&ZOPL(&ZOPN)          SETC '&XMFCTRL'                                   00342100
&ZOPO(&ZOPN)          SETC 'DS'                                         00342200
&ZOPV(&ZOPN)          SETC '&XMFATTR'                                   00342300
&ZOPD(&ZOPN)          SETC '++ CSVQUERY PARM LIST'                      00342400
.*-------------------------------------------------------------------   00342500
.***AM.L02 XVERSION XL1                                                 00342600
&ZOPN                 SETA &ZOPN+1                                      00342700
