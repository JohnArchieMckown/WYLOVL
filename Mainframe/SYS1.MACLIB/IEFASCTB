  DECLARE 1 INSMSCT BDY(DWORD) BASED(SCTPTR),  /* SCT */                03020000
     4 SCTDISKA PTR(24) BDY(WORD),  /* DISK ADDRESS OF SCT */           03820000
     4 SCTTBLID CHAR(1),  /* TABLE ID OF SCT = 2 */                     04620000
     4 SCTSSTAT BIT(8),   /* INTERNAL STEP STATUS */                    05420000
        6 EAADDRBT BIT(1),  /* ADDRSPC=REAL ON EXEC           @Y01029*/ 06220000
        6 EACAUSER BIT(1),  /* CAN CAUSE ROLLOUT */                     07020000
        6 SCTNORST BIT(1),  /* NO RESTART TO BE DONE */                 07820000
