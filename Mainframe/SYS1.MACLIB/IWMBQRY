.***AC.PLV01  PROCESS PLISTVER                                      *   00055600
.********************************************************************   00055700
&XPLISTVER      SETA  0                                                 00055800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00055900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00056000
&XPLISTVER      SETA  0                                                 00056100
.ZPLV0          ANOP  ,                                                 00056200
.*                                                                      00056300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00056400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00056500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00056600
.*-------------------------------------------------------------------   00056700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00056800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00056900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00057100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00057200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057300
                AGO   .ZPVCK2                                           00057400
.ZPVCK3         ANOP  ,                                                 00057500
.*-------------------------------------------------------------------   00057600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00057700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00057800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00058000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00058100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00058200
                AGO   .ZPVCKE                                           00058300
.ZPVCK2         ANOP  ,                                                 00058400
.*-------------------------------------------------------------------   00058500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00058600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00058700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00058800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00058900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00059000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00059100
.ZPVCK6         ANOP  ,                                                 00059200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00059300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00059400
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00059500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00059600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059700
&XPLISTVER      SETA  0                                                 00059800
                AGO   .ZPVCKE                                           00059900
.*-------------------------------------------------------------------   00060000
.ZPVCK4A        ANOP  ,                                                 00060100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00060200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00060300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00060400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00060500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00060600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00060700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00060800
                AGO   .ZPVCK5                                           00060900
.ZPVCK4         ANOP  ,                                                 00061000
&XPLISTVER      SETA  &PLISTVER(1)                                      00061100
.ZPVCK5         ANOP  ,                                                 00061200
.ZPVCKE         ANOP  ,                                                 00061300
.********************************************************************   00061400
