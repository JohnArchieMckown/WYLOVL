.***AC.PLV01  PROCESS PLISTVER                                      *   00045400
.********************************************************************   00045500
&XPLISTVER      SETA  1                                                 00045600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00045700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00045800
&XPLISTVER      SETA  1                                                 00045900
.ZPLV0          ANOP  ,                                                 00046000
.*                                                                      00046100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00046200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00046300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00046400
.*-------------------------------------------------------------------   00046500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00046600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00046700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00046800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00046900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00047000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00047100
                AGO   .ZPVCK2                                           00047200
.ZPVCK3         ANOP  ,                                                 00047300
.*-------------------------------------------------------------------   00047400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00047500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00047600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00047700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00047800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00047900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00048000
                AGO   .ZPVCKE                                           00048100
.ZPVCK2         ANOP  ,                                                 00048200
.*-------------------------------------------------------------------   00048300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00048400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00048500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00048600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00048700
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00048800
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00048900
.ZPVCK6         ANOP  ,                                                 00049000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00049100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00049200
