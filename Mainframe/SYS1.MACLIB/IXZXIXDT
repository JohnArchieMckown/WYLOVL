                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00120400
&ZLC                  SETA  33             COMMMENT COLUMN              00120500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00120600
&ZLV                  SETC  '&ZLV'.' '                                  00120700
                      AGO   .ZL3                                        00120800
.*                                                                      00120900
.ZL4                  ANOP  ,                                           00121000
&ZLV                  SETC  '&ZLV'.' '                                  00121100
.ZL5                  ANOP  ,                                           00121200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00121300
.*                                                                      00121400
&ZLL     &ZLOP &ZLV                                                     00121500
.*                                                                      00121600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00121700
                      AGO   .ZL2           CONTINUE LOOPING             00121800
.*                                                                      00121900
