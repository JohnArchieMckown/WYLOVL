*DECLARE 1 SJPUP SJPUATRB,                                              04650000
*          2 SJPUID   CHAR(4),     /* IDENTIFIER                     */ 04700000
*          2 SJPUVERS FIXED(8),    /* VERSION NUMBER                 */ 04750000
*          2 SJPUFLAG BIT(8),      /* CONTROL FLAGS                  */ 04800000
*            3 SJPUNREC BIT(1),    /* NO RECOVERY                    */ 04850000
*            3 SJPUNOCU BIT(1),    /* NO CLEANUP                     */ 04900000
*            3 *        BIT(6),    /* RESERVED                       */ 04950000
*          2 SJPULEN FIXED(15),    /* LENGTH OF PARAMETER LIST       */ 05000000
*          2 SJPUSTOR PTR(31),     /* LOCAL STORAGE POINTER          */ 05050000
*          2 SJPUREAS FIXED(31),   /* RESULT REASON CODE (RETURNED)  */ 05100000
*          2 SJPUTOKN,                                                  05150000
*            3 SJPUANBK PTR(31),   /* ADDRESS OF CONTROL BLOCK FOR A    05200000
*                                     JCL STATEMENT (JCT, SCT, SIOT     05250000
*                                     OR SWB) OR THE ADDRESS OF A       05300000
*                                     SWB CHAIN                      */ 05350000
*            3 SJPUANCA PTR(31),   /* ADDRESS OF WORD POINTING TO A     05400000
*                                     A SWB CHAIN OR ZERO            */ 05450000
*          2 SJPUSWBA PTR(31),     /* ADDRESS OF AREA CONTAINING        05500000
*                                     KEYWORD DATA                   */ 05550000
*          2 SJPUALEN FIXED(16),   /* LENGTH OF AREA CONTAINING         05600000
*                                     KEYWORD DATA                   */ 05650000
*          2 SJPUFLG2 BIT(8),      /* FLAGS                          */ 05700000
*            3 SJPUNSWA BIT(1),    /* SWBS TO BE BUILT IN NON-SWA       05750000
*                                     SUBPOOL                        */ 05800000
*            3 SJPUWARN BIT(1),    /* CONTINUE PROCESSING AFTER AN      05850000
*                                     ERROR WHICH IS DUE TO CHANGES     05900000
*                                     IN THE JDTS FROM RELEASE TO       05950000
*                                     RELEASE IS ENCOUNTERED     @D1A*/ 06000000
*            3 SJPUOSER BIT(1),    /* SERIALIZATION ON SWB USE COUNT    06012500
*                                     IS NOT REQUIRED            @L2A*/ 06025000
*            3 *        BIT(5),    /* RESERVED                   @L2A*/ 06037500
*          2 SJPURSV2 BIT(8),      /* RESERVED                       */ 06050000
*          2 SJPUJDVT CHAR(8),     /* JDVT NAME                  @L3C*/ 06066600
