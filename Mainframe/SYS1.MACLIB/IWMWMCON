.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00149000
.*******************************************************************    00149100
                      ACTR  10000                                       00149200
.*                                                                      00149300
                      LCLA  &ZLC           COLUMN POSITION              00149400
                      LCLA  &ZOPX          LIST TABLE INDEX             00149500
                      LCLC  &ZLL           LABEL                        00149600
                      LCLC  &ZLOP          OPERATION                    00149700
                      LCLC  &ZLV           VALUE                        00149800
                      LCLC  &ZLD           DESCRIPTION                  00149900
.*                                                                      00150000
&ZOPX                 SETA  1                                           00150100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00150200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00150300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00150400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00150500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00150600
                      AIF   (K'&ZLD LE 0).ZL5                           00150700
&ZLC                  SETA  35             COMMENT COLUMN               00150800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00150900
&ZLC                  SETA  33             COMMMENT COLUMN              00151000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00151100
&ZLV                  SETC  '&ZLV'.' '                                  00151200
                      AGO   .ZL3                                        00151300
.*                                                                      00151400
.ZL4                  ANOP  ,                                           00151500
&ZLV                  SETC  '&ZLV'.' '                                  00151600
.ZL5                  ANOP  ,                                           00151700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00151800
.*                                                                      00151900
&ZLL     &ZLOP &ZLV                                                     00152000
.*                                                                      00152100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00152200
                      AGO   .ZL2           CONTINUE LOOPING             00152300
.*                                                                      00152400
.ZLX                  ANOP  ,                                           00152500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00152600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00152700
         L     14,16(0,0)              GET CVT ADDRESS                  00152800
         L     14,772(14,0)            GET SFT ADDRESS                  00152900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00153000
         LA    15,35                   GET EX FOR SERVICE ROUTINE       00153100
         OR    14,15                   GET PC NUMBER                    00153200
         PC    0(14)                                                    00153300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00153400
