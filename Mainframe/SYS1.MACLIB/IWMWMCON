.***AE.01  START OF IWMWMCON MACRO CODE                                 00117300
.*******************************************************************    00117400
              LCLC  &M0M0001                 NAME                       00117500
&M0M0001      SETC  '&XLABEL'                SET NAME                   00117600
              AIF   (K'&XLABEL NE 0).XLAB2                              00117700
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00117800
.*                                                                      00117900
.XLAB2        ANOP  ,                                                   00118000
              AIF   ('&XMF' EQ 'L').XLAB3                               00118100
&M0M0001 DS    0H                             IWMWMCON-0                00118200
.XLAB3        ANOP  ,                                                   00118300
.*******************************************************************    00118400
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00118500
.*******************************************************************    00118600
              PUSH  PRINT                                               00118700
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00118800
              PRINT OFF         ++ PR01                                 00118900
              AGO   .ZPRNTXX                                            00119000
.ZPRINTX      PRINT ON                                                  00119100
.ZPRNTXX      ANOP  ,                                                   00119200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00119300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00119400
.********************************************************************   00119500
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00119600
.********************************************************************   00119700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00119800
                      LCLA  &ZOPN1         INVOCATION INDEX             00119900
                      LCLC  &ZOPL(4095)    LABEL                        00120000
                      LCLC  &ZOPO(4095)    OPERATION                    00120100
                      LCLC  &ZOPV(4095)    VALUE                        00120200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00120300
.*                                                                      00120400
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00120500
.******************************************************************     00120600
.***AL.L10 IWMWMCON FMT=LIST MF=S|L PARAMETERS                          00120700
.******************************************************************     00120800
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00120900
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00121000
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00121100
&XMFATTR              SETC  '0D'                                        00121200
.*                                                                      00121300
&ZOPN                 SETA  &ZOPN+1                                     00121400
&ZOPL(&ZOPN)          SETC  ' '                                         00121500
&ZOPO(&ZOPN)          SETC  'B'                                         00121600
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00121700
&ZOPD(&ZOPN)          SETC  '++ IWMWMCON ARG PARMS'                     00121800
.*                                                                      00121900
&ZOPN                 SETA  &ZOPN+1                                     00122000
&ZOPL(&ZOPN)          SETC  ' '                                         00122100
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00122200
&ZOPV(&ZOPN)          SETC  '1'                                         00122300
&ZOPD(&ZOPN)          SETC  ' '                                         00122400
.*                                                                      00122500
.ZLMFSL2              ANOP  ,                                           00122600
.*-------------------------------------------------------------------   00122700
.***AM.L00  IWMWMCON FMT=LIST name                                      00122800
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00122900
&ZOPN                 SETA &ZOPN+1                                      00123000
