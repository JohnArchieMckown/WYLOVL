.***AC.PLV01  PROCESS PLISTVER                                      *   00066100
.********************************************************************   00066200
&XPLISTVER      SETA  0                                                 00066300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00066400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00066500
&XPLISTVER      SETA  0                                                 00066600
.ZPLV0          ANOP  ,                                                 00066700
.*                                                                      00066800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00066900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00067000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00067100
.*-------------------------------------------------------------------   00067200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00067300
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00067400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00067500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00067600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00067700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067800
                AGO   .ZPVCK2                                           00067900
.ZPVCK3         ANOP  ,                                                 00068000
.*-------------------------------------------------------------------   00068100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00068200
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00068300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00068500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00068600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068700
                AGO   .ZPVCKE                                           00068800
.ZPVCK2         ANOP  ,                                                 00068900
.*-------------------------------------------------------------------   00069000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00069100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00069200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00069300
                AIF   (&XPLISTVER LT 0).ZPVCK6  TOO LOW                 00069400
                AIF   (&XPLISTVER LE 0).ZPVCKE  NOT HIGH                00069500
.ZPVCK6         ANOP  ,                                                 00069600
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00069700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00069800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00069900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00070000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070100
&XPLISTVER      SETA  0                                                 00070200
                AGO   .ZPVCKE                                           00070300
.*-------------------------------------------------------------------   00070400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00070500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00070600
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00070700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00070800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00070900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00071000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00071100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071200
                AGO   .ZPVCK5                                           00071300
.ZPVCK4         ANOP  ,                                                 00071400
&XPLISTVER      SETA  &PLISTVER(1)                                      00071500
.ZPVCK5         ANOP  ,                                                 00071600
.ZPVCKE         ANOP  ,                                                 00071700
.********************************************************************   00071800
.***AG.MF01 PROCESS MF MACRO FORMS                                      00071900
.********************************************************************   00072000
              ACTR  10000                                               00072100
