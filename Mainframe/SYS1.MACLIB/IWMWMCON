&ZOPO(&ZOPN)          SETC  'LR'                                        00144700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00144800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00144900
.ZMRETX               ANOP  ,                                           00145000
.*******************************************************************    00145100
.***AO.X03  REASON CODE PROCESSING                                      00145200
.*******************************************************************    00145300
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00145400
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00145500
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00145600
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00145700
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00145800
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00145900
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00146000
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00146100
