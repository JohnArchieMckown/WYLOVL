.***AG.MF01 PROCESS MF MACRO FORMS                                      00040900
.********************************************************************   00041000
              ACTR  10000                                               00041100
.*------------------------------------------------------------------    00041200
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00041300
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00041400
.*-------------------------------------------------------------------   00041500
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00041600
.ZG15         ANOP  ,                                                   00041700
&ZTLC         SETC 'S'                                                  00041800
              AIF   ('&MF(1)' NE '&ZTLC').ZG16                          00041900
&XMF          SETC  'S'         SET KEYWORD                             00042000
              AIF   (N'&MF LE 1).ZG15A          CK NUM                  00042100
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00042200
&ZMS          SETC  '&ZMS.&MF"'                                         00042300
&ZMS          SETC  '&ZMS "MF"'                                         00042400
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00042500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00042600
.ZG15A        ANOP  ,                                                   00042700
.*-------------------------------------------------------------------   00042800
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00042900
.ZG16         ANOP  ,                                                   00043000
&ZTLC         SETC 'L'                                                  00043100
              AIF   ('&MF(1)' NE '&ZTLC').ZG23                          00043200
&XMF          SETC  'L'         SET KEYWORD                             00043300
              AIF   (N'&MF LE 3).ZG16A          CK NUM                  00043400
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00043500
&ZMS          SETC  '&ZMS.&MF"'                                         00043600
&ZMS          SETC  '&ZMS "MF"'                                         00043700
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00043800
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00043900
.ZG16A        ANOP  ,                                                   00044000
.*-------------------------------------------------------------------   00044100
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00044200
              AIF   (N'&SYSLIST NE 0).ZG16B     POSITIONALS?            00044300
              AIF   ('&MEMTOKEN' NE '').ZG16B   KEYS?                   00044400
              AIF   ('&TARGET' NE '').ZG16B     KEYS?                   00044500
              AIF   ('&RETCODE' NE '').ZG16B    KEYS?                   00044600
              AIF   ('&RSNCODE' NE '').ZG16B    KEYS?                   00044700
              AGO    .ZG16C     INVOCATION OKAY                         00044800
.ZG16B        ANOP  ,                                                   00044900
&ZMS          SETC  'THE "L" FORM OF "MF" CANNOT'  *MFLA* MNOTE         00045000
&ZMS          SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                   00045100
&ZMS          SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'              00045200
              MNOTE 8,' &ZMS ON THE INVOCATION.'                        00045300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00045400
.ZG16C        ANOP  ,                                                   00045500
.*-------------------------------------------------------------------   00045600
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00045700
&ZXMFCTRL     SETC  'INPUT'     SET IN/OUTPUT                           00045800
&XMFCTRL      SETC  '&MF(2,1)'  SET XVARIABLE                           00045900
              AIF   (N'&MF(2) GT 1).ZG18B       MULTI PARMS             00046000
&XMFCTRL      SETC  '&MF(2)'    SET XVARIABLE                           00046100
.ZG18B        AIF   (K'&MF(2) GT 0).ZG18C                               00046200
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00046300
&ZMS          SETC  'WHEN "L" IS SPECIFIED'     *KRQDA*                 00046400
&ZMS          SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'                 00046500
              MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                        00046600
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00046700
.ZG18C        ANOP  ,                                                   00046800
.*-------------------------------------------------------------------   00046900
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00047000
              AIF   (K'&ZXMFCTRL LE 0).ZG18P    NOT I/O                 00047100
              AIF   ('&XMFCTRL'(1,1) NE '(').ZG18P                      00047200
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00047300
&ZMS          SETC  '"MF='      *KGPRA* MNOTE                           00047400
&ZMS          SETC  '&ZMS.&MF"'                                         00047500
&ZMS          SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'           00047600
              MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 2.'                   00047700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00047800
.ZG18P        ANOP  ,                                                   00047900
.*-------------------------------------------------------------------   00048000
.***AG.KXVAR13 VALUE, OPT INPUT, POS(3) -XVARIABLE                      00048100
&ZXMFATTR     SETC  'INPUT'     SET IN/OUTPUT                           00048200
&XMFATTR      SETC  '0D'                                                00048300
              AIF   (K'&MF(3) LE 0).ZG21B       PARM                    00048400
