.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00111300
.*******************************************************************    00111400
                      ACTR  10000                                       00111500
.*                                                                      00111600
                      LCLA  &ZLC           COLUMN POSITION              00111700
                      LCLA  &ZOPX          LIST TABLE INDEX             00111800
                      LCLC  &ZLL           LABEL                        00111900
                      LCLC  &ZLOP          OPERATION                    00112000
                      LCLC  &ZLV           VALUE                        00112100
                      LCLC  &ZLD           DESCRIPTION                  00112200
.*                                                                      00112300
&ZOPX                 SETA  1                                           00112400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00112500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00112600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00112700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00112800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00112900
                      AIF   (K'&ZLD LE 0).ZL5                           00113000
&ZLC                  SETA  35             COMMENT COLUMN               00113100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00113200
&ZLC                  SETA  33             COMMMENT COLUMN              00113300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00113400
&ZLV                  SETC  '&ZLV'.' '                                  00113500
                      AGO   .ZL3                                        00113600
.*                                                                      00113700
.ZL4                  ANOP  ,                                           00113800
&ZLV                  SETC  '&ZLV'.' '                                  00113900
.ZL5                  ANOP  ,                                           00114000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00114100
.*                                                                      00114200
&ZLL     &ZLOP &ZLV                                                     00114300
.*                                                                      00114400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00114500
                      AGO   .ZL2           CONTINUE LOOPING             00114600
.*                                                                      00114700
.ZLX                  ANOP  ,                                           00114800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00114900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00115000
         L     14,16(0,0)              GET CVT ADDRESS                  00115100
         L     14,772(14,0)            GET SFT ADDRESS                  00115200
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00115300
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00115400
         OR    14,15                   GET PC NUMBER                    00115500
         PC    0(14)                                                    00115600
