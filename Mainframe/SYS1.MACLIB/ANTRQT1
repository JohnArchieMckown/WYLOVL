*                   |   |     ,EXTNBR=xextnbr                         * 00029100
*                   |   |     ,RETINFO=xretinfo                       * 00029200
*                   |   |    [,ASYNCH=xasynch|NO                    * 00029300
*                   |   |        [,ECB=xecb|NO_ECB]]                * 00029400
*                   |    ,REQUEST=SSNAP                              * 00029500
*                   |   |    ,SDEVN=xsdevn                           * 00029600
*                   |   |    ,SVOLSER=xsvolser                       * 00029700
*                   |   |    ,SUCBPTR=xsucbptr                       * 00029800
*                   |   |     ,SEXTINFO=xsextinfo                     * 00029900
*                   |   |     ,SEXTNBR=xsextnbr                       * 00030000
*                   |   |    ,TDEVN=xtdevn                           * 00030100
*                   |   |    ,TVOLSER=xtvolser                       * 00030200
*                   |   |    ,TUCBPTR=xtucbptr                       * 00030300
