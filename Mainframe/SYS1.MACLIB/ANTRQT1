               &PSSID=,&SCCA=,&SSERIAL=,&SSSID=,&PLSS=,&SLSS=,&ASYNCH=,*00530700
               &ECB=,&WAITTIME=,&CRIT=,&MODE=,&MSGREQ=,&PACE=,&LINKS=, *00530800
               &LINK1=,&LINK2=,&LINK3=,&LINK4=,&CGROUP=,&QRYSIZE=,     *00530900
               &QRYINFO=,&ALET=,&BITMAP=,&PATHS=,&OLDVOLSER=,          *00531000
               &NEWVOLSER=,&PRIMARY=,&QUIESCE=,&VOLSER=,&UCBPTR=,      *00531100
               &BASICSIZE=,&BASICINFO=,&EXTNDSIZE=,&EXTNDINFO=,        *00531200
