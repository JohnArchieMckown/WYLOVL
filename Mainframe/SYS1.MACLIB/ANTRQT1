.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              02804500
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         02804600
.*******************************************************************    02804700
.***  PROCESS ANTRQT1 OUTPUT PARAMETERS                                 02804800
.*******************************************************************    02804900
                      ACTR  10000                                       02805000
.*******************************************************************    02805100
.***AO.X02  RETURN CODE PROCESSING                                      02805200
.*******************************************************************    02805300
                      AIF   (N'&RETCODE LE 0).ZMRETX                    02805400
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            02805500
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             02805600
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             02805700
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               02805800
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                02805900
&ZOPN                 SETA  &ZOPN+1                                     02806000
&ZOPL(&ZOPN)          SETC  ' '                                         02806100
&ZOPO(&ZOPN)          SETC  'ST'                                        02806200
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              02806300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        02806400
                      AGO   .ZMRETX                                     02806500
.*                                                                      02806600
.ZMRET2               ANOP  ,                                           02806700
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                02806800
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                02806900
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                02807000
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  02807100
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   02807200
&ZOPN                 SETA  &ZOPN+1                                     02807300
&ZOPL(&ZOPN)          SETC  ' '                                         02807400
&ZOPO(&ZOPN)          SETC  'LR'                                        02807500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 02807600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        02807700
.ZMRETX               ANOP  ,                                           02807800
.*******************************************************************    02807900
.***AO.X03  REASON CODE PROCESSING                                      02808000
.*******************************************************************    02808100
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    02808200
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            02808300
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              02808400
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             02808500
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              02808600
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             02808700
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                02808800
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 02808900
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                02809000
&ZOPN                 SETA  &ZOPN+1                                     02809100
&ZOPL(&ZOPN)          SETC  ' '                                         02809200
&ZOPO(&ZOPN)          SETC  'ST'                                        02809300
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               02809400
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        02809500
                      AGO   .ZMRSNX                                     02809600
.*                                                                      02809700
.ZMRSN2               ANOP  ,                                           02809800
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                02809900
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 02810000
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                02810100
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 02810200
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                02810300
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   02810400
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    02810500
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   02810600
&ZOPN                 SETA  &ZOPN+1                                     02810700
&ZOPL(&ZOPN)          SETC  ' '                                         02810800
&ZOPO(&ZOPN)          SETC  'LR'                                        02810900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  02811000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        02811100
.ZMRSNX               ANOP  ,                                           02811200
.*******************************************************************    02811300
.***AO.X04  LABEL FOR MF=L PROCESSING                                   02811400
.*******************************************************************    02811500
.ZMFLX                ANOP  ,              MF=L PROCESSING              02811600
.*******************************************************************    02811700
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               02811800
.*******************************************************************    02811900
                      ACTR  10000                                       02812000
.*                                                                      02812100
                      LCLA  &ZLC           COLUMN POSITION              02812200
                      LCLA  &ZOPX          LIST TABLE INDEX             02812300
                      LCLC  &ZLL           LABEL                        02812400
                      LCLC  &ZLOP          OPERATION                    02812500
                      LCLC  &ZLV           VALUE                        02812600
                      LCLC  &ZLD           DESCRIPTION                  02812700
.*                                                                      02812800
&ZOPX                 SETA  1                                           02812900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            02813000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              02813100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              02813200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              02813300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              02813400
                      AIF   (K'&ZLD LE 0).ZL5                           02813500
&ZLC                  SETA  35             COMMENT COLUMN               02813600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    02813700
&ZLC                  SETA  33             COMMMENT COLUMN              02813800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   02813900
&ZLV                  SETC  '&ZLV'.' '                                  02814000
                      AGO   .ZL3                                        02814100
.*                                                                      02814200
.ZL4                  ANOP  ,                                           02814300
&ZLV                  SETC  '&ZLV'.' '                                  02814400
.ZL5                  ANOP  ,                                           02814500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               02814600
.*                                                                      02814700
&ZLL     &ZLOP &ZLV                                                     02814800
.*                                                                      02814900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               02815000
                      AGO   .ZL2           CONTINUE LOOPING             02815100
.*                                                                      02815200
.ZLX                  ANOP  ,                                           02815300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                02815400
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                02815500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02815600
         L     14,16(,0)                                                02815700
         L     14,1216(,14)                                             02815800
         L     14,44(,14)                                               02815900
         L     14,8(,14)                                                02816000
         L     14,88(,14)                                               02816100
         ICM   14,15,324(14)                                            02816200
         BNZ   A&SYSNDX                                                 02816300
         L     15,=A(RQST_PC_NUMBER_ZERO)                               02816400
         SLR   0,0                                                      02816500
         B     B&SYSNDX                                                 02816600
A&SYSNDX DS    0H                                                       02816700
         PC    0(14)                                                    02816800
B&SYSNDX DS    0H                                                       02816900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02817000
.ZLINKX               ANOP  ,                                           02817100
.*******************************************************************    02817200
.***AP.X01  FORMAT GENERATED CODE                                       02817300
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            02817400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              02817500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              02817600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              02817700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              02817800
                      AIF   (K'&ZLD LE 0).ZL51                          02817900
&ZLC                  SETA  35             COMMENT COLUMN               02818000
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   02818100
&ZLC                  SETA  33             COMMENT COLUMN               02818200
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  02818300
&ZLV                  SETC  '&ZLV'.' '                                  02818400
                      AGO   .ZL31                                       02818500
.*                                                                      02818600
.ZL41                 ANOP  ,                                           02818700
&ZLV                  SETC  '&ZLV'.' '                                  02818800
.ZL51                 ANOP  ,                                           02818900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               02819000
.*                                                                      02819100
&ZLL     &ZLOP &ZLV                                                     02819200
.*                                                                      02819300
