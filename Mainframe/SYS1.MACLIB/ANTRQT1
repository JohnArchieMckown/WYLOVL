.***AC.PLV01  PROCESS PLISTVER                                      *   00554300
.********************************************************************   00554400
&XPLISTVER      SETA  0                                                 00554500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00554600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00554700
&XPLISTVER      SETA  4                                                 00554800
.ZPLV0          ANOP  ,                                                 00554900
.*                                                                      00555000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00555100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00555200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00555300
.*-------------------------------------------------------------------   00555400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00555500
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00555600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00555700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00555800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00555900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00556000
                AGO   .ZPVCK2                                           00556100
.ZPVCK3         ANOP  ,                                                 00556200
.*-------------------------------------------------------------------   00556300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00556400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00556500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00556600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00556700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00556800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00556900
                AGO   .ZPVCKE                                           00557000
.ZPVCK2         ANOP  ,                                                 00557100
.*-------------------------------------------------------------------   00557200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00557300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00557400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00557500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00557600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00557700
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00557800
.ZPVCK6         ANOP  ,                                                 00557900
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00558000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00558100
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00558200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00558300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00558400
&XPLISTVER      SETA  4                                                 00558500
                AGO   .ZPVCKE                                           00558600
.*-------------------------------------------------------------------   00558700
.ZPVCK4A        ANOP  ,                                                 00558800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00558900
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00559000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00559100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00559200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00559300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00559400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00559500
                AGO   .ZPVCK5                                           00559600
.ZPVCK4         ANOP  ,                                                 00559700
&XPLISTVER      SETA  &PLISTVER(1)                                      00559800
.ZPVCK5         ANOP  ,                                                 00559900
.ZPVCKE         ANOP  ,                                                 00560000
.********************************************************************   00560100
.***AG.MF01 PROCESS MF MACRO FORMS                                      00560200
