.***AL.L12 ANTRQT1 FMT=LIST MF=S|L PARAMETERS                           02593300
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         02593400
.*                                                                      02593500
&ZOPN                 SETA  &ZOPN+1                                     02593600
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                02593700
&ZOPO(&ZOPN)          SETC  'DC'                                        02593800
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   02593900
&ZOPD(&ZOPN)          SETC  '++ ANTRQT1 ARG PARMS'                      02594000
.*                                                                      02594100
.ZLMFSLX              ANOP  ,                                           02594200
.*-------------------------------------------------------------------   02594300
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          02594400
&ZPLLEN               SETA 320                                          02594500
.*                                                                      02594600
.******************************************************************     02594700
.***AI.L01 ANTRQT1 FMT=LIST FILL IN PARAMETER AREA                      02594800
.******************************************************************     02594900
                      ACTR  10000                                       02595000
&ZCVAL                SETC  'LA'           ASSUME XA MODE               02595100
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    02595200
&ZCVAL                SETC  'LAE'          XAX MODE                     02595300
.ZIMFXA               ANOP ,                                            02595400
.*                                                                      02595500
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    02595600
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         02595700
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 02595800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                02595900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 02596000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                02596100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   02596200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    02596300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   02596400
&ZOPN                 SETA  &ZOPN+1                                     02596500
&ZOPL(&ZOPN)          SETC  ' '                                         02596600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    02596700
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              02596800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       02596900
                      AGO   .ZIMFNX                                     02597000
.*                                                                      02597100
.ZIMFN2               ANOP ,                                            02597200
&ZOPN                 SETA  &ZOPN+1                                     02597300
&ZOPL(&ZOPN)          SETC  ' '                                         02597400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    02597500
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                02597600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       02597700
.ZIMFNX               ANOP ,                                            02597800
.********************************************************************   02597900
.***AI.L02 ANTRQT1 FMT=LIST INITIALIZE PARAMETER AREA                   02598000
.********************************************************************   02598100
.***AI.L03 MF=(SLME)                                                    02598200
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      02598300
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            02598400
&ZAWRK                SETA  0                                           02598500
&ZN                   SETA  &ZPLLEN                                     02598600
.ZCLRCONT             AIF   (&ZN LE 256).ZCLRX                          02598700
.*                                                                      02598800
&ZOPN                 SETA  &ZOPN+1                                     02598900
&ZOPL(&ZOPN)          SETC  ' '                                         02599000
&ZOPO(&ZOPN)          SETC  'XC'                                        02599100
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(256,1),&ZAWRK.(1)'                 02599200
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             02599300
.*                                                                      02599400
&ZAWRK                SETA &ZAWRK+256                                   02599500
&ZN                   SETA  &ZN-256                                     02599600
                      AGO   .ZCLRCONT                                   02599700
.*                                                                      02599800
.ZCLRX                ANOP  ,                                           02599900
.*                                                                      02600000
&ZOPN                 SETA  &ZOPN+1                                     02600100
&ZOPL(&ZOPN)          SETC  ' '                                         02600200
&ZOPO(&ZOPN)          SETC  'XC'                                        02600300
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(&ZN,1),&ZAWRK.(1)'                 02600400
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             02600500
.ZIMFLX               ANOP  ,                                           02600600
.*-------------------------------------------------------------------   02600700
.***AI.L60 XEYECATCH CL8                                                02600800
&ZOPN                 SETA  &ZOPN+1                                     02600900
&ZOPL(&ZOPN)          SETC  ' '                                         02601000
