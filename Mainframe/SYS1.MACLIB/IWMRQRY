.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00217600
.*******************************************************************    00217700
                      ACTR  10000                                       00217800
.*                                                                      00217900
                      LCLA  &ZLC           COLUMN POSITION              00218000
                      LCLA  &ZOPX          LIST TABLE INDEX             00218100
                      LCLC  &ZLL           LABEL                        00218200
                      LCLC  &ZLOP          OPERATION                    00218300
                      LCLC  &ZLV           VALUE                        00218400
                      LCLC  &ZLD           DESCRIPTION                  00218500
.*                                                                      00218600
&ZOPX                 SETA  1                                           00218700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00218800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00218900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00219000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00219100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00219200
                      AIF   (K'&ZLD LE 0).ZL5                           00219300
&ZLC                  SETA  35             COMMENT COLUMN               00219400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00219500
&ZLC                  SETA  33             COMMMENT COLUMN              00219600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00219700
&ZLV                  SETC  '&ZLV'.' '                                  00219800
                      AGO   .ZL3                                        00219900
.*                                                                      00220000
.ZL4                  ANOP  ,                                           00220100
&ZLV                  SETC  '&ZLV'.' '                                  00220200
.ZL5                  ANOP  ,                                           00220300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00220400
.*                                                                      00220500
&ZLL     &ZLOP &ZLV                                                     00220600
.*                                                                      00220700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00220800
                      AGO   .ZL2           CONTINUE LOOPING             00220900
.*                                                                      00221000
.ZLX                  ANOP  ,                                           00221100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00221200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00221300
         L     14,16(0,0)              GET CVT ADDRESS                  00221400
.***CHECK FOR VERSION 0 @P3A                                            00221500
                      AIF  (&XPLISTVER EQ 0).VEROK                      00221600
.*-------------------------------------------------------------------   00221700
         TM    CVTOSLV2-CVT(14),X'80'  TEST FOR JBB6604 PRESENCE        00221800
         BNZ   VEROK&SYSNDX            BRANCH IF JBB6604 PRESENCE       00221900
         LA    0,IWMRSNCODEBADVERSION  SET REASON CODE = BAD VERSION    00222000
         LA    15,IWMRETCODEINVOCERROR SET ERROR RETURN CODE            00222100
         B     END&SYSNDX              BRANCH TO END LABEL              00222200
VEROK&SYSNDX DS    0H                  BRANCH LABEL FOR VERSION OK      00222300
.*-------------------------------------------------------------------   00222400
.VEROK                ANOP  ,                                           00222500
.*-------------------------------------------------------------------   00222600
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00222700
         LAE   14,0(14,0)              CLEAR AR 14                      00222800
.NOAR    ANOP                                                           00222900
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00223000
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00223100
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00223200
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00223300
         B     END&SYSNDX              BRANCH TO END LABEL              00223400
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00223500
         L     14,772(14,0)            GET SFT ADDRESS                  00223600
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00223700
         LA    15,15                   GET EX FOR SERVICE ROUTINE       00223800
         OR    14,15                   GET PC NUMBER                    00223900
         PC    0(14)                                                    00224000
END&SYSNDX DS    0H                    END LABEL                        00224100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00224200
.ZLINKX               ANOP  ,                                           00224300
.*******************************************************************    00224400
.***AP.X01  FORMAT GENERATED CODE                                       00224500
