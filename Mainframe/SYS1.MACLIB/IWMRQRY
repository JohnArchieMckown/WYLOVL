&ZOPL(&ZOPN)          SETC  ' '                                         00158500
&ZOPO(&ZOPN)          SETC  'B'                                         00158600
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00158700
&ZOPD(&ZOPN)          SETC  '++ IWMRQRY ARG PARMS'                      00158800
.*                                                                      00158900
&ZOPN                 SETA  &ZOPN+1                                     00159000
&ZOPL(&ZOPN)          SETC  ' '                                         00159100
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00159200
&ZOPV(&ZOPN)          SETC  '1'                                         00159300
&ZOPD(&ZOPN)          SETC  ' '                                         00159400
.*                                                                      00159500
.ZLMFSL2              ANOP  ,                                           00159600
.*-------------------------------------------------------------------   00159700
