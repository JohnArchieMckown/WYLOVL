    %IF GTO_BASE='' %THEN                                               14700000
      %GTO_BASE='BASED';                                                14750000
    DCL                                                                 14800000
      1 GTO GTO_BASE,               /* The GTF trace options.        */ 14850000
       2 GTOBYTE0 BIT(8),                                               14900000
        3 GTOSYSM BIT(1),           /* SYSM - records minimal data      14950000
                                       for system events.               15000000
                                       ------------------------------   15050000
                                       Records events as if each of     15100000
                                       the following options is         15150000
                                       specified, except that minimal   15200000
                                       trace data is recorded: EXT,     15250000
                                       PI, SVC and RR.                  15300000
                                       ------------------------------   15350000
                                       Records events as if each of     15400000
                                       the following options is         15450000
                                       specified: IO, SSCH, CSCH,       15500000
                                       HSCH and MSCH.                   15550000
                                       ------------------------------   15600000
                                       If any of the following          15650000
                                       options are specified, causes    15700000
                                       minimal recording to be done     15750000
                                       for them: DSP, RNIO and SRM.     15800000
                                       ------------------------------   15850000
                                       The following options are        15900000
                                       ignored: EXT, PI, SVC, RR, IO,   15950000
                                       SSCH, CSCH, HSCH and MSCH.    */ 16000000
        3 GTOSYSP BIT(1),           /* SYSP - records comprehensive     16050000
                                       trace data for system events,    16100000
                                       with prompting.                  16150000
                                       ------------------------------   16200000
                                       GTF prompts the operator for     16250000
                                       the only PI, SVC, IO and SSCH    16300000
                                       events that will have data       16350000
                                       recorded.                        16400000
                                       ------------------------------   16450000
                                       Otherwise, GTF records the       16500000
                                       same events as those recorded    16550000
                                       by SYS.                          16600000
                                       ------------------------------   16650000
                                       GTF ignores the following        16700000
                                       options: EXT, PI, SVC, RR, IO,   16750000
                                       SSCH, CSCH, HSCH and MSCH.    */ 16800000
 @EJECT;                                                                16850000
        3 GTOSYS BIT(1),            /* SYS - records comprehensive      16900000
                                       trace data for system events.    16950000
                                       ------------------------------   17000000
                                       Records events as if each of     17050000
                                       the following options is         17100000
                                       specified: EXT, PI, SVC, RR,     17150000
                                       IO, SSCH, CSCH, HSCH and MSCH.   17200000
                                       ------------------------------   17250000
                                       The following options are        17300000
                                       ignored: EXT, PI, SVC, RR, IO,   17350000
                                       SSCH, CSCH, HSCH and MSCH.    */ 17400000
        3 GTOUSR BIT(1),            /* USR - records all user trace     17450000
                                       data passed to GTF by the        17500000
                                       GTRACE macro.                 */ 17550000
        3 GTOTRC BIT(1),            /* TRC - records events             17600000
                                       associated with GTF itself.      17650000
                                       This causes tracing only when    17700000
                                       at least one other type of       17750000
                                       event is being traced.        */ 17800000
        3 GTODSP BIT(1),            /* DSP - records dispatcher         17850000
                                       events. If SYSM is also          17900000
                                       specified, GTF records minimal   17950000
                                       trace data. Otherwise, GTF       18000000
                                       records comprehensive trace      18050000
                                       data.                         */ 18100000
        3 * BIT(1),                 /* Reserved.                     */ 18150000
        3 GTOPCI BIT(1),            /* PCI - records I/O                18200000
                                       interruptions that indicate      18250000
                                       intermediate status. This        18300000
                                       includes program-controlled      18350000
                                       interruptions (PCIs), initial    18400000
                                       status request interruptions,    18450000
                                       and resume and suspend channel   18500000
                                       program interruptions.           18550000
                                       ------------------------------   18600000
                                       When IOP specifies particular    18650000
                                       devices, PCI applies only to     18700000
                                       them.                            18750000
                                       ------------------------------   18800000
                                       PCI causes tracing only when     18850000
                                       one of the following is also     18900000
                                       specified: IO, IOP, SYS, SYSM,   18950000
                                       SYSP.                         */ 19000000
 @EJECT;                                                                19050000
       2 GTOBYTE1 BIT(8),                                               19100000
        3 GTOSVC BIT(1),            /* SVC - records supervisor call    19150000
                                       interruptions. If SYSM is also   19200000
                                       specified, GTF records minimal   19250000
                                       trace data. Otherwise, GTF       19300000
                                       records comprehensive trace      19350000
                                       data.                         */ 19400000
        3 GTOSVCP BIT(1),           /* SVCP - like SVC, but GTF         19450000
                                       prompts the operator for the     19500000
                                       only SVC numbers that will       19550000
                                       have data recorded.           */ 19600000
        3 GTOSIO BIT(1),            /* SIO - records start I/O          19650000
                                       operations - equivalent to       19700000
                                       SSCH.                         */ 19750000
        3 GTOSIOP BIT(1),           /* SIOP - SIO with prompting -      19800000
                                       equivalent to SSCHP.          */ 19850000
        3 GTOPI BIT(1),             /* PI - records program             19900000
                                       interruptions. If SYSM is also   19950000
                                       specified, GTF records minimal   20000000
                                       trace data. Otherwise, GTF       20050000
                                       records comprehensive trace      20100000
                                       data.                         */ 20150000
        3 GTOPIP BIT(1),            /* PIP - like PI, but GTF prompts   20200000
                                       the operator for the only        20250000
                                       program interruption codes       20300000
                                       that will have data recorded. */ 20350000
        3 GTOIO BIT(1),             /* IO - records I/O                 20400000
                                       interruptions. GTF does not      20450000
                                       record intermediate-status       20500000
                                       interruptions unless PCI is      20550000
                                       also specified.               */ 20600000
        3 GTOIOP BIT(1),            /* IOP - like IO, but GTF prompts   20650000
                                       the operator for the only        20700000
                                       devices that will have data      20750000
                                       recorded.                     */ 20800000
 @EJECT;                                                                20850000
       2 GTOBYTE2 BIT(8),                                               20900000
        3 GTOEXT BIT(1),            /* EXT- records external            20950000
                                       interruptions. If SYSM is also   21000000
                                       specified, GTF records minimal   21050000
                                       trace data. Otherwise, GTF       21100000
                                       records comprehensive trace      21150000
                                       data.                         */ 21200000
        3 GTORNIO BIT(1),           /* RNIO - records VTAM (remote      21250000
                                       network) activity. If SYSM is    21300000
                                       also specified, GTF records      21350000
                                       minimal trace data. Otherwise,   21400000
                                       GTF records comprehensive        21450000
                                       trace data. This causes          21500000
                                       tracing only when VTAM tracing   21550000
                                       is active.                    */ 21600000
        3 GTOSRM BIT(1),            /* SRM - records calls to the       21650000
                                       system resource manager. If      21700000
                                       SYSM is also specified, GTF      21750000
                                       records minimal trace data.      21800000
                                       Otherwise, GTF records           21850000
                                       comprehensive trace data.     */ 21900000
        3 GTORR BIT(1),             /* RR - records data associated     21950000
                                       with recovery routines, such     22000000
                                       as ESTAEs and STAEs. If SYSM     22050000
