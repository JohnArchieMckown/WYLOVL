EIXALOW2 DS    X                    /* ALLOWANCE OPTIONS BYTE 3      */ 05450000
EIXALOW3 DS    X                    /* ALLOWANCE OPTIONS BYTE 4      */ 05500000
EIXERR   EQU   X'80'                /* DIS-ALLOWED OPTION    @V23N298*/ 05550000
EIOPTION DS    H                    /* PROCESSING OPTION             */ 05600000
EIRETCOD DS    H                    /* EXIT RETURN CODE              */ 05650000
EIRECALN DS    FL4                  /* RECORD AREA LENGTH    @LA02135*/ 05700000
EIRECLEN DS    FL4                  /* ORIGINAL RECORD LEN   @LA02135*/ 05750000
EIRECPTR DS    FL4                  /* ORIGINAL RECORD ADDR  @LA02135*/ 05800000
EIUSEPTR DS    FL4                  /* USER DATA AREA PTR    @V23N253*/ 05850000
EIDDID   DS    0FL4                 /* EIOP06 DDN/VOLID PTR  @OW36779*/ 05883300
EIXMWPL  DS    FL4                  /* EIOP11 WTO PLIST PTR  @OW36779*/ 05916600
EIXFLAGS DS    0XL4                 /* OTHER FLAGS           @LA09760*/ 05950000
EIXFLAG0 DS    X                    /* FLAG BYTE 0           @LA09760*/ 06000000
EIXABEND EQU   X'80'                /* ABEND MESSAGE FLAG    @LA09760*/ 06050000
EIXNTERR EQU   X'40'                /* NON-TAPE ERR MSG FLAG @LA09760*/ 06100000
EIXWNGOK EQU   X'20'                /* WARNINGS ARE OK  FLAG @LA54862*/ 06150000
EIXTRKER EQU   X'10'                /* track is in error     @OW07425*/ 06175000
EIXFLAG1 DS    X                    /* FLAG BYTE 1           @LA09760*/ 06200000
