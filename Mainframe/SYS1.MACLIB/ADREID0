ADREIBID DC    C'EIDB'              /* BLOCK IDENTIFIER              */ 02650000
ADREIB@  DS    F'0'                                                     02700000
EIOP00   DC    X'0000'              /* FUNCTION STARTUP ENTRY        */ 02750000
EIOP01   DC    X'0001'              /* READING SYSIN RECORD          */ 02800000
EIOP02   DC    X'0002'              /* PRINTING SYSPRINT RECORD      */ 02850000
EIOP03   DC    X'0003'              /* READING TAPE BLOCK RECORD     */ 02900000
EIOP04   DC    X'0004'              /* READING TAPE LOGICAL RECORD   */ 02950000
EIOP05   DC    X'0005'              /* WRITING TAPE LOGICAL RECORD   */ 03000000
EIOP06   DC    X'0006'              /* WRITING TAPE BLOCK RECORD     */ 03050000
EIOP07   DC    X'0007'              /* READING DISK TRACK            */ 03100000
EIOP08   DC    X'0008'              /* WRITING DISK TRACK            */ 03150000
EIOP09   DC    X'0009'              /* READING UTILITY SYSPRINT      */ 03200000
EIOP10   DC    X'000A'              /* WRITING UTILITY SYSPRINT      */ 03250000
EIOP11   DC    X'000B'              /* WRITING WTO MESSAGE           */ 03300000
EIOP12   DC    X'000C'              /* WRITING WTOR MESSAGE          */ 03350000
EIOP13   DC    X'000D'              /* PRESENTING ADRUFO RECORD      */ 03400000
EIOP14   DC    X'000E'              /* FUNCTION TERMINATION ENTRY    */ 03450000
EIOP15   DC    X'000F'              /* PRESENTING WTOR RESPONSE      */ 03500000
EIOP16   DC    X'0010'              /* TAPE VOL SEC/VERIFY       @V22*/ 03550000
EIOP17   DC    X'0011'              /* TAPE VOL MOUNT(NON-SPEC)  @V22*/ 03600000
EIOP18   DC    X'0012'              /* INSERT RESTORE RECORD     @V22*/ 03650000
EIOP19   DC    X'0013'              /* OUTPUT TAPE ERROR         @V22*/ 03700000
EIOP20   DC    X'0014'              /* VOLUME NOTIFICATION       @V24*/ 03750000
EIOP21   DC    X'0015'              /* DATA SET VERIFICATION     @V25*/ 03800000
EIOP22   DC    X'0016'              /* BYPASS VERIFICATION       @V25*/ 03850000
EIOP23   DC    X'0017'              /* DS PROCESSED NOTIFICATION @V25*/ 03900000
VOLSLST  DC    X'02C4'              /* MAX SIZE OF VOLSER LIST          03950000
*                                      FOR EIREC23 (118 * 6) @NA08383*/ 04000000
EIOP24   DC    X'0018'          /* CONCURRENT COPY INIT COMPLETE @V26*/ 04050000
