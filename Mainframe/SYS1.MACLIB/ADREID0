EIXDIS   EQU   X'08'                /* ALLOW DISCONNECT OF EXIT      */ 05150000
EIXWTOR  EQU   X'04'                /* ALLOW WTOR RESPONSE           */ 05200000
EIXSTAT  EQU   X'02'                /* ALLOW SELECTION OF USER STATS */ 05250000
EIXTERM  EQU   X'01'                /* ALLOW TERMINATION OF FUNC @V22*/ 05300000
EIXALOW1 DS    X                    /* ALLOWANCE OPTIONS BYTE 2      */ 05350000
