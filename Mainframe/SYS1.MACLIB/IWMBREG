.*----------------------------------------------------------------***   00121600
.***AH.P02 EXTRA POSITIONALS                                            00121700
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00121800
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00121900
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00122000
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00122100
.ZGLISTX1     ANOP  ,                                                   00122200
&ZCVAL        SETC  ''          ERROR FIELD                             00122300
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00122400
.ZGLIST2      ANOP  ,                                                   00122500
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00122600
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00122700
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00122800
              AGO   .ZGLIST2                                            00122900
.*                                                                      00123000
.ZGLIST3      ANOP  ,                                                   00123100
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00123200
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00123300
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00123400
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00123500
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00123600
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00123700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00123800
.ZGLISTX      ANOP  ,                                                   00123900
