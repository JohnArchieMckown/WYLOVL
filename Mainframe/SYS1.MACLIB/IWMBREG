.***AO.X02  RETURN CODE PROCESSING                                      00157300
.*******************************************************************    00157400
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00157500
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00157600
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00157700
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00157800
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00157900
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00158000
&ZOPN                 SETA  &ZOPN+1                                     00158100
&ZOPL(&ZOPN)          SETC  ' '                                         00158200
&ZOPO(&ZOPN)          SETC  'ST'                                        00158300
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00158400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00158500
                      AGO   .ZMRETX                                     00158600
.*                                                                      00158700
.ZMRET2               ANOP  ,                                           00158800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00158900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00159000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00159100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00159200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00159300
&ZOPN                 SETA  &ZOPN+1                                     00159400
&ZOPL(&ZOPN)          SETC  ' '                                         00159500
&ZOPO(&ZOPN)          SETC  'LR'                                        00159600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00159700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00159800
.ZMRETX               ANOP  ,                                           00159900
.*******************************************************************    00160000
.***AO.X03  REASON CODE PROCESSING                                      00160100
.*******************************************************************    00160200
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00160300
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00160400
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00160500
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00160600
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00160700
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00160800
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00160900
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00161000
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00161100
&ZOPN                 SETA  &ZOPN+1                                     00161200
&ZOPL(&ZOPN)          SETC  ' '                                         00161300
&ZOPO(&ZOPN)          SETC  'ST'                                        00161400
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00161500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00161600
                      AGO   .ZMRSNX                                     00161700
.*                                                                      00161800
.ZMRSN2               ANOP  ,                                           00161900
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00162000
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00162100
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00162200
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00162300
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00162400
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00162500
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00162600
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00162700
&ZOPN                 SETA  &ZOPN+1                                     00162800
&ZOPL(&ZOPN)          SETC  ' '                                         00162900
&ZOPO(&ZOPN)          SETC  'LR'                                        00163000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00163100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00163200
.ZMRSNX               ANOP  ,                                           00163300
