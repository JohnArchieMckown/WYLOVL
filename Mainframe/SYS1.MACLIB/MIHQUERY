.***AB.02  GLOBAL VARIABLES TO CONTROL PRINT AND CODE GENERATION        00033600
.*******************************************************************    00033700
              GBLC  &ZPRINT     YES=PRINT NO=NO PRINT                   00033800
              GBLC  &ZTEST      YES=TEST NO=NO TEST                     00033900
              GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX             00034000
.*                                                                      00034100
              GBLC  &ZMIHQUERY  YES=MACRO USED AT LEAST ONCE            00034200
.*******************************************************************    00034300
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00034400
.*******************************************************************    00034500
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00034600
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00034700
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00034800
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00034900
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00035000
              LCLA  &ZAWRK      USED AS A WORK AREA                     00035100
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00035200
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00035300
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00035400
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00035500
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00035600
              LCLA  &ZMACRET    MACRO RETURN CODE                       00035700
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00035800
.*                                                                      00035900
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00036000
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00036100
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00036200
              LCLC  &ZENDBASEPL                 End of base PL          00036300
.********************************************************************   00036400
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00036500
.********************************************************************   00036600
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00036700
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00036800
              GBLA  &SYSALVL    SYSSTATE ARCHLVL                        00036900
.*                                                                      00037000
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00037100
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00037200
              SPLEVEL  TEST                                             00037300
.ZSYS2        ANOP  ,                                                   00037400
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00037500
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00037600
              SYSSTATE TEST                                             00037700
              AGO   .ZSYS4                                              00037800
.ZSYS3        ANOP  ,                                                   00037900
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00038000
.ZSYS4        ANOP  ,                                                   00038100
.*******************************************************************    00038200
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00038300
.*******************************************************************    00038400
              LCLC  &XUCBPTR    ++ NAME                                 00038500
              LCLC  &XTIMEINT   ++ NAME                                 00038600
              LCLC  &XRETCODE   ++ NAME                                 00038700
              LCLC  &XRSNCODE   ++ NAME                                 00038800
              LCLC  &XMF        ++ VALUE                                00038900
              LCLC  &XMFCTRL    ++ NAME                                 00039000
              LCLC  &XMFATTR    ++ VALUE                                00039100
              LCLC  &XMFSCK     ++ VALUE                                00039200
.*******************************************************************    00039300
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00039400
.*******************************************************************    00039500
              LCLC  &ZXUCBPTR   ++ INPUT NAME                           00039600
              LCLC  &ZXTIMEINT  ++ OUTPUT NAME                          00039700
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00039800
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00039900
              LCLC  &ZXMF       ++ INPUT VALUE                          00040000
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00040100
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00040200
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00040300
.********************************************************************   00040400
.***AG.MF01 PROCESS MF MACRO FORMS                                      00040500
.********************************************************************   00040600
              ACTR  10000                                               00040700
.*------------------------------------------------------------------    00040800
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00040900
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00041000
.*-------------------------------------------------------------------   00041100
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00041200
.ZG15         ANOP  ,                                                   00041300
&ZTLC         SETC 'S'                                                  00041400
              AIF   ('&MF(1)' NE '&ZTLC').ZG16                          00041500
&XMF          SETC  'S'         SET KEYWORD                             00041600
              AIF   (N'&MF LE 1).ZG15A          CK NUM                  00041700
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00041800
&ZMS          SETC  '&ZMS.&MF"'                                         00041900
&ZMS          SETC  '&ZMS "MF"'                                         00042000
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00042100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00042200
.ZG15A        ANOP  ,                                                   00042300
.*-------------------------------------------------------------------   00042400
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00042500
.ZG16         ANOP  ,                                                   00042600
&ZTLC         SETC 'L'                                                  00042700
              AIF   ('&MF(1)' NE '&ZTLC').ZG23                          00042800
&XMF          SETC  'L'         SET KEYWORD                             00042900
              AIF   (N'&MF LE 3).ZG16A          CK NUM                  00043000
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00043100
&ZMS          SETC  '&ZMS.&MF"'                                         00043200
&ZMS          SETC  '&ZMS "MF"'                                         00043300
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00043400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00043500
.ZG16A        ANOP  ,                                                   00043600
.*-------------------------------------------------------------------   00043700
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00043800
              AIF   (N'&SYSLIST NE 0).ZG16B     POSITIONALS?            00043900
              AIF   ('&UCBPTR' NE '').ZG16B     KEYS?                   00044000
              AIF   ('&TIMEINT' NE '').ZG16B    KEYS?                   00044100
              AIF   ('&RETCODE' NE '').ZG16B    KEYS?                   00044200
              AIF   ('&RSNCODE' NE '').ZG16B    KEYS?                   00044300
              AGO    .ZG16C     INVOCATION OKAY                         00044400
.ZG16B        ANOP  ,                                                   00044500
&ZMS          SETC  'THE "L" FORM OF "MF" CANNOT'  *MFLA* MNOTE         00044600
&ZMS          SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                   00044700
&ZMS          SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'              00044800
              MNOTE 8,' &ZMS ON THE INVOCATION.'                        00044900
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00045000
.ZG16C        ANOP  ,                                                   00045100
.*-------------------------------------------------------------------   00045200
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00045300
&ZXMFCTRL     SETC  'INPUT'     SET IN/OUTPUT                           00045400
&XMFCTRL      SETC  '&MF(2,1)'  SET XVARIABLE                           00045500
              AIF   (N'&MF(2) GT 1).ZG18B       MULTI PARMS             00045600
&XMFCTRL      SETC  '&MF(2)'    SET XVARIABLE                           00045700
.ZG18B        AIF   (K'&MF(2) GT 0).ZG18C                               00045800
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00045900
&ZMS          SETC  'WHEN "L" IS SPECIFIED'     *KRQDA*                 00046000
&ZMS          SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'                 00046100
              MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                        00046200
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00046300
.ZG18C        ANOP  ,                                                   00046400
.*-------------------------------------------------------------------   00046500
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00046600
              AIF   (K'&ZXMFCTRL LE 0).ZG18P    NOT I/O                 00046700
              AIF   ('&XMFCTRL'(1,1) NE '(').ZG18P                      00046800
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00046900
&ZMS          SETC  '"MF='      *KGPRA* MNOTE                           00047000
&ZMS          SETC  '&ZMS.&MF"'                                         00047100
