              GBLC  &ZTEST      YES=TEST NO=NO TEST                     00045400
              GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX             00045500
.*                                                                      00045600
              GBLC  &ZIXCCREAT  YES=MACRO USED AT LEAST ONCE            00045700
.*******************************************************************    00045800
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00045900
.*******************************************************************    00046000
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00046100
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00046200
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00046300
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00046400
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00046500
              LCLA  &ZAWRK      USED AS A WORK AREA                     00046600
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00046700
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00046800
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00046900
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00047000
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00047100
              LCLA  &ZMACRET    MACRO RETURN CODE                       00047200
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00047300
.*                                                                      00047400
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00047500
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00047600
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00047700
              LCLC  &ZENDBASEPL                 End of base PL          00047800
.********************************************************************   00047900
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00048000
.********************************************************************   00048100
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00048200
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00048300
.*                                                                      00048400
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00048500
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00048600
              SPLEVEL  TEST                                             00048700
.ZSYS2        ANOP  ,                                                   00048800
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00048900
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00049000
              SYSSTATE TEST                                             00049100
              AGO   .ZSYS4                                              00049200
.ZSYS3        ANOP  ,                                                   00049300
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00049400
.ZSYS4        ANOP  ,                                                   00049500
.*******************************************************************    00049600
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00049700
.*******************************************************************    00049800
              LCLC  &XGRPNAME   ++ NAME                                 00049900
              LCLC  &XANSAREA   ++ NAME                                 00050000
              LCLC  &XANSLEN    ++ NAME                                 00050100
              LCLC  &XMEMNAME   ++ NAME                                 00050200
              LCLC  &XUSTATE    ++ NAME                                 00050300
              LCLC  &XUSLEN     ++ NAME                                 00050400
              LCLC  &XRETCODE   ++ NAME                                 00050500
              LCLC  &XRSNCODE   ++ NAME                                 00050600
              LCLC  &XMF        ++ VALUE                                00050700
              LCLC  &XMFCTRL    ++ NAME                                 00050800
              LCLC  &XMFATTR    ++ VALUE                                00050900
              LCLC  &XMFSCK     ++ VALUE                                00051000
.*******************************************************************    00051100
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00051200
.*******************************************************************    00051300
              LCLC  &ZXGRPNAME  ++ INPUT NAME                           00051400
              LCLC  &ZXANSAREA  ++ OUTPUT NAME                          00051500
              LCLC  &ZXANSLEN   ++ INPUT NAME                           00051600
              LCLC  &ZXMEMNAME  ++ INPUT NAME                           00051700
              LCLC  &ZXUSTATE   ++ INPUT NAME                           00051800
              LCLC  &ZXUSLEN    ++ INPUT NAME                           00051900
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00052000
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00052100
              LCLC  &ZXMF       ++ INPUT VALUE                          00052200
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00052300
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00052400
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00052500
.********************************************************************   00052600
