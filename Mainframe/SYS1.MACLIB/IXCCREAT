.*******************************************************************    00160600
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00160700
.*******************************************************************    00160800
                      ACTR  10000                                       00160900
.*                                                                      00161000
                      LCLA  &ZLC           COLUMN POSITION              00161100
                      LCLA  &ZOPX          LIST TABLE INDEX             00161200
                      LCLC  &ZLL           LABEL                        00161300
                      LCLC  &ZLOP          OPERATION                    00161400
                      LCLC  &ZLV           VALUE                        00161500
                      LCLC  &ZLD           DESCRIPTION                  00161600
.*                                                                      00161700
&ZOPX                 SETA  1                                           00161800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00161900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00162000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00162100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00162200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00162300
                      AIF   (K'&ZLD LE 0).ZL5                           00162400
&ZLC                  SETA  35             COMMENT COLUMN               00162500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00162600
&ZLC                  SETA  33             COMMMENT COLUMN              00162700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00162800
&ZLV                  SETC  '&ZLV'.' '                                  00162900
                      AGO   .ZL3                                        00163000
.*                                                                      00163100
.ZL4                  ANOP  ,                                           00163200
&ZLV                  SETC  '&ZLV'.' '                                  00163300
.ZL5                  ANOP  ,                                           00163400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00163500
.*                                                                      00163600
&ZLL     &ZLOP &ZLV                                                     00163700
.*                                                                      00163800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00163900
                      AGO   .ZL2           CONTINUE LOOPING             00164000
.*                                                                      00164100
.ZLX                  ANOP  ,                                           00164200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00164300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00164400
         L     14,16(0,0)              GET CVT ADDRESS                  00164500
         L     14,772(14,0)            GET SFT ADDRESS                  00164600
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00164700
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00164800
         OR    14,15                   GET PC NUMBER                    00164900
         PC    0(14)                                                    00165000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00165100
.ZLINKX               ANOP  ,                                           00165200
.*******************************************************************    00165300
.***AP.X01  FORMAT GENERATED CODE                                       00165400
