    DECLARE 1 ACEE ACEEBASE IHAALET,/* ACCESSOR ENVIRONMENT ELEMENT     29950000
                                                                @M3C */ 30000000
              2 ACEEACEE CHAR(4),   /* ACRONYM IN EBCDIC -ACEE-      */ 30050000
              2 ACEECORE PTR(31),   /* ACEE SUBPOOL AND LENGTH       */ 30100000
                3 ACEESP   PTR(8),  /* ACEE SUBPOOL NUMBER           */ 30150000
                3 ACEELEN  PTR(24), /* LENGTH OF ACEE                */ 30200000
              2 ACEEVRSN   FIXED(8),/* VERSION = 1.              @L4A*/ 30250000
              2 *          CHAR(3), /* RESERVED                      */ 30300000
              2 ACEEIEP    PTR(31), /* RESERVED FOR INSTALLATION.       30350000
