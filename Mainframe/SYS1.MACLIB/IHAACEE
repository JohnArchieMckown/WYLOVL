*%ACEEPLS:;                         /* PLS ACEE STRUCTURE            */ 29600000
*%IF IHAVIA = 'YES' %THEN                                    /* @M3A */ 29650000
*    %IHAALET = 'VIA(ACEE_ALET)';                            /* @M3A */ 29700000
