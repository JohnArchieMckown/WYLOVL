         LTR   1,1                  TEST IF ENTITY SPECIFIED       @M2A 22330000
         BZ    *+10                                                @M2A 22330500
         MVC   24(39,15),0(1)       MOVE ENTITY NAME               @M2A 22331000
         LR    1,15                 POINT TO WORKAREA              @M2A 22331500
         LA    15,32                SET DIAGNOSE SUBFUNCTION CODE  @M2A 22332000
         LA    0,64                 SET BUFFER LENGTH              @M2A 22332500
         DC    XL1'83',XL1'1F',XL2'00A0' ISSUE DIAGNOSE X'A0'      @M2A 22333000
.*                                                                 @M2A 22333500
.* SAVE THE RETURN CODE IN THE RACROUTE PARAMETER LIST             @M2A 22334000
.*                                                                 @M2A 22334500
         LA    0,84                                                @M2A 22335000
         CLR   0,15                                                @M2A 22335500
         BNE   *+12                                                @M2A 22336000
         LA    15,642                                              @D4C 22336500
         B     IHB&NX.G                                            @M2A 22337000
         LA    0,255                CHECK FOR SEVER                @M2A 22337500
         CLR   0,15                                                @M2A 22338000
         BNE   *+8                                                 @M2A 22338500
         LA    15,12                SEVER RETURN CODE              @R3C 22339300
         B     IHB&NX.G                                           2@R3D 22340100
IHB&NX.G DS    0F                                                  @M2A 22341000
         ST    15,0(14)             SAVE RACF RETURN CODE          @M2A 22341500
         XR    0,0                                                 @M2A 22342000
         XC    4(4,14),4(14)                                       @M2A 22342500
         AGO   .EROUTE                                             @M2A 22343000
.RTE0420 ANOP                                                      @M2A 22343500
         LR    1,14                 RESTORE ADDRESSABILITY TO PARM LIST 22350000
         L     15,16(0,0)           ADDRESS OF CVT                      22400000
         ICM   15,B'1111',248(15)   ADDRESS OF SAF VECTOR TABLE         22450000
         BNZ   IHB&NX.D                                                 22500000
         LA    15,4                 SAF ROUTER NOT AVAILABLE IN SYSTEM  22550000
         B     IHB&NX.E                                                 22600000
IHB&NX.D DS    0H                                                       22650000
         ICM   15,B'1111',12(15)    PICK UP ADDRESS OF SAF ROUTER  @R4C 22666600
         BNZ   IHB&NX.H                                            @R4A 22683200
         LA    15,4                 SAF ROUTER NOT AVAILABLE       @R4A 22699800
         B     IHB&NX.E                                                 22716400
IHB&NX.H DS    0H                                                  @R4A 22733000
         BALR  14,15                BRANCH TO SAF ROUTER                22750000
