         AIF   ('&MF(1)' EQ 'M').EROUTE                            @M3A 22319000
         AIF   (NOT &ICHLFS).RTE0420                              4@R2D 22320500
         L     15,24(0,14)          WORKAREA  ADDRS                @M2A 22322000
         XC    0(64,15),0(15)       CLEAR WORKAREA                 @M2A 22322500
         MVI   3(15),64             STORE WORK AREA LENGTH         @M2A 22323000
         MVC   4(1,15),8(1)         MOVE ATTR                      @M2A 22323500
         LR    0,1                                                 @M2A 22324000
         L     1,40(1)              POINT TO CLASS                 @M2A 22324500
         LTR   1,1                  CLASS SPECIFIED                @M2A 22325000
         BZ    *+10                                                @M2A 22325500
         MVC   7(9,15),0(1)         MOVE CLASS TO WORKAREA         @M2A 22326000
         LR    1,0                                                 @M2A 22326500
         L     1,60(1)              POINT TO USERID                @M2A 22327000
         LTR   1,1                  TEST IF USERID SPECIFIED       @M2A 22327500
         BZ    *+10                                                @M2A 22328000
