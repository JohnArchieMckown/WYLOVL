.***AC.PLV01  PROCESS PLISTVER                                      *   00058200
.********************************************************************   00058300
&XPLISTVER      SETA  0                                                 00058400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00058500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00058600
&XPLISTVER      SETA  0                                                 00058700
.ZPLV0          ANOP  ,                                                 00058800
.*                                                                      00058900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00059000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00059100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00059200
.*-------------------------------------------------------------------   00059300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00059400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00059500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00059600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00059700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00059800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059900
                AGO   .ZPVCK2                                           00060000
.ZPVCK3         ANOP  ,                                                 00060100
.*-------------------------------------------------------------------   00060200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00060300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00060400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00060500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00060600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00060700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00060800
                AGO   .ZPVCKE                                           00060900
.ZPVCK2         ANOP  ,                                                 00061000
.*-------------------------------------------------------------------   00061100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00061200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00061300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00061400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00061500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00061600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00061700
.ZPVCK6         ANOP  ,                                                 00061800
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00061900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00062000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00062100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00062200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062300
&XPLISTVER      SETA  0                                                 00062400
                AGO   .ZPVCKE                                           00062500
.*-------------------------------------------------------------------   00062600
.ZPVCK4A        ANOP  ,                                                 00062700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00062800
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00062900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00063000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00063100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00063200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00063300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063400
                AGO   .ZPVCK5                                           00063500
.ZPVCK4         ANOP  ,                                                 00063600
&XPLISTVER      SETA  &PLISTVER(1)                                      00063700
.ZPVCK5         ANOP  ,                                                 00063800
.ZPVCKE         ANOP  ,                                                 00063900
.********************************************************************   00064000
