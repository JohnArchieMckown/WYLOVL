.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00142900
.*******************************************************************    00143000
                      ACTR  10000                                       00143100
.*                                                                      00143200
                      LCLA  &ZLC           COLUMN POSITION              00143300
                      LCLA  &ZOPX          LIST TABLE INDEX             00143400
                      LCLC  &ZLL           LABEL                        00143500
                      LCLC  &ZLOP          OPERATION                    00143600
                      LCLC  &ZLV           VALUE                        00143700
                      LCLC  &ZLD           DESCRIPTION                  00143800
.*                                                                      00143900
&ZOPX                 SETA  1                                           00144000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00144100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00144200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00144300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00144400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00144500
                      AIF   (K'&ZLD LE 0).ZL5                           00144600
&ZLC                  SETA  35             COMMENT COLUMN               00144700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00144800
&ZLC                  SETA  33             COMMMENT COLUMN              00144900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00145000
&ZLV                  SETC  '&ZLV'.' '                                  00145100
                      AGO   .ZL3                                        00145200
.*                                                                      00145300
.ZL4                  ANOP  ,                                           00145400
&ZLV                  SETC  '&ZLV'.' '                                  00145500
.ZL5                  ANOP  ,                                           00145600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00145700
.*                                                                      00145800
&ZLL     &ZLOP &ZLV                                                     00145900
.*                                                                      00146000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00146100
                      AGO   .ZL2           CONTINUE LOOPING             00146200
.*                                                                      00146300
.ZLX                  ANOP  ,                                           00146400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00146500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00146600
         L     14,16(0,0)              GET CVT ADDRESS                  00146700
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00146800
         LAE   14,0(14,0)              CLEAR AR 14                      00146900
.NOAR    ANOP                                                           00147000
         L     14,772(14,0)            GET SFT ADDRESS                  00147100
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00147200
         LA    15,26                   GET EX FOR SERVICE ROUTINE       00147300
         OR    14,15                   GET PC NUMBER                    00147400
         PC    0(14)                                                    00147500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00147600
