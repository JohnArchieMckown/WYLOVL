.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00118200
&ZPLLEN               SETA 32                                           00118300
.*                                                                      00118400
.******************************************************************     00118500
.***AI.L01 IWMECQRY FMT=LIST FILL IN PARAMETER AREA                     00118600
.******************************************************************     00118700
                      ACTR  10000                                       00118800
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00118900
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00119000
&ZCVAL                SETC  'LAE'          XAX MODE                     00119100
.ZIMFXA               ANOP ,                                            00119200
.*                                                                      00119300
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00119400
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00119500
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00119600
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00119700
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00119800
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00119900
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00120000
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00120100
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00120200
&ZOPN                 SETA  &ZOPN+1                                     00120300
