 %MNTEPLS : ;                                                           07000000
 %DCL MNTVIA CHAR EXTERNAL;                                             07050000
 %DCL MNTEV2 CHAR EXTERNAL;                                             07075000
 %IF MNTEBASE='' %THEN                                                  07100000
   %DO;                                                                 07150000
     %MNTEBASE='BASED(MntEPtr) BDY(Word)';                              07200000
   %END;                                                                07250000
  %IF MNTEBASE='BASED(MntEPtr) BDY(Word)' %THEN                         07300000
    %DO;                                                                07350000
      DCL MntEPtr Ptr(31);         /* Pointer to the MNTE            */ 07400000
    %END;                                                               07450000
 %IF MNTEHBASE='' %THEN                                                 07500000
   %DO;                                                                 07550000
     %MNTEHBASE='BASED(MntEHPtr) BDY(Word)';                            07600000
   %END;                                                                07650000
  %IF MNTEHBASE='BASED(MntEHPtr) BDY(Word)' %THEN                       07700000
    %DO;                                                                07750000
      DCL MntEHPtr Ptr(31);       /* Pointer to the MNTE header     */  07800000
    %END;                                                               07850000
 %IF MNTEV2='' %THEN                                                    07851100
   %DO;                                                                 07852200
    %MNTEHDRTAIL=                   /* mnte version of header         */07853300
    '3   *          Char(4)         /* Reserved for future expansion -  07854400
                                    must be zero on entry        @D2C*/ 07855500
                                    ';                                  07856600
    %MNTEBASETAIL=                  /* mnte version of body           */07857700
    '3 *                Char(54)   /* Reserved for future expansion     07858800
                                                                  @D4C*/07859900
                                   ';                                   07861000
   %END;                                                                07862100
 %ELSE                                                                  07863200
   %DO;                                                                 07864300
    %MNTEHDRTAIL=                  /* mnt2 version of header          */07865400
   '3   MntEHBLen  Fixed(32),      /* Length of mnte body used    @DEA*/07866500
    3   *          Char(8)         /* Reserved for future expansion -   07867600
                                     must be zero on entry       @DEC*/ 07868700
   ';                                                                   07869800
    %MNTEBASETAIL=                 /* mnt2 version of body            */07870900
    '3 MntEntSysName    Char(8),   /* System where File system is to    07872000
                                      be mounted, target system   @DDA*/07873100
     3 MntEntQSysName   Char(8),   /* Quiesce system owner        @DDA*/07874200
     3 MntEntFromSys    Char(8),   /* File system is to be moved from   07875300
                                      this system                 @DDA*/07876400
     3 *                Char(2),   /* reserved, alignment         @DDA*/07877500
     3 MntEntRFlags     Fixed(32), /* Request Flags               @DDA*/07878600
       5 *              Bit(30),   /* Reserved                    @DDA*/07879700
       5 MntEntNewAuto  Bit(1),    /* Change automove as indicated@DDA*/07880800
       5 MntEntChange   Bit(1),    /* Change request              @P3A*/07881900
     3 MntEntStatus2    Fixed(32), /* More status of file system  @DDA*/07883000
     3 MntEntSuccess    Fixed(32), /* Filesystems moved ok        @DDA*/07884100
    3 MntEntSMFInfo,                                                    07885200
      5 MntEntReadCt   Fixed(32), /* Number of reads from filesys @DEA*/07886300
      5 MntEntWriteCt  Fixed(32), /* Number of writes done        @DEA*/07887400
