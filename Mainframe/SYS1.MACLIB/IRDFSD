.***AB.04  MACRO VARIABLES FOR RANGES AND SETS OF ARGUMENTS             00037200
.*******************************************************************    00037300
                  LCLA  &ZI         INDEX FOR ARRAY                     00037400
                  LCLA  &ZJ         INDEX FOR PARMS                     00037500
                  LCLA  &ZK         INDEX FOR PARSE                     00037600
                  LCLA  &ZLO        LOW RANGE VALUE                     00037700
                  LCLA  &ZHI        HIGH RANGE VALUE                    00037800
                  LCLC  &ZW         WORK AREA FOR PARM                  00037900
.********************************************************************   00038000
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00038100
.********************************************************************   00038200
                  GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL             00038300
                  GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE      00038400
                  GBLA  &SYSALVL    SYSSTATE ARCHLVL                    00038500
.*                                                                      00038600
                  AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED      00038700
                  AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED       00038800
                  SPLEVEL  TEST                                         00038900
.ZSYS2            ANOP  ,                                               00039000
                  AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1           00039100
                  AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1           00039200
                  SYSSTATE TEST                                         00039300
                  AGO   .ZSYS4                                          00039400
.ZSYS3            ANOP  ,                                               00039500
&SYSASCE          SETC  'P'         ASSUME SYSSTATE PRIMARY             00039600
.ZSYS4            ANOP  ,                                               00039700
.*******************************************************************    00039800
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00039900
.*******************************************************************    00040000
                  LCLC  &XDEVICE    ++ VALUE                            00040100
                  LCLC  &XDEVICENUMBER          ++ NAME                 00040200
                  LCLC  &XCOUNTERS  ++ VALUE                            00040300
                  LCLC  &XCOUNTERLIST           ++ NAME                 00040400
                  LCLC  &XSUBPOOL   ++ NAME                             00040500
                  LCLC  &XFSDADDRESS            ++ NAME                 00040600
                  LCLC  &XRETCODE   ++ NAME                             00040700
                  LCLC  &XRSNCODE   ++ NAME                             00040800
                  LCLA  &XPLISTVER  ++ VALUE                            00040900
                  LCLC  &XMF        ++ VALUE                            00041000
                  LCLC  &XMFCTRL    ++ NAME                             00041100
                  LCLC  &XMFATTR    ++ VALUE                            00041200
                  LCLC  &XMFSCK     ++ VALUE                            00041300
.*******************************************************************    00041400
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00041500
.*******************************************************************    00041600
                  LCLC  &ZXDEVICE   ++ INPUT VALUE                      00041700
                  LCLC  &ZXDEVICENUMBER         ++ INPUT NAME           00041800
                  LCLC  &ZXCOUNTERS             ++ INPUT VALUE          00041900
                  LCLC  &ZXCOUNTERLIST          ++ INPUT NAME           00042000
                  LCLC  &ZXSUBPOOL  ++ INPUT NAME                       00042100
                  LCLC  &ZXFSDADDRESS           ++ OUTPUT NAME          00042200
                  LCLC  &ZXRETCODE  ++ OUTPUT NAME                      00042300
                  LCLC  &ZXRSNCODE  ++ OUTPUT NAME                      00042400
                  LCLC  &ZXPLISTVER             ++ INPUT VALUE          00042500
                  LCLC  &ZXMF       ++ INPUT VALUE                      00042600
                  LCLC  &ZXMFCTRL   ++ INPUT NAME                       00042700
                  LCLC  &ZXMFATTR   ++ INPUT VALUE                      00042800
                  LCLC  &ZXMFSCK    ++ INPUT VALUE                      00042900
.********************************************************************   00043000
.***AC.PLV01  PROCESS PLISTVER                                      *   00043100
.********************************************************************   00043200
&XPLISTVER      SETA  1                                                 00043300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00043400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00043500
&XPLISTVER      SETA  1                                                 00043600
.ZPLV0          ANOP  ,                                                 00043700
.*                                                                      00043800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00043900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00044000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00044100
.*-------------------------------------------------------------------   00044200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00044300
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00044400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00044500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00044600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00044700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00044800
                AGO   .ZPVCK2                                           00044900
.ZPVCK3         ANOP  ,                                                 00045000
.*-------------------------------------------------------------------   00045100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00045200
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00045300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00045400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00045500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00045600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00045700
                AGO   .ZPVCKE                                           00045800
.ZPVCK2         ANOP  ,                                                 00045900
.*-------------------------------------------------------------------   00046000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00046100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00046200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00046300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00046400
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00046500
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00046600
.ZPVCK6         ANOP  ,                                                 00046700
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00046800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00046900
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00047000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00047100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00047200
&XPLISTVER      SETA  1                                                 00047300
                AGO   .ZPVCKE                                           00047400
.*-------------------------------------------------------------------   00047500
.ZPVCK4A        ANOP  ,                                                 00047600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00047700
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00047800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00047900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00048000
