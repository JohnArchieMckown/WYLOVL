.ZG14A          ANOP  ,                                                 00088200
.*-------------------------------------------------------------------   00088300
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00088400
                AIF   (K'&RSNCODE(1) LE 0).ZG14C  NO PARM               00088500
&ZXRSNCODE      SETC  'OUTPUT'    SET OUTPUT                            00088600
&XRSNCODE       SETC  '&RSNCODE(1)'             SET XVARIABLE           00088700
                AIF   (N'&RSNCODE GT 1).ZG14C                           00088800
&XRSNCODE       SETC  '&RSNCODE'  SET XVARIABLE                         00088900
.ZG14C          ANOP  ,                                                 00089000
.*-------------------------------------------------------------------   00089100
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00089200
                AIF   (K'&ZXRSNCODE LE 0).ZG14R  NOT I/O                00089300
                AIF   ('&XRSNCODE'(1,1) NE '(').ZG14R                   00089400
&ZCGPR          SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                      00089500
                AIF   ('&ZCGPR'(1,1) NE '0').ZG14P                      00089600
                AIF   (K'&ZCGPR GT 2).ZG14Q                             00089700
                AIF   ('&ZCGPR' EQ '0').ZG14Q                           00089800
&ZCGPR          SETC  '&ZCGPR'(2,1)                                     00089900
                AIF   (T'&ZCGPR NE 'N').ZG14Q   INVALID REG NAME        00090000
.ZG14P          ANOP  ,                                                 00090100
                AIF   (T'&ZCGPR NE 'N').ZG14R   NON NUMERIC             00090200
                AIF   ('&ZCGPR' LT '2').ZG14Q                           00090300
                AIF   ('&ZCGPR' LE '12').ZG14R                          00090400
.ZG14Q          ANOP  ,                                                 00090500
&ZMS            SETC  '"RSNCODE='               *KGPRA* MNOTE           00090600
&ZMS            SETC  '&ZMS.&RSNCODE"'                                  00090700
&ZMS            SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'         00090800
                MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                  00090900
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00091000
.ZG14R          ANOP  ,                                                 00091100
.******************************************************************     00091200
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00091300
.******************************************************************     00091400
                ACTR  10000                                             00091500
.*----------------------------------------------------------------***   00091600
.***AH.P02 EXTRA POSITIONALS                                            00091700
                AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS              00091800
                AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL              00091900
                AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL          00092000
                AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL           00092100
.ZGLISTX1       ANOP  ,                                                 00092200
&ZCVAL          SETC  ''          ERROR FIELD                           00092300
&ZAWRK          SETA  0           POSITIONAL NUMBER                     00092400
.ZGLIST2        ANOP  ,                                                 00092500
&ZAWRK          SETA  &ZAWRK+1    NEXT POSITIONAL                       00092600
                AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE              00092700
&ZCVAL          SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                   00092800
                AGO   .ZGLIST2                                          00092900
.*                                                                      00093000
.ZGLIST3        ANOP  ,                                                 00093100
&ZCVAL          SETC '&ZCVAL'(2,K'&ZCVAL-1)     STRIP COMMA             00093200
&ZMS            SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE          00093300
&ZMS            SETC  '&ZMS "&ZCVAL"'                                   00093400
&ZMS            SETC  '&ZMS CANNOT BE USED.'                            00093500
&ZMS            SETC  '&ZMS NO POSITIONAL ARGUMENTS'                    00093600
                MNOTE 8,' &ZMS ARE ALLOWED.'                            00093700
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00093800
