&ZMS            SETC  '&ZMS "RTNCODE"'                                  00084100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00084200
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00084300
.ZG11A          ANOP  ,                                                 00084400
.*-------------------------------------------------------------------   00084500
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00084600
                AIF   (K'&RTNCODE(1) LE 0).ZG11C  NO PARM               00084700
&ZXRTNCODE      SETC  'OUTPUT'    SET OUTPUT                            00084800
&XRTNCODE       SETC  '&RTNCODE(1)'             SET XVARIABLE           00084900
                AIF   (N'&RTNCODE GT 1).ZG11C                           00085000
&XRTNCODE       SETC  '&RTNCODE'  SET XVARIABLE                         00085100
.ZG11C          ANOP  ,                                                 00085200
.*-------------------------------------------------------------------   00085300
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00085400
                AIF   (K'&ZXRTNCODE LE 0).ZG11R  NOT I/O                00085500
                AIF   ('&XRTNCODE'(1,1) NE '(').ZG11R                   00085600
&ZCGPR          SETC  '&XRTNCODE'(2,K'&XRTNCODE-2)                      00085700
                AIF   ('&ZCGPR'(1,1) NE '0').ZG11P                      00085800
                AIF   (K'&ZCGPR GT 2).ZG11Q                             00085900
                AIF   ('&ZCGPR' EQ '0').ZG11Q                           00086000
&ZCGPR          SETC  '&ZCGPR'(2,1)                                     00086100
                AIF   (T'&ZCGPR NE 'N').ZG11Q   INVALID REG NAME        00086200
.ZG11P          ANOP  ,                                                 00086300
                AIF   (T'&ZCGPR NE 'N').ZG11R   NON NUMERIC             00086400
                AIF   ('&ZCGPR' LT '2').ZG11Q                           00086500
                AIF   ('&ZCGPR' LE '12').ZG11R                          00086600
.ZG11Q          ANOP  ,                                                 00086700
&ZMS            SETC  '"RTNCODE='               *KGPRA* MNOTE           00086800
&ZMS            SETC  '&ZMS.&RTNCODE"'                                  00086900
&ZMS            SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'         00087000
                MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                  00087100
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00087200
.ZG11R          ANOP  ,                                                 00087300
.*----------------------------------------------------------------***   00087400
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00087500
                AIF   (N'&RSNCODE LE 1).ZG14A   CK NUM                  00087600
&ZMS            SETC  '"RSNCODE='               *KPOSA* MNOTE           00087700
&ZMS            SETC  '&ZMS.&RSNCODE"'                                  00087800
&ZMS            SETC  '&ZMS "RSNCODE"'                                  00087900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00088000
