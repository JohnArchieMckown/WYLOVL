.***AB.02  GLOBAL VARIABLES TO CONTROL PRINT AND CODE GENERATION        00035700
.*******************************************************************    00035800
                GBLC  &ZPRINT     YES=PRINT NO=NO PRINT                 00035900
                GBLC  &ZTEST      YES=TEST NO=NO TEST                   00036000
                GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX           00036100
.*                                                                      00036200
                GBLC  &ZIXZXIXPI  YES=MACRO USED AT LEAST ONCE          00036300
.*******************************************************************    00036400
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00036500
.*******************************************************************    00036600
                LCLC  &ZMS        ERROR MESSAGE WORK AREA               00036700
                LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS          00036800
                LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED            00036900
                LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR           00037000
                LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE           00037100
                LCLA  &ZAWRK      USED AS A WORK AREA                   00037200
                LCLA  &ZASIZ      USED FOR SIZE OF VALUE                00037300
                LCLA  &ZN         NUMERIC WORK VARIABLE                 00037400
                LCLC  &ZC         CHARACTER WORK VARIABLE               00037500
                LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE           00037600
                LCLA  &ZTLA       VALUE TOO LONG VARIABLE               00037700
                LCLA  &ZMACRET    MACRO RETURN CODE                     00037800
&ZMACRET        SETA  0           ASSUME NO ERRORS                      00037900
.*                                                                      00038000
                LCLC  &ZCSYSLIST(255)           NONXKEY, POS ARRAY      00038100
                LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()            00038200
                LCLA  &ZPLLEN     PARAMETER LIST LENGTH                 00038300
                LCLC  &ZENDBASEPL               End of base PL          00038400
                LCLA  &ZPLWORKA   PARAMETER LIST LENGTH                 00038500
.*******************************************************************    00038600
.***AB.04  MACRO VARIABLES FOR RANGES AND SETS OF ARGUMENTS             00038700
.*******************************************************************    00038800
                LCLA  &ZI         INDEX FOR ARRAY                       00038900
                LCLA  &ZJ         INDEX FOR PARMS                       00039000
                LCLA  &ZK         INDEX FOR PARSE                       00039100
                LCLA  &ZLO        LOW RANGE VALUE                       00039200
                LCLA  &ZHI        HIGH RANGE VALUE                      00039300
                LCLC  &ZW         WORK AREA FOR PARM                    00039400
.********************************************************************   00039500
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00039600
.********************************************************************   00039700
                GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL               00039800
                GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE        00039900
.*                                                                      00040000
                AIF   (K'&SYSASCE GT 0).ZSYS4   SYS STATE DEFINED       00040100
                AIF   (K'&SYSSPLV GT 0).ZSYS2   SP LEVEL DEFINED        00040200
                SPLEVEL  TEST                                           00040300
.ZSYS2          ANOP  ,                                                 00040400
                AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1             00040500
                AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1             00040600
                SYSSTATE TEST                                           00040700
                AGO   .ZSYS4                                            00040800
.ZSYS3          ANOP  ,                                                 00040900
&SYSASCE        SETC  'P'         ASSUME SYSSTATE PRIMARY               00041000
.ZSYS4          ANOP  ,                                                 00041100
.*******************************************************************    00041200
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00041300
.*******************************************************************    00041400
                LCLC  &XGROUPTOKEN              ++ NAME                 00041500
                LCLC  &XPIDS      ++ NAME                               00041600
                LCLC  &XPIDSSIZE  ++ NAME                               00041700
                LCLC  &XRTNCODE   ++ NAME                               00041800
                LCLC  &XRSNCODE   ++ NAME                               00041900
                LCLA  &XPLISTVER  ++ VALUE                              00042000
                LCLC  &XMF        ++ VALUE                              00042100
                LCLC  &XMFCTRL    ++ NAME                               00042200
                LCLC  &XMFATTR    ++ VALUE                              00042300
                LCLC  &XMFSCK     ++ VALUE                              00042400
.*******************************************************************    00042500
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00042600
.*******************************************************************    00042700
                LCLC  &ZXGROUPTOKEN             ++ INPUT NAME           00042800
                LCLC  &ZXPIDS     ++ INOUT NAME                         00042900
                LCLC  &ZXPIDSSIZE               ++ INOUT NAME           00043000
                LCLC  &ZXRTNCODE  ++ OUTPUT NAME                        00043100
                LCLC  &ZXRSNCODE  ++ OUTPUT NAME                        00043200
                LCLC  &ZXPLISTVER               ++ INPUT VALUE          00043300
                LCLC  &ZXMF       ++ INPUT VALUE                        00043400
                LCLC  &ZXMFCTRL   ++ INPUT NAME                         00043500
                LCLC  &ZXMFATTR   ++ INPUT VALUE                        00043600
                LCLC  &ZXMFSCK    ++ INPUT VALUE                        00043700
.********************************************************************   00043800
.***AC.PLV01  PROCESS PLISTVER                                      *   00043900
.********************************************************************   00044000
&XPLISTVER      SETA  1                                                 00044100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00044200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00044300
&XPLISTVER      SETA  1                                                 00044400
.ZPLV0          ANOP  ,                                                 00044500
.*                                                                      00044600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00044700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00044800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00044900
.*-------------------------------------------------------------------   00045000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00045100
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00045200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00045300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00045400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00045500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00045600
                AGO   .ZPVCK2                                           00045700
.ZPVCK3         ANOP  ,                                                 00045800
.*-------------------------------------------------------------------   00045900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00046000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00046100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00046200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00046300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00046400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00046500
                AGO   .ZPVCKE                                           00046600
.ZPVCK2         ANOP  ,                                                 00046700
.*-------------------------------------------------------------------   00046800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00046900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00047000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00047100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00047200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00047300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00047400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00047500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00047600
                AGO   .ZPVCK5                                           00047700
.ZPVCK4         ANOP  ,                                                 00047800
&XPLISTVER      SETA  &PLISTVER(1)                                      00047900
.ZPVCK5         ANOP  ,                                                 00048000
.*-------------------------------------------------------------------   00048100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00048200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00048300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00048400
                AIF   (&XPLISTVER LT 1).ZPVCK6  TOO LOW                 00048500
                AIF   (&XPLISTVER LE 1).ZPVCKE  NOT HIGH                00048600
.ZPVCK6         ANOP   ,                                                00048700
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00048800
