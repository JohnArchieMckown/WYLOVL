&ZOPO(&ZOPN)          SETC  'SPACE'                                     00099400
&ZOPV(&ZOPN)          SETC  '1'                                         00099500
&ZOPD(&ZOPN)          SETC  ' '                                         00099600
.*                                                                      00099700
.ZLMFSL2              ANOP  ,                                           00099800
.*-------------------------------------------------------------------   00099900
.***AM.L01  IXZXIXPI FMT=LIST PARAMETERS                                00100000
