.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00108000
&ZPLLEN               SETA 24                                           00108100
.*                                                                      00108200
.******************************************************************     00108300
.***AI.L01 IXZXIXPI FMT=LIST FILL IN PARAMETER AREA                     00108400
.******************************************************************     00108500
                      ACTR  10000                                       00108600
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00108700
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00108800
&ZCVAL                SETC  'LAE'          XAX MODE                     00108900
.ZIMFXA               ANOP ,                                            00109000
.*                                                                      00109100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00109200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00109300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00109400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00109500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00109600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00109700
