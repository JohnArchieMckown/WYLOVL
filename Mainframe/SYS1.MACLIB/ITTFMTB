.***AG.KMEKY05 NAME, RQD INPUT, ME KEYS -NO ARGUMENTS                   19250000
&XSERVICE1 SETC ''                     INITIALIZE XVARIABLE             19300000
&ZASYSLNDX SETA 0                                                       19350000
.ZG2A    ANOP  ,                       POSITIONAL KEY SEARCH            19400000
&ZASYSLNDX SETA &ZASYSLNDX+1           NEXT POSITIONAL                  19450000
         AIF   (&ZASYSLNDX GT N'&SYSLIST).ZG2B  DONE                    19500000
         AIF   ('&ZCSYSLIST(&ZASYSLNDX)' EQ 'NONXKEY').ZG2A             19550000
         AIF   ('&SYSLIST(&ZASYSLNDX)' NE 'MAP').ZG2A                   19600000
&ZCSYSLIST(&ZASYSLNDX) SETC 'NONXKEY'           KEY IS NON-XKEY         19650000
&XSERVICE1 SETC 'MAP'                  XVARIABLE                        19700000
&XSERVICE2 SETC ''                     NO XVARIABLE PARM                19750000
&XMAP    SETC  'MAP'                   XVARIABLE                        19800000
.*----------------------------------------------------------------***   19900000
.***AG.KMEKY02 NAME, RQD INPUT, ME KEYS -XVARIABLE                      19950000
.ZG2B    AIF   (K'&TABLEDATA LE 0).ZG3D  NO ME KEY                      20000000
         AIF   (N'&TABLEDATA LE 1).ZG3A  CK NUM                         20050000
         MNOTE 8,'"TABLEDATA=&TABLEDATA""TABLEDATA" &ZM1ARG'            20100000
&ZMACRET SETA  8                       MACRO ERROR RETURN CODE          20300000
.ZG3A    AIF   (K'&XSERVICE1 LE 0).ZG3B                                 20350000
         MNOTE 8,'"TABLEDATA=&TABLEDATA" &ZMMTEX "&XSERVICE1" KEY.'     20400000
&ZMACRET SETA  8                       MACRO ERROR RETURN CODE          20650000
         AGO   .ZG3D                                                    20700000
.*                                                                      20750000
.ZG3B    ANOP  ,                                                        20800000
&XSERVICE1 SETC 'TABLEDATA'            XVARIABLE                        20850000
