                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00359900
&ZLC                  SETA  33             COMMENT COLUMN               00360000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00360100
&ZLV                  SETC  '&ZLV'.' '                                  00360200
                      AGO   .ZL31                                       00360300
.*                                                                      00360400
.ZL41                 ANOP  ,                                           00360500
&ZLV                  SETC  '&ZLV'.' '                                  00360600
.ZL51                 ANOP  ,                                           00360700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00360800
.*                                                                      00360900
&ZLL     &ZLOP &ZLV                                                     00361000
.*                                                                      00361100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00361200
                      AGO   .ZL21          CONTINUE LOOPING             00361300
.*                                                                      00361400
.ZLX1                 ANOP  ,                                           00361500
.*******************************************************************    00361600
.***AP.X02  END OF IXCJOIN MACRO                                        00361700
&ZIXCJOIN             SETC  'YES'          SET IXCJOIN USED             00361800
         POP   PRINT                                                    00361900
*                                            IXCJOIN-1                  00362000
         SPACE 1                                                        00362100
.ZLX2                 ANOP  ,                                           00362200
         MEND                                                           00362300
