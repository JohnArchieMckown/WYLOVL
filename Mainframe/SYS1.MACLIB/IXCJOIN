.***AB.07  DEFINE ALL MACRO VARIABLES                                   00094600
.*******************************************************************    00094700
                   LCLC  &XGRPNAME   ++ NAME                            00094800
                   LCLC  &XMSGEXIT   ++ NAME                            00094900
                   LCLC  &XCANREPLY  ++ VALUE                           00095000
                   LCLC  &XGT61KMSG  ++ VALUE                           00095100
                   LCLC  &XNOTIFYEXIT           ++ NAME                 00095200
                   LCLC  &XMEMASSOC  ++ VALUE                           00095300
                   LCLC  &XANSAREA   ++ NAME                            00095400
                   LCLC  &XANSLEN    ++ NAME                            00095500
                   LCLC  &XLASTING   ++ VALUE                           00095600
                   LCLC  &XMEMNAME   ++ NAME                            00095700
                   LCLC  &XGRPEXIT   ++ NAME                            00095800
                   LCLC  &XSTATFLD   ++ NAME                            00095900
                   LCLC  &XSTATEXIT  ++ NAME                            00096000
                   LCLC  &XINTERVAL  ++ NAME                            00096100
                   LCLC  &XUSTATE    ++ NAME                            00096200
                   LCLC  &XUSLEN     ++ NAME                            00096300
                   LCLC  &XMEMDATA   ++ NAME                            00096400
                   LCLC  &XSYSCLEANUPMEM        ++ VALUE                00096500
                   LCLC  &XMSGOUTASID           ++ VALUE                00096600
                   LCLC  &XRETCODE   ++ NAME                            00096700
                   LCLC  &XRSNCODE   ++ NAME                            00096800
                   LCLA  &XPLISTVER  ++ VALUE                           00096900
                   LCLC  &XMF        ++ VALUE                           00097000
                   LCLC  &XMFCTRL    ++ NAME                            00097100
                   LCLC  &XMFATTR    ++ VALUE                           00097200
                   LCLC  &XMFSCK     ++ VALUE                           00097300
.*******************************************************************    00097400
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00097500
.*******************************************************************    00097600
                   LCLC  &ZXGRPNAME  ++ INPUT NAME                      00097700
                   LCLC  &ZXMSGEXIT  ++ INPUT NAME                      00097800
                   LCLC  &ZXCANREPLY            ++ INPUT VALUE          00097900
                   LCLC  &ZXGT61KMSG            ++ INPUT VALUE          00098000
                   LCLC  &ZXNOTIFYEXIT          ++ INPUT NAME           00098100
                   LCLC  &ZXMEMASSOC            ++ INPUT VALUE          00098200
                   LCLC  &ZXANSAREA  ++ OUTPUT NAME                     00098300
                   LCLC  &ZXANSLEN   ++ INPUT NAME                      00098400
                   LCLC  &ZXLASTING  ++ INPUT VALUE                     00098500
                   LCLC  &ZXMEMNAME  ++ INPUT NAME                      00098600
                   LCLC  &ZXGRPEXIT  ++ INPUT NAME                      00098700
                   LCLC  &ZXSTATFLD  ++ INPUT NAME                      00098800
                   LCLC  &ZXSTATEXIT            ++ INPUT NAME           00098900
                   LCLC  &ZXINTERVAL            ++ INPUT NAME           00099000
                   LCLC  &ZXUSTATE   ++ INPUT NAME                      00099100
                   LCLC  &ZXUSLEN    ++ INPUT NAME                      00099200
                   LCLC  &ZXMEMDATA  ++ INPUT NAME                      00099300
                   LCLC  &ZXSYSCLEANUPMEM       ++ INPUT VALUE          00099400
                   LCLC  &ZXMSGOUTASID          ++ INPUT VALUE          00099500
                   LCLC  &ZXRETCODE  ++ OUTPUT NAME                     00099600
                   LCLC  &ZXRSNCODE  ++ OUTPUT NAME                     00099700
                   LCLC  &ZXPLISTVER            ++ INPUT VALUE          00099800
                   LCLC  &ZXMF       ++ INPUT VALUE                     00099900
                   LCLC  &ZXMFCTRL   ++ INPUT NAME                      00100000
                   LCLC  &ZXMFATTR   ++ INPUT VALUE                     00100100
                   LCLC  &ZXMFSCK    ++ INPUT VALUE                     00100200
.********************************************************************   00100300
.***AC.PLV01  PROCESS PLISTVER                                      *   00100400
.********************************************************************   00100500
&XPLISTVER      SETA  0                                                 00100600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00100700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00100800
&XPLISTVER      SETA  1                                                 00100900
.ZPLV0          ANOP  ,                                                 00101000
.*                                                                      00101100
                AIF   ('&CANREPLY' EQ '').ZPLV1E                        00101200
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00101300
&XPLISTVER      SETA  1                                                 00101400
.ZPLV1E         ANOP  ,                                                 00101500
.*                                                                      00101600
                AIF   ('&GT61KMSG' EQ '').ZPLV2E                        00101700
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00101800
&XPLISTVER      SETA  1                                                 00101900
.ZPLV2E         ANOP  ,                                                 00102000
.*                                                                      00102100
                AIF   ('&NOTIFYEXIT' EQ '').ZPLV3E                      00102200
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00102300
&XPLISTVER      SETA  1                                                 00102400
.ZPLV3E         ANOP  ,                                                 00102500
.*                                                                      00102600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00102700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00102800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00102900
.*-------------------------------------------------------------------   00103000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00103100
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00103200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00103300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00103400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00103500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00103600
                AGO   .ZPVCK2                                           00103700
.ZPVCK3         ANOP  ,                                                 00103800
.*-------------------------------------------------------------------   00103900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00104000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00104100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00104200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00104300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00104400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00104500
                AGO   .ZPVCKE                                           00104600
.ZPVCK2         ANOP  ,                                                 00104700
.*-------------------------------------------------------------------   00104800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00104900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00105000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00105100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00105200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00105300
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00105400
.ZPVCK6         ANOP  ,                                                 00105500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00105600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00105700
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00105800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00105900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00106000
&XPLISTVER      SETA  1                                                 00106100
                AGO   .ZPVCKE                                           00106200
.*-------------------------------------------------------------------   00106300
.ZPVCK4A        ANOP  ,                                                 00106400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00106500
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00106600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00106700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00106800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00106900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00107000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00107100
                AGO   .ZPVCK5                                           00107200
.ZPVCK4         ANOP  ,                                                 00107300
&XPLISTVER      SETA  &PLISTVER(1)                                      00107400
.ZPVCK5         ANOP  ,                                                 00107500
.ZPVCKE         ANOP  ,                                                 00107600
.********************************************************************   00107700
