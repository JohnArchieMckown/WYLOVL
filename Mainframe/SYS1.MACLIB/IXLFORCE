.***AC.PLV01  PROCESS PLISTVER                                      *   00071300
.********************************************************************   00071400
&XPLISTVER      SETA  0                                                 00071500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00071600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00071700
&XPLISTVER      SETA  0                                                 00071800
.ZPLV0          ANOP  ,                                                 00071900
.*                                                                      00072000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00072100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00072200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00072300
.*-------------------------------------------------------------------   00072400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00072500
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00072600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00072700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00072800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00072900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00073000
                AGO   .ZPVCK2                                           00073100
.ZPVCK3         ANOP  ,                                                 00073200
.*-------------------------------------------------------------------   00073300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00073400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00073500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00073600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00073700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00073800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00073900
                AGO   .ZPVCKE                                           00074000
.ZPVCK2         ANOP  ,                                                 00074100
.*-------------------------------------------------------------------   00074200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00074300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00074400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00074500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00074600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00074700
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00074800
.ZPVCK6         ANOP  ,                                                 00074900
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00075000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00075100
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00075200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00075300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00075400
&XPLISTVER      SETA  0                                                 00075500
                AGO   .ZPVCKE                                           00075600
.*-------------------------------------------------------------------   00075700
.ZPVCK4A        ANOP  ,                                                 00075800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00075900
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00076000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00076100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00076200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00076300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00076400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00076500
                AGO   .ZPVCK5                                           00076600
.ZPVCK4         ANOP  ,                                                 00076700
&XPLISTVER      SETA  &PLISTVER(1)                                      00076800
.ZPVCK5         ANOP  ,                                                 00076900
.ZPVCKE         ANOP  ,                                                 00077000
.********************************************************************   00077100
