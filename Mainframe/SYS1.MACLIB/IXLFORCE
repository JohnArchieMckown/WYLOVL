.***AH.P02 EXTRA POSITIONALS                                            00159500
                   AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS           00159600
                   AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL           00159700
                   AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL       00159800
                   AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL        00159900
.ZGLISTX1          ANOP  ,                                              00160000
&ZCVAL             SETC  ''          ERROR FIELD                        00160100
&ZAWRK             SETA  0           POSITIONAL NUMBER                  00160200
.ZGLIST2           ANOP  ,                                              00160300
