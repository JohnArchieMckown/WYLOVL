                      ACTR  10000                                       00200300
.*******************************************************************    00200400
.***AO.X02  RETURN CODE PROCESSING                                      00200500
.*******************************************************************    00200600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00200700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00200800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00200900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00201000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00201100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00201200
&ZOPN                 SETA  &ZOPN+1                                     00201300
&ZOPL(&ZOPN)          SETC  ' '                                         00201400
&ZOPO(&ZOPN)          SETC  'ST'                                        00201500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00201600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00201700
                      AGO   .ZMRETX                                     00201800
.*                                                                      00201900
.ZMRET2               ANOP  ,                                           00202000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00202100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00202200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00202300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00202400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00202500
&ZOPN                 SETA  &ZOPN+1                                     00202600
&ZOPL(&ZOPN)          SETC  ' '                                         00202700
&ZOPO(&ZOPN)          SETC  'LR'                                        00202800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00202900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00203000
.ZMRETX               ANOP  ,                                           00203100
.*******************************************************************    00203200
.***AO.X03  REASON CODE PROCESSING                                      00203300
.*******************************************************************    00203400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00203500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00203600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00203700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00203800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00203900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00204000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00204100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00204200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00204300
&ZOPN                 SETA  &ZOPN+1                                     00204400
&ZOPL(&ZOPN)          SETC  ' '                                         00204500
&ZOPO(&ZOPN)          SETC  'ST'                                        00204600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00204700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00204800
                      AGO   .ZMRSNX                                     00204900
.*                                                                      00205000
.ZMRSN2               ANOP  ,                                           00205100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00205200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00205300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00205400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00205500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00205600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00205700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00205800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00205900
&ZOPN                 SETA  &ZOPN+1                                     00206000
&ZOPL(&ZOPN)          SETC  ' '                                         00206100
&ZOPO(&ZOPN)          SETC  'LR'                                        00206200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00206300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00206400
.ZMRSNX               ANOP  ,                                           00206500
.*******************************************************************    00206600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00206700
.*******************************************************************    00206800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00206900
.*******************************************************************    00207000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00207100
.*******************************************************************    00207200
                      ACTR  10000                                       00207300
.*                                                                      00207400
                      LCLA  &ZLC           COLUMN POSITION              00207500
                      LCLA  &ZOPX          LIST TABLE INDEX             00207600
                      LCLC  &ZLL           LABEL                        00207700
                      LCLC  &ZLOP          OPERATION                    00207800
                      LCLC  &ZLV           VALUE                        00207900
                      LCLC  &ZLD           DESCRIPTION                  00208000
.*                                                                      00208100
&ZOPX                 SETA  1                                           00208200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00208300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00208400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00208500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00208600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00208700
                      AIF   (K'&ZLD LE 0).ZL5                           00208800
&ZLC                  SETA  35             COMMENT COLUMN               00208900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00209000
&ZLC                  SETA  33             COMMMENT COLUMN              00209100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00209200
&ZLV                  SETC  '&ZLV'.' '                                  00209300
                      AGO   .ZL3                                        00209400
.*                                                                      00209500
.ZL4                  ANOP  ,                                           00209600
&ZLV                  SETC  '&ZLV'.' '                                  00209700
.ZL5                  ANOP  ,                                           00209800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00209900
.*                                                                      00210000
&ZLL     &ZLOP &ZLV                                                     00210100
.*                                                                      00210200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00210300
                      AGO   .ZL2           CONTINUE LOOPING             00210400
.*                                                                      00210500
.ZLX                  ANOP  ,                                           00210600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00210700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00210800
         L     14,16(0,0)              GET CVT ADDRESS                  00210900
         L     14,772(14,0)            GET SFT ADDRESS                  00211000
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00211100
         LA    15,45                   GET EX FOR SERVICE ROUTINE       00211200
         OR    14,15                   GET PC NUMBER                    00211300
         PC    0(14)                                                    00211400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00211500
.ZLINKX               ANOP  ,                                           00211600
.*******************************************************************    00211700
.***AP.X01  FORMAT GENERATED CODE                                       00211800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00211900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00212000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00212100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00212200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00212300
                      AIF   (K'&ZLD LE 0).ZL51                          00212400
&ZLC                  SETA  35             COMMENT COLUMN               00212500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00212600
&ZLC                  SETA  33             COMMENT COLUMN               00212700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00212800
&ZLV                  SETC  '&ZLV'.' '                                  00212900
                      AGO   .ZL31                                       00213000
.*                                                                      00213100
.ZL41                 ANOP  ,                                           00213200
&ZLV                  SETC  '&ZLV'.' '                                  00213300
.ZL51                 ANOP  ,                                           00213400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00213500
.*                                                                      00213600
&ZLL     &ZLOP &ZLV                                                     00213700
.*                                                                      00213800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00213900
                      AGO   .ZL21          CONTINUE LOOPING             00214000
.*                                                                      00214100
.ZLX1                 ANOP  ,                                           00214200
.*******************************************************************    00214300
.***AP.X02  END OF IXLFORCE MACRO                                       00214400
&ZIXLFORCE            SETC  'YES'          SET IXLFORCE USED            00214500
         POP   PRINT                                                    00214600
