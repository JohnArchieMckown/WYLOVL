.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00206500
.*******************************************************************    00206600
                      ACTR  10000                                       00206700
.*                                                                      00206800
                      LCLA  &ZLC           COLUMN POSITION              00206900
                      LCLA  &ZOPX          LIST TABLE INDEX             00207000
                      LCLC  &ZLL           LABEL                        00207100
                      LCLC  &ZLOP          OPERATION                    00207200
                      LCLC  &ZLV           VALUE                        00207300
                      LCLC  &ZLD           DESCRIPTION                  00207400
.*                                                                      00207500
&ZOPX                 SETA  1                                           00207600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00207700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00207800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00207900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00208000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00208100
                      AIF   (K'&ZLD LE 0).ZL5                           00208200
&ZLC                  SETA  35             COMMENT COLUMN               00208300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00208400
&ZLC                  SETA  33             COMMMENT COLUMN              00208500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00208600
&ZLV                  SETC  '&ZLV'.' '                                  00208700
                      AGO   .ZL3                                        00208800
.*                                                                      00208900
.ZL4                  ANOP  ,                                           00209000
&ZLV                  SETC  '&ZLV'.' '                                  00209100
.ZL5                  ANOP  ,                                           00209200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00209300
.*                                                                      00209400
&ZLL     &ZLOP &ZLV                                                     00209500
.*                                                                      00209600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00209700
                      AGO   .ZL2           CONTINUE LOOPING             00209800
.*                                                                      00209900
.ZLX                  ANOP  ,                                           00210000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00210100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00210200
         AIF    ('&XMF' EQ 'L').LFORM2                                  00210300
         PC     0(15)               .PC to COFMCREA                     00210400
.LFORM2  ANOP                                                           00210500
.*-----------BAL macro code-------------                                00210600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00210700
.ZLINKX               ANOP  ,                                           00210800
.*******************************************************************    00210900
.***AP.X01  FORMAT GENERATED CODE                                       00211000
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00211100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00211200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00211300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00211400
