.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00072000
              AIF   (N'&SYSLIST NE 0).ZG33B     POSITIONALS?            00072100
              AIF   ('&MAJOR' NE '').ZG33B      KEYS?                   00072200
              AIF   ('&REPLACE' NE '').ZG33B    KEYS?                   00072300
              AIF   ('&CINDEX' NE '').ZG33B     KEYS?                   00072400
              AIF   ('&DDNAME' NE '').ZG33B     KEYS?                   00072500
              AIF   ('&MINOR' NE '').ZG33B      KEYS?                   00072600
              AIF   ('&UTOKEN' NE '').ZG33B     KEYS?                   00072700
              AIF   ('&OBJPRTL' NE '').ZG33B    KEYS?                   00072800
              AIF   ('&OBJPLSZ' NE '').ZG33B    KEYS?                   00072900
              AIF   ('&RETCODE' NE '').ZG33B    KEYS?                   00073000
              AIF   ('&RSNCODE' NE '').ZG33B    KEYS?                   00073100
              AGO    .ZG33C     INVOCATION OKAY                         00073200
.ZG33B        ANOP  ,                                                   00073300
&ZMS          SETC  'THE "L" FORM OF "MF"'      *MFLA* MNOTE            00073400
&ZMS          SETC  '&ZMS CAN ONLY HAVE'                                00073500
&ZMS          SETC  '&ZMS THE "PLISTVER" KEY'                           00073600
              MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'              00073700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00073800
.ZG33C        ANOP  ,                                                   00073900
