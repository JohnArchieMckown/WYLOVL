.***AC.PLV01  PROCESS PLISTVER                                      *   00063100
.********************************************************************   00063200
&XPLISTVER      SETA  1                                                 00063300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00063400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00063500
&XPLISTVER      SETA  1                                                 00063600
.ZPLV0          ANOP  ,                                                 00063700
.*                                                                      00063800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00063900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00064000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00064100
.*-------------------------------------------------------------------   00064200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00064300
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00064400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00064600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00064700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064800
                AGO   .ZPVCK2                                           00064900
.ZPVCK3         ANOP  ,                                                 00065000
.*-------------------------------------------------------------------   00065100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00065200
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00065300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00065500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00065600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065700
                AGO   .ZPVCKE                                           00065800
.ZPVCK2         ANOP  ,                                                 00065900
.*-------------------------------------------------------------------   00066000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00066100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00066200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00066300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00066400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00066500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00066600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00066700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066800
                AGO   .ZPVCK5                                           00066900
.ZPVCK4         ANOP  ,                                                 00067000
&XPLISTVER      SETA  &PLISTVER(1)                                      00067100
.ZPVCK5         ANOP  ,                                                 00067200
.*-------------------------------------------------------------------   00067300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00067400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00067500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00067600
                AIF   (&XPLISTVER LT 1).ZPVCK6  TOO LOW                 00067700
                AIF   (&XPLISTVER LE 1).ZPVCKE  NOT HIGH                00067800
.ZPVCK6         ANOP   ,                                                00067900
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00068000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068100
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00068200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00068300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068400
.ZPVCKE         ANOP   ,                                                00068500
.********************************************************************   00068600
