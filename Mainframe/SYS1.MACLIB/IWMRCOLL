&ZOPL(&ZOPN)          SETC  ' '                                         00196200
&ZOPO(&ZOPN)          SETC  'ST'                                        00196300
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00196400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00196500
                      AGO   .ZMRETX                                     00196600
.*                                                                      00196700
.ZMRET2               ANOP  ,                                           00196800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00196900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00197000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00197100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00197200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00197300
&ZOPN                 SETA  &ZOPN+1                                     00197400
&ZOPL(&ZOPN)          SETC  ' '                                         00197500
&ZOPO(&ZOPN)          SETC  'LR'                                        00197600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00197700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00197800
.ZMRETX               ANOP  ,                                           00197900
.*******************************************************************    00198000
.***AO.X03  REASON CODE PROCESSING                                      00198100
.*******************************************************************    00198200
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00198300
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00198400
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00198500
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00198600
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00198700
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00198800
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00198900
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00199000
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00199100
&ZOPN                 SETA  &ZOPN+1                                     00199200
&ZOPL(&ZOPN)          SETC  ' '                                         00199300
&ZOPO(&ZOPN)          SETC  'ST'                                        00199400
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00199500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00199600
                      AGO   .ZMRSNX                                     00199700
.*                                                                      00199800
.ZMRSN2               ANOP  ,                                           00199900
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00200000
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00200100
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00200200
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00200300
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00200400
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00200500
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00200600
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00200700
&ZOPN                 SETA  &ZOPN+1                                     00200800
&ZOPL(&ZOPN)          SETC  ' '                                         00200900
&ZOPO(&ZOPN)          SETC  'LR'                                        00201000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00201100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00201200
.ZMRSNX               ANOP  ,                                           00201300
.*******************************************************************    00201400
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00201500
.*******************************************************************    00201600
.ZMFLX                ANOP  ,              MF=L PROCESSING              00201700
.*******************************************************************    00201800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00201900
.*******************************************************************    00202000
                      ACTR  10000                                       00202100
.*                                                                      00202200
                      LCLA  &ZLC           COLUMN POSITION              00202300
                      LCLA  &ZOPX          LIST TABLE INDEX             00202400
                      LCLC  &ZLL           LABEL                        00202500
                      LCLC  &ZLOP          OPERATION                    00202600
                      LCLC  &ZLV           VALUE                        00202700
                      LCLC  &ZLD           DESCRIPTION                  00202800
.*                                                                      00202900
&ZOPX                 SETA  1                                           00203000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00203100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00203200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00203300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00203400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00203500
                      AIF   (K'&ZLD LE 0).ZL5                           00203600
&ZLC                  SETA  35             COMMENT COLUMN               00203700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00203800
&ZLC                  SETA  33             COMMMENT COLUMN              00203900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00204000
&ZLV                  SETC  '&ZLV'.' '                                  00204100
                      AGO   .ZL3                                        00204200
.*                                                                      00204300
.ZL4                  ANOP  ,                                           00204400
&ZLV                  SETC  '&ZLV'.' '                                  00204500
.ZL5                  ANOP  ,                                           00204600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00204700
.*                                                                      00204800
&ZLL     &ZLOP &ZLV                                                     00204900
.*                                                                      00205000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00205100
                      AGO   .ZL2           CONTINUE LOOPING             00205200
.*                                                                      00205300
.ZLX                  ANOP  ,                                           00205400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00205500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00205600
         L     14,16(0,0)              GET CVT ADDRESS                  00205700
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00205800
         LAE   14,0(14,0)              CLEAR AR 14                      00205900
.NOAR    ANOP                                                           00206000
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00206100
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00206200
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00206300
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00206400
         B     END&SYSNDX              BRANCH TO END LABEL              00206500
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00206600
         L     14,772(14,0)            GET SFT ADDRESS                  00206700
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00206800
         LA    15,14                   GET EX FOR SERVICE ROUTINE       00206900
         OR    14,15                   GET PC NUMBER                    00207000
         PC    0(14)                                                    00207100
END&SYSNDX DS    0H                    END LABEL                        00207200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00207300
.ZLINKX               ANOP  ,                                           00207400
.*******************************************************************    00207500
.***AP.X01  FORMAT GENERATED CODE                                       00207600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00207700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00207800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00207900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00208000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00208100
                      AIF   (K'&ZLD LE 0).ZL51                          00208200
&ZLC                  SETA  35             COMMENT COLUMN               00208300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00208400
&ZLC                  SETA  33             COMMENT COLUMN               00208500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00208600
&ZLV                  SETC  '&ZLV'.' '                                  00208700
                      AGO   .ZL31                                       00208800
.*                                                                      00208900
.ZL41                 ANOP  ,                                           00209000
&ZLV                  SETC  '&ZLV'.' '                                  00209100
.ZL51                 ANOP  ,                                           00209200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00209300
.*                                                                      00209400
&ZLL     &ZLOP &ZLV                                                     00209500
.*                                                                      00209600
