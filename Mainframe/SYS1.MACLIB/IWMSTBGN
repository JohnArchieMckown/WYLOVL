.*                                                                      00124400
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00124500
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00124600
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00124700
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00124800
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00124900
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00125000
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00125100
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00125200
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00125300
