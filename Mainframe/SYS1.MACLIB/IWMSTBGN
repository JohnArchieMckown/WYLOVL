.***AC.PLV01  PROCESS PLISTVER                                      *   00067800
.********************************************************************   00067900
&XPLISTVER      SETA  0                                                 00068000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00068100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00068200
&XPLISTVER      SETA  0                                                 00068300
.ZPLV0          ANOP  ,                                                 00068400
.*                                                                      00068500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00068600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00068700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00068800
.*-------------------------------------------------------------------   00068900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00069000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00069100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00069200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00069300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00069400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069500
                AGO   .ZPVCK2                                           00069600
.ZPVCK3         ANOP  ,                                                 00069700
.*-------------------------------------------------------------------   00069800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00069900
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00070000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00070100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00070200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00070300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070400
                AGO   .ZPVCKE                                           00070500
.ZPVCK2         ANOP  ,                                                 00070600
.*-------------------------------------------------------------------   00070700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00070800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00070900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00071000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00071100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00071200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00071300
.ZPVCK6         ANOP  ,                                                 00071400
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00071500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00071600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00071700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00071800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071900
&XPLISTVER      SETA  0                                                 00072000
                AGO   .ZPVCKE                                           00072100
.*-------------------------------------------------------------------   00072200
.ZPVCK4A        ANOP  ,                                                 00072300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00072400
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00072500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00072600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00072700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00072800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00072900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00073000
                AGO   .ZPVCK5                                           00073100
.ZPVCK4         ANOP  ,                                                 00073200
&XPLISTVER      SETA  &PLISTVER(1)                                      00073300
.ZPVCK5         ANOP  ,                                                 00073400
.ZPVCKE         ANOP  ,                                                 00073500
.********************************************************************   00073600
