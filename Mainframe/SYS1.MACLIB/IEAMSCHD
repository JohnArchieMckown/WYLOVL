.***AC.PLV01  PROCESS PLISTVER                                      *   33584200
.********************************************************************   33584500
&XPLISTVER      SETA  0                                                 33584800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       33585100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   33585400
&XPLISTVER      SETA  2                                                 33585700
.ZPLV0          ANOP  ,                                                 33586000
.*                                                                      33586100
                AIF   ('&DUALPOOLTOKEN' EQ '').ZPLV1E                   33586200
                AIF   (&XPLISTVER GE 2).ZPLV1E                          33586300
&XPLISTVER      SETA  2                                                 33586400
.ZPLV1E         ANOP  ,                                                 33586500
.*                                                                      33586600
                AIF   ('&DUALPOOLTOKEN' EQ '').ZPLV2E                   33586700
                AIF   (&XPLISTVER GE 2).ZPLV2E                          33586800
&XPLISTVER      SETA  2                                                 33586900
.ZPLV2E         ANOP  ,                                                 33587000
.*                                                                      33587100
                AIF   ('&DUALPOOLTOKEN' EQ '').ZPLV3E                   33587200
                AIF   (&XPLISTVER GE 2).ZPLV3E                          33587300
&XPLISTVER      SETA  2                                                 33587400
.ZPLV3E         ANOP  ,                                                 33587500
.*                                                                      33587600
                AIF   ('&DUALPOOLTOKEN' EQ '').ZPLV4E                   33587700
                AIF   (&XPLISTVER GE 2).ZPLV4E                          33587800
&XPLISTVER      SETA  2                                                 33587900
.ZPLV4E         ANOP  ,                                                 33588000
.*                                                                      33588100
                AIF   ('&SYNCH' EQ '').ZPLV5E                           33588200
                AIF   (&XPLISTVER GE 1).ZPLV5E                          33588300
&XPLISTVER      SETA  1                                                 33588400
.ZPLV5E         ANOP  ,                                                 33588500
.*                                                                      33588600
                AIF   ('&SYNCHCOMPADDR' EQ '').ZPLV6E                   33588700
                AIF   (&XPLISTVER GE 1).ZPLV6E                          33588800
&XPLISTVER      SETA  1                                                 33588900
.ZPLV6E         ANOP  ,                                                 33589000
.*                                                                      33589100
                AIF   ('&SYNCHCODEADDR' EQ '').ZPLV7E                   33589200
                AIF   (&XPLISTVER GE 1).ZPLV7E                          33589300
&XPLISTVER      SETA  1                                                 33589400
.ZPLV7E         ANOP  ,                                                 33589500
.*                                                                      33589600
                AIF   ('&SYNCHRSNADDR' EQ '').ZPLV8E                    33589700
                AIF   (&XPLISTVER GE 1).ZPLV8E                          33589800
&XPLISTVER      SETA  1                                                 33589900
.ZPLV8E         ANOP  ,                                                 33590000
.*                                                                      33590100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      33590200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      33590300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  33590400
.*-------------------------------------------------------------------   33590500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 33590600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                33590700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 33590800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 33590900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               33591000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                33591100
                AGO   .ZPVCK2                                           33591200
.ZPVCK3         ANOP  ,                                                 33591300
.*-------------------------------------------------------------------   33591400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  33591500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                33591600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 33591700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         33591800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 33591900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                33592000
                AGO   .ZPVCKE                                           33592300
.ZPVCK2         ANOP  ,                                                 33592600
.*-------------------------------------------------------------------   33592900
&ZCVAL          SETC  '&PLISTVER(1)'                                    33597100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             33597400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             33597700
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        33597900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      33598100
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     33598300
.ZPVCK6         ANOP  ,                                                 33598600
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                33598900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 33599200
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         33599500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         33599800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                33600100
&XPLISTVER      SETA  2                                                 33600400
                AGO   .ZPVCKE                                           33600700
.*-------------------------------------------------------------------   33601000
.ZPVCK4A        ANOP  ,                                                 33601300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        33601600
