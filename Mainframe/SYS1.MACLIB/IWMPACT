.***AC.PLV01  PROCESS PLISTVER                                      *   00070200
.********************************************************************   00070300
&XPLISTVER      SETA  0                                                 00070400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00070500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00070600
&XPLISTVER      SETA  0                                                 00070700
.ZPLV0          ANOP  ,                                                 00070800
.*                                                                      00070900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00071000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00071100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00071200
.*-------------------------------------------------------------------   00071300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00071400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00071500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00071600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00071700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00071800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071900
                AGO   .ZPVCK2                                           00072000
.ZPVCK3         ANOP  ,                                                 00072100
.*-------------------------------------------------------------------   00072200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00072300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00072400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00072500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00072600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00072700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00072800
                AGO   .ZPVCKE                                           00072900
.ZPVCK2         ANOP  ,                                                 00073000
.*-------------------------------------------------------------------   00073100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00073200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00073300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00073400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00073500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00073600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00073700
.ZPVCK6         ANOP  ,                                                 00073800
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00073900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00074000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00074100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00074200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00074300
&XPLISTVER      SETA  0                                                 00074400
                AGO   .ZPVCKE                                           00074500
.*-------------------------------------------------------------------   00074600
.ZPVCK4A        ANOP  ,                                                 00074700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00074800
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00074900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00075000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00075100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00075200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00075300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00075400
                AGO   .ZPVCK5                                           00075500
.ZPVCK4         ANOP  ,                                                 00075600
&XPLISTVER      SETA  &PLISTVER(1)                                      00075700
.ZPVCK5         ANOP  ,                                                 00075800
.ZPVCKE         ANOP  ,                                                 00075900
