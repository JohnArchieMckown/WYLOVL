.*                                                                      00135800
.ZI3E                 ANOP  ,                                           00135900
.*-------------------------------------------------------------------   00136000
&ZOPN1                SETA &ZOPN                                        00136100
.*******************************************************************    00136200
.***  PROCESS IWMPACT OUTPUT PARAMETERS                                 00136300
.*******************************************************************    00136400
                      ACTR  10000                                       00136500
.*------------------------------------------------------------------    00136600
.***AO.L02 XSYSTEM_NAME CL8                                             00136700
         AIF   ('&ZXSYSTEM_NAME' EQ 'OUTPUT').ZO4A  OUTPUT              00136800
         AIF   ('&ZXSYSTEM_NAME' NE 'INOUT').ZO4C  INOUT                00136900
.ZO4A    AIF   ('&XSYSTEM_NAME'(1,1) NE '(').ZO4B  ADDR IN GPR          00137000
&ZCGPR   SETC  '&XSYSTEM_NAME'(2,K'&XSYSTEM_NAME-2)                     00137100
&ZOPN                 SETA  &ZOPN+1                                     00137200
&ZOPL(&ZOPN)          SETC  ' '                                         00137300
&ZOPO(&ZOPN)          SETC  'MVC'                                       00137400
&ZOPV(&ZOPN)          SETC  '0(8,&ZCGPR),12(1)'                         00137500
&ZOPD(&ZOPN)          SETC  '++ XSYSTEM_NAME CL8'                       00137600
                      AGO   .ZO4C                                       00137700
.*                                                                      00137800
.ZO4B                 ANOP  ,                                           00137900
&ZAWRK   SETA  K'&XSYSTEM_NAME                                          00138000
         AIF   ('&XSYSTEM_NAME'(&ZAWRK,1) NE ')').ZO4G                  00138100
&ZN      SETA  2                                                        00138200
.ZO4H    ANOP                                                           00138300
         AIF   (&ZN GE &ZAWRK).ZO4G                                     00138400
         AIF   ('&XSYSTEM_NAME'(&ZN,1) NE '(').ZO4I                     00138500
&ZC      SETC  '&XSYSTEM_NAME'(1,&ZN).'8'.','                           00138600
&ZC      SETC  '&ZC'.'&XSYSTEM_NAME'(&ZN+1,&ZAWRK-&ZN).',12(1)'         00138700
         AGO   .ZO4J                                                    00138800
.ZO4I    ANOP                                                           00138900
&ZN      SETA  &ZN+1                                                    00139000
         AGO   .ZO4H                                                    00139100
.ZO4G    ANOP                                                           00139200
&ZC      SETC  '&XSYSTEM_NAME.(8),12(1)'                                00139300
.ZO4J    ANOP                                                           00139400
&ZOPN                 SETA  &ZOPN+1                                     00139500
&ZOPL(&ZOPN)          SETC  ' '                                         00139600
&ZOPO(&ZOPN)          SETC  'MVC'                                       00139700
&ZOPV(&ZOPN)          SETC  '&ZC'                                       00139800
&ZOPD(&ZOPN)          SETC  '++ XSYSTEM_NAME CL8'                       00139900
.ZO4C                 ANOP  ,                                           00140000
.*******************************************************************    00140100
.***AO.X02  RETURN CODE PROCESSING                                      00140200
.*******************************************************************    00140300
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00140400
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00140500
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00140600
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00140700
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00140800
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00140900
&ZOPN                 SETA  &ZOPN+1                                     00141000
&ZOPL(&ZOPN)          SETC  ' '                                         00141100
&ZOPO(&ZOPN)          SETC  'ST'                                        00141200
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00141300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00141400
                      AGO   .ZMRETX                                     00141500
.*                                                                      00141600
.ZMRET2               ANOP  ,                                           00141700
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00141800
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00141900
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00142000
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00142100
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00142200
&ZOPN                 SETA  &ZOPN+1                                     00142300
&ZOPL(&ZOPN)          SETC  ' '                                         00142400
&ZOPO(&ZOPN)          SETC  'LR'                                        00142500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00142600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00142700
.ZMRETX               ANOP  ,                                           00142800
.*******************************************************************    00142900
.***AO.X03  REASON CODE PROCESSING                                      00143000
.*******************************************************************    00143100
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00143200
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00143300
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00143400
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00143500
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00143600
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00143700
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00143800
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00143900
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00144000
&ZOPN                 SETA  &ZOPN+1                                     00144100
&ZOPL(&ZOPN)          SETC  ' '                                         00144200
&ZOPO(&ZOPN)          SETC  'ST'                                        00144300
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00144400
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00144500
                      AGO   .ZMRSNX                                     00144600
.*                                                                      00144700
.ZMRSN2               ANOP  ,                                           00144800
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00144900
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00145000
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00145100
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00145200
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00145300
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00145400
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00145500
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00145600
&ZOPN                 SETA  &ZOPN+1                                     00145700
&ZOPL(&ZOPN)          SETC  ' '                                         00145800
&ZOPO(&ZOPN)          SETC  'LR'                                        00145900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00146000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00146100
.ZMRSNX               ANOP  ,                                           00146200
.*******************************************************************    00146300
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00146400
.*******************************************************************    00146500
.ZMFLX                ANOP  ,              MF=L PROCESSING              00146600
.*******************************************************************    00146700
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00146800
.*******************************************************************    00146900
                      ACTR  10000                                       00147000
.*                                                                      00147100
                      LCLA  &ZLC           COLUMN POSITION              00147200
                      LCLA  &ZOPX          LIST TABLE INDEX             00147300
                      LCLC  &ZLL           LABEL                        00147400
                      LCLC  &ZLOP          OPERATION                    00147500
                      LCLC  &ZLV           VALUE                        00147600
                      LCLC  &ZLD           DESCRIPTION                  00147700
.*                                                                      00147800
&ZOPX                 SETA  1                                           00147900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00148000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00148100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00148200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00148300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00148400
                      AIF   (K'&ZLD LE 0).ZL5                           00148500
&ZLC                  SETA  35             COMMENT COLUMN               00148600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00148700
&ZLC                  SETA  33             COMMMENT COLUMN              00148800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00148900
&ZLV                  SETC  '&ZLV'.' '                                  00149000
                      AGO   .ZL3                                        00149100
.*                                                                      00149200
.ZL4                  ANOP  ,                                           00149300
&ZLV                  SETC  '&ZLV'.' '                                  00149400
.ZL5                  ANOP  ,                                           00149500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00149600
.*                                                                      00149700
&ZLL     &ZLOP &ZLV                                                     00149800
.*                                                                      00149900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00150000
                      AGO   .ZL2           CONTINUE LOOPING             00150100
.*                                                                      00150200
.ZLX                  ANOP  ,                                           00150300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00150400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00150500
         L     14,16(0,0)              GET CVT ADDRESS                  00150600
         TM    CVTOSLV1-CVT(14),X'10'      TEST FOR SP520 RELEASE       00150700
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00150800
         LA    0,IWMRSNCODENOPOLMGT    SET REASON CODE = NO WLM         00150900
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00151000
         B     END&SYSNDX              BRANCH TO END LABEL              00151100
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00151200
         L     14,772(14,0)            GET SFT ADDRESS                  00151300
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00151400
         LA    15,34                   GET EX FOR SERVICE ROUTINE       00151500
         OR    14,15                   GET PC NUMBER                    00151600
         PC    0(14)                                                    00151700
END&SYSNDX DS    0H                    END LABEL                        00151800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00151900
.ZLINKX               ANOP  ,                                           00152000
.*******************************************************************    00152100
.***AP.X01  FORMAT GENERATED CODE                                       00152200
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00152300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00152400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00152500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00152600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00152700
                      AIF   (K'&ZLD LE 0).ZL51                          00152800
&ZLC                  SETA  35             COMMENT COLUMN               00152900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00153000
&ZLC                  SETA  33             COMMENT COLUMN               00153100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00153200
&ZLV                  SETC  '&ZLV'.' '                                  00153300
                      AGO   .ZL31                                       00153400
.*                                                                      00153500
.ZL41                 ANOP  ,                                           00153600
&ZLV                  SETC  '&ZLV'.' '                                  00153700
.ZL51                 ANOP  ,                                           00153800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00153900
.*                                                                      00154000
&ZLL     &ZLOP &ZLV                                                     00154100
.*                                                                      00154200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00154300
                      AGO   .ZL21          CONTINUE LOOPING             00154400
.*                                                                      00154500
.ZLX1                 ANOP  ,                                           00154600
