&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00129100
&ZOPD(&ZOPN)          SETC  '++ IWMPACT ARG PARMS'                      00129200
.*                                                                      00129300
.ZLMFSLX              ANOP  ,                                           00129400
.*-------------------------------------------------------------------   00129500
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00129600
&ZPLLEN               SETA 28                                           00129700
.*                                                                      00129800
.******************************************************************     00129900
.***AI.L01 IWMPACT FMT=LIST FILL IN PARAMETER AREA                      00130000
.******************************************************************     00130100
                      ACTR  10000                                       00130200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00130300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00130400
&ZCVAL                SETC  'LAE'          XAX MODE                     00130500
.ZIMFXA               ANOP ,                                            00130600
.*                                                                      00130700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00130800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00130900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00131000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00131100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00131200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00131300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00131400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00131500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00131600
&ZOPN                 SETA  &ZOPN+1                                     00131700
&ZOPL(&ZOPN)          SETC  ' '                                         00131800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00131900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00132000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00132100
                      AGO   .ZIMFNX                                     00132200
.*                                                                      00132300
.ZIMFN2               ANOP ,                                            00132400
&ZOPN                 SETA  &ZOPN+1                                     00132500
&ZOPL(&ZOPN)          SETC  ' '                                         00132600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00132700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00132800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00132900
.ZIMFNX               ANOP ,                                            00133000
.********************************************************************   00133100
.***AI.L02 IWMPACT FMT=LIST INITIALIZE PARAMETER AREA                   00133200
.********************************************************************   00133300
.***AI.L03 MF=(SLE)                                                     00133400
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00133500
.*                                                                      00133600
