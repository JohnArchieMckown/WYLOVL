&ZOPV(&ZOPN)          SETC  '0(&ZPLLEN,1),0(1)'                         00134000
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00134100
.ZIMFLX               ANOP  ,                                           00134200
.*-------------------------------------------------------------------   00134300
.***AI.L25  XPOLICY_NAME CL8                                            00134400
                      AIF   ('&ZXPOLICY_NAME' EQ '').ZI3E               00134500
                      AIF   ('&ZXPOLICY_NAME' EQ 'OUTPUT').ZI3E         00134600
&ZCGPR                SETC  '&XPOLICY_NAME'                             00134700
.*                                                                      00134800
                      AIF   ('&XPOLICY_NAME'(1,1) NE '(').ZI3B          00134900
&ZCGPR   SETC  '0('.'&XPOLICY_NAME'(2,K'&XPOLICY_NAME-2).')'            00135000
.ZI3B                 ANOP  ,                                           00135100
.*                                                                      00135200
&ZOPN                 SETA  &ZOPN+1                                     00135300
