&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00160900
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00161000
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00161100
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00161200
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00161300
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00161400
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00161500
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00161600
&ZOPN                 SETA  &ZOPN+1                                     00161700
&ZOPL(&ZOPN)          SETC  ' '                                         00161800
&ZOPO(&ZOPN)          SETC  'LR'                                        00161900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00162000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00162100
.ZMRSNX               ANOP  ,                                           00162200
