.*******************************************************************    00150800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00150900
.*******************************************************************    00151000
                      ACTR  10000                                       00151100
.*                                                                      00151200
                      LCLA  &ZLC           COLUMN POSITION              00151300
                      LCLA  &ZOPX          LIST TABLE INDEX             00151400
                      LCLC  &ZLL           LABEL                        00151500
                      LCLC  &ZLOP          OPERATION                    00151600
                      LCLC  &ZLV           VALUE                        00151700
                      LCLC  &ZLD           DESCRIPTION                  00151800
.*                                                                      00151900
&ZOPX                 SETA  1                                           00152000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00152100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00152200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00152300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00152400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00152500
                      AIF   (K'&ZLD LE 0).ZL5                           00152600
&ZLC                  SETA  35             COMMENT COLUMN               00152700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00152800
&ZLC                  SETA  33             COMMMENT COLUMN              00152900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00153000
&ZLV                  SETC  '&ZLV'.' '                                  00153100
                      AGO   .ZL3                                        00153200
.*                                                                      00153300
.ZL4                  ANOP  ,                                           00153400
&ZLV                  SETC  '&ZLV'.' '                                  00153500
.ZL5                  ANOP  ,                                           00153600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00153700
.*                                                                      00153800
&ZLL     &ZLOP &ZLV                                                     00153900
.*                                                                      00154000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00154100
                      AGO   .ZL2           CONTINUE LOOPING             00154200
.*                                                                      00154300
.ZLX                  ANOP  ,                                           00154400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00154500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00154600
.*-----------BAL invocation code-------------                           00154700
         LA    0,7                ++ SET IOCINFO FUNCTION CODE          00154800
         AIF    ('&XLINKAGE' NE 'BRANCH').LNKPC1                        00154900
.*********************************************************************  00155000
.*                                                                   *  00155100
.*  Get addressability to CVT                                        *  00155200
.*                                                                   *  00155300
.*********************************************************************  00155400
         L     15,16(0,0)                                               00155500
.*********************************************************************  00155600
.*                                                                   *  00155700
.*  Get addressability to IOCOM                                      *  00155800
.*                                                                   *  00155900
.*********************************************************************  00156000
         L     15,124(15,0)                                             00156100
.*********************************************************************  00156200
.*                                                                   *  00156300
.*  Get addressability to IOVT                                       *  00156400
.*                                                                   *  00156500
.*********************************************************************  00156600
         L     15,208(15,0)                                             00156700
         L     15,12(15,0)        ++ SET ADDR OF UCB SERVICES           00156800
         BALR  14,15              ++ BRANCH TO UCB SERVICES             00156900
         AGO   .DONE1                                                   00157000
.LNKPC1  ANOP                                                           00157100
         L     14,16(0,0)         ++ GET CVT ADDRESS                    00157200
         L     14,772(14,0)       ++ GET SFT ADDRESS                    00157300
.*********************************************************************  00157400
.*                                                                   *  00157500
.*  Get LX/EX for UCB Services                                       *  00157600
.*                                                                   *  00157700
.*  Note: LX/EX is calculated by multiplying the PC number (79)      *  00157800
.*        times the length of a PC/AUTH table entry (4) and          *  00157900
.*        then subtracting 4.                                        *  00158000
.*                                                                   *  00158100
.*********************************************************************  00158200
         L     14,312(14,0)       ++ GET LX/EX FOR UCB SERVICES         00158300
         PC    0(14)              ++ PC TO UCB SERVICES                 00158400
.DONE1   ANOP                                                           00158500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00158600
.ZLINKX               ANOP  ,                                           00158700
.*******************************************************************    00158800
.***AP.X01  FORMAT GENERATED CODE                                       00158900
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00159000
