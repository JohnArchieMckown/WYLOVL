.***AB.02  GLOBAL VARIABLES TO CONTROL PRINT AND CODE GENERATION        00036300
.*******************************************************************    00036400
              GBLC  &ZPRINT     YES=PRINT NO=NO PRINT                   00036500
              GBLC  &ZTEST      YES=TEST NO=NO TEST                     00036600
              GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX             00036700
.*                                                                      00036800
              GBLC  &ZIOCINFO   YES=MACRO USED AT LEAST ONCE            00036900
.*******************************************************************    00037000
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00037100
.*******************************************************************    00037200
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00037300
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00037400
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00037500
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00037600
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00037700
              LCLA  &ZAWRK      USED AS A WORK AREA                     00037800
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00037900
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00038000
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00038100
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00038200
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00038300
              LCLA  &ZMACRET    MACRO RETURN CODE                       00038400
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00038500
.*                                                                      00038600
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00038700
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00038800
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00038900
.********************************************************************   00039000
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00039100
.********************************************************************   00039200
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00039300
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00039400
.*                                                                      00039500
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00039600
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00039700
              SPLEVEL  TEST                                             00039800
.ZSYS2        ANOP  ,                                                   00039900
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00040000
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00040100
              SYSSTATE TEST                                             00040200
              AGO   .ZSYS4                                              00040300
.ZSYS3        ANOP  ,                                                   00040400
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00040500
.ZSYS4        ANOP  ,                                                   00040600
.*******************************************************************    00040700
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00040800
.*******************************************************************    00040900
              LCLC  &XIOCTOKEN  ++ NAME                                 00041000
              LCLC  &XMAXMBI    ++ NAME                                 00041100
              LCLC  &XIOCAREA   ++ NAME                                 00041200
              LCLC  &XUCBSTOR   ++ NAME                                 00041300
              LCLC  &XLINKAGE   ++ VALUE                                00041400
              LCLC  &XRETCODE   ++ NAME                                 00041500
              LCLC  &XRSNCODE   ++ NAME                                 00041600
              LCLC  &XMF        ++ VALUE                                00041700
              LCLC  &XMFCTRL    ++ NAME                                 00041800
              LCLC  &XMFATTR    ++ VALUE                                00041900
              LCLC  &XMFSCK     ++ VALUE                                00042000
.*******************************************************************    00042100
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00042200
.*******************************************************************    00042300
              LCLC  &ZXIOCTOKEN                 ++ OUTPUT NAME          00042400
              LCLC  &ZXMAXMBI   ++ OUTPUT NAME                          00042500
              LCLC  &ZXIOCAREA  ++ OUTPUT NAME                          00042600
              LCLC  &ZXUCBSTOR  ++ OUTPUT NAME                          00042700
              LCLC  &ZXLINKAGE  ++ INPUT VALUE                          00042800
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00042900
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00043000
              LCLC  &ZXMF       ++ INPUT VALUE                          00043100
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00043200
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00043300
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00043400
.********************************************************************   00043500
.***AG.MF01 PROCESS MF MACRO FORMS                                      00043600
.********************************************************************   00043700
              ACTR  10000                                               00043800
.*------------------------------------------------------------------    00043900
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00044000
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00044100
.*-------------------------------------------------------------------   00044200
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00044300
.ZG26         ANOP  ,                                                   00044400
&ZTLC         SETC 'S'                                                  00044500
              AIF   ('&MF(1)' NE '&ZTLC').ZG27                          00044600
&XMF          SETC  'S'         SET KEYWORD                             00044700
              AIF   (N'&MF LE 1).ZG26A          CK NUM                  00044800
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00044900
&ZMS          SETC  '&ZMS.&MF"'                                         00045000
&ZMS          SETC  '&ZMS "MF"'                                         00045100
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00045200
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00045300
.ZG26A        ANOP  ,                                                   00045400
.*-------------------------------------------------------------------   00045500
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00045600
.ZG27         ANOP  ,                                                   00045700
&ZTLC         SETC 'L'                                                  00045800
              AIF   ('&MF(1)' NE '&ZTLC').ZG34                          00045900
&XMF          SETC  'L'         SET KEYWORD                             00046000
              AIF   (N'&MF LE 3).ZG27A          CK NUM                  00046100
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00046200
&ZMS          SETC  '&ZMS.&MF"'                                         00046300
&ZMS          SETC  '&ZMS "MF"'                                         00046400
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00046500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00046600
.ZG27A        ANOP  ,                                                   00046700
.*-------------------------------------------------------------------   00046800
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00046900
              AIF   (N'&SYSLIST NE 0).ZG27B     POSITIONALS?            00047000
              AIF   ('&IOCTOKEN' NE '').ZG27B   KEYS?                   00047100
              AIF   ('&MAXMBI' NE '').ZG27B     KEYS?                   00047200
              AIF   ('&IOCAREA' NE '').ZG27B    KEYS?                   00047300
              AIF   ('&UCBSTOR' NE '').ZG27B    KEYS?                   00047400
              AIF   ('&LINKAGE' NE '').ZG27B    KEYS?                   00047500
              AIF   ('&RETCODE' NE '').ZG27B    KEYS?                   00047600
              AIF   ('&RSNCODE' NE '').ZG27B    KEYS?                   00047700
              AGO    .ZG27C     INVOCATION OKAY                         00047800
.ZG27B        ANOP  ,                                                   00047900
&ZMS          SETC  'THE "L" FORM OF "MF" CANNOT'  *MFLA* MNOTE         00048000
&ZMS          SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                   00048100
&ZMS          SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'              00048200
              MNOTE 8,' &ZMS ON THE INVOCATION.'                        00048300
