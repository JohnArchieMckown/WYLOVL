*DECLARE 1 UTILP BDY(WORD),        /* PARAMETER LIST FOR ARCUTIL     */ 09850000
*      5 UPID CHAR(8),             /* PARM LIST ID                   */ 09900000
*      5 UPVERS FIXED(8),          /* PARM LIST VERSION              */ 09950000
*      5 UPRECORD BIT(8),          /* REQUESTED RECORDS              */ 10000000
*        10 UPREMIGR BIT(1), /* ON IF UTILITY IS TO                     10050000
*                                           CREATE MIGRATED DATA SET    10100000
*                                           INFORMATION RECORDS      */ 10150000
*        10 UPREBACK BIT(1), /* ON IF UTILITY IS TO                     10200000
*                                           CREATE BACKED UP DATA SET   10250000
*                                           INFORMATION RECORDS      */ 10300000
*        10 UPREDASD BIT(1), /* ON IF UTILITY IS TO                     10350000
*                                           CREATE DASD CAPACITY        10400000
*                                           PLANNING RECORDS         */ 10450000
*        10 UPRETAPE BIT(1), /* ON IF UTILITY IS TO                     10500000
*                                           CREATE TAPE CAPACITY        10550000
*                                           PLANNING RECORDS         */ 10600000
*        10 * BIT(4),                   /* (RESERVED)                */ 10650000
*      5 * CHAR(1),                /* RESERVED                       */ 10700000
*      5 UPOPTION  BIT(8),         /* PROCESSING OPTIONS             */ 10750000
*        10 UPSPALL             BIT(1), /* ON IF SNAP PROCESSING        10800000
*                                           IS ALWAYS REQUESTED.     */ 10850000
*        10 UPSPERR             BIT(1), /* ON IF SNAP PROCESSING        10900000
*                                           IS REQUESTED FOR ERRORS. */ 10950000
*        10 * BIT(6),                   /* (RESERVED)                */ 11000000
*      5 UPOUTDCB PTR(31), /* ADDRESS OF OUTPUT DS DCB               */ 11050000
*      5 UPSTAMP CHAR(8),           /* TIME AND DATE STRUCTURE       */ 11100000
*        7 UPHTIME CHAR(4),         /* TIME STAMP: HHMMSSTH          */ 11150000
*        7 UPHDATE CHAR(4),         /* DATE STAMP: YYYYDDDF          */ 11200000
*      5 UPEXITP PTR(31),          /* ADDRESS OF USER EXIT ENTRY @02A*/ 11250000
*      5 UPEXAREA PTR(31),         /* ADDRESS OF CONTROL AREA    @02A*/ 11300000
*      5 * CHAR(12),               /* (RESERVED)                 @02C*/ 11350000
*      5 UPNUMIGR FIXED(31), /* NUMBER OF TYPE 'M' RECORDS           */ 11400000
*      5 UPNUBACK    FIXED(31), /* NUMBER OF TYPE 'B' RECORDS        */ 11450000
*      5 UPNUDASD    FIXED(31), /* NUMBER OF TYPE 'C' RECORDS        */ 11500000
*      5 UPNUTAPE    FIXED(31), /* NUMBER OF TYPE 'T' RECORDS        */ 11550000
