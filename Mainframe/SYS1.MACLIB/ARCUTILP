*DECLARE                                                                16100000
*/* BACKUP VERSION INFORMATION (RECORD TYPE 'B')             @YA29290*/ 16150000
* 1 UBBDSI DEFINED(DCUDATA), /* BACKUP DATA SET INFORMATION             16200000
*                                    (DEFINED ON DCUDATA)            */ 16250000
*   2 UBDSNAM CHAR(44),          /* DATA SET NAME                    */ 16300000
