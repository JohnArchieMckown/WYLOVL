* %BPXL1 :  ;                      /* PL/X mapping                   */ 18950000
 %DCL PFX CHAR; /* Short form of prefix           */                    19000000
 %PFX = TIOSPFX;                   /* Set short form value           */ 19050000
 %IF TIOSBASE='' %THEN             /* If base not specified...       */ 19100000
   %DO;                                                                 19150000
     %TIOSBASE='BASED(TiosPtr)';   /*   set default base             */ 19200000
   %END;                                                                19250000
  %IF TIOSBASE='BASED(TiosPtr)' %THEN  /* If default base            */ 19300000
    %DO;                           /* Declare the base pointer       */ 19350000
      DCL TiosPtr Ptr(31);         /* Pointer to the PPRP            */ 19400000
    %END;                                                               19450000
