       TIOSBASE,                    /* termios structure             */ 19800000
     5 PFX%%c_cflag bit(32),        /*    Control Flags              */ 19850000
       10 PFX%%c_cRsvdSpd   Bit(16),/* Reserved for baud rates       */ 19900000
          15 PFX%%c_Ospeed  Bit(08),/* Output Baud rate          @P3A*/ 19950000
          15 PFX%%c_Ispeed  Bit(08),/* Input Baud rate           @P3A*/ 20000000
       10 PFX%%c_cRsvd      Bit(01),/* Reserved                  @D5C*/ 20050000
