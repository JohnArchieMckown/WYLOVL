                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00152000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00152100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00152200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00152300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00152400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00152500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00152600
&ZOPN                 SETA  &ZOPN+1                                     00152700
&ZOPL(&ZOPN)          SETC  ' '                                         00152800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00152900
