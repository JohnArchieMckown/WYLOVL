.***AB.02  GLOBAL VARIABLES TO CONTROL PRINT AND CODE GENERATION        00045200
.*******************************************************************    00045300
              GBLC  &ZPRINT     YES=PRINT NO=NO PRINT                   00045400
              GBLC  &ZTEST      YES=TEST NO=NO TEST                     00045500
              GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX             00045600
.*                                                                      00045700
              GBLC  &ZIXCSSMO   YES=MACRO USED AT LEAST ONCE            00045800
.*******************************************************************    00045900
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00046000
.*******************************************************************    00046100
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00046200
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00046300
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00046400
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00046500
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00046600
              LCLA  &ZAWRK      USED AS A WORK AREA                     00046700
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00046800
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00046900
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00047000
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00047100
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00047200
              LCLA  &ZMACRET    MACRO RETURN CODE                       00047300
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00047400
.*                                                                      00047500
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00047600
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00047700
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00047800
              LCLC  &ZENDBASEPL                 End of base PL          00047900
              LCLA  &ZPLWORKA   PARAMETER LIST LENGTH                   00048000
.*******************************************************************    00048100
.***AB.04  MACRO VARIABLES FOR RANGES AND SETS OF ARGUMENTS             00048200
.*******************************************************************    00048300
              LCLA  &ZI         INDEX FOR ARRAY                         00048400
              LCLA  &ZJ         INDEX FOR PARMS                         00048500
              LCLA  &ZK         INDEX FOR PARSE                         00048600
              LCLA  &ZLO        LOW RANGE VALUE                         00048700
              LCLA  &ZHI        HIGH RANGE VALUE                        00048800
              LCLC  &ZW         WORK AREA FOR PARM                      00048900
.********************************************************************   00049000
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00049100
.********************************************************************   00049200
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00049300
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00049400
.*                                                                      00049500
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00049600
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00049700
              SPLEVEL  TEST                                             00049800
.ZSYS2        ANOP  ,                                                   00049900
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00050000
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00050100
              SYSSTATE TEST                                             00050200
              AGO   .ZSYS4                                              00050300
.ZSYS3        ANOP  ,                                                   00050400
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00050500
.ZSYS4        ANOP  ,                                                   00050600
.*******************************************************************    00050700
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00050800
.*******************************************************************    00050900
              LCLC  &XCMPTOKEN  ++ NAME                                 00051000
              LCLC  &XMSGBUF    ++ NAME                                 00051100
              LCLC  &XMSGLEN    ++ NAME                                 00051200
              LCLC  &XMSGCNTL   ++ NAME                                 00051300
              LCLC  &XSENDTO    ++ VALUE                                00051400
              LCLC  &XTARGET    ++ NAME                                 00051500
              LCLC  &XTARGETCT  ++ NAME                                 00051600
              LCLC  &XRETCODE   ++ NAME                                 00051700
              LCLC  &XRSNCODE   ++ NAME                                 00051800
              LCLA  &XPLISTVER  ++ VALUE                                00051900
              LCLC  &XMF        ++ VALUE                                00052000
              LCLC  &XMFCTRL    ++ NAME                                 00052100
              LCLC  &XMFATTR    ++ VALUE                                00052200
              LCLC  &XMFSCK     ++ VALUE                                00052300
.*******************************************************************    00052400
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00052500
.*******************************************************************    00052600
              LCLC  &ZXCMPTOKEN                 ++ INPUT NAME           00052700
              LCLC  &ZXMSGBUF   ++ INPUT NAME                           00052800
              LCLC  &ZXMSGLEN   ++ INPUT NAME                           00052900
              LCLC  &ZXMSGCNTL  ++ INPUT NAME                           00053000
              LCLC  &ZXSENDTO   ++ INPUT VALUE                          00053100
              LCLC  &ZXTARGET   ++ INPUT NAME                           00053200
              LCLC  &ZXTARGETCT                 ++ INPUT NAME           00053300
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00053400
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00053500
              LCLC  &ZXPLISTVER                 ++ INPUT VALUE          00053600
              LCLC  &ZXMF       ++ INPUT VALUE                          00053700
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00053800
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00053900
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00054000
.********************************************************************   00054100
.***AC.PLV01  PROCESS PLISTVER                                      *   00054200
.********************************************************************   00054300
&XPLISTVER      SETA  0                                                 00054400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00054500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00054600
&XPLISTVER      SETA  0                                                 00054700
.ZPLV0          ANOP  ,                                                 00054800
.*                                                                      00054900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00055000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00055100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00055200
.*-------------------------------------------------------------------   00055300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00055400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00055500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00055700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00055800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00055900
                AGO   .ZPVCK2                                           00056000
.ZPVCK3         ANOP  ,                                                 00056100
.*-------------------------------------------------------------------   00056200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00056300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00056400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00056500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00056600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00056700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056800
                AGO   .ZPVCKE                                           00056900
.ZPVCK2         ANOP  ,                                                 00057000
.*-------------------------------------------------------------------   00057100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00057200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00057300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00057400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00057500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00057600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00057700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00057800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057900
                AGO   .ZPVCK5                                           00058000
.ZPVCK4         ANOP  ,                                                 00058100
&XPLISTVER      SETA  &PLISTVER(1)                                      00058200
.ZPVCK5         ANOP  ,                                                 00058300
.*-------------------------------------------------------------------   00058400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00058500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00058600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00058700
                AIF   (&XPLISTVER LT 0).ZPVCK6  TOO LOW                 00058800
                AIF   (&XPLISTVER LE 0).ZPVCKE  NOT HIGH                00058900
.ZPVCK6         ANOP   ,                                                00059000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00059100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00059200
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00059300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00059400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059500
.ZPVCKE         ANOP   ,                                                00059600
.********************************************************************   00059700
