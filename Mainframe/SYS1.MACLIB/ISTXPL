         AIF   ('&ISTXPL' EQ 'NO').L0002                                03850000
         AIF   ('&DSECT' EQ 'YES').L0003                                03900000
         AIF   ('&ISTXPL' NE 'YES').L0002                               03950000
ISTXPL   DS    0X        Monitor Exit Parameter List                    04000000
         AGO   .L0004                                                   04050000
.L0003   ANOP                                                           04100000
ISTXPL   DSECT           Monitor Exit Parameter List                    04150000
.L0004   ANOP                                                           04200000
XPLHEAD  DS    0CL0024   Control Block Header                           04250000
XPLHEAD_LEN DS 1FL2      Entire XPL length in bytes - includes this    *04300000
                         field, header, and vectors                     04350000
XPLHEAD_HLEN DS 1FL2     XPL header length in bytes                     04400000
XPLHEAD_TRANS DS 1FL1    Reason for transmission                        04450000
XPLHEAD_CORR DS 1CL0002  Correlator, PRID from CNMRU, Zero if          *04500000
                         Unsolicited                                    04550000
XPLHEAD_MONACBN DS 1CL0008 Requesting monitor ACB name,                *04600000
                         Blanks if unsolcted                            04650000
XPLHEAD_FLAGS DS 0BL1    Flag byte                                      04700000
XPLHEAD_GEN EQU X'FC'    General Use                                    04750000
XPLHEAD_SMFIM EQU X'04'  SMF Interval Missed                            04800000
XPLHEAD_SEG EQU X'03'    Segmenting indicator                           04850000
XPLHEAD_FIRST EQU X'02'  First Segment indicator                        04900000
XPLHEAD_LAST EQU X'01'   Last Segment indicator                         04950000
         ORG   XPLHEAD_FLAGS+X'00000001'                                05000000
XPLHEAD_TIME DS 1CL0008  SMF interval time stamp for SMF data           05050000
XPLHEAD_END DS 0C        Begin First Vector                             05100000
ISTXPL_LEN EQU *-ISTXPL                                                 05150000
.L0002   ANOP                                                           05200000
