.***AC.PLV01  PROCESS PLISTVER                                      *   21021000
.********************************************************************   21050400
&XPLISTVER      SETA  0                                                 21079800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       21109200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   21138600
&XPLISTVER      SETA  0                                                 21168000
.ZPLV0          ANOP  ,                                                 21197400
.*                                                                      21226800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      21256200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      21285600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  21315000
.*-------------------------------------------------------------------   21344400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 21373800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                21403200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 21432600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 21462000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               21491400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                21520800
                AGO   .ZPVCK2                                           21550200
.ZPVCK3         ANOP  ,                                                 21579600
.*-------------------------------------------------------------------   21609000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  21638400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                21667800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 21697200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         21726600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 21756000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                21785400
                AGO   .ZPVCKE                                           21814800
.ZPVCK2         ANOP  ,                                                 21844200
.*-------------------------------------------------------------------   21873600
&ZCVAL          SETC  '&PLISTVER(1)'                                    21903000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             21932400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             21961800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        21991200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      22020600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     22050000
