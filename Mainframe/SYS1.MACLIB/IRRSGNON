.*************************************************************** 24@L2D 49600000
.*** END OF MACRO PARAMETER SYNTAX CHECKING                        @L2C 49650000
.**************************************************************** 3@L2D 49700000
.***                                                                    49750000
.*** START OF IRRSGNON MACRO CODE                                  @L2C 49800000
.***                                                                    49850000
.*******************************************************************    49900000
              LCLC  &M0M0001                  LABEL NAME                49950000
&M0M0001      SETC  '&XLABEL'                 SET LABEL                 50000000
              AIF   (K'&XLABEL NE 0).LABELOK  LABEL WAS SPECIFIED  @L2C 50050000
&M0M0001      SETC  'M00M&SYSNDX'             DEFAULT LABEL             50100000
.*                                                                      50150000
.LABELOK      ANOP  ,                                              @L2C 50200000
&M0M0001 DS    0H                             IRRSGNON-0                50250000
.*******************************************************************    50300000
.*** ENSURE PRINT IS ON (TO PRINT MACRO EXPANSION)                 @L2C 50350000
.*******************************************************************    50400000
         PUSH  PRINT                                                    50450000
         PRINT ON                                                       50500000
.*******************************************************************    50550000
.*** PRINT CONTROLS, DEFAULT IS YES                                     50600000
.*******************************************************************    50650000
              AIF   ('&ZPRINT' NE 'NO').PRINTON  PRINT IS ON       @L2C 50700000
              PRINT OFF         ++ PR01                                 50750000
.PRINTON      ANOP  ,                                              @L2C 50800000
.********************************************************************   50850000
.*** INTERNAL ARRAY TO PRODUCE FORMATTED CODE                           50900000
.********************************************************************   50950000
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       51000000
.*                                                                 @L2D 51050000
                      LCLC  &ZOPL(4095)    LABEL                        51100000
                      LCLC  &ZOPO(4095)    OPERATION                    51150000
                      LCLC  &ZOPV(4095)    VALUE                        51200000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  51250000
.*                                                                      51300000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     51350000
.******************************************************************     51400000
.*** IRRSGNON LIST PROCESSING FOR MF=S/L PARAMETERS.               @L2C 51450000
.*** SET ARRAY VARIABLES TO PRODUCE ASSEMBLER STATEMENTS FOR       @L2A 51500000
.*** BUILDING THE PARAMETER LIST.                                  @L2A 51550000
.******************************************************************     51600000
&XMFCTRL              SETC  'ICH'.'&SYSNDX'                             51650000
                      AIF   ('&XMF' EQ 'L').CONTLIST               @L2C 51700000
                      AIF   ('&XMF' NE 'S').PROCMFME               @L2C 51750000
         B     &XMFCTRL.S                                               51800000
.*                                                                      51850000
&ZOPN                 SETA  &ZOPN+1                                     51900000
&ZOPL(&ZOPN)          SETC  ' '                                         51950000
&ZOPO(&ZOPN)          SETC  'SPACE'                                     52000000
&ZOPV(&ZOPN)          SETC  '1'                                         52050000
&ZOPD(&ZOPN)          SETC  ' '                                         52100000
.*                                                                      52150000
.CONTLIST             ANOP  ,     CONTINUE LIST PROCESSING         @L2C 52200000
.***************************************************************** @L3A 52250000
.*** Set to fullword boundary to be sure parameter list does not   @L3A 52300000
.*** have bytes skipped in it before the first address declare.    @L3A 52350000
.*** This could happen if the plist started on a halfword          @L3A 52400000
.*** boundary but not a fullword boundary.  (Done for MF=L/S)      @L3A 52450000
.***************************************************************** @L3A 52500000
.*** ALIGN ON FULLWORD BOUNDARY FOR DECLARES                       @L3A 52550000
&ZOPN                 SETA &ZOPN+1                                 @L3A 52600000
&ZOPL(&ZOPN)          SETC '&ICHL'                                 @L3A 52650000
&ZOPO(&ZOPN)          SETC 'DS'                                    @L3A 52700000
&ZOPV(&ZOPN)          SETC '0F'                                    @L3A 52750000
&ZOPD(&ZOPN)          SETC '++ BOUNDARY ALIGNMENT'                 @L3A 52800000
.*************************************************************** 11@L2D 52850000
.*** SET DECLARES FOR PRODUCING FORMATTED CODE THAT WILL FILL IN   @L2A 52900000
.*** THE PARAMETER LIST WHEN MF=L/S.                               @L2A 52950000
.********************************************************************   53000000
.*** SET UP LISTLEN DECLARE STATEMENT                              @L2C 53050000
&ZOPN                 SETA &ZOPN+1                                      53100000
&ZOPL(&ZOPN)          SETC ' '                                     @L3C 53150000
&ZOPO(&ZOPN)          SETC 'DC'                                    @L4C 53200000
&ZOPV(&ZOPN)          SETC 'AL2(&XMFCTRL.L)'                       @L4C 53250000
&ZOPD(&ZOPN)          SETC '++ SIGPLLEN'                           @L2C 53300000
.*-------------------------------------------------------------------   53350000
.*** SET UP TYPE DECLARE STATEMENT                                 @L2C 53400000
&ZOPN                 SETA &ZOPN+1                                      53450000
&ZOPL(&ZOPN)          SETC ' '                                     @D2C 53500000
&ZOPO(&ZOPN)          SETC 'DC'                                    @P1C 53550000
&ZOPV(&ZOPN)          SETC 'FL1''&XTYPE'''                         @D2C 53600000
&ZOPD(&ZOPN)          SETC '++ &ZXTYPE  SIGTYPE'                   @L2C 53650000
.*-------------------------------------------------------------------   53700000
.*** SET UP LSTTYPE DECLARE STATEMENT                              @L2C 53750000
&ZOPN                 SETA &ZOPN+1                                      53800000
&ZOPL(&ZOPN)          SETC ' '                                          53850000
&ZOPO(&ZOPN)          SETC 'DC'                                    @P1C 53900000
&ZOPV(&ZOPN)          SETC 'FL1''&XLSTTYPE'''                      @D2C 53950000
&ZOPD(&ZOPN)          SETC '++ &ZXLSTTYPE  SIGLTYPE'               @L2C 54000000
.*-------------------------------------------------------------------   54050000
.*** SET UP APPL DECLARE STATEMENT                                 @L2C 54100000
&ZOPN                 SETA &ZOPN+1                                      54150000
&ZOPL(&ZOPN)          SETC ' '                                          54200000
&ZOPO(&ZOPN)          SETC 'DC'                                    @P1C 54250000
                      AIF  (('&XAPPL'(1,1) EQ '''')                    X54300000
                       AND ('&XMF' EQ 'L')).EXPL1    EXPLICIT MF=L @L3A 54350000
                      AIF  (('&XAPPL'(1,1) EQ '''')                    X54400000
                       AND ('&XMF' EQ 'S')).APPLSLOT EXPLICIT MF=S @L3A 54450000
                      AIF  ('&XAPPL'(1,1) EQ '(').APPLSLOT         @L3A 54500000
&ZOPV(&ZOPN)          SETC 'A(&XAPPL)'       PROCESS VARIABLE FORM @P3C 54550000
                      AGO  .APPLCONT                               @P4A 54600000
.EXPL1                ANOP ,                 PROCESS EXPLICIT FORM @L3C 54650000
ICH&SYSNDX DC    CL8&XAPPL                                              54700000
