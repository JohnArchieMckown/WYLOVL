.*******************************************************************    00404100
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00404200
.*******************************************************************    00404300
                      ACTR  10000                                       00404400
.*                                                                      00404500
                      LCLA  &ZLC           COLUMN POSITION              00404600
                      LCLA  &ZOPX          LIST TABLE INDEX             00404700
                      LCLC  &ZLL           LABEL                        00404800
                      LCLC  &ZLOP          OPERATION                    00404900
                      LCLC  &ZLV           VALUE                        00405000
                      LCLC  &ZLD           DESCRIPTION                  00405100
.*                                                                      00405200
&ZOPX                 SETA  1                                           00405300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00405400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00405500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00405600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00405700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00405800
                      AIF   (K'&ZLD LE 0).ZL5                           00405900
&ZLC                  SETA  35             COMMENT COLUMN               00406000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00406100
&ZLC                  SETA  33             COMMMENT COLUMN              00406200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00406300
&ZLV                  SETC  '&ZLV'.' '                                  00406400
                      AGO   .ZL3                                        00406500
.*                                                                      00406600
.ZL4                  ANOP  ,                                           00406700
&ZLV                  SETC  '&ZLV'.' '                                  00406800
.ZL5                  ANOP  ,                                           00406900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00407000
.*                                                                      00407100
&ZLL     &ZLOP &ZLV                                                     00407200
.*                                                                      00407300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00407400
                      AGO   .ZL2           CONTINUE LOOPING             00407500
.*                                                                      00407600
.ZLX                  ANOP  ,                                           00407700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00407800
