.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          81914300
&ZPLLEN               SETA 128                                          81931200
.*                                                                      81948100
.******************************************************************     81965000
.***AI.L01 IXLCSP FMT=LIST FILL IN PARAMETER AREA                       81981900
.******************************************************************     81998800
                      ACTR  10000                                       82015700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               82032600
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    82049500
&ZCVAL                SETC  'LAE'          XAX MODE                     82066400
.ZIMFXA               ANOP ,                                            82083300
.*                                                                      82100200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    82117100
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         82134000
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 82150900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                82167800
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 82184700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                82201600
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   82218500
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    82235400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   82252300
&ZOPN                 SETA  &ZOPN+1                                     82269200
&ZOPL(&ZOPN)          SETC  ' '                                         82286100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    82303000
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              82319900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       82336800
                      AGO   .ZIMFNX                                     82353700
.*                                                                      82370600
.ZIMFN2               ANOP ,                                            82387500
&ZOPN                 SETA  &ZOPN+1                                     82404400
&ZOPL(&ZOPN)          SETC  ' '                                         82421300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    82438200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                82455100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       82472000
.ZIMFNX               ANOP ,                                            82488900
.********************************************************************   82505800
.***AI.L02 IXLCSP FMT=LIST INITIALIZE PARAMETER AREA                    82522700
.********************************************************************   82539600
.***AI.L03 MF=(SLME)                                                    82556500
