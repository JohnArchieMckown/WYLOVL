.***AC.PLV01  PROCESS PLISTVER                                      *   24150100
.********************************************************************   24167000
&XPLISTVER      SETA  0                                                 24183900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       24200800
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   24217700
&XPLISTVER      SETA  1                                                 24234600
.ZPLV0          ANOP  ,                                                 24251500
.*                                                                      24254300
                AIF   ('&KEYTYPE' EQ '').ZPLV1E                         24257100
                AIF   (&XPLISTVER GE 1).ZPLV1E                          24259900
&XPLISTVER      SETA  1                                                 24262700
.ZPLV1E         ANOP  ,                                                 24265500
.*                                                                      24268400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      24285300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      24302200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  24319100
.*-------------------------------------------------------------------   24336000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 24352900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                24369800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 24386700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 24403600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               24420500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                24437400
                AGO   .ZPVCK2                                           24454300
.ZPVCK3         ANOP  ,                                                 24471200
.*-------------------------------------------------------------------   24488100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  24505000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                24521900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 24538800
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         24555700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 24572600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                24589500
                AGO   .ZPVCKE                                           24606400
.ZPVCK2         ANOP  ,                                                 24623300
.*-------------------------------------------------------------------   24640200
&ZCVAL          SETC  '&PLISTVER(1)'                                    24657100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             24674000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             24690900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        24707800
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      24724700
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     24741600
.ZPVCK6         ANOP  ,                                                 24758500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                24775400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 24792300
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         24809200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         24826100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                24843000
&XPLISTVER      SETA  1                                                 24859900
                AGO   .ZPVCKE                                           24876800
.*-------------------------------------------------------------------   24893700
.ZPVCK4A        ANOP  ,                                                 24910600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        24927500
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                24944400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               24961300
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          24978200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 24995100
                MNOTE 8,' &ZMS IS REQUIRED.'                            25012000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                25028900
                AGO   .ZPVCK5                                           25045800
.ZPVCK4         ANOP  ,                                                 25062700
&XPLISTVER      SETA  &PLISTVER(1)                                      25079600
.ZPVCK5         ANOP  ,                                                 25096500
.ZPVCKE         ANOP  ,                                                 25113400
