                      AGO   .ZL2           CONTINUE LOOPING             71700000
.*                                                                      71750000
.ZLX                  ANOP  ,                                           71800000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                71850000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  71900000
.*********************************************************************  71950000
.* Gets addressability to the IOVT and check to see if space-        *  72000000
.* switching PCs are enabled.  If not, return a return code 4 and    *  72050000
.* reason code 2 to the caller.                                      *  72100000
.*                                                                   *  72150000
.* Note: This handles the cases where a user may attempt to call     *  72200000
.*       IOSVIOSW at a point where the PC tables are not yet         *  72250000
.*       initialized                                                 *  72300000
.*********************************************************************  72350000
         L     15,16(0,0)         ++ GET CVT ADDRESS                    72400000
         L     14,124(15,0)       ++ GET IOCOM ADDRESS                  72450000
         L     14,208(14,0)       ++ GET IOVT ADDRESS                   72500000
         TM    5(14),B'00001000'  ++ CHECK IOVTSSPC FLAG                72550000
         BNZ   IOSW1&SYSNDX       ++ IF FLAG IS ON, ISSUE PC            72600000
         LA    15,4               ++ SET WARNING RETURN CODE            72650000
         LA    0,2                ++ SET WARNING REASON CODE            72700000
         B     IOSW2&SYSNDX       ++ BRANCH AROUND PC                   72750000
.*********************************************************************  72800000
.*                                                                   *  72850000
.*  PC linkage                                                       *  72900000
.*                                                                   *  72950000
.*********************************************************************  73000000
IOSW1&SYSNDX DS    0H                                                   73050000
         L     14,772(15,0)       ++ GET SFT ADDRESS                    73100000
.*********************************************************************  73150000
.*                                                                   *  73200000
.*  Get LX/EX for IOSWITCH Service                                   *  73250000
.*                                                                   *  73300000
.*  Note: LX/EX is calculated by multiplying the PC number (129)     *  73350000
.*        times the length of a PC/AUTH table entry (4) and          *  73400000
.*        then subtracting 4.                                        *  73450000
.*                                                                   *  73500000
.*********************************************************************  73550000
         L     14,512(14,0)       ++ GET LX/EX FOR IOSWITCH Service     73600000
         PC    0(14)              ++ PC TO IOSVIOSW                     73650000
         LA    1,#Z&SYSNDX.A                                            73700000
IOSW2&SYSNDX DS    0H                                                   73750000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  73800000
.ZLINKX               ANOP  ,                                           73850000
.*******************************************************************    73900000
.***AP.X01  FORMAT GENERATED CODE                                       73950000
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            74000000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              74050000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              74100000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              74150000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              74200000
                      AIF   (K'&ZLD LE 0).ZL51                          74250000
&ZLC                  SETA  35             COMMENT COLUMN               74300000
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   74350000
&ZLC                  SETA  33             COMMENT COLUMN               74400000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  74450000
&ZLV                  SETC  '&ZLV'.' '                                  74500000
                      AGO   .ZL31                                       74550000
.*                                                                      74600000
.ZL41                 ANOP  ,                                           74650000
&ZLV                  SETC  '&ZLV'.' '                                  74700000
.ZL51                 ANOP  ,                                           74750000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               74800000
.*                                                                      74850000
&ZLL     &ZLOP &ZLV                                                     74900000
.*                                                                      74950000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               75000000
                      AGO   .ZL21          CONTINUE LOOPING             75050000
.*                                                                      75100000
.ZLX1                 ANOP  ,                                           75150000
