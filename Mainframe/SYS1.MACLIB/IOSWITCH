.***AC.PLV01  PROCESS PLISTVER                                      *   20500000
.********************************************************************   20550000
&XPLISTVER      SETA  0                                                 20600000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       20650000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   20700000
&XPLISTVER      SETA  0                                                 20750000
.ZPLV0          ANOP  ,                                                 20800000
.*                                                                      20850000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      20900000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      20950000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  21000000
.*-------------------------------------------------------------------   21050000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 21100000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                21150000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 21200000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 21250000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               21300000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                21350000
                AGO   .ZPVCK2                                           21400000
.ZPVCK3         ANOP  ,                                                 21450000
.*-------------------------------------------------------------------   21500000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  21550000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                21600000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 21650000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         21700000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 21750000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                21800000
                AGO   .ZPVCKE                                           21850000
.ZPVCK2         ANOP  ,                                                 21900000
.*-------------------------------------------------------------------   21950000
&ZCVAL          SETC  '&PLISTVER(1)'                                    22000000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             22050000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             22100000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        22150000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      22200000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     22250000
.ZPVCK6         ANOP  ,                                                 22300000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                22350000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 22400000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         22450000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         22500000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                22550000
&XPLISTVER      SETA  0                                                 22600000
                AGO   .ZPVCKE                                           22650000
.*-------------------------------------------------------------------   22700000
.ZPVCK4A        ANOP  ,                                                 22750000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        22800000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                22850000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               22900000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          22950000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 23000000
                MNOTE 8,' &ZMS IS REQUIRED.'                            23050000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                23100000
                AGO   .ZPVCK5                                           23150000
.ZPVCK4         ANOP  ,                                                 23200000
&XPLISTVER      SETA  &PLISTVER(1)                                      23250000
.ZPVCK5         ANOP  ,                                                 23300000
.ZPVCKE         ANOP  ,                                                 23350000
.********************************************************************   23400000
.***AG.MF01 PROCESS MF MACRO FORMS                                      23450000
