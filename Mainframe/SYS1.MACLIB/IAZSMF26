                                TOTAL LENGTH OF TYPE 26 RECORD          24200000
.SKIP    ANOP                                                           24300000
         MEND       */                                                  24400000
 %IAZ26:;                                                               24500000
 %  DCL  EQUSW FIXED EXTERNAL; /*SWITCH TO DETERMINE WHETHER TO         24600000
                                 GENERATE EQUATES FOR WRITING REC    */ 24700000
    DCL 1 SMFRCD26 BASED(SMF26PTR) BDY(WORD),                           24800000
 /* RECORD TYPE 26 - JOB PURGE BY SUBSYSTEM                          */ 24900000
 /* WRITTEN WHEN ALL SYSOUT FINISHED FOR JOB(FORE & BACKGROUND)      */ 25000000
        2 SMF26BHD,               /* BEGINING BASE+HDR               */ 25100000
         3 SMF26LEN FIXED(15),    /* RDW RECORD LENGTH               */ 25200000
         3 SMF26SEG FIXED(15),    /* RDW SEGMENT DESCRIPTOR          */ 25300000
         3 SMF26FLG BIT(8),       /* OPERATING ENVIRONMENT           */ 25400000
         3 SMF26RTY FIXED(8),     /* RECORD TYPE 26                  */ 25500000
         3 SMF26TME FIXED(31) BDY(BYTE), /* TOD RECORD WRITTEN       */ 25600000
         3 SMF26DTE CHAR(4),      /* DATE RECORD WRITTEN             */ 25700000
         3 SMF26SID CHAR(4),      /* SYSTEM ID FROM INSTALLATION     */ 25800000
         3 SMF26JBN CHAR(8),      /* JOB NAME                        */ 25900000
         3 SMF26RST FIXED(31) BDY(BYTE), /*READER START TIME(.01 SEC)*/ 26000000
         3 SMF26RSD CHAR(4),      /* READER START DATE               */ 26100000
         3 SMF26UIF CHAR(8),      /* USER IDENTIFICATION FIELD       */ 26200000
