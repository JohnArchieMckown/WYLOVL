.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00484300
.*******************************************************************    00484400
                      ACTR  10000                                       00484500
.*                                                                      00484600
                      LCLA  &ZLC           COLUMN POSITION              00484700
                      LCLA  &ZOPX          LIST TABLE INDEX             00484800
                      LCLC  &ZLL           LABEL                        00484900
                      LCLC  &ZLOP          OPERATION                    00485000
                      LCLC  &ZLV           VALUE                        00485100
                      LCLC  &ZLD           DESCRIPTION                  00485200
.*                                                                      00485300
&ZOPX                 SETA  1                                           00485400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00485500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00485600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00485700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00485800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00485900
                      AIF   (K'&ZLD LE 0).ZL5                           00486000
&ZLC                  SETA  35             COMMENT COLUMN               00486100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00486200
&ZLC                  SETA  33             COMMMENT COLUMN              00486300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00486400
&ZLV                  SETC  '&ZLV'.' '                                  00486500
                      AGO   .ZL3                                        00486600
.*                                                                      00486700
.ZL4                  ANOP  ,                                           00486800
&ZLV                  SETC  '&ZLV'.' '                                  00486900
.ZL5                  ANOP  ,                                           00487000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00487100
.*                                                                      00487200
&ZLL     &ZLOP &ZLV                                                     00487300
.*                                                                      00487400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00487500
                      AGO   .ZL2           CONTINUE LOOPING             00487600
.*                                                                      00487700
.ZLX                  ANOP  ,                                           00487800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00487900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00488000
