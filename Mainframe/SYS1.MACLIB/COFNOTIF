.***AG.MF01 PROCESS MF MACRO FORMS                                      00089200
.********************************************************************   00089300
               ACTR  10000                                              00089400
.*------------------------------------------------------------------    00089500
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00089600
&XMF           SETC  ''          INITIALIZE XVARIABLE                   00089700
.*-------------------------------------------------------------------   00089800
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00089900
.ZG94          ANOP  ,                                                  00090000
&ZTLC          SETC 'S'                                                 00090100
               AIF   ('&MF(1)' NE '&ZTLC').ZG95                         00090200
&XMF           SETC  'S'         SET KEYWORD                            00090300
&XMFSCK        SETC  'COMPLETE'  SET XMFSCK                             00090400
               AIF   (N'&MF LE 1).ZG94A         CK NUM                  00090500
&ZMS           SETC  '"MF='      *KPOSA* MNOTE                          00090600
&ZMS           SETC  '&ZMS.&MF"'                                        00090700
&ZMS           SETC  '&ZMS "MF"'                                        00090800
               MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                00090900
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00091000
.ZG94A         ANOP  ,                                                  00091100
.*-------------------------------------------------------------------   00091200
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00091300
.ZG95          ANOP  ,                                                  00091400
&ZTLC          SETC 'L'                                                 00091500
               AIF   ('&MF(1)' NE '&ZTLC').ZG102                        00091600
&XMF           SETC  'L'         SET KEYWORD                            00091700
               AIF   (N'&MF LE 3).ZG95A         CK NUM                  00091800
&ZMS           SETC  '"MF='      *KPOSA* MNOTE                          00091900
&ZMS           SETC  '&ZMS.&MF"'                                        00092000
&ZMS           SETC  '&ZMS "MF"'                                        00092100
               MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'               00092200
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00092300
.ZG95A         ANOP  ,                                                  00092400
.*-------------------------------------------------------------------   00092500
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00092600
               AIF   (N'&SYSLIST NE 0).ZG95B    POSITIONALS?            00092700
               AIF   ('&FUNC' NE '').ZG95B      KEYS?                   00092800
               AIF   ('&CLASS' NE '').ZG95B     KEYS?                   00092900
               AIF   ('&MAJLIST' NE '').ZG95B   KEYS?                   00093000
               AIF   ('&MAJNUM' NE '').ZG95B    KEYS?                   00093100
               AIF   ('&MAJLEN' NE '').ZG95B    KEYS?                   00093200
               AIF   ('&MAJOR' NE '').ZG95B     KEYS?                   00093300
               AIF   ('&MINLIST' NE '').ZG95B   KEYS?                   00093400
               AIF   ('&MINNUM' NE '').ZG95B    KEYS?                   00093500
               AIF   ('&MINLEN' NE '').ZG95B    KEYS?                   00093600
               AIF   ('&VOLUME' NE '').ZG95B    KEYS?                   00093700
               AIF   ('&REMOTE' NE '').ZG95B    KEYS?                   00093800
               AIF   ('&TIMESTAMP' NE '').ZG95B  KEYS?                  00093900
               AIF   ('&RETCODE' NE '').ZG95B   KEYS?                   00094000
               AIF   ('&RSNCODE' NE '').ZG95B   KEYS?                   00094100
               AGO    .ZG95C     INVOCATION OKAY                        00094200
.ZG95B         ANOP  ,                                                  00094300
&ZMS           SETC  'THE "L" FORM OF "MF" CANNOT'  *MFLA* MNOTE        00094400
&ZMS           SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                  00094500
&ZMS           SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'             00094600
               MNOTE 8,' &ZMS ON THE INVOCATION.'                       00094700
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00094800
