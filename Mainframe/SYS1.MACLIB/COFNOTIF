&ZLL     &ZLOP &ZLV                                                     00490600
.*                                                                      00490700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00490800
                      AGO   .ZL21          CONTINUE LOOPING             00490900
.*                                                                      00491000
.ZLX1                 ANOP  ,                                           00491100
.*******************************************************************    00491200
.***AP.X02  END OF COFNOTIF MACRO                                       00491300
&ZCOFNOTIF            SETC  'YES'          SET COFNOTIF USED            00491400
         POP   PRINT                                                    00491500
*                                            COFNOTIF-1                 00491600
         SPACE 1                                                        00491700
.ZLX2                 ANOP  ,                                           00491800
         MEND                                                           00491900
**********************************************************************/ 00492000
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00492100
*%COFNOTIF: MACRO KEYS(FUNC,CLASS,MAJLIST,MAJNUM,MAJLEN,MAJOR,MINLIST,  00492200
*                 MINNUM,MINLEN,VOLUME,REMOTE,TIMESTAMP,RETCODE,        00492300
*                 RSNCODE,MF);                                          00492400
*  ANS('?'||MACLABEL||' '||'COFNOTIP '||MACLIST||                       00492500
*         ' '||MACKEYS||';') COL(MACLMAR);                              00492600
*%END COFNOTIF;                                                         00492700
