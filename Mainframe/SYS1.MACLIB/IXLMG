.***AC.PLV01  PROCESS PLISTVER                                      *   00083900
.********************************************************************   00084000
&XPLISTVER      SETA  0                                                 00084100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00084200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00084300
&XPLISTVER      SETA  2                                                 00084400
.ZPLV0          ANOP  ,                                                 00084500
.*                                                                      00084600
                AIF   ('&STRCOPYCNTLS' EQ '').ZPLV1E                    00084700
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00084800
&XPLISTVER      SETA  2                                                 00084900
.ZPLV1E         ANOP  ,                                                 00085000
.*                                                                      00085100
                AIF   ('&AMDALEVEL' EQ '').ZPLV2E                       00085200
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00085300
&XPLISTVER      SETA  1                                                 00085400
.ZPLV2E         ANOP  ,                                                 00085500
.*                                                                      00085600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00085700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00085800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00085900
.*-------------------------------------------------------------------   00086000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00086100
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00086200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00086300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00086400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00086500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00086600
                AGO   .ZPVCK2                                           00086700
.ZPVCK3         ANOP  ,                                                 00086800
.*-------------------------------------------------------------------   00086900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00087000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00087100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00087200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00087300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00087400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00087500
                AGO   .ZPVCKE                                           00087600
.ZPVCK2         ANOP  ,                                                 00087700
.*-------------------------------------------------------------------   00087800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00087900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00088000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00088100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00088200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00088300
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00088400
.ZPVCK6         ANOP  ,                                                 00088500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00088600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00088700
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00088800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00088900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00089000
&XPLISTVER      SETA  2                                                 00089100
                AGO   .ZPVCKE                                           00089200
.*-------------------------------------------------------------------   00089300
.ZPVCK4A        ANOP  ,                                                 00089400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00089500
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00089600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00089700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00089800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00089900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00090000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00090100
                AGO   .ZPVCK5                                           00090200
.ZPVCK4         ANOP  ,                                                 00090300
&XPLISTVER      SETA  &PLISTVER(1)                                      00090400
.ZPVCK5         ANOP  ,                                                 00090500
.ZPVCKE         ANOP  ,                                                 00090600
.********************************************************************   00090700
