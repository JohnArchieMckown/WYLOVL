                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00248800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00248900
&ZOPN                 SETA  &ZOPN+1                                     00249000
&ZOPL(&ZOPN)          SETC  ' '                                         00249100
&ZOPO(&ZOPN)          SETC  'LR'                                        00249200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00249300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00249400
.ZMRSNX               ANOP  ,                                           00249500
.*******************************************************************    00249600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00249700
.*******************************************************************    00249800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00249900
.*******************************************************************    00250000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00250100
.*******************************************************************    00250200
                      ACTR  10000                                       00250300
.*                                                                      00250400
                      LCLA  &ZLC           COLUMN POSITION              00250500
                      LCLA  &ZOPX          LIST TABLE INDEX             00250600
                      LCLC  &ZLL           LABEL                        00250700
                      LCLC  &ZLOP          OPERATION                    00250800
                      LCLC  &ZLV           VALUE                        00250900
                      LCLC  &ZLD           DESCRIPTION                  00251000
.*                                                                      00251100
&ZOPX                 SETA  1                                           00251200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00251300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00251400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00251500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00251600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00251700
                      AIF   (K'&ZLD LE 0).ZL5                           00251800
&ZLC                  SETA  35             COMMENT COLUMN               00251900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00252000
&ZLC                  SETA  33             COMMMENT COLUMN              00252100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00252200
&ZLV                  SETC  '&ZLV'.' '                                  00252300
                      AGO   .ZL3                                        00252400
.*                                                                      00252500
.ZL4                  ANOP  ,                                           00252600
&ZLV                  SETC  '&ZLV'.' '                                  00252700
.ZL5                  ANOP  ,                                           00252800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00252900
.*                                                                      00253000
&ZLL     &ZLOP &ZLV                                                     00253100
.*                                                                      00253200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00253300
                      AGO   .ZL2           CONTINUE LOOPING             00253400
.*                                                                      00253500
.ZLX                  ANOP  ,                                           00253600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00253700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00253800
         L     14,16(0,0)              GET CVT ADDRESS                  00253900
         L     14,772(14,0)            GET SFT ADDRESS                  00254000
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00254100
         LA    15,18                   GET EX FOR SERVICE ROUTINE       00254200
         OR    14,15                   GET PC NUMBER                    00254300
         PC    0(14)                                                    00254400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00254500
.ZLINKX               ANOP  ,                                           00254600
.*******************************************************************    00254700
.***AP.X01  FORMAT GENERATED CODE                                       00254800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00254900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00255000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00255100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00255200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00255300
                      AIF   (K'&ZLD LE 0).ZL51                          00255400
&ZLC                  SETA  35             COMMENT COLUMN               00255500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00255600
&ZLC                  SETA  33             COMMENT COLUMN               00255700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00255800
&ZLV                  SETC  '&ZLV'.' '                                  00255900
                      AGO   .ZL31                                       00256000
.*                                                                      00256100
.ZL41                 ANOP  ,                                           00256200
&ZLV                  SETC  '&ZLV'.' '                                  00256300
.ZL51                 ANOP  ,                                           00256400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00256500
.*                                                                      00256600
&ZLL     &ZLOP &ZLV                                                     00256700
.*                                                                      00256800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00256900
                      AGO   .ZL21          CONTINUE LOOPING             00257000
.*                                                                      00257100
.ZLX1                 ANOP  ,                                           00257200
.*******************************************************************    00257300
