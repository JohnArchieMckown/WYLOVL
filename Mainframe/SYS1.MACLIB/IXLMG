&ZCGPR   SETC  '0('.'&XSTGCLASS'(2,K'&XSTGCLASS-2).')'                  00240900
                      AGO   .ZI20D                                      00241000
.ZI20B                ANOP  ,                                           00241100
.*                                                                      00241200
&ZC                   SETC  '&XSTGCLASS'(1,1)                           00241300
                      AIF   (T'&ZC NE 'N').ZI20D                        00241400
&ZC                   SETC  '&XSTGCLASS'                                00241500
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI20D               00241600
&ZCGPR                SETC  '=AL1(&XSTGCLASS)'                          00241700
.*                                                                      00241800
.ZI20D                ANOP  ,                                           00241900
.*                                                                      00242000
&ZOPN                 SETA  &ZOPN+1                                     00242100
&ZOPL(&ZOPN)          SETC  ' '                                         00242200
&ZOPO(&ZOPN)          SETC  'MVC'                                       00242300
&ZOPV(&ZOPN)          SETC  '48(1,1),&ZCGPR'                            00242400
&ZOPD(&ZOPN)          SETC  '++ &ZXSTGCLASS XSTGCLASS'                  00242500
.*                                                                      00242600
.ZI20E                ANOP  ,                                           00242700
.*-------------------------------------------------------------------   00242800
&ZOPN1                SETA &ZOPN                                        00242900
.*******************************************************************    00243000
.***  PROCESS IXLMG OUTPUT PARAMETERS                                   00243100
.*******************************************************************    00243200
                      ACTR  10000                                       00243300
.*******************************************************************    00243400
.***AO.X02  RETURN CODE PROCESSING                                      00243500
.*******************************************************************    00243600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00243700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00243800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00243900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00244000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00244100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00244200
&ZOPN                 SETA  &ZOPN+1                                     00244300
&ZOPL(&ZOPN)          SETC  ' '                                         00244400
&ZOPO(&ZOPN)          SETC  'ST'                                        00244500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00244600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00244700
                      AGO   .ZMRETX                                     00244800
.*                                                                      00244900
.ZMRET2               ANOP  ,                                           00245000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00245100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00245200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00245300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00245400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00245500
&ZOPN                 SETA  &ZOPN+1                                     00245600
&ZOPL(&ZOPN)          SETC  ' '                                         00245700
&ZOPO(&ZOPN)          SETC  'LR'                                        00245800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00245900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00246000
.ZMRETX               ANOP  ,                                           00246100
.*******************************************************************    00246200
.***AO.X03  REASON CODE PROCESSING                                      00246300
.*******************************************************************    00246400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00246500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00246600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00246700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00246800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00246900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00247000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00247100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00247200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00247300
&ZOPN                 SETA  &ZOPN+1                                     00247400
&ZOPL(&ZOPN)          SETC  ' '                                         00247500
&ZOPO(&ZOPN)          SETC  'ST'                                        00247600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00247700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00247800
                      AGO   .ZMRSNX                                     00247900
.*                                                                      00248000
.ZMRSN2               ANOP  ,                                           00248100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00248200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00248300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00248400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00248500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00248600
