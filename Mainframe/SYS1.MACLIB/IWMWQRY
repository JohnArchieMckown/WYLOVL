.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00134500
.*******************************************************************    00134600
                      ACTR  10000                                       00134700
.*                                                                      00134800
                      LCLA  &ZLC           COLUMN POSITION              00134900
                      LCLA  &ZOPX          LIST TABLE INDEX             00135000
                      LCLC  &ZLL           LABEL                        00135100
                      LCLC  &ZLOP          OPERATION                    00135200
                      LCLC  &ZLV           VALUE                        00135300
                      LCLC  &ZLD           DESCRIPTION                  00135400
.*                                                                      00135500
&ZOPX                 SETA  1                                           00135600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00135700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00135800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00135900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00136000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00136100
                      AIF   (K'&ZLD LE 0).ZL5                           00136200
&ZLC                  SETA  35             COMMENT COLUMN               00136300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00136400
&ZLC                  SETA  33             COMMMENT COLUMN              00136500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00136600
&ZLV                  SETC  '&ZLV'.' '                                  00136700
                      AGO   .ZL3                                        00136800
.*                                                                      00136900
.ZL4                  ANOP  ,                                           00137000
&ZLV                  SETC  '&ZLV'.' '                                  00137100
.ZL5                  ANOP  ,                                           00137200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00137300
.*                                                                      00137400
&ZLL     &ZLOP &ZLV                                                     00137500
.*                                                                      00137600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00137700
                      AGO   .ZL2           CONTINUE LOOPING             00137800
.*                                                                      00137900
.ZLX                  ANOP  ,                                           00138000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00138100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00138200
         L     14,16(0,0)              GET CVT ADDRESS                  00138300
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00138400
         LAE   14,0(14,0)              CLEAR AR 14                      00138500
.NOAR    ANOP                                                           00138600
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00138700
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00138800
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00138900
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00139000
         B     END&SYSNDX              BRANCH TO END LABEL              00139100
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00139200
         L     14,772(14,0)            GET SFT ADDRESS                  00139300
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00139400
         LA    15,8                    GET EX FOR SERVICE ROUTINE       00139500
         OR    14,15                   GET PC NUMBER                    00139600
         PC    0(14)                                                    00139700
END&SYSNDX DS    0H                    END LABEL                        00139800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00139900
.ZLINKX               ANOP  ,                                           00140000
.*******************************************************************    00140100
