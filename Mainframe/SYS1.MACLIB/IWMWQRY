.***AB.02  GLOBAL VARIABLES TO CONTROL PRINT AND CODE GENERATION        00037700
.*******************************************************************    00037800
              GBLC  &ZPRINT     YES=PRINT NO=NO PRINT                   00037900
              GBLC  &ZTEST      YES=TEST NO=NO TEST                     00038000
              GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX             00038100
.*                                                                      00038200
              GBLC  &ZIWMWQRY   YES=MACRO USED AT LEAST ONCE            00038300
.*******************************************************************    00038400
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00038500
.*******************************************************************    00038600
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00038700
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00038800
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00038900
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00039000
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00039100
              LCLA  &ZAWRK      USED AS A WORK AREA                     00039200
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00039300
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00039400
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00039500
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00039600
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00039700
              LCLA  &ZMACRET    MACRO RETURN CODE                       00039800
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00039900
.*                                                                      00040000
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00040100
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00040200
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00040300
.********************************************************************   00040400
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00040500
.********************************************************************   00040600
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00040700
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00040800
.*                                                                      00040900
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00041000
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00041100
              SPLEVEL  TEST                                             00041200
.ZSYS2        ANOP  ,                                                   00041300
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00041400
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00041500
              SYSSTATE TEST                                             00041600
              AGO   .ZSYS4                                              00041700
.ZSYS3        ANOP  ,                                                   00041800
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00041900
.ZSYS4        ANOP  ,                                                   00042000
.*******************************************************************    00042100
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00042200
.*******************************************************************    00042300
              LCLC  &XSERVCLS   ++ NAME                                 00042400
              LCLC  &XANSLEN    ++ NAME                                 00042500
              LCLC  &XANSAREA   ++ NAME                                 00042600
              LCLC  &XQUERYLEN  ++ NAME                                 00042700
              LCLC  &XRETCODE   ++ NAME                                 00042800
              LCLC  &XRSNCODE   ++ NAME                                 00042900
              LCLC  &XMF        ++ VALUE                                00043000
              LCLC  &XMFCTRL    ++ NAME                                 00043100
              LCLC  &XMFATTR    ++ VALUE                                00043200
              LCLC  &XMFSCK     ++ VALUE                                00043300
.*******************************************************************    00043400
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00043500
.*******************************************************************    00043600
              LCLC  &ZXSERVCLS  ++ INPUT NAME                           00043700
              LCLC  &ZXANSLEN   ++ INPUT NAME                           00043800
              LCLC  &ZXANSAREA  ++ OUTPUT NAME                          00043900
              LCLC  &ZXQUERYLEN                 ++ OUTPUT NAME          00044000
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00044100
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00044200
              LCLC  &ZXMF       ++ INPUT VALUE                          00044300
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00044400
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00044500
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00044600
.********************************************************************   00044700
.***AG.MF01 PROCESS MF MACRO FORMS                                      00044800
.********************************************************************   00044900
              ACTR  10000                                               00045000
.*------------------------------------------------------------------    00045100
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00045200
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00045300
.*-------------------------------------------------------------------   00045400
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00045500
.ZG21         ANOP  ,                                                   00045600
&ZTLC         SETC 'S'                                                  00045700
              AIF   ('&MF(1)' NE '&ZTLC').ZG22                          00045800
&XMF          SETC  'S'         SET KEYWORD                             00045900
              AIF   (N'&MF LE 1).ZG21A          CK NUM                  00046000
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00046100
&ZMS          SETC  '&ZMS.&MF"'                                         00046200
&ZMS          SETC  '&ZMS "MF"'                                         00046300
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00046400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00046500
.ZG21A        ANOP  ,                                                   00046600
.*-------------------------------------------------------------------   00046700
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00046800
.ZG22         ANOP  ,                                                   00046900
&ZTLC         SETC 'L'                                                  00047000
              AIF   ('&MF(1)' NE '&ZTLC').ZG29                          00047100
&XMF          SETC  'L'         SET KEYWORD                             00047200
              AIF   (N'&MF LE 3).ZG22A          CK NUM                  00047300
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00047400
&ZMS          SETC  '&ZMS.&MF"'                                         00047500
&ZMS          SETC  '&ZMS "MF"'                                         00047600
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00047700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00047800
.ZG22A        ANOP  ,                                                   00047900
.*-------------------------------------------------------------------   00048000
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00048100
