.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00138300
.*******************************************************************    00138400
                      ACTR  10000                                       00138500
.*                                                                      00138600
                      LCLA  &ZLC           COLUMN POSITION              00138700
                      LCLA  &ZOPX          LIST TABLE INDEX             00138800
                      LCLC  &ZLL           LABEL                        00138900
                      LCLC  &ZLOP          OPERATION                    00139000
                      LCLC  &ZLV           VALUE                        00139100
                      LCLC  &ZLD           DESCRIPTION                  00139200
.*                                                                      00139300
&ZOPX                 SETA  1                                           00139400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00139500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00139600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00139700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00139800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00139900
                      AIF   (K'&ZLD LE 0).ZL5                           00140000
&ZLC                  SETA  35             COMMENT COLUMN               00140100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00140200
&ZLC                  SETA  33             COMMMENT COLUMN              00140300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00140400
&ZLV                  SETC  '&ZLV'.' '                                  00140500
                      AGO   .ZL3                                        00140600
.*                                                                      00140700
.ZL4                  ANOP  ,                                           00140800
&ZLV                  SETC  '&ZLV'.' '                                  00140900
.ZL5                  ANOP  ,                                           00141000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00141100
.*                                                                      00141200
&ZLL     &ZLOP &ZLV                                                     00141300
.*                                                                      00141400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00141500
                      AGO   .ZL2           CONTINUE LOOPING             00141600
.*                                                                      00141700
.ZLX                  ANOP  ,                                           00141800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00141900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00142000
         L     14,16(0,0)              GET CVT ADDRESS                  00142100
         TM    CVTOSLV2-CVT(14),X'80'  TEST FOR OS/390 RELEASE IV       00142200
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00142300
         LA    0,IWMRSNCODENOSCHENV    SET REASON CODE = NO WLM SVCS    00142400
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00142500
         B     END&SYSNDX              BRANCH TO END LABEL              00142600
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00142700
         L     14,16(0,0)              GET CVT ADDRESS                  00142800
         L     14,772(14,0)            GET SFT ADDRESS                  00142900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00143000
         LA    15,56                   GET EX FOR SERVICE ROUTINE       00143100
         OR    14,15                   GET PC NUMBER                    00143200
         PC    0(14)                                                    00143300
END&SYSNDX DS    0H                    END LABEL                        00143400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00143500
