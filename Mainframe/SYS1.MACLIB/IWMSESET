.***AC.PLV01  PROCESS PLISTVER                                      *   00057700
.********************************************************************   00057800
&XPLISTVER      SETA  0                                                 00057900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00058000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00058100
&XPLISTVER      SETA  0                                                 00058200
.ZPLV0          ANOP  ,                                                 00058300
.*                                                                      00058400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00058500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00058600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00058700
.*-------------------------------------------------------------------   00058800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00058900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00059000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00059100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00059200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00059300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059400
                AGO   .ZPVCK2                                           00059500
.ZPVCK3         ANOP  ,                                                 00059600
.*-------------------------------------------------------------------   00059700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00059800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00059900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00060000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00060100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00060200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00060300
                AGO   .ZPVCKE                                           00060400
.ZPVCK2         ANOP  ,                                                 00060500
.*-------------------------------------------------------------------   00060600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00060700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00060800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00060900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00061000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00061100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00061200
.ZPVCK6         ANOP  ,                                                 00061300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00061400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00061500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00061600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00061700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00061800
&XPLISTVER      SETA  0                                                 00061900
                AGO   .ZPVCKE                                           00062000
.*-------------------------------------------------------------------   00062100
.ZPVCK4A        ANOP  ,                                                 00062200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00062300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00062400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00062500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00062600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00062700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00062800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062900
                AGO   .ZPVCK5                                           00063000
.ZPVCK4         ANOP  ,                                                 00063100
&XPLISTVER      SETA  &PLISTVER(1)                                      00063200
.ZPVCK5         ANOP  ,                                                 00063300
.ZPVCKE         ANOP  ,                                                 00063400
