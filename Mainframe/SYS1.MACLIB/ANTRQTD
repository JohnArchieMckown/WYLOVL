.***AC.PLV01  PROCESS PLISTVER                                      *   00252900
.********************************************************************   00253000
&XPLISTVER      SETA  0                                                 00253100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00253200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00253300
&XPLISTVER      SETA  4                                                 00253400
.ZPLV0          ANOP  ,                                                 00253500
.*                                                                      00253600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00253700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00253800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00253900
.*-------------------------------------------------------------------   00254000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00254100
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00254200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00254300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00254400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00254500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00254600
                AGO   .ZPVCK2                                           00254700
.ZPVCK3         ANOP  ,                                                 00254800
.*-------------------------------------------------------------------   00254900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00255000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00255100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00255200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00255300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00255400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00255500
                AGO   .ZPVCKE                                           00255600
.ZPVCK2         ANOP  ,                                                 00255700
.*-------------------------------------------------------------------   00255800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00255900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00256000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00256100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00256200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00256300
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00256400
.ZPVCK6         ANOP  ,                                                 00256500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00256600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00256700
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00256800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00256900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00257000
&XPLISTVER      SETA  4                                                 00257100
                AGO   .ZPVCKE                                           00257200
.*-------------------------------------------------------------------   00257300
.ZPVCK4A        ANOP  ,                                                 00257400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00257500
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00257600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00257700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00257800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00257900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00258000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00258100
                AGO   .ZPVCK5                                           00258200
.ZPVCK4         ANOP  ,                                                 00258300
&XPLISTVER      SETA  &PLISTVER(1)                                      00258400
.ZPVCK5         ANOP  ,                                                 00258500
.ZPVCKE         ANOP  ,                                                 00258600
.********************************************************************   00258700
