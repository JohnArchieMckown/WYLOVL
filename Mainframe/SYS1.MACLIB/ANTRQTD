                 AIF   ('&QRYINFO' NE '').ZG144B  KEYS?                 00263900
                 AIF   ('&ALET' NE '').ZG144B   KEYS?                   00264000
                 AIF   ('&BITMAPLEN' NE '').ZG144B  KEYS?               00264100
                 AIF   ('&EXTENTLEN' NE '').ZG144B  KEYS?               00264200
                 AIF   ('&SRCBITMAP' NE '').ZG144B  KEYS?               00264300
                 AIF   ('&TGTBITMAP' NE '').ZG144B  KEYS?               00264400
