.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              00800500
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         00800600
.*******************************************************************    00800700
.***  PROCESS ANTRQTD OUTPUT PARAMETERS                                 00800800
.*******************************************************************    00800900
                      ACTR  10000                                       00801000
.*******************************************************************    00801100
.***AO.X02  RETURN CODE PROCESSING                                      00801200
.*******************************************************************    00801300
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00801400
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00801500
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00801600
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00801700
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00801800
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00801900
&ZOPN                 SETA  &ZOPN+1                                     00802000
&ZOPL(&ZOPN)          SETC  ' '                                         00802100
&ZOPO(&ZOPN)          SETC  'ST'                                        00802200
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00802300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00802400
                      AGO   .ZMRETX                                     00802500
.*                                                                      00802600
.ZMRET2               ANOP  ,                                           00802700
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00802800
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00802900
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00803000
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00803100
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00803200
&ZOPN                 SETA  &ZOPN+1                                     00803300
&ZOPL(&ZOPN)          SETC  ' '                                         00803400
&ZOPO(&ZOPN)          SETC  'LR'                                        00803500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00803600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00803700
.ZMRETX               ANOP  ,                                           00803800
.*******************************************************************    00803900
.***AO.X03  REASON CODE PROCESSING                                      00804000
.*******************************************************************    00804100
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00804200
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00804300
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00804400
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00804500
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00804600
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00804700
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00804800
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00804900
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00805000
&ZOPN                 SETA  &ZOPN+1                                     00805100
&ZOPL(&ZOPN)          SETC  ' '                                         00805200
&ZOPO(&ZOPN)          SETC  'ST'                                        00805300
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00805400
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00805500
                      AGO   .ZMRSNX                                     00805600
.*                                                                      00805700
.ZMRSN2               ANOP  ,                                           00805800
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00805900
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00806000
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00806100
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00806200
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00806300
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00806400
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00806500
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00806600
&ZOPN                 SETA  &ZOPN+1                                     00806700
&ZOPL(&ZOPN)          SETC  ' '                                         00806800
&ZOPO(&ZOPN)          SETC  'LR'                                        00806900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00807000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00807100
.ZMRSNX               ANOP  ,                                           00807200
.*******************************************************************    00807300
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00807400
.*******************************************************************    00807500
.ZMFLX                ANOP  ,              MF=L PROCESSING              00807600
.*******************************************************************    00807700
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00807800
.*******************************************************************    00807900
                      ACTR  10000                                       00808000
.*                                                                      00808100
                      LCLA  &ZLC           COLUMN POSITION              00808200
                      LCLA  &ZOPX          LIST TABLE INDEX             00808300
                      LCLC  &ZLL           LABEL                        00808400
                      LCLC  &ZLOP          OPERATION                    00808500
                      LCLC  &ZLV           VALUE                        00808600
                      LCLC  &ZLD           DESCRIPTION                  00808700
.*                                                                      00808800
&ZOPX                 SETA  1                                           00808900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00809000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00809100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00809200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00809300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00809400
                      AIF   (K'&ZLD LE 0).ZL5                           00809500
&ZLC                  SETA  35             COMMENT COLUMN               00809600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00809700
&ZLC                  SETA  33             COMMMENT COLUMN              00809800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00809900
&ZLV                  SETC  '&ZLV'.' '                                  00810000
                      AGO   .ZL3                                        00810100
.*                                                                      00810200
.ZL4                  ANOP  ,                                           00810300
&ZLV                  SETC  '&ZLV'.' '                                  00810400
.ZL5                  ANOP  ,                                           00810500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00810600
.*                                                                      00810700
&ZLL     &ZLOP &ZLV                                                     00810800
.*                                                                      00810900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00811000
                      AGO   .ZL2           CONTINUE LOOPING             00811100
.*                                                                      00811200
.ZLX                  ANOP  ,                                           00811300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00811400
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00811500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00811600
         L     14,16(,0)                                                00811700
         L     14,1216(,14)                                             00811800
         L     14,44(,14)                                               00811900
         L     14,8(,14)                                                00812000
         L     14,88(,14)                                               00812100
         ICM   14,15,324(14)                                            00812200
         BNZ   A&SYSNDX                                                 00812300
         L     15,=A(RQST_PC_NUMBER_ZERO)                               00812400
         SLR   0,0                                                      00812500
         B     B&SYSNDX                                                 00812600
A&SYSNDX DS    0H                                                       00812700
         PC    0(14)                                                    00812800
B&SYSNDX DS    0H                                                       00812900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00813000
.ZLINKX               ANOP  ,                                           00813100
.*******************************************************************    00813200
.***AP.X01  FORMAT GENERATED CODE                                       00813300
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00813400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00813500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00813600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00813700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00813800
                      AIF   (K'&ZLD LE 0).ZL51                          00813900
&ZLC                  SETA  35             COMMENT COLUMN               00814000
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00814100
&ZLC                  SETA  33             COMMENT COLUMN               00814200
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00814300
&ZLV                  SETC  '&ZLV'.' '                                  00814400
                      AGO   .ZL31                                       00814500
.*                                                                      00814600
.ZL41                 ANOP  ,                                           00814700
&ZLV                  SETC  '&ZLV'.' '                                  00814800
.ZL51                 ANOP  ,                                           00814900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00815000
.*                                                                      00815100
&ZLL     &ZLOP &ZLV                                                     00815200
.*                                                                      00815300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00815400
                      AGO   .ZL21          CONTINUE LOOPING             00815500
.*                                                                      00815600
.ZLX1                 ANOP  ,                                           00815700
.*******************************************************************    00815800
.***AP.X02  END OF ANTRQTD MACRO                                        00815900
&ZANTRQTD             SETC  'YES'          SET ANTRQTD USED             00816000
         POP   PRINT                                                    00816100
*                                            ANTRQTD-4                  00816200
         SPACE 1                                                        00816300
.ZLX2                 ANOP  ,                                           00816400
         MEND                                                           00816500
