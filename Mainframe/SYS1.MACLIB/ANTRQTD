*01* EXAMPLES:                                                        * 00215500
*                                                                     * 00215600
*                                                                     * 00215700
*      Operation:  Query status for device 10F4 in a                  * 00215800
*      FlashCopy session.                                             * 00215900
*                                                                     * 00216000
*               TITLE 'TESTER: Test ANTRQST'                          * 00216100
*      TESTER   CSECT ,                                               * 00216200
*               USING *,15                                            * 00216300
*               B     PROLOG                                          * 00216400
*               DC    AL1(16)                                         * 00216500
*               DC    C'TESTER    99.193'                             * 00216600
*               DROP  15                                              * 00216700
*      PROLOG   STM   14,12,12(13)                                    * 00216800
*               LR    12,15                                           * 00216900
*               USING TESTER,12                                       * 00217000
*               LR    15,13                                           * 00217100
*               LA    13,WORKDATA                                     * 00217200
*               USING WORKDATA,13                                     * 00217300
*               ST    15,4(,13)                                       * 00217400
*               ST    13,8(,15)                                       * 00217500
*               LM    15,01,16(15)                                    * 00217600
*               MVC   XDEVN,=XL2'10F4'                                * 00217700
*               MVC   QUERYSIZ,=AL2(512)                              * 00217800
*               ANTRQST ILK=ESSRVCS,                                  * 00217900
*          X                                                          * 00218000
*                     REQUEST=FCQUERY,                                * 00218100
*          X                                                          * 00218200
*                     DEVN=XDEVN,                                     * 00218300
*          X                                                          * 00218400
*                     QRYSIZE=QUERYSIZ,                               * 00218500
*          X                                                          * 00218600
*                     QRYINFO=QUERYINF,                               * 00218700
*          X                                                          * 00218800
*                     RETINFO=RETRNINF,                               * 00218900
*          X                                                          * 00219000
*                     RETCODE=RTNCD,                                  * 00219100
*          X                                                          * 00219200
*                     RSNCODE=RSNCD,                                  * 00219300
*          X                                                          * 00219400
*                     MF=(E,P_LIST)                                   * 00219500
*               LTR   15,15                                           * 00219600
*               BZ    SKIP1                                           * 00219700
*               MVC   RTCD,RTNCD                                      * 00219800
*               MVC   RSCD,RSNCD                                      * 00219900
*               B     SKIP2                                           * 00220000
*      SKIP1    DS    0H                                              * 00220100
*               MVC   RTCD,RTC                                        * 00220200
*               MVC   RSCD,RSC                                        * 00220300
*      SKIP2    DS    0H                                              * 00220400
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 00220500
*               BNE   SKIP3                                           * 00220600
*               WTO   'ANTAS000 NOT ACTIVE'                           * 00220700
*      SKIP3    DS    0H                                              * 00220800
*               L     15,RTCD                                         * 00220900
*               L     0,RSCD                                          * 00221000
*               L     13,4(,13)                                       * 00221100
*               L     14,12(,13)                                      * 00221200
*               LM    1,12,24(13)                                     * 00221300
*               BR    14                                              * 00221400
*      WORKDATA DS    0F                                              * 00221500
*      SAVE1    DS    18F                                             * 00221600
*      SAVE2    DS    18F                                             * 00221700
*      RTNCD    DS    F                                               * 00221800
*      RSNCD    DS    F                                               * 00221900
*      RTCD     DS    F                                               * 00222000
*      RSCD     DS    F                                               * 00222100
*      XDEVN    DS    XL2                                             * 00222200
*      QUERYSIZ DS    XL2                                             * 00222300
*      QUERYINF DS    CL512                                           * 00222400
*      RETRNINF DS    0F,CL100                                        * 00222500
*               ORG   RETRNINF                                        * 00222600
*      RTC      DS    F                                               * 00222700
*      RSC      DS    F                                               * 00222800
*               ORG   RETRNINF+100                                    * 00222900
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 00223000
*               LTORG                                                 * 00223100
*               END   TESTER                                          * 00223200
*                                                                     * 00223300
*                                                                     * 00223400
*                                                                     * 00223500
*      Operation:  Getting status for device 0883 in a                * 00223600
*      FlashCopy session with ASYNCH parameter.                       * 00223700
*                                                                     * 00223800
*               TITLE 'TESTER: Test ANTRQST'                          * 00223900
*      TESTER   CSECT ,                                               * 00224000
*               USING *,15                                            * 00224100
*               B     PROLOG                                          * 00224200
*               DC    AL1(16)                                         * 00224300
*               DC    C'TESTER    99.193'                             * 00224400
*               DROP  15                                              * 00224500
*      PROLOG   STM   14,12,12(13)                                    * 00224600
*               LR    12,15                                           * 00224700
*               USING TESTER,12                                       * 00224800
*               LR    15,13                                           * 00224900
*               LA    13,WORKDATA                                     * 00225000
*               USING WORKDATA,13                                     * 00225100
*               ST    15,4(,13)                                       * 00225200
*               ST    13,8(,15)                                       * 00225300
*               LM    15,01,16(15)                                    * 00225400
*               MVC   XDEVN,=XL2'0883'                                * 00225500
*               MVC   XQRYSIZE,=AL2(512)                              * 00225600
*               MVC   XASYNCH,=CL3'YES'                               * 00225700
*               XC    XECB,XECB                                       * 00225800
*               ANTRQST ILK=ESSRVCS,                                  * 00225900
*               X                                                     * 00226000
*                     REQUEST=FCQUERY,                                * 00226100
*               X                                                     * 00226200
*                     DEVN=XDEVN,                                     * 00226300
*               X                                                     * 00226400
*                     QRYSIZE=XQRYSIZE,                               * 00226500
*               X                                                     * 00226600
*                     QRYINFO=XQRYINFO,                               * 00226700
*               X                                                     * 00226800
*                     RETINFO=XRETINFO,                               * 00226900
*               X                                                     * 00227000
*                     ASYNCH=XASYNCH,                                 * 00227100
*               X                                                     * 00227200
*                     ECB=XECB,                                       * 00227300
*               X                                                     * 00227400
*                     RETCODE=RTNCD,                                  * 00227500
*               X                                                     * 00227600
*                     RSNCODE=RSNCD,                                  * 00227700
*               X                                                     * 00227800
*                     MF=(E,P_LIST)                                   * 00227900
*               CLC   RTNCD,=F'0'                                     * 00228000
*               BE    SKIP1                                           * 00228100
*               MVC   RTCD,RTNCD                                      * 00228200
*               MVC   RSCD,RSNCD                                      * 00228300
*               B     SKIP2                                           * 00228400
*      SKIP1    DS    0H                                              * 00228500
*               WAIT  ECB=XECB                                        * 00228600
*               NC    XECB,=XL4'3FFFFFFF'                             * 00228700
*               CLC   XECB,=F'0'                                      * 00228800
*               BE    SKIP1A                                          * 00228900
*               MVC   RTCD,XECB                                       * 00229000
*               MVC   RSCD,=F'0'                                      * 00229100
*               B     SKIP2                                           * 00229200
*      SKIP1A   DS    0H                                              * 00229300
*               MVC   RTCD,RTC                                        * 00229400
*               MVC   RSCD,RSC                                        * 00229500
*      SKIP2    DS    0H                                              * 00229600
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 00229700
*               BNE   SKIP3                                           * 00229800
*               WTO   'ANTAS000 NOT ACTIVE'                           * 00229900
*      SKIP3    DS    0H                                              * 00230000
*               L     15,RTCD                                         * 00230100
*               L     0,RSCD                                          * 00230200
*               L     13,4(,13)                                       * 00230300
*               L     14,12(,13)                                      * 00230400
*               LM    1,12,24(13)                                     * 00230500
*               BR    14                                              * 00230600
*               LTORG                                                 * 00230700
*      WORKDATA DS    0F                                              * 00230800
*               DS    18F                                             * 00230900
*      RTNCD    DS    F                                               * 00231000
*      RSNCD    DS    F                                               * 00231100
*      RTCD     DS    F                                               * 00231200
*      RSCD     DS    F                                               * 00231300
*      XECB     DS    F                                               * 00231400
*      XASYNCH  DS    CL3                                             * 00231500
*      XDEVN    DS    XL2                                             * 00231600
*      XQRYSIZE DS    XL2                                             * 00231700
*      XQRYINFO DS    CL512                                           * 00231800
*      XRETINFO DS    0F,CL100                                        * 00231900
*               ORG   XRETINFO                                        * 00232000
*      RTC      DS    F                                               * 00232100
*      RSC      DS    F                                               * 00232200
*               ORG   XRETINFO+100                                    * 00232300
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 00232400
*               END   TESTER                                          * 00232500
*                                                                     * 00232600
*                                                                     * 00232700
*01* MACRO USAGE NOTES:                                               * 00232800
*                                                                     * 00232900
*                                                                     * 00233000
*    1.  Printing of the macro expansion is controlled by the ZPRINT  * 00233100
*        global macro variable.  Any value other than 'NO' will       * 00233200
*        result in the macro expansion being printed.  The default is * 00233300
*        'YES'.  The following examples illustrate how the ZPRINT     * 00233400
*        variable should be set.                                      * 00233500
*                                                                     * 00233600
*                    GBLC &ZPRINT                                     * 00233700
*            &ZPRINT SETC 'YES'          &ZPRINT SETC 'NO'            * 00233800
*                                                                     * 00233900
*                                                                     * 00234000
*    2.  When using the NOCHECK option in conjunction with the Modify * 00234100
*        and/or  Execute  forms,  be  sure  that  it is preceded by a * 00234200
*        Modify   or  Execute  form  invocation  which  specifies  or * 00234300
*        defaults  to  the  COMPLETE  option.   This insures that the * 00234400
*        parameter  list  is  initialized completely.   The parameter * 00234500
*        list  field which identifies the version number of the macro * 00234600
*        is only set when the Standard form or the Complete option of * 00234700
*        the  Execute/Modify  form  is  used,  or  when  PLISTVER  is * 00234800
*        explicitly  specified.  Be sure that the resulting parameter * 00234900
*        list version number covers all the keys that you use.        * 00235000
*                                                                     * 00235100
*                                                                     * 00235200
*    3.  The  ANTRQTD macro supports multiple versions. The macro key * 00235300
*        list  below  contains the version level in which the key was * 00235400
*        first  introduced.  Positionals  are  identified by the word * 00235500
*        'Positional'  followed by the numeric value of its position. * 00235600
*        Nested  positionals  are  preceded  by their outer key.  The * 00235700
*        lowest value represents the original version and the highest * 00235800
*        value  represents  the  most  recent  version of the ANTRQTD * 00235900
*        macro.    When  specifying  PLISTVER,  be sure that it is as * 00236000
*        least  as high as the highest version number of all the keys * 00236100
*        being used.                                                  * 00236200
*                                                                     * 00236300
*        VERSION   KEY                                                * 00236400
*                                                                     * 00236500
*              0      ALET                                            * 00236600
*                     ASYNCH                                          * 00236700
*                     BITMAPLEN                                       * 00236800
*                     DEVN                                            * 00236900
*                     ECB                                             * 00237000
*                     EXTENTLEN                                       * 00237100
*                     ILK                                             * 00237200
*                     MODE                                            * 00237300
*                     MSGREQ                                          * 00237400
*                     ONLINTGT                                        * 00237500
*                     QRYINFO                                         * 00237600
*                     QRYSIZE                                         * 00237700
*                     REQUEST                                         * 00237800
*                     RESYNC                                          * 00237900
*                     RETINFO                                         * 00238000
*                     SDEVN                                           * 00238100
*                     SRCBITMAP                                       * 00238200
*                     SRCEXTENTS                                      * 00238300
*                     TDEVN                                           * 00238400
*                     TGTBITMAP                                       * 00238500
*                     TGTEXTENTS                                      * 00238600
*                     WAITTIME                                        * 00238700
*                                                                     * 00238800
*                                                                     * 00238900
*                                                                     * 00239000
*01* CHANGE ACTIVITY:                                                 * 00239100
*                                                                     * 00239200
*   Flag LineItem  FMID    Date   ID    Comment                       * 00239300
*                                                                     * 00239400
*    $00=OW42971  HDZ11F0 000417 TUCHAD:                              * 00239500
*    $01=OW46167  HDZ11F0 000417 TUCHAD:                              * 00239600
****** END OF SPECIFICATIONS *****************************************/ 00239700
