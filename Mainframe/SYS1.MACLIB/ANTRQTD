*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00816700
*%ANTRQTD: MACRO KEYS(ILK,REQUEST,RETINFO,SDEVN,TDEVN,SRCEXTENTS,       00816800
*                 TGTEXTENTS,MODE,ONLINTGT,MSGREQ,ASYNCH,ECB,WAITTIME,  00816900
*                 DEVN,QRYSIZE,QRYINFO,ALET,BITMAPLEN,EXTENTLEN,        00817000
*                 SRCBITMAP,TGTBITMAP,RESYNC,RETCODE,RSNCODE,PLISTVER,  00817100
*                 MF);                                                  00817200
*  ANS('?'||MACLABEL||' '||'ANTRQTDP '||MACLIST||                       00817300
*         ' '||MACKEYS||';') COL(MACLMAR);                              00817400
*%END ANTRQTD;                                                          00817500
