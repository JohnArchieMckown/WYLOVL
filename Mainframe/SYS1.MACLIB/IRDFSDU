&ZOPD(&ZOPN)          SETC  '++ INPUT XPLISTLEN XL2'                    00098500
.*-------------------------------------------------------------------   00098600
.***AI.L25  XTOLERANCE CL8                                              00098700
                      AIF   ('&ZXTOLERANCE' EQ '').ZI5E                 00098800
                      AIF   ('&ZXTOLERANCE' EQ 'OUTPUT').ZI5E           00098900
&ZCGPR                SETC  '&XTOLERANCE'                               00099000
.*                                                                      00099100
                      AIF   ('&XTOLERANCE'(1,1) NE '(').ZI5B            00099200
&ZCGPR   SETC  '0('.'&XTOLERANCE'(2,K'&XTOLERANCE-2).')'                00099300
.ZI5B                 ANOP  ,                                           00099400
.*                                                                      00099500
&ZOPN                 SETA  &ZOPN+1                                     00099600
