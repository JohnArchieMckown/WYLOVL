.*                                                                      00100100
.ZI5E                 ANOP  ,                                           00100200
.*-------------------------------------------------------------------   00100300
&ZOPN1                SETA &ZOPN                                        00100400
.*******************************************************************    00100500
.***  PROCESS IRDFSDU OUTPUT PARAMETERS                                 00100600
.*******************************************************************    00100700
                      ACTR  10000                                       00100800
.*******************************************************************    00100900
.***AO.X02  RETURN CODE PROCESSING                                      00101000
.*******************************************************************    00101100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00101200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00101300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00101400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00101500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00101600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00101700
&ZOPN                 SETA  &ZOPN+1                                     00101800
&ZOPL(&ZOPN)          SETC  ' '                                         00101900
&ZOPO(&ZOPN)          SETC  'ST'                                        00102000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00102100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00102200
                      AGO   .ZMRETX                                     00102300
.*                                                                      00102400
.ZMRET2               ANOP  ,                                           00102500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00102600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00102700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00102800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00102900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00103000
&ZOPN                 SETA  &ZOPN+1                                     00103100
&ZOPL(&ZOPN)          SETC  ' '                                         00103200
&ZOPO(&ZOPN)          SETC  'LR'                                        00103300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00103400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00103500
.ZMRETX               ANOP  ,                                           00103600
.*******************************************************************    00103700
.***AO.X03  REASON CODE PROCESSING                                      00103800
.*******************************************************************    00103900
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00104000
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00104100
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00104200
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00104300
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00104400
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00104500
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00104600
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00104700
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00104800
&ZOPN                 SETA  &ZOPN+1                                     00104900
&ZOPL(&ZOPN)          SETC  ' '                                         00105000
&ZOPO(&ZOPN)          SETC  'ST'                                        00105100
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00105200
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00105300
                      AGO   .ZMRSNX                                     00105400
.*                                                                      00105500
.ZMRSN2               ANOP  ,                                           00105600
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00105700
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00105800
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00105900
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00106000
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00106100
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00106200
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00106300
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00106400
&ZOPN                 SETA  &ZOPN+1                                     00106500
&ZOPL(&ZOPN)          SETC  ' '                                         00106600
&ZOPO(&ZOPN)          SETC  'LR'                                        00106700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00106800
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00106900
.ZMRSNX               ANOP  ,                                           00107000
.*******************************************************************    00107100
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00107200
.*******************************************************************    00107300
.ZMFLX                ANOP  ,              MF=L PROCESSING              00107400
.*******************************************************************    00107500
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00107600
.*******************************************************************    00107700
                      ACTR  10000                                       00107800
.*                                                                      00107900
                      LCLA  &ZLC           COLUMN POSITION              00108000
                      LCLA  &ZOPX          LIST TABLE INDEX             00108100
                      LCLC  &ZLL           LABEL                        00108200
                      LCLC  &ZLOP          OPERATION                    00108300
                      LCLC  &ZLV           VALUE                        00108400
                      LCLC  &ZLD           DESCRIPTION                  00108500
.*                                                                      00108600
&ZOPX                 SETA  1                                           00108700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00108800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00108900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00109000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00109100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00109200
                      AIF   (K'&ZLD LE 0).ZL5                           00109300
&ZLC                  SETA  35             COMMENT COLUMN               00109400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00109500
&ZLC                  SETA  33             COMMMENT COLUMN              00109600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00109700
&ZLV                  SETC  '&ZLV'.' '                                  00109800
                      AGO   .ZL3                                        00109900
.*                                                                      00110000
.ZL4                  ANOP  ,                                           00110100
&ZLV                  SETC  '&ZLV'.' '                                  00110200
.ZL5                  ANOP  ,                                           00110300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00110400
.*                                                                      00110500
&ZLL     &ZLOP &ZLV                                                     00110600
.*                                                                      00110700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00110800
                      AGO   .ZL2           CONTINUE LOOPING             00110900
.*                                                                      00111000
.ZLX                  ANOP  ,                                           00111100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00111200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00111300
         L    15,16(0,0)          ++ GET CVT ADDRESS                    00111400
         L    15,124(,15)         ++ GET IOCOM ADDRESS                  00111500
         L    15,208(,15)         ++ GET IOVT ADDRESS                   00111600
         ICM  15,15,244(15)       ++ CHECK IOVTFSDA ADDRESS             00111700
         BNZ   IRDF&SYSNDX        ++ IF VALID, CALL ROUTINE             00111800
         LA   15,4                ++ SET WARNING RETURN CODE            00111900
         LA   0,1                 ++ SET WARNING REASON CODE            00112000
         B     IRDF2&SYSNDX       ++ BRANCH AROUND CALL                 00112100
IRDF&SYSNDX DS     0H                                                   00112200
         BALR 14,15               ++ CALL SERVICE ROUTINE               00112300
IRDF2&SYSNDX DS     0H                                                  00112400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00112500
.ZLINKX               ANOP  ,                                           00112600
.*******************************************************************    00112700
.***AP.X01  FORMAT GENERATED CODE                                       00112800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00112900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00113000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00113100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00113200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00113300
                      AIF   (K'&ZLD LE 0).ZL51                          00113400
&ZLC                  SETA  35             COMMENT COLUMN               00113500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00113600
&ZLC                  SETA  33             COMMENT COLUMN               00113700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00113800
&ZLV                  SETC  '&ZLV'.' '                                  00113900
                      AGO   .ZL31                                       00114000
.*                                                                      00114100
.ZL41                 ANOP  ,                                           00114200
&ZLV                  SETC  '&ZLV'.' '                                  00114300
.ZL51                 ANOP  ,                                           00114400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00114500
.*                                                                      00114600
&ZLL     &ZLOP &ZLV                                                     00114700
.*                                                                      00114800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00114900
                      AGO   .ZL21          CONTINUE LOOPING             00115000
.*                                                                      00115100
.ZLX1                 ANOP  ,                                           00115200
.*******************************************************************    00115300
.***AP.X02  END OF IRDFSDU MACRO                                        00115400
&ZIRDFSDU             SETC  'YES'          SET IRDFSDU USED             00115500
         POP   PRINT                                                    00115600
*                                            IRDFSDU-1                  00115700
         SPACE 1                                                        00115800
.ZLX2                 ANOP  ,                                           00115900
         MEND                                                           00116000
