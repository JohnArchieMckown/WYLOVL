.*                                                                      00090300
.ZLMFSLX              ANOP  ,                                           00090400
.*-------------------------------------------------------------------   00090500
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00090600
&ZPLLEN               SETA 24                                           00090700
.*                                                                      00090800
.******************************************************************     00090900
.***AI.L01 IRDFSDU FMT=LIST FILL IN PARAMETER AREA                      00091000
.******************************************************************     00091100
                      ACTR  10000                                       00091200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00091300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00091400
&ZCVAL                SETC  'LAE'          XAX MODE                     00091500
.ZIMFXA               ANOP ,                                            00091600
.*                                                                      00091700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00091800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00091900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00092000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00092100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00092200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00092300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00092400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00092500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00092600
&ZOPN                 SETA  &ZOPN+1                                     00092700
&ZOPL(&ZOPN)          SETC  ' '                                         00092800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00092900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00093000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00093100
                      AGO   .ZIMFNX                                     00093200
.*                                                                      00093300
.ZIMFN2               ANOP ,                                            00093400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00093500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00093600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00093700
&ZOPN                 SETA  &ZOPN+1                                     00093800
&ZOPL(&ZOPN)          SETC  ' '                                         00093900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00094000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00094100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00094200
                      AGO   .ZIMFNX                                     00094300
.ZIMFNX1              ANOP ,                                            00094400
&ZOPN                 SETA  &ZOPN+1                                     00094500
&ZOPL(&ZOPN)          SETC  ' '                                         00094600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00094700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00094800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00094900
.ZIMFNX               ANOP ,                                            00095000
.********************************************************************   00095100
.***AI.L02 IRDFSDU FMT=LIST INITIALIZE PARAMETER AREA                   00095200
.********************************************************************   00095300
.***AI.L03 MF=(SLE)                                                     00095400
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00095500
.*                                                                      00095600
