.*******************************************************************    00076400
.***AE.01  START OF IRDFSDU MACRO CODE                                  00076500
.*******************************************************************    00076600
               LCLC  &M0M0001                 NAME                      00076700
&M0M0001       SETC  '&XLABEL'                SET NAME                  00076800
               AIF   (K'&XLABEL NE 0).XLAB2                             00076900
&M0M0001       SETC  'M00M&SYSNDX'            DEFAULT NAME              00077000
.*                                                                      00077100
.XLAB2         ANOP  ,                                                  00077200
               AIF   ('&XMF' EQ 'L').XLAB3                              00077300
&M0M0001 DS    0H                             IRDFSDU-1                 00077400
.XLAB3         ANOP  ,                                                  00077500
.*******************************************************************    00077600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00077700
.*******************************************************************    00077800
              PUSH  PRINT                                               00077900
               AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON         00078000
               PRINT OFF         ++ PR01                                00078100
               AGO   .ZPRNTXX                                           00078200
.ZPRINTX       PRINT ON                                                 00078300
.ZPRNTXX       ANOP  ,                                                  00078400
.********************************************************************   00078500
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00078600
.********************************************************************   00078700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00078800
                      LCLA  &ZOPN1         INVOCATION INDEX             00078900
                      LCLC  &ZOPL(4095)    LABEL                        00079000
                      LCLC  &ZOPO(4095)    OPERATION                    00079100
                      LCLC  &ZOPV(4095)    VALUE                        00079200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00079300
.*                                                                      00079400
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00079500
.******************************************************************     00079600
.***AL.L10 IRDFSDU FMT=LIST MF=S|L PARAMETERS                           00079700
.******************************************************************     00079800
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00079900
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00080000
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00080100
&XMFATTR              SETC  '0D'                                        00080200
.*                                                                      00080300
&ZOPN                 SETA  &ZOPN+1                                     00080400
&ZOPL(&ZOPN)          SETC  ' '                                         00080500
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00080600
&ZOPO(&ZOPN)          SETC  'B'                                         00080700
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00080800
                      AGO   .ZALVL6B                                    00080900
.ZALVL6A              ANOP                                              00081000
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00081100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00081200
.ZALVL6B              ANOP                                              00081300
&ZOPD(&ZOPN)          SETC  '++ IRDFSDU ARG PARMS'                      00081400
.*                                                                      00081500
&ZOPN                 SETA  &ZOPN+1                                     00081600
&ZOPL(&ZOPN)          SETC  ' '                                         00081700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00081800
&ZOPV(&ZOPN)          SETC  '1'                                         00081900
&ZOPD(&ZOPN)          SETC  ' '                                         00082000
.*                                                                      00082100
.ZLMFSL2              ANOP  ,                                           00082200
.*-------------------------------------------------------------------   00082300
.***AM.L00  IRDFSDU FMT=LIST name                                       00082400
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00082500
