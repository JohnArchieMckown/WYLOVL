.***AC.PLV01  PROCESS PLISTVER                                      *   00034000
.********************************************************************   00034100
&XPLISTVER      SETA  1                                                 00034200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00034300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00034400
&XPLISTVER      SETA  1                                                 00034500
.ZPLV0          ANOP  ,                                                 00034600
.*                                                                      00034700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00034800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00034900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00035000
.*-------------------------------------------------------------------   00035100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00035200
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00035300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00035400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00035500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00035600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00035700
                AGO   .ZPVCK2                                           00035800
.ZPVCK3         ANOP  ,                                                 00035900
.*-------------------------------------------------------------------   00036000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00036100
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00036200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00036300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00036400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00036500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00036600
                AGO   .ZPVCKE                                           00036700
.ZPVCK2         ANOP  ,                                                 00036800
.*-------------------------------------------------------------------   00036900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00037000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00037100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00037200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00037300
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00037400
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00037500
.ZPVCK6         ANOP  ,                                                 00037600
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00037700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00037800
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00037900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00038000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00038100
&XPLISTVER      SETA  1                                                 00038200
                AGO   .ZPVCKE                                           00038300
.*-------------------------------------------------------------------   00038400
.ZPVCK4A        ANOP  ,                                                 00038500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00038600
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00038700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00038800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00038900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00039000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00039100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00039200
                AGO   .ZPVCK5                                           00039300
.ZPVCK4         ANOP  ,                                                 00039400
&XPLISTVER      SETA  &PLISTVER(1)                                      00039500
.ZPVCK5         ANOP  ,                                                 00039600
.ZPVCKE         ANOP  ,                                                 00039700
.********************************************************************   00039800
.***AG.MF01 PROCESS MF MACRO FORMS                                      00039900
.********************************************************************   00040000
               ACTR  10000                                              00040100
.*------------------------------------------------------------------    00040200
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00040300
&XMF           SETC  ''          INITIALIZE XVARIABLE                   00040400
.*-------------------------------------------------------------------   00040500
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00040600
.ZG15          ANOP  ,                                                  00040700
&ZTLC          SETC 'S'                                                 00040800
               AIF   ('&MF(1)' NE '&ZTLC').ZG16                         00040900
&XMF           SETC  'S'         SET KEYWORD                            00041000
               AIF   (N'&MF LE 1).ZG15A         CK NUM                  00041100
&ZMS           SETC  '"MF='      *KPOSA* MNOTE                          00041200
&ZMS           SETC  '&ZMS.&MF"'                                        00041300
&ZMS           SETC  '&ZMS "MF"'                                        00041400
               MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                00041500
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00041600
.ZG15A         ANOP  ,                                                  00041700
.*-------------------------------------------------------------------   00041800
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00041900
.ZG16          ANOP  ,                                                  00042000
&ZTLC          SETC 'L'                                                 00042100
               AIF   ('&MF(1)' NE '&ZTLC').ZG23                         00042200
&XMF           SETC  'L'         SET KEYWORD                            00042300
               AIF   (N'&MF LE 3).ZG16A         CK NUM                  00042400
&ZMS           SETC  '"MF='      *KPOSA* MNOTE                          00042500
&ZMS           SETC  '&ZMS.&MF"'                                        00042600
&ZMS           SETC  '&ZMS "MF"'                                        00042700
               MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'               00042800
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00042900
.ZG16A         ANOP  ,                                                  00043000
.*-------------------------------------------------------------------   00043100
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00043200
               AIF   (N'&SYSLIST NE 0).ZG16B    POSITIONALS?            00043300
               AIF   ('&TOLERANCE' NE '').ZG16B  KEYS?                  00043400
               AIF   ('&RETCODE' NE '').ZG16B   KEYS?                   00043500
               AIF   ('&RSNCODE' NE '').ZG16B   KEYS?                   00043600
               AGO    .ZG16C     INVOCATION OKAY                        00043700
.ZG16B         ANOP  ,                                                  00043800
&ZMS           SETC  'THE "L" FORM OF "MF"'     *MFLA* MNOTE            00043900
&ZMS           SETC  '&ZMS CAN ONLY HAVE'                               00044000
&ZMS           SETC  '&ZMS THE "PLISTVER" KEY'                          00044100
               MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'             00044200
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00044300
.ZG16C         ANOP  ,                                                  00044400
.*-------------------------------------------------------------------   00044500
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00044600
&ZXMFCTRL      SETC  'INPUT'     SET IN/OUTPUT                          00044700
&XMFCTRL       SETC  '&MF(2,1)'  SET XVARIABLE                          00044800
               AIF   (N'&MF(2) GT 1).ZG18B      MULTI PARMS             00044900
&XMFCTRL       SETC  '&MF(2)'    SET XVARIABLE                          00045000
.ZG18B         AIF   (K'&MF(2) GT 0).ZG18C                              00045100
&ZXMFCTRL      SETC  ''          RESET IN/OUTPUT                        00045200
&ZMS           SETC  'WHEN "L" IS SPECIFIED'    *KRQDA*                 00045300
&ZMS           SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'                00045400
               MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                       00045500
