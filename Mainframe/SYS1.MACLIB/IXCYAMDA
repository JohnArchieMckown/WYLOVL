**/ IXCYAMDA_2:;                                                        12022400
* %IXCYAMDA_INCLUDED = 'YES';                                           12034600
* %DCL ZCBPRINT CHAR EXT;                                               12046800
* %DEACTIVATE ZCBPRINT;                                                 12059000
* %DCL IXCYAMDA_LIST CHAR EXT;                                          12071200
* %DEACTIVATE IXCYAMDA_LIST;                                            12083400
* %IF IXCYAMDA_LIST ^= 'NO' &                                           12095600
*    ZCBPRINT ^= 'NO' %THEN                                             12107800
*   %GOTO IXCYAMDA_3;                                                   12120000
*   @LIST PUSH NOECHO;                                                  12132200
*   @LIST NOASSEMBLE NOECHO;                                            12144400
*   @LIST OFF C NOECHO;                                                 12156600
* %IXCYAMDA_3:;                                                         12168800
*/* Start of PL/X Source                                             */ 12181000
* %IF AMDA_ALET = '' %THEN                                              12200000
*   %AMDA_ALET = '0';                                                   12250000
* %AMDA_VIA='';                                                         12300000
* %IF AMDA_ALET^='0' %THEN                                              12350000
*  %AMDA_VIA='VIA(AMDA_ALET)';                                          12400000
*   DECLARE                                                             12450000
*     1    AMDAREA BASED AMDA_VIA, /* Data area returned to caller  */  12500000
*      2    AMDATLEN FIXED(32),     /* Total length of output data area 12550000
*                                      needed to contain all the        12600000
*                                      requested information.  This     12650000
*                                      length includes the area for     12700000
*                                      the records that were returned   12750000
*                                      on this call.                 */ 12800000
*      2    AMDA#PTH FIXED(32),     /* Number of path entries        */ 12850000
*      2    AMDALPTH FIXED(32),     /* Length of path data           */ 12900000
*      2    AMDAOPTH FIXED(32),     /* Offset to path entries        */ 12950000
*                                                                       13000000
*      2    AMDA#MPE FIXED(32),     /* Number of pending message        13050000
*                                      entries                       */ 13100000
*      2    AMDALMPE FIXED(32),     /* Length of pending message        13150000
*                                      data                          */ 13200000
*      2    AMDAOMPE FIXED(32),     /* Offset to pending message        13250000
*                                      entries                       */ 13300000
*                                                                       13350000
*      2    AMDA#SYS FIXED(32),     /* Number of system entries      */ 13400000
*      2    AMDALSYS FIXED(32),     /* Length of system data         */ 13450000
*      2    AMDAOSYS FIXED(32),     /* Offset to system entries      */ 13500000
*                                                                       13550000
*      2    AMDA#SD  FIXED(32),     /* Number of source/destination     13578500
*                                      entries.                      */ 13607000
*      2    AMDALSD  FIXED(32),     /* Length of source/destination     13635500
*                                      data entries                  */ 13664000
*      2    AMDAOSD  FIXED(32),     /* Offset to source/destination     13692500
*                                      entries.                      */ 13721000
*                                                                       13750000
*      2    AMDA#MUS FIXED(32),     /* Number of member message use     13775000
*                                      summary entries           @L6A*/ 13800000
*      2    AMDALMUS FIXED(32),     /* Length of member message space   13825000
*                                      summary entries           @L6A*/ 13850000
*      2    AMDAOMUS FIXED(32);     /* Offset to member message use     13875000
*                                      summary entries           @L6A*/ 13900000
*                                                                       13925000
*                                                                       13950000
*   DECLARE                                                             14100000
*     1    AMDPATH  BASED AMDA_VIA, /* Path activity entry, placed in   14150000
*                                      the data area first if           14200000
*                                      requested                     */ 14250000
*      3    AMDPTYPE BIT(8),        /* Indication of type of data    */ 14300000
*        5    *        BIT(7),      /* Reserved and set to 0         */ 14350000
*        5    AMDPTYPP BIT(1),      /* Indicates path data           */ 14400000
*      3    *        BIT(8),        /* Reserved and set to 0         */ 14450000
