          IKJTMPWA &WORKA1=YES                                          00217000
          AIF   ('&WORKA1' EQ 'NO').WORKA2                              00218000
*********************************************************************/  00219000
**** TMPWRKA1 -- TMP PROBLEM PROGRAM STATE DYNAMIC WORK AREA        */  00220000
****                                                                */  00221000
****   THIS DYNAMIC WORK AREA IS GOTTEN FROM SUBPOOL 1, PP KEY, BY  */  00222000
****   IKJEFT01  DURING  TMP INITIALIZATION AND ANOTHER ONE         */  00223000
****   WILL BE GOTTEN IN IKJEFTSC IF A PARALLEL TMP IS NEEDED AND   */  00224000
****   THIS WORKAREA WILL BE FREED WHEN THE PARALLEL TMP IS FINISHED*/  00225000
****   WHILE THE ONE GOTTEN FOR THE NON-PARALLEL SIDE BY IKJEFT01   */  00226000
****   WILL NOT BE FREED UNTIL THE END OF THE TERMINAL SESSION.     */  00227000
****   DURING THE TMP SEVERITY 2 STAE RETRY ALL OF STORAGE IS FREED */  00228000
****   AND THIS WORKAREA MUST BE REINITIALIZED.                     */  00229000
****                                                        @E1113KJ*/  00230000
*********************************************************************/  00231000
         IKJTPL                                                         00232000
TMPWRKA1 EQU  TPL    WORK AREA BEGINS WITH TEST PARAMETER LIST          00233000
*     /* TMP COMMON VARIABLES AND WORK AREAS                        */  00234000
TMPNECB  DS    F                     /* ECB FOR STAI WAIT           */  00235000
TMPCECB  DS    F                     /* ECB FOR ATTACHED CP         */  00236000
TMPIECB  DS    F                     /* ECB FOR STAI POST           */  00237000
TMPAECB  DS    F                     /* ECB FOR ATTN POST           */  00238000
TMPCMDWT DS    F                     /* PTR TO CMD FROM ATTN EXIT   */  00239000
*                                                                       00240000
TMPSWS   DS    F                     /* TMP INTERNAL SWITCHES       */  00241000
TMPTEST  EQU   X'80'                 /* TEST PROGRAM IN CONTROL     */  00242000
TMPCMDW  EQU   X'40'                 /* COMMAND WAITING             */  00243000
TMPNFCMD EQU   X'20'                 /* FIRST COMMAND IS PROCESSED  */  00244000
TMPACTRL EQU   X'10'                 /* TMP ATTN EXIT IS IN CONTROL */  00245000
TMPSCTRL EQU   X'08'                 /* TMP STAI EXIT IS IN CONTROL */  00246000
ABND806  EQU   X'04'                 /* NO-MODULE FOUND BY FETCH    */  00247000
FRSTLAB  EQU   X'02'                 /* 1ST LEVEL ATTACHEE ABENDED  */  00248000
NONSCUR  EQU   X'01'                /* SECURITY AUTHORIZATION FAILS */  00249000
*                                                                       00250000
ATCHNOW  EQU   X'80'                 /* ABEND OCCURRED IN ATTACH    */  00251000
LOADNOW  EQU   X'40'                 /* ABEND OCCURRED IN LOAD      */  00252000
LINKNOW  EQU   X'20'                 /* ABEND OCCURRED IN LINK      */  00253000
FRSTEX   EQU   X'10'                /* FIRST EXPL/IMPLICIT EXEC TRY */  00254000
CALLNOW  EQU   X'08'                 /* CALL FUNCTION ACTIVE        */  00255000
TMP1TIME EQU   X'04'                 /* ESTAI ENTERED(TEST) @ZTM5046*/  00256000
T7TDONE  EQU   X'02'                 /* TSEVENT ISSUED     @ZA66275 */  00257000
SKPATTN  EQU   X'01'                 /* 1-BYPASS ATTN       @ZA89253*/  00258000
*                                                                       00259000
TMP1TSFE EQU   X'80'                 /* ERROR OCCURRED IN CLIST         00260000
*                                       WHILE IN TSF/CLIST MODE.        00261000
*                                                           @PBY0316*/  00262000
CALLSWS  EQU   TMPSWS+3              /* TMP-CALL INTERNAL SWITCHES  */  00263000
PDLPRES  EQU   X'80'                 /* PDL RETURNED BY PARSE       */  00264000
DSOPEN   EQU   X'40'                 /* DATA SET IS OPEN            */  00265000
BLANKB   EQU   X'10'                 /* DATA SET NAME PROCESSED     */  00266000
DORELS   EQU   X'08'                 /* RELEASE PDL NOW             */  00267000
GMBRNOW  EQU   X'04'                 /* GET MEMBER NAME             */  00268000
PCFDA    EQU   X'02'                 /* PCF DIRECT ATTACH   @YA18897*/  00269000
*        EQU   X'01'                 /* RESERVED FLAG       @YA18897*/  00270000
*     /* RESERVED AREAS                                             */  00271000
TMPT9ECB DS  A                       /* ECB USED FOR COMMUNICATION      00272000
*                                       BETWEEN IKJEFT09 AND IKJURPS    00273000
*                                                           @E24D2H3*/  00274000
TMPURPA  DS  A                       /* ANCHOR FOR URP REQUEST BLOCK    00275000
*                                       CHAIN FOR IKJEFT09  @E24D2H3*/  00276000
RESCOMM  DS     CL8                  /*                     @E24D2H3*/  00277000
RESCOM2  DS     CL16                                                    00278000
RESCOM3  DS     CL16                                                    00279000
RESCOM4  DS     CL16                                                    00280000
         DS     CL4                /* RESERVED WAS FLOFLGS  @ZA46174*/  00281000
CPPLPTR  DS     F                    /* PTR TO CP PARM LIST         */  00282000
CSOAPTR  DS     F                    /* PTR TO CMD SCAN PARM LIST   */  00283000
CSPLPTR  DS     F                    /* PTR TO CMD SCAN PARM LIST   */  00284000
DAPLPTR  DS     F                    /* PTR TO DAIR PARM LIST       */  00285000
GTPBPTR  DS     F                    /* PTR TO GETLINE PARM BLOCK   */  00286000
IOPLPTR  DS     F                    /* PTR TO I/O RTNS PARM LIST   */  00287000
PGPBPTR  DS     F                    /* PTR TO PUTGET PARM BLOCK    */  00288000
PPLPTR   DS     F                    /* PTR TO PARSE PARM LIST      */  00289000
PTPBPTR  DS     F                    /* PTR TO PUTLINE PARM BLOCK   */  00290000
STPLPTR  DS     F                    /* PTR TO STACK PARM LIST      */  00291000
ACEEPTR  DS     F                    /* ADDR OF ACEE                */  00292000
ASCANAP  DS     F                    /* ADDR OF ATTN SCAN ANSWER    */  00293000
ASRPLPTR DS     F                    /* ADDR OF ATTN SRPL           */  00294000
ATTCHPTR DS     F                    /* ADDR OF ATTACH PARM LIST    */  00295000
CDCBPTR  DS     F                    /* PTR TO CALL DCB             */  00296000
DCBPTR   DS     F                    /* PTR TO DCB                  */  00297000
DYNAPPTR DS     F                    /* PTR TO DYNALLOC PARM LIST   */  00298000
EBCDPTR  DS     F                    /* PTR TO TRANSLATE TABLE      */  00299000
READYPTR DS     F                    /* ADDR OF TMP MODE MESSAGE    */  00300000
SCANAP   DS     F                    /* ADDR OF SCAN ANSWER AREA    */  00301000
SRPLPTR  DS     F                    /* ADDR OF SRPL                */  00302000
         DS     F                    /* RESERVED            @ZA96882*/  00303000
STPBPTR  DS     F                    /* ADDR OF STACK PARM LIST     */  00304000
*     /* RESERVE SPACE FOR PARAMETER LISTS, BLOCKS                  */  00305000
         DS     0D                   /* ALIGN TO DOUBLEWORD         */  00306000
ABMSGSP  DS     CL41                  /* MESSAGE AREA       @PID0241*/  00307000
*     /* PUTLINE ACTIVE SEGMENT LIST    LIST                        */  00308000
ACTSL    DS     0F                   /* NAME OF LIST                */  00309000
ACTSEG   DS     28F                  /* SEGMENTS                    */  00310000
ACTSEGA  EQU    ACTSEG               /* FIRST HWORD OF SEGMENT      */  00311000
ACTSEGB  EQU    ACTSEG+2             /* SECOND HWORD OF SEGMENT     */  00312000
AMSGLIST DS     0F                   /* ATTN MESSAGE LIST           */  00313000
ANUMSEG  DS     F                    /* NUMBER OF MESSAGE SEGMENTS  */  00314000
AMSGSEG  DS     2F                   /* ARRAY OF SEGMENT PTRS       */  00315000
ARCODE   DS     F                    /* ATTN RETURN CODE SAVE AREA  */  00316000
ASCANFLG DS     F                    /* ATTN SCAN FLAGS             */  00317000
ASRPARM  DS     5F                   /* ATTN SR PARM AREA           */  00318000
ATTCHSP  DS     18F                  /* ATTACH PARM LIST SP @ZTY0371*/  00319000
BLDLLST  DS     0CL68                /* BLDL ENTRY                  */  00320000
XTRCLST  DS     0CL12                /* EXTRACT LIST                */  00321000
BLDLENT  DS     H                    /* NUM OF ENTRIES              */  00322000
BLDLELNG DS     H                    /* LENGTH OF ENTRY             */  00323000
BLDLNAME DS     CL8                  /* NAME OF COMMAND             */  00324000
BLDLTTRZ DS     CL56                 /* PAD TO FULL WORD    @ZA40795*/  00325000
         ORG    BLDLTTRZ             /* Redefine BLDLTTRZ   @WA36096*/  00325100
BLDLTMP_TT DS   CL2                  /* TT (relative track) returned    00325200
*                                       from BLDL           @WA36096*/  00325300
BLDLTMP_R DS    C                    /* R (record number) returned      00325400
*                                       from BLDL           @WA36096*/  00325500
         ORG    ,                    /* End of redefine     @WA36096*/  00325600
         DS     0D                   /* ALIGN TO DWORD              */  00326000
CDCBSP   DS     CL140                /* CALL DCB SPACE              */  00327000
CLOSESP  DS     CL12                 /* CLOSE PL SPACE              */  00328000
CPPLSP   DS     4F                   /* CPPL SPACE                  */  00329000
CSOASP   DS     2F                   /* CSOA SPACE                  */  00330000
CSOASP2  DS     2F                   /* 2ND CSOA SP (ATTN)          */  00331000
CSPLSP   DS     6F                   /* CSPL SPACE                  */  00332000
CSPLSP2  DS     6F                   /* 2ND CSPL SP (ATTN)          */  00333000
CTLBKSP  DS     0F                   /* NAME OF BLOCK SPACE         */  00334000
CTLBLKL  DS     F                    /* LENGTH OF BLOCK SPACE       */  00335000
CTLBLKA  DS     F                    /* LOC OF BLOCK SPACE          */  00336000
CTLBLKN  DS     F                    /* SUBPOOL                     */  00337000
DAPBSP   DS     21F                  /* DAIR PARM BLK SPACE         */  00338000
DAPLSP   DS     5F                   /* DAIR PARM LIST SPACE        */  00339000
         DS     0D                   /* ALIGN TO DOUBLEWORD         */  00340000
DCBSP    DS     CL140                /* DCB SPACE                   */  00341000
DYNASP   DS     10F                  /* DYNALLOC PL         @PID0180*/  00342000
DYNATUB  DS     BL4                  /* BIT FORM OF THE PLATFORM        00343000
*                                       TCB ADDRESS USED SO THAT THE    00344000
*                                       ADDRESS, NORMALLY ON A          00345000
*                                       WORD BOUNDARY, CAN BE COPIED    00346000
*                                       INTO THE TEXT UNIT PARM THAT'S  00347000
*                                       ON A HALFWORD                   00348000
