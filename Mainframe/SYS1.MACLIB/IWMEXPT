                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             34000000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        34050000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      34100000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     34150000
.ZPVCK6         ANOP  ,                                                 34200000
