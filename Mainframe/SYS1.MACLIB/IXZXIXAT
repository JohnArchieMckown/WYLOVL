.***AC.PLV01  PROCESS PLISTVER                                      *   00066400
.********************************************************************   00066500
&XPLISTVER      SETA  5                                                 00066600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00066700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00066800
&XPLISTVER      SETA  6                                                 00066900
.ZPLV0          ANOP  ,                                                 00067000
.*                                                                      00067100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00067200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00067300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00067400
.*-------------------------------------------------------------------   00067500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00067600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00067700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00067800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00067900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00068000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068100
                AGO   .ZPVCK2                                           00068200
.ZPVCK3         ANOP  ,                                                 00068300
.*-------------------------------------------------------------------   00068400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00068500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00068600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00068800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00068900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069000
                AGO   .ZPVCKE                                           00069100
.ZPVCK2         ANOP  ,                                                 00069200
.*-------------------------------------------------------------------   00069300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00069400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00069500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00069600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00069700
                AIF   (&ZN LT 5).ZPVCK6    TOO LOW                      00069800
                AIF   (&ZN LE 6).ZPVCK4A   NOT HIGH                     00069900
.ZPVCK6         ANOP  ,                                                 00070000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00070100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00070200
&ZMS            SETC  '&ZMS THE RANGE IS "5-6"'                         00070300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00070400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070500
&XPLISTVER      SETA  6                                                 00070600
                AGO   .ZPVCKE                                           00070700
.*-------------------------------------------------------------------   00070800
.ZPVCK4A        ANOP  ,                                                 00070900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00071000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00071100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00071200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00071300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00071400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00071500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071600
                AGO   .ZPVCK5                                           00071700
.ZPVCK4         ANOP  ,                                                 00071800
&XPLISTVER      SETA  &PLISTVER(1)                                      00071900
.ZPVCK5         ANOP  ,                                                 00072000
.ZPVCKE         ANOP  ,                                                 00072100
.********************************************************************   00072200
