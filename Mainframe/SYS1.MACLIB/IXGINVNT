.*******************************************************************    01268100
.***  PROCESS IXGINVNT OUTPUT PARAMETERS                                01268200
.*******************************************************************    01268300
                      ACTR  10000                                       01268400
.*******************************************************************    01268500
.***AO.X02  RETURN CODE PROCESSING                                      01268600
.*******************************************************************    01268700
                      AIF   (N'&RETCODE LE 0).ZMRETX                    01268800
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            01268900
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             01269000
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             01269100
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               01269200
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                01269300
&ZOPN                 SETA  &ZOPN+1                                     01269400
&ZOPL(&ZOPN)          SETC  ' '                                         01269500
&ZOPO(&ZOPN)          SETC  'ST'                                        01269600
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              01269700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01269800
                      AGO   .ZMRETX                                     01269900
.*                                                                      01270000
.ZMRET2               ANOP  ,                                           01270100
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                01270200
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                01270300
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                01270400
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  01270500
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   01270600
&ZOPN                 SETA  &ZOPN+1                                     01270700
&ZOPL(&ZOPN)          SETC  ' '                                         01270800
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  01270900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 01271000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01271100
.ZMRETX               ANOP  ,                                           01271200
.*******************************************************************    01271300
.***AO.X03  REASON CODE PROCESSING                                      01271400
.*******************************************************************    01271500
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    01271600
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            01271700
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              01271800
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             01271900
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              01272000
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             01272100
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                01272200
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 01272300
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                01272400
&ZOPN                 SETA  &ZOPN+1                                     01272500
