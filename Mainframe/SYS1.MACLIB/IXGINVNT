.******************************************************************     01046800
.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        01046900
.******************************************************************     01047000
                      ACTR  10000                                       01047100
.*----------------------------------------------------------------***   01047200
.***AF.P02 EXTRA POSITIONALS                                            01047300
                      AIF   (N'&SYSLIST LE 0).ZELISTX  NO EXTRAS        01047400
&ZCVAL                SETC  ''          ERROR FIELD                     01047500
&ZAWRK                SETA  0           POSITIONAL NUMBER               01047600
.ZELIST2              ANOP  ,                                           01047700
&ZAWRK                SETA  &ZAWRK+1    NEXT POSITIONAL                 01047800
                      AIF   (&ZAWRK GT N'&SYSLIST).ZELIST3  DONE        01047900
&ZCVAL                SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA             01048000
                      AGO   .ZELIST2                                    01048100
.*                                                                      01048200
.ZELIST3              ANOP  ,                                           01048300
&ZCVAL                SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA          01048400
&ZMS                  SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE    01048500
&ZMS                  SETC  '&ZMS "&ZCVAL"'                             01048600
&ZMS                  SETC  '&ZMS CANNOT BE USED.'                      01048700
&ZMS                  SETC  '&ZMS NO POSITIONAL ARGUMENTS'              01048800
                      MNOTE 8,' &ZMS ARE ALLOWED.'                      01048900
&ZMACRET              SETA  8           SET MACRO ERROR RETURN CODE     01049000
.ZELISTX              ANOP  ,                                           01049100
&ZASYSLNDX            SETA  0                                           01049200
.*-------------------------------------------------------------------   01049300
.***AE.MF02 MF=(SLME)                                                   01049400
.ZEMFX                ANOP  ,           END OF PARTIAL SYNTAX CHECK     01049500
.*******************************************************************    01049600
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       01049700
.*******************************************************************    01049800
                      AIF   (&ZMACRET NE 0).ZLX2  ERROR DETECTED        01049900
.*******************************************************************    01050000
.***AE.01  START OF IXGINVNT MACRO CODE                                 01050100
.*******************************************************************    01050200
                      LCLC  &M0M0001                 NAME               01050300
&M0M0001              SETC  '&XLABEL'                SET NAME           01050400
                      AIF   (K'&XLABEL NE 0).XLAB2                      01050500
&M0M0001              SETC  'M00M&SYSNDX'            DEFAULT NAME       01050600
.*                                                                      01050700
.XLAB2                ANOP  ,                                           01050800
                      AIF   ('&XMF' EQ 'L').XLAB3                       01050900
&M0M0001 DS    0H                             IXGINVNT-2                01051000
.XLAB3                ANOP  ,                                           01051100
.*******************************************************************    01051200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              01051300
.*******************************************************************    01051400
              PUSH  PRINT                                               01051500
                      AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON  01051600
                      PRINT OFF         ++ PR01                         01051700
                      AGO   .ZPRNTXX                                    01051800
.ZPRINTX              PRINT ON                                          01051900
.ZPRNTXX              ANOP  ,                                           01052000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01052100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01052200
.********************************************************************   01052300
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     01052400
.********************************************************************   01052500
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       01052600
                      LCLA  &ZOPN1         INVOCATION INDEX             01052700
                      LCLC  &ZOPL(4095)    LABEL                        01052800
                      LCLC  &ZOPO(4095)    OPERATION                    01052900
                      LCLC  &ZOPV(4095)    VALUE                        01053000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  01053100
.*                                                                      01053200
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     01053300
.******************************************************************     01053400
.***AL.L10 IXGINVNT FMT=LIST MF=S|L PARAMETERS                          01053500
.******************************************************************     01053600
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     01053700
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     01053800
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     01053900
&XMFATTR              SETC  '0D'                                        01054000
.*                                                                      01054100
&ZOPN                 SETA  &ZOPN+1                                     01054200
&ZOPL(&ZOPN)          SETC  ' '                                         01054300
                      AIF   (&SYSALVL NE 0).ZALVL6A                     01054400
&ZOPO(&ZOPN)          SETC  'B'                                         01054500
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                01054600
                      AGO   .ZALVL6B                                    01054700
.ZALVL6A              ANOP                                              01054800
&ZOPO(&ZOPN)          SETC  'BRAS'                                      01054900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              01055000
.ZALVL6B              ANOP                                              01055100
&ZOPD(&ZOPN)          SETC  '++ IXGINVNT ARG PARMS'                     01055200
.*                                                                      01055300
&ZOPN                 SETA  &ZOPN+1                                     01055400
&ZOPL(&ZOPN)          SETC  ' '                                         01055500
&ZOPO(&ZOPN)          SETC  'SPACE'                                     01055600
&ZOPV(&ZOPN)          SETC  '1'                                         01055700
&ZOPD(&ZOPN)          SETC  ' '                                         01055800
.*                                                                      01055900
.ZLMFSL2              ANOP  ,                                           01056000
.*-------------------------------------------------------------------   01056100
.***AM.L00  IXGINVNT FMT=LIST NAME                                      01056200
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    01056300
&ZOPN                 SETA &ZOPN+1                                      01056400
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   01056500
&ZOPO(&ZOPN)          SETC 'EQU'                                        01056600
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   01056700
