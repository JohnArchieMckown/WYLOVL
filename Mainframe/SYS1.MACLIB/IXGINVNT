&ZMS                  SETC  '"RETCODE='         *KPOSA* MNOTE           01039300
&ZMS                  SETC  '&ZMS.&RETCODE"'                            01039400
&ZMS                  SETC  '&ZMS "RETCODE"'                            01039500
                      MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'         01039600
&ZMACRET              SETA  8           SET MACRO ERROR RETURN CODE     01039700
.ZE223A               ANOP  ,                                           01039800
.*-------------------------------------------------------------------   01039900
.***AE.KXVAR04 NAME, OPT OUTPUT, KEYS -XVAR ARGUMENT                    01040000
                      AIF   (K'&RETCODE(1) LE 0).ZE223B NO PRM          01040100
&ZXRETCODE            SETC  'OUTPUT'    SET IN/OUTPUT                   01040200
&XRETCODE             SETC  '&RETCODE(1)'       SET XVARIABLE           01040300
                      AIF   (N'&RETCODE GT 1).ZE223B                    01040400
.*                    ANOP              MULTIPLE PARMS                  01040500
&XRETCODE             SETC  '&RETCODE'  SET XVARIABLE                   01040600
.ZE223B               ANOP  ,                                           01040700
.*-------------------------------------------------------------------   01040800
.***AE.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   01040900
                      AIF   (K'&ZXRETCODE LE 0).ZE223R  NOT I/O         01041000
                      AIF   ('&XRETCODE'(1,1) NE '(').ZE223R            01041100
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                01041200
                      AIF   ('&ZCGPR'(1,1) NE '0').ZE223P               01041300
                      AIF   (K'&ZCGPR GT 2).ZE223Q                      01041400
                      AIF   ('&ZCGPR' EQ '0').ZE223Q                    01041500
&ZCGPR                SETC  '&ZCGPR'(2,1)                               01041600
                      AIF   (T'&ZCGPR NE 'N').ZE223Q  INVALID REG NAME  01041700
.ZE223P               ANOP  ,                                           01041800
                      AIF   (T'&ZCGPR NE 'N').ZE223R  NON NUMERIC       01041900
                      AIF   ('&ZCGPR' LT '2').ZE223Q                    01042000
                      AIF   ('&ZCGPR' LE '12').ZE223R                   01042100
.ZE223Q               ANOP  ,                                           01042200
&ZMS                  SETC  '"RETCODE='         *KGPRA* MNOTE           01042300
&ZMS                  SETC  '&ZMS.&RETCODE"'                            01042400
&ZMS                  SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'   01042500
                      MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'            01042600
&ZMACRET              SETA  8           SET MACRO ERROR RETURN CODE     01042700
.ZE223R               ANOP  ,                                           01042800
.*----------------------------------------------------------------***   01042900
.***AE.KXVAR03 NAME, OPT OUTPUT, KEYS -XVAR KEY                         01043000
