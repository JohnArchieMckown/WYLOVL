                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01124700
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01124800
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01124900
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01125000
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01125100
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01125200
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01125300
&ZOPN                 SETA  &ZOPN+1                                     01125400
&ZOPL(&ZOPN)          SETC  ' '                                         01125500
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01125600
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01125700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01125800
                      AGO   .ZIMFNX                                     01125900
.*                                                                      01126000
.ZIMFN2               ANOP ,                                            01126100
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01126200
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01126300
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01126400
&ZOPN                 SETA  &ZOPN+1                                     01126500
&ZOPL(&ZOPN)          SETC  ' '                                         01126600
&ZOPO(&ZOPN)          SETC  'LAE'                                       01126700
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01126800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01126900
                      AGO   .ZIMFNX                                     01127000
.ZIMFNX1              ANOP ,                                            01127100
