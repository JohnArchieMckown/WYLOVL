&ZOPO(&ZOPN)          SETC  'ST'                                        01272700
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               01272800
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        01272900
                      AGO   .ZMRSNX                                     01273000
.*                                                                      01273100
.ZMRSN2               ANOP  ,                                           01273200
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                01273300
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 01273400
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                01273500
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 01273600
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                01273700
