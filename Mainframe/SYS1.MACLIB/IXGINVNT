                      AGO   .ZL2           CONTINUE LOOPING             01278500
.*                                                                      01278600
.ZLX                  ANOP  ,                                           01278700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01278800
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01278900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01279000
         L     15,16(0,0)           GET CVT ADDRESS                     01279100
         TM    1265(15),X'10'       TEST IF LOGGER FUNCTIONS AVAILABLE  01279200
         BO    *+16                 BRANCH IF FUNCTIONS AVAILABLE       01279300
         LA    0,2068               LOAD FAILURE REASON CODE            01279400
         LA    15,8                 LOAD FAILURE RETURN CODE            01279500
         B     *+22                 BRANCH BACK TO USER                 01279600
         L     15,140(15,0)         GET ECVT ADDRESS                    01279700
         L     15,288(15,0)         GET LSAB ADDRESS                    01279800
         L     15,124(15,0)         GET ENTRY POINT ADDRESS             01279900
         LA    0,8                  EX OF INVENTORY SERVICE             01280000
         BALR  14,15                INVOKE SERVICE ROUTINE              01280100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01280200
