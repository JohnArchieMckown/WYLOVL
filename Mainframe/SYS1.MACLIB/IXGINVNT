.***AC.PLV01  PROCESS PLISTVER                                      *   00412800
.********************************************************************   00412900
&XPLISTVER      SETA  0                                                 00413000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00413100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00413200
&XPLISTVER      SETA  2                                                 00413300
.ZPLV0          ANOP  ,                                                 00413400
.*                                                                      00413500
                AIF   ('&DASDONLY' EQ '').ZPLV1E                        00413600
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00413700
&XPLISTVER      SETA  2                                                 00413800
.ZPLV1E         ANOP  ,                                                 00413900
.*                                                                      00414000
                AIF   ('&RMNAME' EQ '').ZPLV2E                          00414100
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00414200
&XPLISTVER      SETA  1                                                 00414300
.ZPLV2E         ANOP  ,                                                 00414400
.*                                                                      00414500
                AIF   ('&DESCRIPTION' EQ '').ZPLV3E                     00414600
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00414700
&XPLISTVER      SETA  1                                                 00414800
.ZPLV3E         ANOP  ,                                                 00414900
.*                                                                      00415000
                AIF   ('&RETPD' EQ '').ZPLV4E                           00415100
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00415200
&XPLISTVER      SETA  1                                                 00415300
.ZPLV4E         ANOP  ,                                                 00415400
.*                                                                      00415500
                AIF   ('&DIAG' EQ '').ZPLV5E                            00415600
                AIF   (&XPLISTVER GE 1).ZPLV5E                          00415700
&XPLISTVER      SETA  1                                                 00415800
.ZPLV5E         ANOP  ,                                                 00415900
.*                                                                      00416000
                AIF   ('&OFFLOADRECALL' EQ '').ZPLV6E                   00416100
                AIF   (&XPLISTVER GE 2).ZPLV6E                          00416200
&XPLISTVER      SETA  2                                                 00416300
.ZPLV6E         ANOP  ,                                                 00416400
.*                                                                      00416500
                AIF   ('&RMNAME' EQ '').ZPLV7E                          00416600
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00416700
&XPLISTVER      SETA  1                                                 00416800
.ZPLV7E         ANOP  ,                                                 00416900
.*                                                                      00417000
                AIF   ('&DESCRIPTION' EQ '').ZPLV8E                     00417100
                AIF   (&XPLISTVER GE 1).ZPLV8E                          00417200
&XPLISTVER      SETA  1                                                 00417300
.ZPLV8E         ANOP  ,                                                 00417400
.*                                                                      00417500
                AIF   ('&RETPD' EQ '').ZPLV9E                           00417600
                AIF   (&XPLISTVER GE 1).ZPLV9E                          00417700
&XPLISTVER      SETA  1                                                 00417800
.ZPLV9E         ANOP  ,                                                 00417900
.*                                                                      00418000
                AIF   ('&DIAG' EQ '').ZPLV10E                           00418100
                AIF   (&XPLISTVER GE 1).ZPLV10E                         00418200
&XPLISTVER      SETA  1                                                 00418300
.ZPLV10E        ANOP  ,                                                 00418400
.*                                                                      00418500
                AIF   ('&OFFLOADRECALL' EQ '').ZPLV11E                  00418600
                AIF   (&XPLISTVER GE 2).ZPLV11E                         00418700
&XPLISTVER      SETA  2                                                 00418800
.ZPLV11E        ANOP  ,                                                 00418900
.*                                                                      00419000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00419100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00419200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00419300
.*-------------------------------------------------------------------   00419400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00419500
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00419600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00419700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00419800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00419900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00420000
                AGO   .ZPVCK2                                           00420100
.ZPVCK3         ANOP  ,                                                 00420200
.*-------------------------------------------------------------------   00420300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00420400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00420500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00420600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00420700
