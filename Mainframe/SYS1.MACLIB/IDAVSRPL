         LCLB  &NEX,&EX,&FME,&NFME,&NRRN,&RRN    RESPOND CODES  X3004BS 03750000
         LCLB  &QRESP,&NQRESP                    RESPOND CODES @G40AKSL 03800000
         LCLB  &DATA,&CANCEL,&QC,&QEC,&CHASE     CONTROL CODES  X3004BS 03850000
         LCLB  &RELQ,&CSDT,&CLEAR,&STSN          CONTROL CODES X03004HS 03900000
         LCLB  &SHUTD,&RQR,&SHUTC                CONTROL CODES @ZA07554 03950000
         LCLB  &BID,&LUS,&SIGNAL,&RTR,&RSHUTD    CONTROL CODES X03004HS 04000000
         LCLB  &SBI,&BIS,&BIND,&UNBIND           CONTROL CODES @G40AKCM 04050000
         LCLB  &STDRD,&ALT                       CODESEL CODE  @Z40BHUC 04100000
         LCLB  &SET,&TESTSET,&RESET,&IGNORE      OBSQAC CODES   X3004BS 04150000
         LCLB  &TESTPOS,&TESTNEG,&INVALID        OBSQAC CODES   X3004BS 04200000
         LCLB  &ISET,&ITESTSE,&IRESET,&IIGNORE   IBSQAC CODES   X3004BS 04250000
         LCLB  &ITESTPO,&ITESTNE,&IINVALI        IBSQAC CODES   X3004BS 04300000
         LCLB  &OCPM,&OSTATE,&OFI,&ORR           SSENSEO CODES X03004HS 04350000
         LCLB  &IPATH,&ICPM,&ISTATE,&IFI,&IRR    SSENSEI CODES X03004HS 04400000
         LCLB  &BB,&NBB,&EB,&NEB                 BRACKET CODES X03004HS 04450000
         LCLB  &NO,&YES                          SWITCHC CODES  X3004BS 04500000
         LCLB  &UNSOL,&EOB,&EOM,&EOT,&LOGOFF     DATAFLG CODES   X03004 04550000
         LCLB  &LG,&SOH                          DATAFLG CODES   X03004 04600000
         LCLB  &BRYES,&BRNO             BRANCH CODES            X04SVHS 04650000
         LCLB  &COMPLET                 IO CODE                 X04SVHS 04700000
         LCLB  &CRYES,&CRNO             ENCRIPTION CODES       @G40AKSL 04750000
         LCLC  &Z16,&Z8                 UNUSED PORTION OF BITSTRING     04800000
.*                                                                      04850000
&Z16     SETC  '0000000000000000'       INITIALIZE CONSTANT     X04SVHS 04900000
&Z8      SETC  '00000000'               INITIALIZE CONSTANT    @Z40BHUC 04950000
.* SCAN RPL KEYWORDS ************************************************** 05000000
