.*             OPTCD IS PROCESSED BY THE INNER MACRO IDAVSOPT  @ZA34940 06300000
.********************************************************************** 06340000
.*                                      CHNGDIR                         06380000
.R000100 AIF   ('&CHNGDIR' EQ '').R000600 *** CHNGDIR ***       X3004BS 06420000
         IDAVSCB3 ARG=&CHNGDIR,KWTC=X'65',BIT=YES,CHKAM=VTAM,  @ZA34940-06460000
               AM=&AM                                          @ZA34940 06500000
.*                                                              X04SVHS 06900000
&I       SETA  0                        CLEAR INDEX VARIABLE    X3004BS 06950000
&A       SETA  0                        ZERO ERROR COUNTER      X04SVHS 07000000
.R000200 ANOP                                                   X3004BS 07050000
&I       SETA  &I+1                     BUMP INDEX INTO LIST    X04SVHS 07100000
         AIF   (&I GT N'&CHNGDIR).R000300 CHECK FOR END OF LIST X04SVHS 07150000
&NCMD    SETB  ('&CHNGDIR(&I)' EQ 'NCMD' OR &NCMD)              X3004BS 07200000
&CMD     SETB  ('&CHNGDIR(&I)' EQ 'CMD' OR &CMD)                X3004BS 07250000
&NREQ    SETB  ('&CHNGDIR(&I)' EQ 'NREQ' OR &NREQ)              X3004BS 07300000
&CREQ    SETB  ('&CHNGDIR(&I)' EQ 'REQ' OR &CREQ)               X3004BS 07350000
         AIF   (&NCMD+&CMD+&NREQ+&CREQ+&A EQ &I).R000200        X04SVHS 07400000
.*             CHK FOR NEW BIT SET                                      07450000
         IDAERMAC 5,CHNGDIR,&CHNGDIR(&I)                           @L9C 07500000
&IDACB01 SETB  1                        SET ERROR SWITCH        X3004BS 07550000
&A       SETA  &A+1                     BUMP ERROR COUNTER      X04SVHS 07600000
         AGO   .R000200                 CHECK NEXT OPTION       X04SVHS 07650000
.*             CHECK FOR MUTUALLY EXCLUSIVE OPTIONS                     07700000
.R000300 AIF   (NOT(&NCMD AND &CMD)).R000400                    X3004BS 07750000
         IDAERMAC 8,CHNGDIR,NCMD,CMD                               @L9C 07800000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 07840000
.R000400 AIF   (NOT(&NREQ AND &CREQ)).R000500                   X3004BS 07880000
         IDAERMAC 8,CHNGDIR,NREQ,REQ                               @L9C 07920000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 08000000
.R000500 ANOP                                                   X3004BS 08050000
         AIF   ('&CALLER' EQ 'RPL').R000600                     X04SVHS 08100000
.*             IF CALLED BY RPL, VALUE SET IN RPLRH3 UNDER CODESEL CODE 08150000
&IDAVALC(&IDAENO) SETC '&NCMD&CMD&NREQ&CREQ.000000000000&Z16'   X3004BS 08200000
.********************************************************************** 08250000
.R000600 ANOP                                                  X03004HS 08300000
.*                                      BRACKET                         08350000
         AIF   ('&BRACKET' EQ '').R001010                      X03004HS 08400000
         IDAVSCB3 ARG=&BRACKET,KWTC=X'85',CHKAM=VTAM,AM=&AM,   @ZA34940-08450000
               BIT=YES                                         @ZA34940 08480000
&I       SETA  0                        INITIALIZE INDEX VALUE X03004HS 08550000
&A       SETA  0                        ZERO ERROR COUNTER      X04SVHS 08600000
.R000700 ANOP                                                  X03004HS 08650000
&I       SETA  &I+1                     BUMP INDEX INTO LIST    X04SVHS 08700000
         AIF   (&I GT N'&BRACKET).R000800                      X03004HS 08750000
.*             DETERMINE IF ALL SUBPARMS HAVE BEEN CHECKED     X03004HS 08800000
&BB      SETB  ('&BRACKET(&I)' EQ 'BB' OR &BB)                 X03004HS 08850000
&NBB     SETB  ('&BRACKET(&I)' EQ 'NBB' OR &NBB)               X03004HS 08900000
&EB      SETB  ('&BRACKET(&I)' EQ 'EB' OR &EB)                 X03004HS 08950000
&NEB     SETB  ('&BRACKET(&I)' EQ 'NEB' OR &NEB)               X03004HS 09000000
         AIF   (&BB+&NBB+&EB+&NEB+&A EQ &I).R000700             X04SVHS 09050000
.*                                      CHK FOR NEW BIT SET             09100000
         IDAERMAC 5,BRACKET,&BRACKET(&I)                           @L9C 09150000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09200000
&A       SETA  &A+1                     BUMP ERROR COUNTER      X04SVHS 09250000
         AGO   .R000700                 CHK NEXT SUBPARM       X03004HS 09300000
.R000800 ANOP                                                  X03004HS 09350000
         AIF   (NOT(&BB AND &NBB)).R000900 CHK MUTUALLY        X03004HS 09400000
.*                                      EXCLUSIVE KEYWORDS     X03004HS 09450000
         IDAERMAC 8,BRACKET,BB,NBB                                 @L9C 09500000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09550000
.R000900 ANOP                                                  X03004HS 09600000
         AIF   (NOT(&EB AND &NEB)).R001000 CHK MUTUALLY        X03004HS 09650000
.*                                      EXCLUSIVE KEYWORDS     X03004HS 09700000
         IDAERMAC 8,BRACKET,EB,NEB                                 @L9C 09750000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09800000
.R001000 ANOP                                                  X03004HS 09850000
         AIF   ('&CALLER' EQ 'RPL').R001010                     X04SVHS 09900000
.*             IF CALLED BY RPL, VALUE SET IN RPLRH3 UNDER CODESEL CODE 09950000
&IDAVALC(&IDAENO) SETC '&BB&NBB&EB&NEB.000000000000&Z16'       X03004HS 10000000
.*                                                              X04SVHS 10050000
.********************************************************************** 10100000
.*                                      CODESEL                @Z40BHUC 10150000
.R001010 AIF   ('&CODESEL' EQ '').R001070 *** CODESEL ***      @Z40BHUC 10200000
         IDAVSCB3 ARG=&CODESEL,KWTC=X'91',CHKAM=VTAM,AM=&AM,   @ZA34940-10250000
               BIT=YES                                         @ZA34940 10290000
&STDRD SETB ('&CODESEL' EQ 'STANDARD' OR '&CODESEL' EQ '(STANDARD)')    10330000
