         AIF   ('&RELEASE(2)' NE 'CHECK').ERLVL1 INVALID VALUE     @L7A 03600000
         AIF   ('&MF(1)' NE 'E').ERLVL2 CHECK ONLY WITH MF=E       @L7A 03650000
&RLVLCHK SETB  1              INDICATE RELEASE-CHECK REQUESTED @YA24122 03675000
.ERLVL2R ANOP                                                      @L7A 03700000
.NORLVLC ANOP                                                      @L7A 03750000
.ERLVL1R ANOP                                                      @L7A 03800000
.* CHECK FOR RACROUTE AND RELEASE 1.9 AND HIGHER                  @L7A  03806200
         AIF   ('&RELEASE(1)' EQ '').NOERR01                       @Q1A 03812500
         AIF   ('&RELEASE(1)'(1,3) GE '1.9' AND NOT &ICHRAC).ERR14 @P1C 03820800
.ERTN14  ANOP                                                      @P1A 03829100
.NOERR01 ANOP                                                      @Q1A 03837500
.*                                                                 @L7A 03850000
.* PERFORM PARAMETER DIAGNOSIS                                          03900000
.*                                                                 @L7A 03950000
.* CHECK SPECIFIED PARAMETERS AGAINST RACF LEVEL                   @L7A 04000000
         AIF   ('&RELEASE(1)' GE '1.7').ERLVLAR                @YA09590 04050000
         AIF   (T'&LOC NE 'O' OR T'&LOG NE 'O' OR                      X04100000
               T'&STAT NE 'O' AND NOT &ICHRAC).ERLVLA              @L7A 04150000
.ERLVLAR ANOP                                                      @L7A 04200000
.* VALIDATE ENVIR PARAMETER                                             04250000
         AIF   ('&ENVIR' EQ 'DELETE' OR '&ENVIR' EQ '' OR              *04283300
               '&ENVIR' EQ 'CREATE' OR '&ENVIR' EQ 'CHANGE'  OR        *04316600
               '&ENVIR' EQ 'VERIFY').ERTN002                       @M1C 04349900
         AIF   ('&ENVIR' EQ 'VERIFYX').ERTN002                     @M1A 04383200
         AGO   .ERR002                                             @M1A 04416500
.ERTN002 ANOP                                                           04450000
.*  VERIFY ALLOWED ONLY IF RACINIT INVOKED FROM RACROUTE           @L6A 04500000
         AIF   ('&ENVIR' EQ 'VERIFY' AND NOT &ICHRAC).ERR07        @L6A 04550000
         AIF   ('&ENVIR' EQ 'VERIFYX' AND NOT &ICHRAC).ERR11       @M1A 04560000
&MVALUE  SETC  'REQUEST TYPE'                                      @P7C 04568000
         AIF   ('&ENVIR' NE 'VERIFYX').ERTN07                      @Q1A 04576000
         AIF   ('&RELEASE(1)' EQ '').ERR10                         @Q1A 04584000
