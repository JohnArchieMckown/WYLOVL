   3 R782SQAL CHAR(LENGTH(VSDAT)),   /* SQA ALLOC AREA SIZE      @V1A*/ 07510001
   3 R782SQEX CHAR(LENGTH(VSDAT)),   /* SQA EXPANSION TO CSA     @V1A*/ 07520001
      /* COMMON STORAGE DETAIL SECTION */                               07530001
   3 R782227K(10) CHAR(LENGTH(VSDSINGL)), /* CSA SUBPOOL 227 BY KEY, IN 07540001
              ORDER KEY 0, 1, 2, 3, 4, 5, 6, 7, 8-F, ALL         @V1A*/ 07550001
   3 R782228K(10) CHAR(LENGTH(VSDSINGL)), /* CSA SUBPOOL 228 BY KEY     07560001
                                                                 @V1A*/ 07570001
   3 R782231K(10) CHAR(LENGTH(VSDSINGL)), /* CSA SUBPOOL 231 BY KEY     07580001
                                                                 @V1A*/ 07590001
   3 R782241K(10) CHAR(LENGTH(VSDSINGL)), /* CSA SUBPOOL 241 BY KEY     07600001
                                                                 @V1A*/ 07610001
   3 R782226     CHAR(LENGTH(VSDSINGL)), /* SQA SUBPOOL 226      @V1A*/ 07620001
     5 R782226K  CHAR(LENGTH(VSDSINGL)), /* SQA SUBPOOL 226  @WA05544*/ 07630001
   3 R782239     CHAR(LENGTH(VSDSINGL)), /* SQA SUBPOOL 239      @V1A*/ 07640001
     5 R782239K  CHAR(LENGTH(VSDSINGL)), /* SQA SUBPOOL 239  @WA05544*/ 07650001
   3 R782245     CHAR(LENGTH(VSDSINGL)), /* SQA SUBPOOL 245      @V1A*/ 07660001
     5 R782245K  CHAR(LENGTH(VSDSINGL)); /* SQA SUBPOOL 245  @WA05544*/ 07670001
      /* PRIVATE AREA SUMMARY DATA SECTION */                           07680001
 DCL 1 R782PVT                                                          07690001
  %IF SMFARRAY^='YES' %THEN %GOTO SKIPPVT;                              07700001
  (*)                                                                   07710001
  %SKIPPVT:;                                                            07720001
  BASED(ADDR(SMF78HDR)+SMF78ASS) BDY(WORD), /* VIRTUAL STORAGE PRIVATE  07730001
                                               DATA              @V1A*/ 07740001
   3 R782JOBN CHAR(8),                   /* NAME OF JOB BEING           07750001
                                            MONITORED            @V1A*/ 07760001
   3 R782RDTM CHAR(4),                   /* READER START TIME    @V1A*/ 07770001
   3 R782RDDT CHAR(4),                   /* READER START DATE    @V1A*/ 07780001
   3 R782SUBI FIXED(15),                 /* INDEX OF FIRST SUBPOOL      07790001
                                            ENTRY (R782PVSP) FOR THIS   07800001
                                            JOB                  @V1A*/ 07810001
   3 R782SUBN FIXED(15),                 /* INDEX OF LAST SUBPOOL ENTRY 07820001
                                            FOR THIS JOB         @V1A*/ 07830001
   3 R782STEP CHAR(8),                   /* NAME OF STEP ACTIVE WHEN    07840001
                                            MONITORING BEGAN     @V1A*/ 07850001
   3 R782PGMN CHAR(8),                   /* PROGRAM NAME OF JOB BEING   07860001
                                            MONITORED            @V1A*/ 07870001
   3 R782FLGS BIT(16),                                                  07880001
     5 R782ACTV BIT(1),                  /* 1: JOB ACTIVE AT START OF   07890001
                                            INTERVAL             @V1A*/ 07900001
     5 R782TERM BIT(1),                  /* 1: JOB TERMINATED DURING    07910001
                                            INTERVAL             @V1A*/ 07920001
     5 R782GLCH BIT(1),                  /* 1: GETMAIN LIMIT CHANGED    07930001
                                            DURING INTERVAL      @V1A*/ 07940001
     5 R782INVL BIT(1),                  /* 1: DATA INVALID DUE TO RMF  07950001
                                            ABEND WHILE SAMPLING @V1A*/ 07960001
   3 R782RSV BIT(16),                    /* RESERVED             @V1A*/ 07970001
   3 R782SAMP FIXED(31),                 /* NUMBER OF SAMPLES    @V1A*/ 07980001
   3 R782REGR FIXED(31),                 /* REGION REQUESTED BY JCL     07990001
                                            (BYTES)              @V1A*/ 08000001
   3 R782RGAB FIXED(31),                 /* BELOW 16M REGION ASSIGNED   08010001
                                            BY EXITS (BYTES)     @V1A*/ 08020001
   3 R782RGAA FIXED(31),                 /* ABOVE 16M REGION ASSIGNED   08030001
                                            BY EXITS (BYTES)     @V1A*/ 08040001
   3 R782GMLB FIXED(31),                 /* GETMAIN LIMIT BELOW 16M     08050001
                                            (BYTES)              @V1A*/ 08060001
   3 R782GMLA FIXED(32),                 /* GETMAIN LIMIT ABOVE 16M     08070001
                                            (BYTES)              @V1A*/ 08080001
   3 R782URAB FIXED(31),                 /* USER REGION ADDRESS (BELOW  08090001
                                            16M)                 @V1A*/ 08100001
   3 R782URAA FIXED(31),                 /* USER REGION ADDRESS (ABOVE  08110001
                                            16M)                 @V1A*/ 08120001
   3 R782LSFP CHAR(LENGTH(VSDAT)),       /* LSQA/SWA/229/230 FREE       08130001
                                            PAGES                @V1A*/ 08140001
   3 R782LSFB CHAR(LENGTH(VSDAT)),       /* LSQA/SWA/229/230 LARGEST    08150001
                                            FREE BLOCK           @V1A*/ 08160001
   3 R782LSAL CHAR(LENGTH(VSDAT)),       /* LSQA/SWA/229/230            08170001
