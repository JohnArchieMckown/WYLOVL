                                  valid                         @A1A*/  09010001
       5 *       BIT(4),       /* reserved                      @A1A*/  09020001
     3 *        BIT(24),       /* reserved                      @A1A*/  09030001
     3 R783TNM  CHAR(44),      /* IODF name                     @A1A*/  09040001
     3 R783TSF  CHAR(2),       /* Suffix of IODF Name           @A1A*/  09050001
     3 *        CHAR(2),       /* Reserved                      @A1A*/  09060001
     3 R783TOK  CHAR(16),      /* Partially Token Information   @A1A*/  09070001
       5 R783TDT CHAR(8),      /* IODF Creation Date mm/dd/yy           09080001
                                                            @YA64585*/  09090001
       5 R783TTM CHAR(8),      /* IODF Creation Time hh.mm.ss           09100001
                                                            @YA64585*/  09110001
     3 R783TDY  CHAR(10),      /* IODF creation date mm/dd/yyyy         09111001
                                                            @WA19913*/  09112001
     3 *        CHAR(2);       /* Reserved                  @WA19913*/  09113001
         @SPACE;                                                        09114001
 /********************************************************************/ 09115001
