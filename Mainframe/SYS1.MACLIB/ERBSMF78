   3 R782LPAI FIXED(31),             /* INTERMODULE SPACE IN LPA @V1A*/ 07370001
   3 R782ELPI FIXED(31),            /* INTERMODULE SPACE IN ELPA @V1A*/ 07380001
   3 R782MR FIXED(31),         /* MAXIMUM USER REGION BELOW 16M  @V1A*/ 07390001
