      /* CSA/SQA USAGE SUMMARY SECTION */                               07410001
   3 R782SQAU CHAR(LENGTH(VSDAT)),   /* SQA USAGE                @V1A*/ 07420001
   3 R782CSAU CHAR(LENGTH(VSDAT)),   /* CSA USAGE                @V1A*/ 07430001
