 /********************************************************************/ 08340001
 /*                                                                  */ 08350001
 /*          Relocate block for sub type 3 - Monitor I               */ 08360001
 /*          I/O queuing data in the 3090, 9021, 9121, and 9221      */ 08370001
 /*          series environment.                                 @Z1C*/ 08380001
 /*                                                                  */ 08390001
 /*          There exists one IOQ global section which contains      */ 08400001
 /*          global information for one interval and one             */ 08410001
 /*          triplet for the IOP initiative queue data section.      */ 08420001
 /*                                                                  */ 08430001
 /*          For Rel. 4.1.2 there are always 4 IOP               @Z1C*/ 08440001
 /*          initiative queue data sections.                         */ 08450001
 /*                                                                  */ 08460001
 /*          For each LCU there exists an  I/O queuing               */ 08470001
 /*          configuration section (256 bytes) and a data        @Z1C*/ 08480001
 /*          section (12 bytes). The maximum number of channel       */ 08490001
 /*          paths which can belong to a LCU is currently 8.     @Z1C*/ 08500001
 /*          Therefore the number of configuration data sections     */ 08510001
 /*          contained in a configuration section is also 8.     @Z1C*/ 08520001
 /*                                                                  */ 08530001
 /********************************************************************/ 08540001
         @SPACE;                                                        08550001
 /********************************************************************/ 08560001
 /* IOQ Global section.                                              */ 08570001
 /* (one per interval, described by SMF78QDS).                       */ 08580001
 /********************************************************************/ 08590001
         @SPACE;                                                        08600001
   DECLARE                                                              08610001
   1 R783GD      BASED(ADDR(SMF78HDR)+SMF78QDS) BDY(WORD),              08620001
     3 R783GFLG BIT(8),             /* IOQ global flag           @P1A*/ 08630001
       5 R783GCMF BIT(1),           /* X'80' Data is invalid due to     08640001
                                             failure of the channel     08650001
                                             measurement facility       08660001
                                                                 @P1A*/ 08670001
       5 R783GDIF BIT(1),           /* X'40' DIAGNOSE interface failed  08680001
                                                                 @P1A*/ 08690001
       5 R783GSPQ BIT(1),           /* X'20' Store Primary Queue Data   08700001
                                       not supported         @WA04821*/ 08710001
       5 R783GCMH BIT(1),           /* X'10' DCM supported by hardware  08711007
                                                                 @I1A*/ 08711107
       5 R783GCMD BIT(1),           /* X'08' Configuration contains     08712008
                                       DCM managed channels      @I1A*/ 08713008
       5 * BIT(3),                  /* Reserved                  @I1C*/ 08720007
     3 *        CHAR(1),            /* Reserved                  @P1A*/ 08730001
     3 R783GNTR FIXED(15),          /* Number of triplets        @P1A*/ 08740001
     3 R783GIDS PTR(31),            /* Offset to IOP initiative queue   08750001
                                       data section              @P1A*/ 08760001
     3 R783GIDL FIXED(15),          /* Length of IOP initiative queue   08770001
                                       data section              @P1A*/ 08780001
     3 R783GIDN FIXED(15),          /* Number of IOP initiative queue   08790001
                                       data sections             @P1A*/ 08800001
         @SPACE;                                                        08810001
     3 R783GSAM FIXED(32),          /* CPMT sample count         @Z1C*/ 08820001
         @SPACE;                                                        08830001
  /* Record Control fields                                      @Z1A*/  08840001
         @SPACE;                                                        08850001
     3  R783TSR FIXED(16),     /* Total number of small records @Z1A*/  08860001
     3  *       CHAR(2),       /* Reserved for future use       @Z1A*/  08870001
     3  R783TOT FIXED(32),     /* Total number of data sections         08880001
                                  in large SMF record           @Z1A*/  08890001
     3  R783NXT FIXED(32),     /* Number of data sections               08900001
                                  in following records          @Z1A*/  08910001
         @SPACE;                                              /*@Z1A*/  08920001
  /* Dynamic I/O Enhancements                                   @A1A*/  08930001
     3  R783CFL BIT(8),        /* Configuration Change Flags    @A1A*/  08940001
       5 R783DCA BIT(1),       /* Configuration changed last            08950001
                                  interval                      @A1A*/  08960001
       5 R783IAC BIT(1),       /* Configuration Changed since           08970001
                                  last IPL                      @A1A*/  08980001
       5 R783IOD BIT(1),       /* System IPLed via IODF         @A1A*/  08990001
