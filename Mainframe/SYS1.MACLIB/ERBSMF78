   3 R782USPA CHAR(LENGTH(VSDAT));/*USER REGION PAGES ALLOCATED @V1A*/  08250001
      /* PRIVATE AREA SUBPOOL DATA SECTION */                           08260001
  DCL 1 R782PVSP(*) BASED (ADDR(SMF78HDR)+SMF78SPS) BDY(WORD),          08270001
                                         /* SUBPOOL NUMBER       @V1A*/ 08280001
