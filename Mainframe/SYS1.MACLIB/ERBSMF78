    @SPACE;                                                             06960001
  %LR782:;                                                              06970001
  %IF IFAR782 = '' %THEN %GOTO LR783; /*                         @V1A*/ 06980001
      /* RELOCATE BLOCK FOR SUBTYPE 2 - VIRTUAL STORAGE DATA.  THERE    06990001
