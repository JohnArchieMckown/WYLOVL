 /*                                                                  */ 05760001
 /*            RELOCATE BLOCK FOR SUB TYPE 1 - MONITOR I             */ 05770001
 /*            I/O QUEUING DATA                                      */ 05780001
 /*                                                                  */ 05790001
 /*            FOR EACH LCU THERE EXISTS AN I/O QUEUING              */ 05800001
 /*            CONFIGURATION SECTION (60 BYTES) AND A                */ 05810001
 /*            DATA SECTION (28) BYTES. THE MAXIMUM NUMBER           */ 05820001
 /*            OF CHANNEL PATHS WHICH CAN BELONG TO A LCU IS         */ 05830001
 /*            CURRENTLY 4. THEREFORE THE NUMBER OF CONFIGURATION    */ 05840001
 /*            DATA SECTIONS CONTAINED IN A CONFIGURATION            */ 05850001
 /*            SECTION IS ALSO 4.                                    */ 05860001
 /*                                                                  */ 05870001
 /********************************************************************/ 05880001
    @SPACE;                                                             05890001
    /* I/O QUEUING CONFIGURATION CONTROL SECTION */                     05900001
    @SPACE;                                                             05910001
    DECLARE                                                             05920001
      @SPACE;                                                           05930001
      1 R781CS                                                          05940001
      %IF SMFARRAY ^= 'YES' %THEN                                       05950001
      %GOTO SKCSARY;                                                    05960001
                (*)                                                     05970001
      %SKCSARY:;                                                        05980001
                    BASED(ADDR(SMF78HDR)+SMF78DCS) BDY(WORD),           05990001
        3 R781CC,                   /* CONFIGURATION CONTROL         */ 06000001
          5 R781ID1 CHAR(2),        /* LOGICAL CONTROL UNIT IDENTIFIER  06010001
                                                                 @P1C*/ 06020001
          5 R781NTR FIXED(15),      /* NUMBER OF DESCRIPTOR TRIPLETS */ 06030001
          5 R781CPDS FIXED(31),     /* OFFSET TO CONFIGURATION DATA     06040001
                                       SECTION                       */ 06050001
          5 R781CPDL FIXED(15),     /* LENGTH OF CONFIGURATION DATA     06060001
                                       SECTION                       */ 06070001
          5 R781CPDN FIXED(15),     /* NUMBER OF CONFIGURATION DATA     06080001
                                       SECTIONS                      */ 06090001
        3 R781CD CHAR(              /* CONFIGURATION DATA            */ 06100001
              R781CHPN*LENGTH(R781CPD));                                06110001
    @SPACE;                                                             06120001
    DECLARE                                                             06130001
      @SPACE;                                                           06140001
      R781CHPN BINARY FIXED(15)     /* NUMBER OF CONFIGURATION DATA     06150001
                                       SECTIONS GENERATED            */ 06160001
          CONSTANT(4);                                                  06170001
    @SPACE;                                                             06180001
    /* I/O QUEUING CONFIGURATION DATA SECTION */                        06190001
    @SPACE;                                                             06200001
    DECLARE                                                             06210001
      @SPACE;                                                           06220001
      1 R781CPD                                                         06230001
      %IF SMFARRAY ^= 'YES' %THEN                                       06240001
      %GOTO SKCPARY;                                                    06250001
                (*)                                                     06260001
      %SKCPARY:;                                                        06270001
                    CHAR(12) BASED(ADDR(R781CS)+R781CPDS) BDY(WORD),    06280001
        3 R781CPID CHAR(1),         /* CHANNEL PATH IDENTIFIER       */ 06290001
        3 R781CPST BIT(8),          /* CHANNEL PATH STATUS           */ 06300001
          5 R781CPI BIT(1),         /* =1 IF CHANNEL PATH INSTALLED  */ 06310001
          5 R781CPO BIT(1),         /* =1 IF CHANNEL PATH ONLINE     */ 06320001
          5 R781CPV BIT(1),         /* =1 IF CHANNEL PATH VARIED     */ 06330001
          5 R781VPOF BIT(1),        /* =1 This channel path is offline  06340001
                                          to all devices of the LCU     06350001
                                                                 @P1A*/ 06360001
          5 R781VP  BIT(1),         /* =1 The connectivity of this      06370001
                                          channel path to devices of    06380001
                                          this LCU has manipulated via  06390001
                                          VARY PATH during this         06400001
                                          measurement interval   @P1A*/ 06410001
          5 * BIT(3),               /* unused                    @P1C*/ 06420001
        3 R781CUN FIXED(15),        /* NUMBER OF CONTROL UNITS          06430001
                                       ATTACHED                      */ 06440001
        3 R781CU CHAR(8),           /* CONTROL UNITS ATTACHED TO        06450001
                                       THIS CHANNEL PATH             */ 06460001
          5 R781CU1 CHAR(2),        /* 1ST CONTROL UNIT IDENTIFIER   */ 06470001
          5 R781CU2 CHAR(2),        /* 2ND CONTROL UNIT IDENTIFIER   */ 06480001
          5 R781CU3 CHAR(2),        /* 3RD CONTROL UNIT IDENTIFIER   */ 06490001
          5 R781CU4 CHAR(2);        /* 4TH CONTROL UNIT IDENTIFIER   */ 06500001
    @SPACE;                                                             06510001
    /* I/O QUEUING DATA SECTION */                                      06520001
    @SPACE;                                                             06530001
    DECLARE                                                             06540001
      @SPACE;                                                           06550001
      1 R781DS                                                          06560001
      %IF SMFARRAY ^= 'YES' %THEN                                       06570001
      %GOTO SKDSARY;                                                    06580001
               (*)                                                      06590001
      %SKDSARY:;                                                        06600001
                   BASED(ADDR(SMF78HDR)+SMF78ASS) BDY(WORD),            06610001
        3 *       CHAR(1),          /* Reserved                  @P1C*/ 06620001
        3 R781DST BIT(8),           /* DATA STATUS                   */ 06630001
          5 R781CPS BIT(1),         /* =1 IF NO STCPS DATA AVAILABLE */ 06640001
