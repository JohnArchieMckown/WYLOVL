         AIF   (&J EQ 2).GENAG                                 @G29AN2F 19050000
&L       SETA  27                                              @G29AN2F 19100000
         AGO   .DUPARM                                         @G29AN2F 19150000
.*                                                                      19200000
.*  CODE FOR NEWLY DEFINED SSOB EXTENSIONS SHOULD BE ADDED HERE         19250000
.*                                                                      19300000
.*  CHECK IF PARAMETER HAS ALREADY BEEN SPECIFIED                       19350000
.DUPARM  ANOP                                                           19400000
         AIF   (&M(&L) EQ 1).ERR3                                       19450000
.*  INDICATE PARAMETER HAS BEEN SPECIFIED                               19500000
&M(&L)   SETB  1                                                        19550000
.*  HAVE ALL EXTENSION ID PARAMETERS BEEN CHECKED -                     19600000
         AIF   (&K EQ &N).PASS2                                         19650000
&K       SETA  &K+1                                                     19700000
.*  GO CHECK NEXT PARAMETER                                             19750000
         AGO   .EXTIDCK                                                 19800000
.*                                                                      19850000
.*  SET UP FOR PASS 2 - SSOB MAPPING GENERATION                         19900000
.*                                                                      19950000
.PASS2   ANOP                                                           20000000
.*  RESET INDEX                                                         20050000
&K       SETA  1                                                        20100000
.*  SET PASS 2 INDICATOR                                                20150000
&J       SETA  2                                                        20200000
.*  GO GENERATE SSOB HEADER                                             20250000
         AGO   .GENHDR                                                  20300000
.*                                                                      20350000
.*  GENERATE DSECT CARD IF SPECIFIED                                    20400000
.*                                                                      20450000
.GENDSCT ANOP                                                           20500000
         AIF   (&I EQ 1).SKPDSCT                                        20550000
SSOBEXT  DSECT                                                          20600000
.SKPDSCT ANOP                                                           20650000
