*    3 UCBVHRSN BIT(1),            /* Vary command hierarchy reason     98459500
*                                     indicator                      */ 98467600
*    3 UCBVLRSN BIT(1),            /* Vary command library reason       98473000
*                                     indicator                  @LEA*/ 98478400
*%IF IHAUCBSS^='YES' %THEN         /*                            @L7A*/ 98483800
*    %GOTO UCBP8;                  /*                            @L7A*/ 98491900
*   2 UCBDUMMY    CHAR(3),         /* Dummy UCBs use this field to      98495100
*                                     identify the type of dummy UCB.   98498300
*                                     VIO, SMS and JES are valid        98501500
*                                     types of dummy UCBs        @M5C*/ 98504700
*%GOTO UCBP9;                      /*                            @L7A*/ 98508100
*%UCBP8: ;                         /*                            @L7A*/ 98516200
*   2 UCBNAME     CHAR(3) BDY(WORD,2), /* Device number (EBCDIC)     */ 98524300
*%UCBP9: ;                         /*                            @L7A*/ 98532400
*/********************************************************************/ 98540500
*/*      UCBTYP field - 4 bytes of device unique data                */ 98548600
*/********************************************************************/ 98556700
*   2 UCBTYP      CHAR(4) BDY(WORD), /* Device type                  */ 98564800
*    3 UCBTBYT1    BIT(8),         /* Model bits                     */ 98572900
*     4 UCB1FEA0 BIT(1),           /* Bit 0                          */ 98581000
*     4 UCB1FEA1 BIT(1),           /* Bit 1                          */ 98589100
*     4 UCB1FEA2 BIT(1),           /* Bit 2                          */ 98597200
*     4 UCB1FEA3 BIT(1),           /* Bit 3                          */ 98605300
*     4 UCB1FEA4 BIT(1),           /* Bit 4                          */ 98613400
*     4 UCB1FEA5 BIT(1),           /* Bit 5                          */ 98621500
*     4 UCB1FEA6 BIT(1),           /* Bit 6                          */ 98629600
*     4 UCB1FEA7 BIT(1),           /* Bit 7                          */ 98637700
*    3 UCBTBYT2    BIT(8),         /* Option flags                   */ 98645800
*     4 UCB2OPT0 BIT(1),           /* Flag 0                         */ 98653900
*     4 UCB2OPT1 BIT(1),           /* Flag 1                         */ 98662000
*     4 UCB2OPT2 BIT(1),           /* Flag 2                         */ 98670100
*     4 UCB2OPT3 BIT(1),           /* Flag 3                         */ 98678200
*     4 UCB2OPT4 BIT(1),           /* Flag 4                         */ 98686300
*     4 UCB2OPT5 BIT(1),           /* Flag 5                         */ 98694400
*     4 UCB2OPT6 BIT(1),           /* Flag 6                         */ 98702500
*      5 UCBVLPWR BIT(1),          /* Volume requires alternate power   98710600
*                                     source device                  */ 98718700
*     4 UCB2OPT7 BIT(1),           /* Flag 7                         */ 98726800
*      5 UCBDVPWR BIT(1),          /* Device has alternate power        98734900
*                                     source                         */ 98743000
*    3 UCBTBYT3    BIT(8),         /* Class bits                     */ 98751100
*     4 UCBDVCLS    BIT(8),        /* Same as UCBTBYT3               */ 98759200
*      5 UCB3TAPE BIT(1),          /* Tape                           */ 98767300
*      5 UCB3COMM BIT(1),          /* Communications                 */ 98775400
* %UCB3CTC='''41''X';              /* Channel-to-channel adapter     */ 98783500
*      5 UCB3DACC BIT(1),          /* Direct access                  */ 98791600
*      5 UCB3DISP BIT(1),          /* Display                        */ 98799700
*      5 UCB3UREC BIT(1),          /* Unit record                    */ 98807800
*      5 UCB3CHAR BIT(1),          /* Character reader               */ 98815900
*      5 UCBRSV10 BIT(1),          /* Reserved                       */ 98824000
*      5 UCBRSV11 BIT(1),          /* Reserved                       */ 98832100
*    3 UCBTBYT4    CHAR(1),        /* Device code                    */ 98840200
*     4 UCBUNTYP    CHAR(1),       /* Same as UCBTBYT4               */ 98848300
*%IF IHAUCBSS^='YES' %THEN         /*                            @L7A*/ 98856400
*    %GOTO UCBP10;                 /*                            @L7A*/ 98864500
*   2 *           CHAR(4),         /*                            @L7A*/ 98872600
*%GOTO UCBP11;                     /*                            @L7A*/ 98880700
*%UCBP10: ;                        /*                            @L7A*/ 98888800
*   2 UCBEXTPT    PTR(31),         /* Address of UCB common extension   98894200
*                                     (Valid only for AMODE 24 modules  98895200
*                                     and for UCBs which are defined    98896200
*                                     below 16 Mb)                      98897200
*                                     IBM recommends using UCBLOOK      98898400
*                                     to obtain the address of the UCB  98900500
*                                     common extension.          @M6C*/ 98902700
*%UCBP11: ;                        /*                            @L7A*/ 98905000
*/********************************************************************/ 98913100
*/*      I/O Supervisor flag byte                                    */ 98921200
*/********************************************************************/ 98929300
*    3 UCBFLC      BIT(8),         /* I/O supervisor flag byte C     */ 98937400
*     4 UCBATTP  BIT(1),           /* Attention pending              */ 98945500
*     4 UCBITFP  BIT(1),           /* Intercept condition pending    */ 98953600
*     4 UCBUDE   BIT(1),           /* Unsolicited device end            98961700
*                                     received                       */ 98969800
*     4 *        BIT(1),           /* Reserved - set to zero         */ 98977900
*     4 UCBIVRS  BIT(1),           /* Intervention required message     98986000
*                                     issued                         */ 98994100
*     4 UCBIVRR  BIT(1),           /* Intervention required message     99002200
*                                     is needed                      */ 99010300
*     4 *        BIT(1),           /* Reserved - set to zero         */ 99018400
*     4 UCBDDRSW BIT(1),           /* DDR switch pending on this        99026500
*                                     device                         */ 99034600
*%IF IHAUCBSS^='YES' %THEN         /*                            @L7A*/ 99042700
*    %GOTO UCBP12;                 /*                            @L7A*/ 99050800
*    3 *           CHAR(3),        /* Reserved                   @L7A*/ 99058900
*%GOTO UCBP13;                     /*                            @L7A*/ 99067000
*%UCBP12: ;                        /*                            @L7A*/ 99075100
*    3 UCBEXTP     PTR(24),        /* Address of UCB common extension   99080500
*                                     (Valid only for UCBs which are    99081900
*                                     defined below 16Mb)               99083300
*                                     IBM recommends using UCBLOOK      99084700
*                                     to obtain the address of the UCB  99086800
*                                     common extension.          @M6C*/ 99089000
*%UCBP13: ;                        /*                            @L7A*/ 99091300
*/*                                                                  */ 99099400
*/********************************************************************/ 99107500
*/*                                                                  */ 99115600
*/*      UCB device dependent segments start at UCBDEV.              */ 99123700
*/*                                                                  */ 99131800
*   2 UCBDEV      CHAR(0) BDY(WORD);                                    99139900
*/*                                                                  */ 99148000
*/********************************************************************/ 99156100
*%IF IHAUCBSS='YES' %THEN          /*                            @L7A*/ 99164200
*    %GOTO UCBP14;                 /*                            @L7A*/ 99172300
*%UCBP14A: ;                       /* If this unreferenced label is     99180400
*                                     removed, then the PLS compiler    99188500
*                                     will not print the next block     99196600
*                                     comment                    @L7A*/ 99204700
*/********************************************************************/ 99212800
*/*                                                                  */ 99220900
*/*      UCB common extension                                        */ 99229000
*/*                                                                  */ 99237100
*/*      This extension is pointed to by the UCBEXTP field in the    */ 99245200
*/*      common segment of below 16MB Actual UCBs.  However, UCBLOOK */ 99248400
*/*      will provide the UCB Common Extension address in this case  */ 99251600
*/*      as well, and is the recommended method of obtaining the     */ 99254800
*/*      address.                                                    */ 99258000
*/*                                                                  */ 99261400
*/*      The IOSCMXA service (or UCBLOOK equivalent) must be used    */ 99261500
*/*      to obtain the UCB Common Extension pointer for all above    */ 99261600
*/*      16MB UCBs and for all Captured UCBs.  The IOSCMXA service   */ 99261700
*/*      will also find the Common Extension address in below 16MB   */ 99261800
*/*      UCBs.                                                       */ 99261900
*/*                                                                  */ 99262000
*/*  Notes- The following describes in a step-by-step fashion how    */ 99262100
*/*         the basing expression for the UCB Common Extension       */ 99262200
*/*         works.  In above 16MB UCBs and in Captured UCBs,         */ 99262300
*/*         the UCBEXTP field contains the least significant         */ 99262400
*/*         three bytes of the Actual (above 16MB) UCB Common        */ 99262500
*/*         Extension Address.  The low order bit of UCBEXTP will    */ 99262600
*/*         indicate if the UCB was built above 16MB when on.        */ 99262700
*/*         This bit will later be used in the Common Extension      */ 99262800
*/*         basing expression.  The intent of this basing            */ 99262900
*/*         expression is to allow a simple recompile to             */ 99263000
*/*         handle the addressability change of the UCB Common       */ 99263100
*/*         Extension in above 16MB UCBs, because UCBEXTP can no     */ 99263200
*/*         longer directly point to the Common Extension            */ 99263300
*/*         (3-byte field).  The basing expression will also         */ 99263400
*/*         allow the Common Extension to grow without               */ 99263500
*/*         necessitating further recompiles so long as it           */ 99263600
*/*         remains within one page of the UCB Common Segment.       */ 99263700
*/*                                                                  */ 99263800
*/* 1-Strip off the byte offset from the UCBOB address.              */ 99263900
*/*   (ADDR(UCBOB) & 'FFFFF000'X)                                    */ 99264000
*/*                                                                  */ 99264100
*/* 2-Keep only the byte offset of UCBEXTPT.  Note that UCBEXTPT is  */ 99264200
*/*   a 4 byte field of which the first byte is flags.  Also note    */ 99264300
*/*   that the low order bit of UCBEXTPT will not be used as part    */ 99264400
*/*   of the address as it indicates where the UCB was built.        */ 99264500
*/*   ('00000FFE'X & UCBEXTPT)                                       */ 99264600
*/*                                                                  */ 99264700
*/* 3-Or step 1 and step 2 together.  Essentially, this takes the    */ 99264800
*/*   Captured UCBOB page address and ORing it with the Actual Common*/ 99264900
*/*   Extension's byte offset.                                       */ 99265000
*/*   Step 1 | Step 2                                                */ 99265100
*/*                                                                  */ 99265200
*/*   At this point, it is possible that the address is off by one   */ 99265300
*/*   page if the UCB Common Extension and UCB Common Segment reside */ 99265400
*/*   on two consecutive pages.  The remaining steps determine if    */ 99265500
*/*   this is the case and makes the necessary correction.  This     */ 99265600
*/*   correction is only needed if the UCB was built below 16MB.     */ 99265700
*/*   Only below 16MB UCBs are permitted to cross page boundaries.   */ 99265800
*/*                                                                  */ 99265900
*/* 4-Determine all the differences between the UCBOB address and    */ 99266000
*/*   UCBEXTPT by using exclusive OR.                                */ 99266100
*/*   (ADDR(UCBOB) && UCBEXTPT)                                      */ 99266200
*/*                                                                  */ 99266300
*/* 5-Get the low order bit of UCBEXTPT.  When on, this bit          */ 99266400
*/*   indicates the UCB was built above 16MB.                        */ 99266500
*/*   (UCBEXTP & '00000001'X)                                    @M4C*/ 99266600
*/*                                                                  */ 99266700
*/* 6-Shift this bit left to the lower order bit position of         */ 99266800
*/*   the page address.                                              */ 99266900
*/*   (step 4 * 4096)                                                */ 99267000
*/*                                                                  */ 99267100
*/* 7-When the bit found in step 5 is on, no page address correction */ 99267200
*/*   is needed.  Above 16MB UCBs and Captured UCBs cannot cross     */ 99267300
*/*   page boundaries.  The correction is needed for below 16MB      */ 99267400
*/*   UCBs.  The bit is therefore flipped to properly participate    */ 99267500
*/*   in the arithmetic.                                             */ 99267600
*/*   ('00001000'X && step 6)                                        */ 99267700
*/*                                                                  */ 99267800
*/* 8-Only want to perform correction if the UCB Common Segment and  */ 99267900
*/*   the UCB Common Extension do not reside on the same page.       */ 99268000
*/*   Step 4 & step 7                                                */ 99268100
*/*                                                                  */ 99268200
*/* 9-Subtract the result of step 8 from step 3 to correct the       */ 99268300
*/*   page address.  If the two segments are on the same page or     */ 99268400
*/*   the UCB was built above 16MB, no correction will be made.      */ 99268500
*/*                                                              @P7C*/ 99268600
*/********************************************************************/ 99268700
*%IF UCB_CLASS ^= 'PUBLIC' %THEN   /*                            @0CA*/ 99268900
*  %GOTO UCBP15;                   /*                            @0CA*/ 99269100
*DECLARE                                                                99269500
*  1 UCBCMEXT BASED(((ADDR(UCBOB) & 'FFFFF000'X) |                      99269700
*                   ('00000FFE'X & UCBOB.UCBEXTPT)) -                   99269900
*                   ((ADDR(UCBOB) && UCBOB.UCBEXTPT) &                  99270100
*                   ('00001000'X &&                                     99270300
*                   ((UCBOB.UCBEXTP & '000001'X) * 4096)))), /*         99270500
*                                      Establish proper basing for      99270700
*                                      the UCB Common Extension. @0CA*/ 99270900
*%GOTO UCBP15A;                    /*                            @0CA*/ 99271100
*%UCBP15: ;                        /*                            @0CA*/ 99271300
*DECLARE                                                                99271500
*  1 UCBCMEXT BASED(((ADDR(UCBOB) & 'FFFFF000'X) |                      99272200
*                   ('00000FFE'X & UCBEXTPT)) -                         99274900
*                   ((ADDR(UCBOB) && UCBEXTPT) &                        99277600
*                   ('00001000'X &&                                     99279200
*                   ((UCBEXTP & '000001'X) * 4096)))), /*               99280800
*                                      Establish proper basing for      99282400
*                                      the UCB Common Extension. @M4C*/ 99284000
*%UCBP15A: ;                       /*                            @0CA*/ 99284800
*                                                                       99285700
*/********************************************************************/ 99293800
*/*                                                                  */ 99301900
*%INCLUDE SYSLIB(IOSDUCBC) NOSEG;  /* Common extension fields    @L7A*/ 99310000
