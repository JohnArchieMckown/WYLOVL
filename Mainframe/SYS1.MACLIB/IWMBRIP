.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00144500
.*******************************************************************    00144600
                      ACTR  10000                                       00144700
.*                                                                      00144800
                      LCLA  &ZLC           COLUMN POSITION              00144900
                      LCLA  &ZOPX          LIST TABLE INDEX             00145000
                      LCLC  &ZLL           LABEL                        00145100
                      LCLC  &ZLOP          OPERATION                    00145200
                      LCLC  &ZLV           VALUE                        00145300
                      LCLC  &ZLD           DESCRIPTION                  00145400
.*                                                                      00145500
&ZOPX                 SETA  1                                           00145600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00145700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00145800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00145900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00146000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00146100
                      AIF   (K'&ZLD LE 0).ZL5                           00146200
&ZLC                  SETA  35             COMMENT COLUMN               00146300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00146400
&ZLC                  SETA  33             COMMMENT COLUMN              00146500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00146600
&ZLV                  SETC  '&ZLV'.' '                                  00146700
                      AGO   .ZL3                                        00146800
.*                                                                      00146900
.ZL4                  ANOP  ,                                           00147000
&ZLV                  SETC  '&ZLV'.' '                                  00147100
.ZL5                  ANOP  ,                                           00147200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00147300
.*                                                                      00147400
&ZLL     &ZLOP &ZLV                                                     00147500
.*                                                                      00147600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00147700
                      AGO   .ZL2           CONTINUE LOOPING             00147800
.*                                                                      00147900
.ZLX                  ANOP  ,                                           00148000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00148100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00148200
         L     14,16(0,0)              GET CVT ADDRESS                  00148300
         L     14,772(14,0)            GET SFT ADDRESS                  00148400
