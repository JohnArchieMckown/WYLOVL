.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00095000
&ZPLLEN               SETA 12                                           00095100
.*                                                                      00095200
.******************************************************************     00095300
.***AI.L01 IWMMDELE FMT=LIST FILL IN PARAMETER AREA                     00095400
.******************************************************************     00095500
                      ACTR  10000                                       00095600
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00095700
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00095800
&ZCVAL                SETC  'LAE'          XAX MODE                     00095900
.ZIMFXA               ANOP ,                                            00096000
.*                                                                      00096100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00096200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00096300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00096400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00096500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00096600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00096700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00096800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00096900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00097000
&ZOPN                 SETA  &ZOPN+1                                     00097100
&ZOPL(&ZOPN)          SETC  ' '                                         00097200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00097300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00097400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00097500
                      AGO   .ZIMFNX                                     00097600
.*                                                                      00097700
.ZIMFN2               ANOP ,                                            00097800
&ZOPN                 SETA  &ZOPN+1                                     00097900
&ZOPL(&ZOPN)          SETC  ' '                                         00098000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00098100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00098200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00098300
.ZIMFNX               ANOP ,                                            00098400
.********************************************************************   00098500
.***AI.L02 IWMMDELE FMT=LIST INITIALIZE PARAMETER AREA                  00098600
