&ZOPN1                SETA &ZOPN                                        00102400
.*******************************************************************    00102500
.***  PROCESS IWMMDELE OUTPUT PARAMETERS                                00102600
.*******************************************************************    00102700
                      ACTR  10000                                       00102800
.*******************************************************************    00102900
.***AO.X02  RETURN CODE PROCESSING                                      00103000
.*******************************************************************    00103100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00103200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00103300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00103400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00103500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00103600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00103700
&ZOPN                 SETA  &ZOPN+1                                     00103800
&ZOPL(&ZOPN)          SETC  ' '                                         00103900
&ZOPO(&ZOPN)          SETC  'ST'                                        00104000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00104100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00104200
                      AGO   .ZMRETX                                     00104300
.*                                                                      00104400
.ZMRET2               ANOP  ,                                           00104500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00104600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00104700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00104800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00104900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00105000
&ZOPN                 SETA  &ZOPN+1                                     00105100
&ZOPL(&ZOPN)          SETC  ' '                                         00105200
&ZOPO(&ZOPN)          SETC  'LR'                                        00105300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00105400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00105500
.ZMRETX               ANOP  ,                                           00105600
.*******************************************************************    00105700
.***AO.X03  REASON CODE PROCESSING                                      00105800
.*******************************************************************    00105900
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00106000
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00106100
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00106200
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00106300
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00106400
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00106500
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00106600
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00106700
