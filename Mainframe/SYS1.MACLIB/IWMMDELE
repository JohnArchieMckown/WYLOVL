.***AO.X04  LABEL FOR MF=L PROCESSING                                   00109200
.*******************************************************************    00109300
.ZMFLX                ANOP  ,              MF=L PROCESSING              00109400
.*******************************************************************    00109500
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00109600
.*******************************************************************    00109700
                      ACTR  10000                                       00109800
.*                                                                      00109900
                      LCLA  &ZLC           COLUMN POSITION              00110000
                      LCLA  &ZOPX          LIST TABLE INDEX             00110100
                      LCLC  &ZLL           LABEL                        00110200
                      LCLC  &ZLOP          OPERATION                    00110300
                      LCLC  &ZLV           VALUE                        00110400
                      LCLC  &ZLD           DESCRIPTION                  00110500
.*                                                                      00110600
&ZOPX                 SETA  1                                           00110700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00110800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00110900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00111000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00111100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00111200
                      AIF   (K'&ZLD LE 0).ZL5                           00111300
&ZLC                  SETA  35             COMMENT COLUMN               00111400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00111500
&ZLC                  SETA  33             COMMMENT COLUMN              00111600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00111700
&ZLV                  SETC  '&ZLV'.' '                                  00111800
                      AGO   .ZL3                                        00111900
.*                                                                      00112000
.ZL4                  ANOP  ,                                           00112100
&ZLV                  SETC  '&ZLV'.' '                                  00112200
.ZL5                  ANOP  ,                                           00112300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00112400
.*                                                                      00112500
&ZLL     &ZLOP &ZLV                                                     00112600
.*                                                                      00112700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00112800
                      AGO   .ZL2           CONTINUE LOOPING             00112900
.*                                                                      00113000
.ZLX                  ANOP  ,                                           00113100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00113200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00113300
         L     14,16(0,0)              GET CVT ADDRESS                  00113400
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00113500
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00113600
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00113700
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00113800
         B     END&SYSNDX              BRANCH TO END LABEL              00113900
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00114000
         L     14,772(14,0)            GET SFT ADDRESS                  00114100
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00114200
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00114300
         OR    14,15                   GET PC NUMBER                    00114400
         PC    0(14)                                                    00114500
END&SYSNDX DS    0H                    END LABEL                        00114600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00114700
.ZLINKX               ANOP  ,                                           00114800
.*******************************************************************    00114900
.***AP.X01  FORMAT GENERATED CODE                                       00115000
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00115100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00115200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00115300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00115400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00115500
                      AIF   (K'&ZLD LE 0).ZL51                          00115600
&ZLC                  SETA  35             COMMENT COLUMN               00115700
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00115800
&ZLC                  SETA  33             COMMENT COLUMN               00115900
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00116000
&ZLV                  SETC  '&ZLV'.' '                                  00116100
                      AGO   .ZL31                                       00116200
.*                                                                      00116300
.ZL41                 ANOP  ,                                           00116400
&ZLV                  SETC  '&ZLV'.' '                                  00116500
.ZL51                 ANOP  ,                                           00116600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00116700
.*                                                                      00116800
&ZLL     &ZLOP &ZLV                                                     00116900
.*                                                                      00117000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00117100
                      AGO   .ZL21          CONTINUE LOOPING             00117200
.*                                                                      00117300
.ZLX1                 ANOP  ,                                           00117400
.*******************************************************************    00117500
.***AP.X02  END OF IWMMDELE MACRO                                       00117600
