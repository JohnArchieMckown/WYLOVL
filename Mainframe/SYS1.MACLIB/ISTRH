RH       DS    0XL3                    REQUEST OR RESPONSE HEADER       02150000
         SPACE 1                                                        02200000
RHF0     DS    X                       RH BYTE 0                        02250000
RHQS     EQU   B'10000000'             RH TYPE INDICATOR:               02300000
*                                      (Q) 0=REQUEST                    02350000
*                                      (S) 1=RESPONSE                   02400000
         SPACE 1                                                        02450000
RHQSREQ  EQU   B'00000000'             REQUEST UNIT                     02500000
RHQSRSP  EQU   B'10000000'             RESPONSE UNIT                    02550000
         SPACE 1                                                        02600000
RHRUCAT  EQU   B'01100000'             RU CATEGORY INDICATOR:           02650000
*                                      (B) 00=FM DATA                   02700000
*                                      (B) 01=NETWORK CONTROL           02750000
*                                      (B) 10=DATA FLOW CONTROL         02800000
*                                      (B) 11=SESSION CONTROL           02850000
         SPACE 1                                                        02900000
RHFMD    EQU   B'00000000'             FUNCTION MANAGEMENT DATA RU      02950000
RHNC     EQU   B'00100000'             NETWORK CONTROL RU               03000000
RHDFC    EQU   B'01000000'             DATA FLOW CONTROL RU             03050000
RHSC     EQU   B'01100000'             SESSION CONTROL RU               03100000
         SPACE 1                                                        03150000
RHF0B3   EQU   B'00010000'             (B) RESERVED: NOT ARCHITECTED    03200000
RHFI     EQU   B'00001000'             FORMAT INDICATOR:                03250000
*                                      (B) 0=NO FM HEADER, OR           03300000
