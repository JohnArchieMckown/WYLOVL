***                                                                  ** 02720008
*** THE FOLLOWING DECLARATIONS DEFINE THE ENTRY AND RETURN CODES     ** 02730008
*** USED BY THE CLIST VARIABLE ACCESS ROUTINE (POINTED TO BY         ** 02740008
*** TSVSVACC).                                                       ** 02750008
***                                                                  ** 02760008
*********************************************************************** 02770008
*       ENTRY CODES                                                     02780008
TSVERETR EQU   1                  RETURN VARIABLE VALUE                 02790008
TSVEUPDT EQU   2                  UPDATE VARIABLE                       02800008
TSVELOC  EQU   3                  LOCATE / LOCATE NEXT                  02810008
TSVERSVD EQU   4                  RESERVED                              02820008
TSVNOIMP EQU   18                 NO IMPLICIT                  @ZA87430 02830008
*       RETURN CODES                                                    02840008
TSVROK   EQU   0                  EVERY THING OK                        02850008
TSVRNORS EQU   4                  VARIABLE RETURNED SHOULDN'T           02860008
*                                         BE RE-SCANNED                 02870008
TSVREVAL EQU   8                  VARIABLE RETURNED REQUIRES            02880008
*                                         EVALUATION                    02890008
TSVRLAB  EQU   12                 VARIABLE RETURNED IS A                02900008
*                                         LABEL                         02910008
TSVRNAUP EQU   16                 SYSTEM VARIABLE - CAN'T BE            02920008
*                                        UPDATED BY THE USER            02930008
TSVRNOM  EQU   20                 FOR LOCATE - NO VARIABLE              02940008
*                                         RETURNED - THERE ARE NO       02950008
*                                         MORE VARIABLES                02960008
TSVRPROC EQU   24                 VARIABLE RETURNED IS A PROCEDURE      02970008
*                                         NAME                 @E1402S3 02980008
TSVRSVD2 EQU   30                 RESERVED                     @E21D2G9 02990008
TSVRGETF EQU   32                 GETMAIN/FREEMAIN FAILURE              03000008
TSVRNSIZ EQU   36                 SYMBOL NAME TOO LARGE                 03010008
*                                         OR SMALL                      03020008
TSVRENV  EQU   40                 INCORRECT ENVIRONMENT                 03030008
TSVRPARM EQU   44                 INVALID ENTRY CODE                    03040008
TSVRDUP  EQU   48                 DUPLICATE SYMBOL FOUND                03050008
TSVRUNDF EQU   52                 UNDEFINED VARIABLE                    03060008
TSVRGLER EQU   56                 TOO MANY GLOBAL VARIABLES             03070008
TSVRUNDG EQU   60                 UNDEFINED GLOBAL VARIABLE             03080008
TSVRINVR EQU   64                 VARIABLE NOT VALID AS A CALL BY       03090008
*                                           REFERENCE VARIABLE          03100008
TSVRUNDR EQU   68                 UNDEFINED CALL BY REFERENCE VARIABLE  03110008
TSVIREXX EQU   80                 VARIABLE NAME IS NOT VALID FOR REXX   03120008
*                                                              @E21D2G9 03130008
TSVREXXE EQU   81                 AN UNEXPECTED RETURN CODE WAS         03140008
*                                 RECEIVED FROM A REXX ROUTINE @E21D2G9 03150008
*                                                                       03160008
*       FLAG INDICATORS FOR TSVTFLG1                           @DG10015 03170008
*                                                                       03180008
TSVTNCTU EQU   X'80'              Instorage copy of system notices      03190008
*                                 needs to be updated          @WA03618 03200008
TSVTNETL EQU   X'40'              None of the TSO/E Exits               03210008
*                                 were found in LPA/ELPA       @WA03618 03220008
TSVTUPDP EQU   X'20'              IKJBCMSG posted for parmlib update    03230008
*                                 signalling                   @WA03618 03240008
         MEND , */                                                      03250008
* %TSVTPLS  :  ;                                                        03260008
* /* MAPPING OF TSO VECTOR TABLE                                     */ 03270008
* DECLARE                                                               03280008
*   1 TSVT BASED(TSVTPTR) BDY(DWORD),         /*           @E2267F2 */  03290008
*                                                                       03300008
*    2 TSVTTSVT CHAR(4),          /* ACRONYM IN EBCDIC "TSVT"        */ 03310008
*    2 TSVTLEV CHAR(1),           /* TSVT VERSION                    */ 03320008
*    2 TSVTFLG1 CHAR(1),          /* Flag Indicators         @DG10015*/ 03330008
*      3 TSVTNCTU BIT(1),         /* Instorage copy of system notices   03340008
*                                    needs to be updated     @DG10015*/ 03350008
*      3 TSVTNETL BIT(1),         /* None of the TSO/E Exits            03360008
*                                    were found in LPA/ELPA  @E23D2D5*/ 03370008
*      3 TSVTUPDP BIT(1),         /* IKJBCMSG posted for parmlib update 03380008
*                                    signalling              @YA34930*/ 03390008
*      3 *        BIT(5),         /* Reserved Flags          @YA34930*/ 03400008
*    2 TSVTRSV1 CHAR(2),          /* RESERVED                @DG10015*/ 03410008
*    2 TSVTNCT  PTR(31),          /* ADDRESS OF THE MOST CURRENT        03420008
*                                    NOTICE TABLE                    */ 03430008
*    2 TSVTVACC PTR(31),          /* ADDRESS OF THE CLIST VARIABLE      03440008
*                                    ACCESS ROUTINE                  */ 03450008
*    2 TSVTASF PTR(31),           /* ADDRESS OF THE AUTHORIZED          03460008
*                                    SERVICE FACILITY ROUTINE        */ 03470008
*/********************************************************************/ 03480008
*/*                  TSO/E R2.1 SUPPORT                   @E2267F2  **/ 03490008
*/*                                                                 **/ 03500008
*/********************************************************************/ 03510008
*   2  TSVTLTBL PTR(31), /* ADDRESS OF LOGON ADDRESS TABLE              03520008
*                                                           @E2267F2 */ 03530008
