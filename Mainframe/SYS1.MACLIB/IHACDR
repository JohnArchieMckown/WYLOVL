*/* Start of PL/X Source                                             */ 00035000
* DCL 1 CDR BDY(DWORD) BASED(CDRPTR), /* Configuration data record   */ 00035100
*       3 CDRFIELD(*) CHAR(32),   /* A CDR consists of a variable       00035200
*                                    number of 32-byte fields        */ 00035300
*         5 CDRFINFO,             /* CDR field information           */ 00035400
*           7 CDRFLDID BIT(2),    /* Field identifier - identifies the  00035500
*                                    contents of the 32-byte field.     00035600
*                                    The content of the remaining       00035700
*                                    fields of this structure depend    00035800
*                                    on the field identifier.        */ 00035900
*           7 * BIT(6),                                                 00036000
*           7 * CHAR(31);                                               00036100
*     /* Values for CDRFLDID                                         */ 00036200
* DCL                                                                   00036300
*   CDRFUNUS BIT(8) CONSTANT('00000000'B), /* Unused             @L3C*/ 00036400
*   CDRFSNEQ BIT(8) CONSTANT('01000000'B), /* SNEQ               @L3C*/ 00036500
*   CDRFGNEQ BIT(8) CONSTANT('10000000'B), /* GNEQ               @L3C*/ 00036600
*   CDRFNED  BIT(8) CONSTANT('11000000'B); /* NED                @L3C*/ 00036700
*                                                                       00036800
*     /* GNEQ                                                        */ 00036900
* DCL 1 GNEQ BDY(DWORD) BASED(GNEQPTR), /* General Node-Element         00037000
*                                          Qualifier                 */ 00037100
*       3 GNEFLAGS,               /* Flags                           */ 00037200
*         5 GNEFLDID BIT(2),      /* Field identifier                */ 00037300
*         5 * BIT(6),             /* Reserved                        */ 00037400
*       3 GNERS BIT(8),           /* Record selector                 */ 00037500
*       3 GNEINTID FIXED(16),     /* Interface ID                    */ 00037600
*       3 GNEDDTO FIXED(8),       /* Device-Dependent Time Out   @01C*/ 00037750
*       3 * CHAR(1),              /* Reserved                    @02C*/ 00037810
*       3 GNEMIHPT FIXED(8),      /* MIH primary time out        @02A*/ 00037820
*       3 GNEMIHST FIXED(8),      /* MIH secondary time out      @02A*/ 00037830
*       3 GNEXINFO CHAR(24),      /* General node element extended      00037900
*                                    information                     */ 00038000
*         5 GNEQFLDS(24) FIXED(8); /* Q fields                       */ 00038100
*                                                                       00038200
*     /* SNEQ                                                        */ 00038300
* DCL 1 SNEQ BDY(DWORD) BASED(SNEQPTR), /* Specific Node-Element        00038400
*                                          Qualifier                 */ 00038500
*       3 SNEFLAGS,               /* Flags                           */ 00038600
*         5 SNEFLDID BIT(2),      /* Field identifier                */ 00038700
*         5 * BIT(6),             /* Reserved                        */ 00038800
*       3 * CHAR(7),              /* Reserved                        */ 00038900
*       3 SNEXINFO CHAR(24);      /* Specific node element extended     00039000
*                                    information                     */ 00039100
*                                                                       00039200
*     /* NED                                                         */ 00039300
* DCL 1 NED BDY(DWORD) BASED(NEDPTR), /* Node-Element Descriptor     */ 00039400
*       3 NEDFLAGS,               /* Flags                           */ 00039500
*         5 NEDFLDID BIT(2),      /* Field identifier                */ 00039600
*         5 NEDTOKEN BIT(1),      /* Token indicator                 */ 00039700
*         5 NEDSNIND BIT(1),      /* Serial number indicator         */ 00039800
*         5 NEDSUBSN BIT(1),      /* Substitute serial number           00039900
*                                    indicator                       */ 00040000
*         5 NEDRECON BIT(1),      /* Reconfiguration NED indicator   */ 00040100
*         5 NEDEMULA BIT(1),      /* Emulation NED indicator         */ 00040200
*         5 * BIT(1),             /* Reserved                        */ 00040300
*       3 NEDTYPE FIXED(8),       /* Type of node element described     00040400
*                                    by this NED                     */ 00040500
*       3 NEDCLASS FIXED(8),      /* I/O device class. Valid only for   00040600
*                                    I/O device type NEDs            */ 00040700
*       3 NEDFLAG2 FIXED(8),                                /*   @L3A*/ 00040800
*         5 * BIT(7),             /* Reserved                    @L3C*/ 00040900
*         5 NEDLEVEL BIT(1),      /* Level indicator             @L3C*/ 00041000
*       3 NEDID,                  /* Node element identifier         */ 00041100
*         5 NEDTYPEN CHAR(6),     /* Type number                     */ 00041200
*         5 NEDMODN CHAR(3),      /* Model number                    */ 00041300
*         5 NEDMANUF CHAR(3),     /* Manufacturer                    */ 00041400
*         5 NEDPMANU CHAR(2),     /* Plant of manufacture            */ 00041500
*         5 NEDSEQN CHAR(12),     /* Sequence number                 */ 00041600
*         5 NEDTAG FIXED(16);     /* Tag                             */ 00041700
*     /* Values for NEDTYPE                                          */ 00041800
* DCL                                                                   00041900
*   NEDTUNSP FIXED(8) CONSTANT(0), /* Unspecified                    */ 00042000
*   NEDTIODV FIXED(8) CONSTANT(1), /* I/O device                     */ 00042100
*   NEDTCU   FIXED(8) CONSTANT(2); /* Control Unit                   */ 00042200
*     /* Values for NEDCLASS                                         */ 00042300
* DCL                                                                   00042400
*   NEDCUNSP FIXED(8) CONSTANT(0), /* Unspecified                    */ 00042500
*   NEDCDASD FIXED(8) CONSTANT(1), /* DASD                           */ 00042600
*   NEDCTAPE FIXED(8) CONSTANT(2), /* Magnetic tape                  */ 00042700
*   NEDCURIN FIXED(8) CONSTANT(3), /* Unit record (input)            */ 00042800
*   NEDCUROT FIXED(8) CONSTANT(4), /* Unit record (output)           */ 00042900
*   NEDCPRT  FIXED(8) CONSTANT(5), /* Printer                        */ 00043000
*   NEDCCOMM FIXED(8) CONSTANT(6), /* Communications controller      */ 00043100
*   NEDCFST  FIXED(8) CONSTANT(7), /* Full screen terminal           */ 00043200
*   NEDCLMT  FIXED(8) CONSTANT(8), /* Line mode terminal             */ 00043300
