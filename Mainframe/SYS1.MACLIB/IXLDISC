.***AC.PLV01  PROCESS PLISTVER                                      *   00046000
.********************************************************************   00046100
&XPLISTVER      SETA  0                                                 00046200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00046300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00046400
&XPLISTVER      SETA  0                                                 00046500
.ZPLV0          ANOP  ,                                                 00046600
.*                                                                      00046700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00046800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00046900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00047000
.*-------------------------------------------------------------------   00047100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00047200
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00047300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00047400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00047500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00047600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00047700
                AGO   .ZPVCK2                                           00047800
.ZPVCK3         ANOP  ,                                                 00047900
.*-------------------------------------------------------------------   00048000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00048100
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00048200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00048300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00048400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00048500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00048600
