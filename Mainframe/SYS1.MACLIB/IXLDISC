.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00130100
.*******************************************************************    00130200
                      ACTR  10000                                       00130300
.*                                                                      00130400
                      LCLA  &ZLC           COLUMN POSITION              00130500
                      LCLA  &ZOPX          LIST TABLE INDEX             00130600
                      LCLC  &ZLL           LABEL                        00130700
                      LCLC  &ZLOP          OPERATION                    00130800
                      LCLC  &ZLV           VALUE                        00130900
                      LCLC  &ZLD           DESCRIPTION                  00131000
.*                                                                      00131100
&ZOPX                 SETA  1                                           00131200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00131300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00131400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00131500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00131600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00131700
                      AIF   (K'&ZLD LE 0).ZL5                           00131800
&ZLC                  SETA  35             COMMENT COLUMN               00131900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00132000
&ZLC                  SETA  33             COMMMENT COLUMN              00132100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00132200
&ZLV                  SETC  '&ZLV'.' '                                  00132300
                      AGO   .ZL3                                        00132400
.*                                                                      00132500
.ZL4                  ANOP  ,                                           00132600
&ZLV                  SETC  '&ZLV'.' '                                  00132700
.ZL5                  ANOP  ,                                           00132800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00132900
.*                                                                      00133000
&ZLL     &ZLOP &ZLV                                                     00133100
.*                                                                      00133200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00133300
                      AGO   .ZL2           CONTINUE LOOPING             00133400
.*                                                                      00133500
.ZLX                  ANOP  ,                                           00133600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00133700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00133800
         L     14,16(0,0)              GET CVT ADDRESS                  00133900
         L     14,772(14,0)            GET SFT ADDRESS                  00134000
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00134100
         LA    15,20                   GET EX FOR SERVICE ROUTINE       00134200
         OR    14,15                   GET PC NUMBER                    00134300
         PC    0(14)                                                    00134400
