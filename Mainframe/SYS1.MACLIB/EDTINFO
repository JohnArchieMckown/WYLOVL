              AIF   ('&XMF' EQ 'L').XLAB3                               00756000
&M0M0001 DS    0H                             EDTINFO-1                 00756100
.XLAB3        ANOP  ,                                                   00756200
.*******************************************************************    00756300
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00756400
.*******************************************************************    00756500
              PUSH  PRINT                                               00756600
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00756700
              PRINT OFF         ++ PR01                                 00756800
              AGO   .ZPRNTXX                                            00756900
.ZPRINTX      PRINT ON                                                  00757000
.ZPRNTXX      ANOP  ,                                                   00757100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00757200
              AIF   (K'&XMAXELIG LE 0).ZEDTIM01                    @P2A 00757300
              AIF   (K'&XOUTUNIT GT 0).ZEDTIM02                    @P2A 00757400
              AIF   (K'&XOUTDEV GT 0).ZEDTIM02                     @P2A 00757500
&ZMS          SETC  'ONE OR MORE OF THE FOLLOWING'                 @P2A 00757600
&ZMS          SETC  '&ZMS KEYS ARE REQUIRED: '                     @P2A 00757700
&ZMS          SETC  '&ZMS " OUTUNIT",'                             @P2A 00757800
              MNOTE 8,' &ZMS " OUTDEV".'                           @P2A 00757900
&ZMACRET      SETA  8                 Set macro error return code  @P2A 00758000
.ZEDTIM02     ANOP  ,                                              @P2A 00758100
.ZEDTIM01     ANOP  ,                                              @P2A 00758200
              AIF   (K'&XRTNUNAFF LE 0).ZEDTIR01                   @L3A 00758300
