.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00928400
.*******************************************************************    00928500
                      ACTR  10000                                       00928600
.*                                                                      00928700
                      LCLA  &ZLC           COLUMN POSITION              00928800
                      LCLA  &ZOPX          LIST TABLE INDEX             00928900
                      LCLC  &ZLL           LABEL                        00929000
                      LCLC  &ZLOP          OPERATION                    00929100
                      LCLC  &ZLV           VALUE                        00929200
                      LCLC  &ZLD           DESCRIPTION                  00929300
.*                                                                      00929400
&ZOPX                 SETA  1                                           00929500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00929600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00929700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00929800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00929900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00930000
                      AIF   (K'&ZLD LE 0).ZL5                           00930100
&ZLC                  SETA  35             COMMENT COLUMN               00930200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00930300
&ZLC                  SETA  33             COMMMENT COLUMN              00930400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00930500
&ZLV                  SETC  '&ZLV'.' '                                  00930600
                      AGO   .ZL3                                        00930700
.*                                                                      00930800
.ZL4                  ANOP  ,                                           00930900
&ZLV                  SETC  '&ZLV'.' '                                  00931000
.ZL5                  ANOP  ,                                           00931100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00931200
.*                                                                      00931300
&ZLL     &ZLOP &ZLV                                                     00931400
.*                                                                      00931500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00931600
                      AGO   .ZL2           CONTINUE LOOPING             00931700
.*                                                                      00931800
.ZLX                  ANOP  ,                                           00931900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00932000
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00932100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00932200
         L     15,16(0,0)         ++ Get CVT address                    00932300
         L     15,772(15,0)       ++ Get SFT address                    00932400
         L     15,320(15,0)       ++ Get PC number                      00932500
         PC    0(15)              ++ PC to Unit Verification            00932600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00932700
.ZLINKX               ANOP  ,                                           00932800
.*******************************************************************    00932900
.***AP.X01  FORMAT GENERATED CODE                                       00933000
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00933100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00933200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00933300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00933400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00933500
                      AIF   (K'&ZLD LE 0).ZL51                          00933600
&ZLC                  SETA  35             COMMENT COLUMN               00933700
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00933800
&ZLC                  SETA  33             COMMENT COLUMN               00933900
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00934000
&ZLV                  SETC  '&ZLV'.' '                                  00934100
                      AGO   .ZL31                                       00934200
.*                                                                      00934300
.ZL41                 ANOP  ,                                           00934400
&ZLV                  SETC  '&ZLV'.' '                                  00934500
.ZL51                 ANOP  ,                                           00934600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00934700
.*                                                                      00934800
&ZLL     &ZLOP &ZLV                                                     00934900
.*                                                                      00935000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00935100
                      AGO   .ZL21          CONTINUE LOOPING             00935200
.*                                                                      00935300
.ZLX1                 ANOP  ,                                           00935400
