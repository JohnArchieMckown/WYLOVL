.***AC.PLV01  PROCESS PLISTVER                                      *   00130600
.********************************************************************   00130700
&XPLISTVER      SETA  1                                                 00130800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00130900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00131000
&XPLISTVER      SETA  1                                                 00131100
.ZPLV0          ANOP  ,                                                 00131200
.*                                                                      00131300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00131400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00131500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00131600
.*-------------------------------------------------------------------   00131700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00131800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00131900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00132000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00132100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00132200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00132300
                AGO   .ZPVCK2                                           00132400
.ZPVCK3         ANOP  ,                                                 00132500
.*-------------------------------------------------------------------   00132600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00132700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00132800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00132900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00133000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00133100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00133200
                AGO   .ZPVCKE                                           00133300
.ZPVCK2         ANOP  ,                                                 00133400
.*-------------------------------------------------------------------   00133500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00133600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00133700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00133800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00133900
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00134000
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00134100
.ZPVCK6         ANOP  ,                                                 00134200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00134300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00134400
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00134500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00134600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00134700
&XPLISTVER      SETA  1                                                 00134800
                AGO   .ZPVCKE                                           00134900
.*-------------------------------------------------------------------   00135000
.ZPVCK4A        ANOP  ,                                                 00135100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00135200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00135300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00135400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00135500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00135600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00135700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00135800
                AGO   .ZPVCK5                                           00135900
.ZPVCK4         ANOP  ,                                                 00136000
&XPLISTVER      SETA  &PLISTVER(1)                                      00136100
.ZPVCK5         ANOP  ,                                                 00136200
.ZPVCKE         ANOP  ,                                                 00136300
.********************************************************************   00136400
