.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00810500
&ZPLLEN               SETA 88                                           00810600
.*                                                                      00810700
.******************************************************************     00810800
.***AI.L01 EDTINFO FMT=LIST FILL IN PARAMETER AREA                      00810900
.******************************************************************     00811000
                      ACTR  10000                                       00811100
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00811200
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00811300
&ZCVAL                SETC  'LAE'          XAX MODE                     00811400
.ZIMFXA               ANOP ,                                            00811500
.*                                                                      00811600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00811700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00811800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00811900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00812000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00812100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00812200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00812300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00812400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00812500
&ZOPN                 SETA  &ZOPN+1                                     00812600
&ZOPL(&ZOPN)          SETC  ' '                                         00812700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00812800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00812900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00813000
                      AGO   .ZIMFNX                                     00813100
.*                                                                      00813200
.ZIMFN2               ANOP ,                                            00813300
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00813400
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00813500
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00813600
&ZOPN                 SETA  &ZOPN+1                                     00813700
&ZOPL(&ZOPN)          SETC  ' '                                         00813800
&ZOPO(&ZOPN)          SETC  'LAE'                                       00813900
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00814000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00814100
                      AGO   .ZIMFNX                                     00814200
.ZIMFNX1              ANOP ,                                            00814300
&ZOPN                 SETA  &ZOPN+1                                     00814400
&ZOPL(&ZOPN)          SETC  ' '                                         00814500
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00814600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00814700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00814800
.ZIMFNX               ANOP ,                                            00814900
.********************************************************************   00815000
