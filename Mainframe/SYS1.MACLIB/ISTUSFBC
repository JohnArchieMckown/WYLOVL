*                                      PENDING SESSION TERMINATED       13600000
*                                                            @R495808*/ 13650000
*USF6APIS BIT(8) CONSTANT('15'X),   /* MUST ISSUE APPCCMD    @R495808*/ 13700000
*USFNONSW BIT(8) CONSTANT('16'X),   /* SWITCHED OPERATION ATTEMPTED     13750000
*                                      ON NONSWITCHED DEVICE     @B3A*/ 13800000
*USFNOCRY BIT(8) CONSTANT('17'X),   /* REJECT A REQUEST FOR ENCRYPTION  13850000
*                                      WHEN SESSION DOES NOT SUPPORT    13900000
*                                      CRYPTOGRAPHY              @X2A*/ 13950000
*USFNOSES BIT(8) CONSTANT('18'X),   /* XES IS NOT ACCESSABLE     @I3A*/ 14000000
*USFNOSYS BIT(8) CONSTANT('19'X),   /* Application not resident in a    14050000
*                                      SYSPLEX node              @I3A*/ 14100000
