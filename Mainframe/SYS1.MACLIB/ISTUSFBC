*/***    REASON CODE EQUATES FOR RPLFDB2 IF RPLRTNCD EQUALS X'14' ****/ 14650000
*/*                                                                  */ 14700000
*DECLARE                                                                14750000
*USFNONVR BIT(8) CONSTANT('00'X),   /* RPL CONT A NON-VTAM REQ CODE  */ 14800000
*                                   /* RPL ECB/EXIT NOT POSTED/INVOKD*/ 14850000
*USFNOTAS BIT(8) CONSTANT('01'X),   /* NOT ASSIGNED                  */ 14900000
*USFEXTAZ BIT(8) CONSTANT('02'X),   /* RPL IND EXIT, EXIT ADDR IS 0  */ 14950000
*                                   /* RPL ECB/EXIT NOT POSTED/INVOKD*/ 15000000
*USFEXTEZ BIT(8) CONSTANT('03'X),   /* RPL IND EXT ECB, ECB ADDR IS 0*/ 15050000
*                                   /* RPL ECB/EXIT NOT POSTED/INVOKD*/ 15100000
*USFCRPLN BIT(8) CONSTANT('04'X),   /* CHECKED RPL IS NOT ACTIVE     */ 15150000
*                                   /* ONLY OCCURS FOLLOWING         */ 15200000
*                                   /* A CHECK MACRO REQUEST         */ 15250000
*USFCBERR BIT(8) CONSTANT('10'X),   /* RPL POINTS TO INVALID ACB     */ 15300000
*USFRNORT BIT(8) CONSTANT('11'X),   /* NO RTYPE SPECIFIED            */ 15350000
*USFCLSIP BIT(8) CONSTANT('12'X),   /* CLSDST IN PROGRESS            */ 15400000
*USFCIDNG BIT(8) CONSTANT('13'X),   /* CID IS INVALID                */ 15450000
*USFILDOP BIT(8) CONSTANT('14'X),   /* LDO COMMAND FIELD IS INVALID  */ 15500000
*USFWANCR BIT(8) CONSTANT('15'X),   /* READ NOT CHAINED              */ 15550000
*USFSTOOD BIT(8) CONSTANT('16'X),   /* SOLICIT SPECIFIC TO           */ 15600000
*                                   /* OUTPUT ONLY DEVICE            */ 15650000
