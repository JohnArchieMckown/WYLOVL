*/***    RPLRTNCD        CONTAINS A FEEDBACK CODE.  IF THE RPL    ****/ 06550000
*/*                      REQUEST IS UNSUCCESSFUL THEN REGISTER       */ 06600000
*/*                      ZERO WILL ALSO CONTAIN THIS CODE. FOR A     */ 06650000
*/*                      CERTAIN GROUP OF ERRORS, ONLY REGISTER      */ 06700000
*/*                      ZERO WILL CONTAIN THE FEEDBACK CODE AND     */ 06750000
*/*                      NO FEEDBACK INFORMATION WILL BE PLACED IN   */ 06800000
*/*                      THE RPL.                                    */ 06850000
*/*                                                                  */ 06900000
*/*                      THE FEEDBACK CODE EQUATES ARE AS FOLLOWS:   */ 06950000
*DECLARE                                                                07000000
*USFAOK   BIT(8) CONSTANT('00'X),   /* NORMAL COMPLETION/COND COMP   */ 07050000
*USFXORDC BIT(8) CONSTANT('04'X),   /* EXTRAORDINARY COMPLETION      */ 07100000
*USFRESSU BIT(8) CONSTANT('08'X),   /* REISSUE THIS REQUEST          */ 07150000
*USFDAMGE BIT(8) CONSTANT('0C'X),   /* DAMAGE - INTEGRITY OF REQ/DEV */ 07200000
*USFENVER BIT(8) CONSTANT('10'X),   /* ENVIRONMENT ERROR             */ 07250000
*USFLOGIC BIT(8) CONSTANT('14'X),   /* USER LOGIC ERROR              */ 07300000
*USFRLGIC BIT(8) CONSTANT('18'X),   /* USER LOGIC ERROR                 07350000
*                                      - SETONLY IN REG ZERO @R495808*/ 07400000
*USF6IVR6 BIT(8) CONSTANT('1C'X),   /* INVALID RPL6                     07450000
*                                      - SETONLY IN REG ZERO @R495808*/ 07500000
*USF6CHEK BIT(8) CONSTANT('20'X),   /* RPL/RPL6 IN WRONG STATE          07550000
*                                      - SETONLY IN REG ZERO @R495808*/ 07600000
*USF6WRCK BIT(8) CONSTANT('24'X),   /* WRONG CHECK MACRO ISSUED         07650000
*                                      - SETONLY IN REG ZERO @R495808*/ 07700000
*USF3CHEK BIT(8) CONSTANT('28'X),   /* RPL/RPL3 IN WRONG STATE          07750000
*                                      - SETONLY IN REG ZERO     @D1A*/ 07800000
*USF3WRCK BIT(8) CONSTANT('2C'X);   /* WRONG CHECK MACRO ISSUED         07850000
*                                      - SETONLY IN REG ZERO     @D1A*/ 07900000
*/*                                                                  */ 07950000
*/*                                                                  */ 08000000
*/********************************************************************/ 08050000
*/*                                                                  */ 08100000
*/*      RPLFDB2         CONTAINS A REASON CODE.  THIS REASON CODE   */ 08150000
*/*                      INDICATES ADDITIONAL INFORMATION ABOUT THE  */ 08200000
*/*                      FEEDBACK CODE.                              */ 08250000
*/*                                                                  */ 08300000
*/***    REASON CODE EQUATES FOR RPLFDB2 IF RPLRTNCD EQUALS X'00' ****/ 08350000
*/*                                                                  */ 08400000
*DECLARE                                                                08450000
*USFAOOK  BIT(8) CONSTANT('00'X),   /* OPERATION SUCCESSFULLY COMP   */ 08500000
*USFRCWNP BIT(8) CONSTANT('01'X),   /* RESET COND WAS NO-OPED        */ 08550000
*USFRCDPR BIT(8) CONSTANT('02'X),   /* RESET CONDITIONAL SUCCESSFUL -*/ 08600000
*                                   /* READ-AHEAD DATA PRESENT       */ 08650000
*USFYTCTN BIT(8) CONSTANT('03'X),   /* YIELDED TO CONTENTION         */ 08700000
*USFYTCTL BIT(8) CONSTANT('04'X),   /* YIELDED TO CONT., ER LK SET   */ 08750000
*USFATSFI BIT(8) CONSTANT('05'X),   /* AREA TOO SMALL FOR INQ/INTRP  */ 08800000
*USFNOIN  BIT(8) CONSTANT('06'X),   /* NO INPUT AVAILABLE            */ 08850000
*USFIIINA BIT(8) CONSTANT('07'X),   /* INQUIRE INFORMATION NOT AVAIL */ 08900000
*USFDSTIU BIT(8) CONSTANT('08'X),   /* DESTINATION IN USE            */ 08950000
*USFNLGFA BIT(8) CONSTANT('09'X),   /* NO LOGON FND FOR ACCEPT MATCH */ 09000000
*USFANC   BIT(8) CONSTANT('0A'X),   /* APPL IS NOT CONNECTABLE WHEN     09050000
*                                      RPLFDB3 = USFIINA (X'04')        09100000
*                                                            @R488902*/ 09150000
*USF6APPC BIT(8) CONSTANT('0B'X),   /* INDICATES THAT AN ERROR OCCURRED 09200000
*                                      RUNNING APPC, AND REFER TO THE   09250000
*                                      RPL6 PRIMARY AND SECONDARY       09300000
*                                      RETURN CODES          @R495808*/ 09350000
*USF3VCNS BIT(8) CONSTANT('0C'X),   /* CONDITIONAL COMPLETION FOR       09400000
*                                      VCNSCMD                   @D1A*/ 09450000
*USFINQPS BIT(8) CONSTANT('0D'X),   /* MORE SESSIONS PENDING RECOVERY   09500000
*                                      ON WHICH TO INQUIRE       @B2A*/ 09550000
*USFTHRCM BIT(8) CONSTANT('0E'X);   /* CS/390 RESERVED           @F3A*/ 09575000
*/*      IF, FOLLOWING A SYNCHRONOUS RPL REQUEST MACRO OR CHECK      */ 09600000
*/*      MACRO, REGISTER 15 CONTAINS X'00' THEN REGISTER ZERO WILL   */ 09650000
*/*      CONTAIN ONE OF THE ABOVE REASON CODE VALUES                 */ 09700000
*/*                                                                  */ 09750000
*/*                                                                  */ 09800000
*/***    REASON CODE EQUATES FOR RPLFDB2 IF RPLRTNCD EQUALS X'04' ****/ 09850000
*/*                                                                  */ 09900000
*DECLARE                                                                09950000
*USFRVIRC BIT(8) CONSTANT('00'X),   /* RVI RECEIVED, ERROR LOCK SET  */ 10000000
*USFATNRC BIT(8) CONSTANT('01'X),   /* ATTN RECEIVED, ERROR LOCK SET */ 10050000
*USFBSCSM BIT(8) CONSTANT('02'X),   /* BSC STATUS MSG RECEIVED       */ 10100000
*USFEXRQ  BIT(8) CONSTANT('03'X),   /* EXCEPTION REQUEST RECEIVED    */ 10150000
*USFEXRS  BIT(8) CONSTANT('04'X),   /* EXCEPTION RESPONSE RECEIVED   */ 10200000
*USFNQN   BIT(8) CONSTANT('05'X);   /* RESOURCE KNOWN AS NQN ONLY       10250000
*                                                                @KGA*/ 10300000
*/*                                                                  */ 10350000
*/*                                                                  */ 10400000
*/***    REASON CODE EQUATES FOR RPLFDB2 IF RPLRTNCD EQUALS X'08' ****/ 10450000
*/*                                                                  */ 10500000
*DECLARE                                                                10550000
*USFSTALF BIT(8) CONSTANT('00'X);   /* TEMPORARY OUT OF STORAGE      */ 10600000
*                                   /* SITUATION EXISTS, RPL         */ 10650000
*                                   /* ECB/EXIT NOT POSTED/INVOKED   */ 10700000
*/*                                                                  */ 10750000
*/*                                                                  */ 10800000
*/***    REASON CODE EQUATES FOR RPLFDB2 IF RPLRTNCD EQUALS X'0C' ****/ 10850000
*/*                                                                  */ 10900000
*DECLARE                                                                10950000
*USFIOEDU BIT(8) CONSTANT('00'X),   /* I/O ERROR, DEVICE STILL USABLE*/ 11000000
*                                   /* ERROR LOCK SET                */ 11050000
*USFDVUNS BIT(8) CONSTANT('01'X),   /* I/O ERROR, DEVICE NOT USABLE  */ 11100000
*                                   /* ERROR LOCK SET                */ 11150000
*USFUNTRM BIT(8) CONSTANT('02'X),   /* REQUEST RESET BY TEST REQ MSG */ 11200000
*USFBTHEX BIT(8) CONSTANT('03'X),   /* BUFFER THRESHOLD EXCEEDED     */ 11250000
*USFBTEOR BIT(8) CONSTANT('04'X),   /* BUF THRESHOLD EXCEEDED,       */ 11300000
*                                   /* ONLY READS ALLOW              */ 11350000
*USFNCPAO BIT(8) CONSTANT('05'X),   /* NCP ABENDED, RESTART O.K.     */ 11400000
*USFLIORP BIT(8) CONSTANT('06'X),   /* LAST I/O REQUEST PURGED       */ 11450000
*USFRECIP BIT(8) CONSTANT('07'X),   /* RECOVERY IN PROGRESS          */ 11500000
*USFRTRAF BIT(8) CONSTANT('08'X),   /* RECORD TERM RESTRTD AFTER FAIL*/ 11550000
*USFQOPDC BIT(8) CONSTANT('09'X),   /* Q'ED OPNDST CANCELED BY CLSDST*/ 11600000
*USFUSRES BIT(8) CONSTANT('0A'X),   /* REQUEST RESET BY THE USER     */ 11650000
*USFCLOCC BIT(8) CONSTANT('0B'X),   /* CLSDST OR TERMSESS ISSUED OR     11700000
*                                      UNBIND SENT IN LIEU OF NEGATIVE  11750000
*                                      BIND RESPONSE         @R500011*/ 11800000
*USFCLRED BIT(8) CONSTANT('0C'X),   /* REQUEST WAS CLEAR''ED         */ 11850000
*USFPREXC BIT(8) CONSTANT('0D'X),   /* SEND CANCELLED DUE TO         */ 11900000
*                                   /* PRIOR EXCEPTION CONDITION     */ 11950000
*USFPOQLE BIT(8) CONSTANT('0E'X);   /* SEND CANCELLED DUE TO POA     */ 12000000
*                                   /* QUEUE LIMIT EXCEEDED      @01A*/ 12050000
*/*                                                                  */ 12100000
*/*                                                                  */ 12150000
*/***    REASON CODE EQUATES FOR RPLFDB2 IF RPLRTNCD EQUALS X'10' ****/ 12200000
*/*                                                                  */ 12250000
*DECLARE                                                                12300000
*USFTANAV BIT(8) CONSTANT('00'X),   /* TERMINAL OR APPL NOT AVAILABLE*/ 12350000
*USFSBFAL BIT(8) CONSTANT('01'X),   /* SESSION BIND FAILED           */ 12400000
*USFTAPUA BIT(8) CONSTANT('02'X),   /* TARGET APPL UNACCEPTABLE      */ 12450000
*USFVTHAL BIT(8) CONSTANT('03'X),   /* VTAM IS HALTING               */ 12500000
*USFILRS  BIT(8) CONSTANT('04'X),   /* INCOMPATIBLE DEFINITION       */ 12550000
*USFPCF   BIT(8) CONSTANT('05'X),   /* PERMANENT FAILURE IN PATH     */ 12600000
*USFANS   BIT(8) CONSTANT('06'X),   /* AUTO NETWORK SHUTDOWN         */ 12650000
*USFVOFOC BIT(8) CONSTANT('07'X),   /* VARY DEACT IMMEDIATE OCCURRED */ 12700000
*USFDISCO BIT(8) CONSTANT('08'X),   /* DISCONNECT OCCURRED           */ 12750000
*USFUTSCR BIT(8) CONSTANT('09'X),   /* UNCONDITIONAL TERMINATE       */ 12800000
*                                   /* SELF COMMAND RECEIVED         */ 12850000
*USFSYERR BIT(8) CONSTANT('0A'X),   /* APPARENT VTAM ERROR           */ 12900000
*USFDIDOL BIT(8) CONSTANT('0B'X),   /* DISCONNECT ON DIAL-OUT LINE   */ 12950000
*USFDIDIL BIT(8) CONSTANT('0C'X),   /* DISCONNECT ON DIAL-IN LINE    */ 13000000
*/*  NOTE - X'0D' AND X'0E' - RPL ECB/EXIT NOT POSTED/INVOKED        */ 13050000
*USFVTMNA BIT(8) CONSTANT('0D'X),   /* VTAM INACTIVE FOR THAT APPL   */ 13100000
*USFABNDO BIT(8) CONSTANT('0E'X),   /* ABEND CONDITION HAS OCCURRED  */ 13150000
*/*                                                                  */ 13200000
*USFVTBFO BIT(8) CONSTANT('0F'X),   /* VTAM BUFFER OVERFLOW          */ 13250000
*USFCTERM BIT(8) CONSTANT('10'X),   /* CONDITIONAL TERM SELF @ZA17016*/ 13300000
*USFOSDTF BIT(8) CONSTANT('11'X),   /* SDT FAILURE ON OPNDST @ZA17016*/ 13350000
*USFMFF   BIT(8) CONSTANT('12'X),   /*MACRO FUNCTION FAILED,SENSE INC*/ 13400000
*USF6APRJ BIT(8) CONSTANT('13'X),   /* ATTEMPT TO START 6.2 SESSION:    13450000
*                                      REQUEST REJECTED      @R495808*/ 13500000
