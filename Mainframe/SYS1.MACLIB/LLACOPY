                      AIF   (K'&ZLD LE 0).ZL5                           50300000
&ZLC                  SETA  35             COMMENT COLUMN               50350000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    50400000
&ZLC                  SETA  33             COMMMENT COLUMN              50450000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   50500000
&ZLV                  SETC  '&ZLV'.' '                                  50550000
                      AGO   .ZL3                                        50600000
.*                                                                      50650000
.ZL4                  ANOP  ,                                           50700000
&ZLV                  SETC  '&ZLV'.' '                                  50750000
.ZL5                  ANOP  ,                                           50800000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               50850000
.*                                                                      50900000
