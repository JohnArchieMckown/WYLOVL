.***AC.PLV01  PROCESS PLISTVER                                      *   00075100
.********************************************************************   00075200
&XPLISTVER      SETA  0                                                 00075300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00075400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00075500
&XPLISTVER      SETA  0                                                 00075600
.ZPLV0          ANOP  ,                                                 00075700
.*                                                                      00075800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00075900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00076000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00076100
.*-------------------------------------------------------------------   00076200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00076300
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00076400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00076500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00076600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00076700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00076800
                AGO   .ZPVCK2                                           00076900
.ZPVCK3         ANOP  ,                                                 00077000
.*-------------------------------------------------------------------   00077100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00077200
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00077300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00077400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00077500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00077600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00077700
                AGO   .ZPVCKE                                           00077800
.ZPVCK2         ANOP  ,                                                 00077900
.*-------------------------------------------------------------------   00078000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00078100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00078200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00078300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00078400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00078500
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00078600
.ZPVCK6         ANOP  ,                                                 00078700
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00078800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00078900
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00079000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00079100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00079200
&XPLISTVER      SETA  0                                                 00079300
                AGO   .ZPVCKE                                           00079400
.*-------------------------------------------------------------------   00079500
.ZPVCK4A        ANOP  ,                                                 00079600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00079700
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00079800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00079900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00080000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00080100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00080200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00080300
                AGO   .ZPVCK5                                           00080400
.ZPVCK4         ANOP  ,                                                 00080500
&XPLISTVER      SETA  &PLISTVER(1)                                      00080600
.ZPVCK5         ANOP  ,                                                 00080700
.ZPVCKE         ANOP  ,                                                 00080800
.********************************************************************   00080900
