.***AP.X01  FORMAT GENERATED CODE                                       00169500
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00169600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00169700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00169800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00169900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00170000
                      AIF   (K'&ZLD LE 0).ZL51                          00170100
&ZLC                  SETA  35             COMMENT COLUMN               00170200
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00170300
&ZLC                  SETA  33             COMMENT COLUMN               00170400
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00170500
&ZLV                  SETC  '&ZLV'.' '                                  00170600
                      AGO   .ZL31                                       00170700
.*                                                                      00170800
.ZL41                 ANOP  ,                                           00170900
&ZLV                  SETC  '&ZLV'.' '                                  00171000
.ZL51                 ANOP  ,                                           00171100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00171200
.*                                                                      00171300
&ZLL     &ZLOP &ZLV                                                     00171400
.*                                                                      00171500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00171600
                      AGO   .ZL21          CONTINUE LOOPING             00171700
.*                                                                      00171800
.ZLX1                 ANOP  ,                                           00171900
