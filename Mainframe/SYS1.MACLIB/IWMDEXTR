.*******************************************************************    00163700
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00163800
.*******************************************************************    00163900
                      ACTR  10000                                       00164000
.*                                                                      00164100
                      LCLA  &ZLC           COLUMN POSITION              00164200
                      LCLA  &ZOPX          LIST TABLE INDEX             00164300
                      LCLC  &ZLL           LABEL                        00164400
                      LCLC  &ZLOP          OPERATION                    00164500
                      LCLC  &ZLV           VALUE                        00164600
                      LCLC  &ZLD           DESCRIPTION                  00164700
.*                                                                      00164800
&ZOPX                 SETA  1                                           00164900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00165000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00165100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00165200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00165300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00165400
                      AIF   (K'&ZLD LE 0).ZL5                           00165500
&ZLC                  SETA  35             COMMENT COLUMN               00165600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00165700
&ZLC                  SETA  33             COMMMENT COLUMN              00165800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00165900
&ZLV                  SETC  '&ZLV'.' '                                  00166000
                      AGO   .ZL3                                        00166100
.*                                                                      00166200
.ZL4                  ANOP  ,                                           00166300
&ZLV                  SETC  '&ZLV'.' '                                  00166400
.ZL5                  ANOP  ,                                           00166500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00166600
.*                                                                      00166700
&ZLL     &ZLOP &ZLV                                                     00166800
.*                                                                      00166900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00167000
                      AGO   .ZL2           CONTINUE LOOPING             00167100
.*                                                                      00167200
.ZLX                  ANOP  ,                                           00167300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00167400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00167500
         L     14,16(0,0)              GET CVT ADDRESS                  00167600
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00167700
         LAE   14,0(14,0)              CLEAR AR 14                      00167800
.NOAR    ANOP                                                           00167900
         TM    CVTOSLV1-CVT(14),X'10'      TEST FOR SP520 RELEASE       00168000
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00168100
         LA    0,IWMRSNCODENOPOLMGT    SET REASON CODE = NO WLM SVCS    00168200
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00168300
         B     END&SYSNDX              BRANCH TO END LABEL              00168400
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00168500
         L     14,772(14,0)            GET SFT ADDRESS                  00168600
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00168700
         LA    15,32                   GET EX FOR SERVICE ROUTINE       00168800
         OR    14,15                   GET PC NUMBER                    00168900
         PC    0(14)                                                    00169000
END&SYSNDX DS    0H                    END LABEL                        00169100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00169200
.ZLINKX               ANOP  ,                                           00169300
