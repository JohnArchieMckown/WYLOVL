.*******************************************************************    00034200
              GBLC  &ZPRINT     YES=PRINT NO=NO PRINT                   00034300
              GBLC  &ZTEST      YES=TEST NO=NO TEST                     00034400
              GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX             00034500
.*                                                                      00034600
              GBLC  &ZIXLPURGE  YES=MACRO USED AT LEAST ONCE            00034700
.*******************************************************************    00034800
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00034900
.*******************************************************************    00035000
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00035100
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00035200
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00035300
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00035400
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00035500
              LCLA  &ZAWRK      USED AS A WORK AREA                     00035600
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00035700
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00035800
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00035900
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00036000
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00036100
              LCLA  &ZMACRET    MACRO RETURN CODE                       00036200
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00036300
.*                                                                      00036400
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00036500
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00036600
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00036700
.********************************************************************   00036800
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00036900
.********************************************************************   00037000
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00037100
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00037200
.*                                                                      00037300
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00037400
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00037500
              SPLEVEL  TEST                                             00037600
.ZSYS2        ANOP  ,                                                   00037700
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00037800
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00037900
              SYSSTATE TEST                                             00038000
              AGO   .ZSYS4                                              00038100
.ZSYS3        ANOP  ,                                                   00038200
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00038300
.ZSYS4        ANOP  ,                                                   00038400
.*******************************************************************    00038500
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00038600
.*******************************************************************    00038700
              LCLC  &XSCOPE     ++ NAME                                 00038800
              LCLC  &XCONTOKEN  ++ NAME                                 00038900
              LCLC  &XREQID     ++ NAME                                 00039000
              LCLC  &XSTOKEN    ++ NAME                                 00039100
              LCLC  &XTTOKEN    ++ NAME                                 00039200
              LCLC  &XRETCODE   ++ NAME                                 00039300
              LCLC  &XRSNCODE   ++ NAME                                 00039400
              LCLC  &XMF        ++ VALUE                                00039500
              LCLC  &XMFCTRL    ++ NAME                                 00039600
              LCLC  &XMFATTR    ++ VALUE                                00039700
              LCLC  &XMFSCK     ++ VALUE                                00039800
.*******************************************************************    00039900
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00040000
.*******************************************************************    00040100
              LCLC  &ZXSCOPE    ++ INPUT NAME                           00040200
              LCLC  &ZXCONTOKEN                 ++ INPUT NAME           00040300
              LCLC  &ZXREQID    ++ INPUT NAME                           00040400
              LCLC  &ZXSTOKEN   ++ INPUT NAME                           00040500
              LCLC  &ZXTTOKEN   ++ INPUT NAME                           00040600
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00040700
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00040800
              LCLC  &ZXMF       ++ INPUT VALUE                          00040900
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00041000
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00041100
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00041200
.********************************************************************   00041300
.***AG.MF01 PROCESS MF MACRO FORMS                                      00041400
.********************************************************************   00041500
              ACTR  10000                                               00041600
.*------------------------------------------------------------------    00041700
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00041800
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00041900
.*-------------------------------------------------------------------   00042000
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00042100
.ZG17         ANOP  ,                                                   00042200
&ZTLC         SETC 'S'                                                  00042300
              AIF   ('&MF(1)' NE '&ZTLC').ZG18                          00042400
&XMF          SETC  'S'         SET KEYWORD                             00042500
              AIF   (N'&MF LE 1).ZG17A          CK NUM                  00042600
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00042700
&ZMS          SETC  '&ZMS.&MF"'                                         00042800
&ZMS          SETC  '&ZMS "MF"'                                         00042900
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00043000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00043100
.ZG17A        ANOP  ,                                                   00043200
.*-------------------------------------------------------------------   00043300
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00043400
.ZG18         ANOP  ,                                                   00043500
&ZTLC         SETC 'L'                                                  00043600
              AIF   ('&MF(1)' NE '&ZTLC').ZG25                          00043700
&XMF          SETC  'L'         SET KEYWORD                             00043800
              AIF   (N'&MF LE 3).ZG18A          CK NUM                  00043900
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00044000
&ZMS          SETC  '&ZMS.&MF"'                                         00044100
&ZMS          SETC  '&ZMS "MF"'                                         00044200
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00044300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00044400
.ZG18A        ANOP  ,                                                   00044500
.*-------------------------------------------------------------------   00044600
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00044700
              AIF   (N'&SYSLIST NE 0).ZG18B     POSITIONALS?            00044800
              AIF   ('&CONTOKEN' NE '').ZG18B   KEYS?                   00044900
              AIF   ('&REQID' NE '').ZG18B      KEYS?                   00045000
              AIF   ('&STOKEN' NE '').ZG18B     KEYS?                   00045100
              AIF   ('&TTOKEN' NE '').ZG18B     KEYS?                   00045200
              AIF   ('&RETCODE' NE '').ZG18B    KEYS?                   00045300
              AIF   ('&RSNCODE' NE '').ZG18B    KEYS?                   00045400
              AGO    .ZG18C     INVOCATION OKAY                         00045500
.ZG18B        ANOP  ,                                                   00045600
&ZMS          SETC  'THE "L" FORM OF "MF" CANNOT'  *MFLA* MNOTE         00045700
&ZMS          SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                   00045800
&ZMS          SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'              00045900
              MNOTE 8,' &ZMS ON THE INVOCATION.'                        00046000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00046100
.ZG18C        ANOP  ,                                                   00046200
.*-------------------------------------------------------------------   00046300
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00046400
&ZXMFCTRL     SETC  'INPUT'     SET IN/OUTPUT                           00046500
&XMFCTRL      SETC  '&MF(2,1)'  SET XVARIABLE                           00046600
              AIF   (N'&MF(2) GT 1).ZG20B       MULTI PARMS             00046700
