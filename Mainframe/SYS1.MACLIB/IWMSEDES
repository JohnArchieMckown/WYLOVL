&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00129500
                      AGO   .ZMRETX                                     00129600
.*                                                                      00129700
.ZMRET2               ANOP  ,                                           00129800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00129900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00130000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00130100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00130200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00130300
&ZOPN                 SETA  &ZOPN+1                                     00130400
&ZOPL(&ZOPN)          SETC  ' '                                         00130500
