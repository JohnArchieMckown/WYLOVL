.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00134900
.*******************************************************************    00135000
                      ACTR  10000                                       00135100
.*                                                                      00135200
                      LCLA  &ZLC           COLUMN POSITION              00135300
                      LCLA  &ZOPX          LIST TABLE INDEX             00135400
                      LCLC  &ZLL           LABEL                        00135500
                      LCLC  &ZLOP          OPERATION                    00135600
                      LCLC  &ZLV           VALUE                        00135700
                      LCLC  &ZLD           DESCRIPTION                  00135800
.*                                                                      00135900
&ZOPX                 SETA  1                                           00136000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00136100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00136200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00136300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00136400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00136500
                      AIF   (K'&ZLD LE 0).ZL5                           00136600
&ZLC                  SETA  35             COMMENT COLUMN               00136700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00136800
&ZLC                  SETA  33             COMMMENT COLUMN              00136900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00137000
&ZLV                  SETC  '&ZLV'.' '                                  00137100
                      AGO   .ZL3                                        00137200
.*                                                                      00137300
.ZL4                  ANOP  ,                                           00137400
&ZLV                  SETC  '&ZLV'.' '                                  00137500
.ZL5                  ANOP  ,                                           00137600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00137700
.*                                                                      00137800
&ZLL     &ZLOP &ZLV                                                     00137900
.*                                                                      00138000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00138100
                      AGO   .ZL2           CONTINUE LOOPING             00138200
.*                                                                      00138300
.ZLX                  ANOP  ,                                           00138400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00138500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00138600
         L     14,16(0,0)              GET CVT ADDRESS                  00138700
         TM    CVTOSLV2-CVT(14),X'80'  TEST FOR OS/390 RELEASE IV       00138800
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00138900
         LA    0,IWMRSNCODENOSCHENV    SET REASON CODE = NO WLM SVCS    00139000
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00139100
         B     END&SYSNDX              BRANCH TO END LABEL              00139200
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00139300
         L     14,16(0,0)              GET CVT ADDRESS                  00139400
         L     14,772(14,0)            GET SFT ADDRESS                  00139500
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00139600
         LA    15,58                   GET EX FOR SERVICE ROUTINE       00139700
         OR    14,15                   GET PC NUMBER                    00139800
         PC    0(14)                                                    00139900
END&SYSNDX DS    0H                    END LABEL                        00140000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00140100
.ZLINKX               ANOP  ,                                           00140200
