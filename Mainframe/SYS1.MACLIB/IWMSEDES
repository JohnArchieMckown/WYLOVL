&ZOPN1                SETA &ZOPN                                        00127700
.*******************************************************************    00127800
.***  PROCESS IWMSEDES OUTPUT PARAMETERS                                00127900
.*******************************************************************    00128000
                      ACTR  10000                                       00128100
.*******************************************************************    00128200
.***AO.X02  RETURN CODE PROCESSING                                      00128300
.*******************************************************************    00128400
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00128500
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00128600
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00128700
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00128800
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00128900
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00129000
&ZOPN                 SETA  &ZOPN+1                                     00129100
&ZOPL(&ZOPN)          SETC  ' '                                         00129200
&ZOPO(&ZOPN)          SETC  'ST'                                        00129300
