&ZOPL(&ZOPN)          SETC ' '                                          00125100
&ZOPO(&ZOPN)          SETC 'SPACE'                                      00125200
&ZOPV(&ZOPN)          SETC '1'                                          00125300
&ZOPD(&ZOPN)          SETC ' '                                          00125400
.*-------------------------------------------------------------------   00125500
.***AL.L12 IWMSEQRY FMT=LIST MF=S|L PARAMETERS                          00125600
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00125700
