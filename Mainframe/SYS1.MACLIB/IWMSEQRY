.***AG.KCK02 NAME, OPT RQD INPUT, POS(2) -GPR OUT OF RANGE              00079900
              AIF   (K'&ZXMFCTRL LE 0).ZG31R    NOT I/O                 00080000
              AIF   ('&XMFCTRL'(1,1) NE '(').ZG31R                      00080100
&ZCGPR        SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)                          00080200
              AIF   ('&ZCGPR'(1,1) NE '0').ZG31P                        00080300
              AIF   (K'&ZCGPR GT 2).ZG31Q                               00080400
              AIF   ('&ZCGPR' EQ '0').ZG31Q                             00080500
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00080600
              AIF   (T'&ZCGPR NE 'N').ZG31Q     INVALID REG NAME        00080700
.ZG31P        ANOP  ,                                                   00080800
              AIF   (T'&ZCGPR NE 'N').ZG31R     NON NUMERIC             00080900
              AIF   ('&ZCGPR' LT '1').ZG31Q                             00081000
              AIF   ('&ZCGPR' LE '12').ZG31R                            00081100
.ZG31Q        ANOP  ,                                                   00081200
&ZMS          SETC  '"MF='      *KGPRA* MNOTE                           00081300
&ZMS          SETC  '&ZMS.&MF"'                                         00081400
&ZMS          SETC  '&ZMS ARGUMENT 2 MUST SPECIFY A REGISTER'           00081500
              MNOTE 8,' &ZMS IN THE RANGE (1)-(12).'                    00081600
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00081700
.ZG31R        ANOP  ,                                                   00081800
