.******************************************************************     00108600
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00108700
.******************************************************************     00108800
              ACTR  10000                                               00108900
.*----------------------------------------------------------------***   00109000
.***AH.P02 EXTRA POSITIONALS                                            00109100
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00109200
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00109300
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00109400
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00109500
.ZGLISTX1     ANOP  ,                                                   00109600
&ZCVAL        SETC  ''          ERROR FIELD                             00109700
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00109800
.ZGLIST2      ANOP  ,                                                   00109900
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00110000
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00110100
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00110200
              AGO   .ZGLIST2                                            00110300
.*                                                                      00110400
.ZGLIST3      ANOP  ,                                                   00110500
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00110600
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00110700
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00110800
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00110900
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00111000
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00111100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00111200
.ZGLISTX      ANOP  ,                                                   00111300
&ZASYSLNDX    SETA  0                                                   00111400
