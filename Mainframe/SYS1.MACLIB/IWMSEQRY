.***AG.MF04 MF=(SLE)                                                    00111600
.ZGMFX        ANOP  ,           END OF FULL SYNTAX CHECK                00111700
.*******************************************************************    00111800
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00111900
.*******************************************************************    00112000
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          00112100
.*******************************************************************    00112200
.***AE.01  START OF IWMSEQRY MACRO CODE                                 00112300
.*******************************************************************    00112400
              LCLC  &M0M0001                 NAME                       00112500
&M0M0001      SETC  '&XLABEL'                SET NAME                   00112600
              AIF   (K'&XLABEL NE 0).XLAB2                              00112700
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00112800
.*                                                                      00112900
.XLAB2        ANOP  ,                                                   00113000
              AIF   ('&XMF' EQ 'L').XLAB3                               00113100
&M0M0001 DS    0H                             IWMSEQRY-0                00113200
.XLAB3        ANOP  ,                                                   00113300
.*******************************************************************    00113400
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00113500
.*******************************************************************    00113600
              PUSH  PRINT                                               00113700
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00113800
              PRINT OFF         ++ PR01                                 00113900
              AGO   .ZPRNTXX                                            00114000
.ZPRINTX      PRINT ON                                                  00114100
.ZPRNTXX      ANOP  ,                                                   00114200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00114300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00114400
.********************************************************************   00114500
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00114600
.********************************************************************   00114700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00114800
                      LCLA  &ZOPN1         INVOCATION INDEX             00114900
                      LCLC  &ZOPL(4095)    LABEL                        00115000
                      LCLC  &ZOPO(4095)    OPERATION                    00115100
                      LCLC  &ZOPV(4095)    VALUE                        00115200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00115300
.*                                                                      00115400
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00115500
.******************************************************************     00115600
.***AL.L10 IWMSEQRY FMT=LIST MF=S|L PARAMETERS                          00115700
.******************************************************************     00115800
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00115900
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00116000
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00116100
&XMFATTR              SETC  '0D'                                        00116200
.*                                                                      00116300
&ZOPN                 SETA  &ZOPN+1                                     00116400
&ZOPL(&ZOPN)          SETC  ' '                                         00116500
&ZOPO(&ZOPN)          SETC  'B'                                         00116600
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00116700
&ZOPD(&ZOPN)          SETC  '++ IWMSEQRY ARG PARMS'                     00116800
.*                                                                      00116900
&ZOPN                 SETA  &ZOPN+1                                     00117000
&ZOPL(&ZOPN)          SETC  ' '                                         00117100
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00117200
&ZOPV(&ZOPN)          SETC  '1'                                         00117300
&ZOPD(&ZOPN)          SETC  ' '                                         00117400
.*                                                                      00117500
.ZLMFSL2              ANOP  ,                                           00117600
.*-------------------------------------------------------------------   00117700
.***AM.L00  IWMSEQRY FMT=LIST name                                      00117800
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00117900
&ZOPN                 SETA &ZOPN+1                                      00118000
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00118100
