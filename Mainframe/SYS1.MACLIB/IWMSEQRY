              AIF   (N'&MF LE 1).ZG21A          CK NUM                  00068100
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00068200
&ZMS          SETC  '&ZMS.&MF"'                                         00068300
&ZMS          SETC  '&ZMS "MF"'                                         00068400
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00068500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00068600
.ZG21A        ANOP  ,                                                   00068700
.*-------------------------------------------------------------------   00068800
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00068900
.ZG22         ANOP  ,                                                   00069000
&ZTLC         SETC 'L'                                                  00069100
              AIF   ('&MF(1)' NE '&ZTLC').ZG29                          00069200
&XMF          SETC  'L'         SET KEYWORD                             00069300
              AIF   (N'&MF LE 3).ZG22A          CK NUM                  00069400
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00069500
&ZMS          SETC  '&ZMS.&MF"'                                         00069600
&ZMS          SETC  '&ZMS "MF"'                                         00069700
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00069800
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00069900
.ZG22A        ANOP  ,                                                   00070000
.*-------------------------------------------------------------------   00070100
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00070200
              AIF   (N'&SYSLIST NE 0).ZG22B     POSITIONALS?            00070300
              AIF   ('&ANSLEN' NE '').ZG22B     KEYS?                   00070400
              AIF   ('&ANSAREA' NE '').ZG22B    KEYS?                   00070500
              AIF   ('&QUERYLEN' NE '').ZG22B   KEYS?                   00070600
              AIF   ('&RETCODE' NE '').ZG22B    KEYS?                   00070700
              AIF   ('&RSNCODE' NE '').ZG22B    KEYS?                   00070800
              AGO    .ZG22C     INVOCATION OKAY                         00070900
.ZG22B        ANOP  ,                                                   00071000
&ZMS          SETC  'THE "L" FORM OF "MF"'      *MFLA* MNOTE            00071100
&ZMS          SETC  '&ZMS CAN ONLY HAVE'                                00071200
&ZMS          SETC  '&ZMS THE "PLISTVER" KEY'                           00071300
              MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'              00071400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00071500
.ZG22C        ANOP  ,                                                   00071600
.*-------------------------------------------------------------------   00071700
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00071800
&ZXMFCTRL     SETC  'INPUT'     SET IN/OUTPUT                           00071900
&XMFCTRL      SETC  '&MF(2,1)'  SET XVARIABLE                           00072000
              AIF   (N'&MF(2) GT 1).ZG24B       MULTI PARMS             00072100
&XMFCTRL      SETC  '&MF(2)'    SET XVARIABLE                           00072200
.ZG24B        AIF   (K'&MF(2) GT 0).ZG24C                               00072300
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00072400
&ZMS          SETC  'WHEN "L" IS SPECIFIED'     *KRQDA*                 00072500
&ZMS          SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'                 00072600
              MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                        00072700
