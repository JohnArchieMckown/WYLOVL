&ZLV                  SETC  '&ZLV'.' '                                  00150800
.ZL5                  ANOP  ,                                           00150900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00151000
.*                                                                      00151100
&ZLL     &ZLOP &ZLV                                                     00151200
.*                                                                      00151300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00151400
                      AGO   .ZL2           CONTINUE LOOPING             00151500
.*                                                                      00151600
.ZLX                  ANOP  ,                                           00151700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00151800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00151900
         L     14,16(0,0)              GET CVT ADDRESS                  00152000
         TM    CVTOSLV2-CVT(14),X'80'  TEST FOR OS/390 RELEASE IV       00152100
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00152200
         LA    0,IWMRSNCODENOSCHENV    SET REASON CODE = NO WLM SVCS    00152300
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00152400
         B     END&SYSNDX              BRANCH TO END LABEL              00152500
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00152600
         L     14,16(0,0)              GET CVT ADDRESS                  00152700
         L     14,772(14,0)            GET SFT ADDRESS                  00152800
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00152900
         LA    15,55                   GET EX FOR SERVICE ROUTINE       00153000
         OR    14,15                   GET PC NUMBER                    00153100
         PC    0(14)                                                    00153200
END&SYSNDX DS    0H                    END LABEL                        00153300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00153400
