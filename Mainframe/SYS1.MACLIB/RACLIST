               &FILTER=,&GLOBAL=                                   @L3C 01266600
.******* DEFINE LOCAL SET SYMBOLS                                       01300000
.*** THESE BITS MAP TO RLPLFLAG IN ICHRLS00,  PXFLAG IN ICHGLS00,  @L4A 01316600
.*** AND RLSTFLG2 IN IRRPRLST                                      @L4A 01333200
         LCLA  &BIT0,&BIT1,&BIT2,&BIT3,&BIT4,&BIT5,&BIT6,&BIT7     @L1A 01350000
         LCLA  &BIX0,&BIX1,&BIX2,&BIX3,&BIX4,&BIX5,&BIX6,&BIX7     @L1A 01400000
         LCLA  &OFFSET,&CLNGTH                                          01450000
.*** THESE BITS MAP TO RLSTFLG2 IN IRRPRLST                        @L3A 01456200
         LCLA  &GBIT0,&GBIT1,&GBIT2,&GBIT3,&GBIT4,&GBIT5,&GBIT6,&GBIT7  01462500
         LCLA  &GBIX0,&GBIX1,&GBIX2,&GBIX3,&GBIX4,&GBIX5,&GBIX6,&GBIX7  01475000
.*                                                                 @L3A 01481200
         LCLA  &PARMLEN                                            @L3A 01487500
         LCLB  &MFFLAG                                                  01500000
         LCLC  &SUBSTR                                                  01550000
.******* DEFINE GLOBAL SET VARIABLES                               @T1A 01600000
         GBLB  &ICHRAC              ICHRAC - A FLAG WHICH, WHEN    @T1A 01650000
.*                                  ON, INDICATES THAT THIS MACRO  @T1A 01700000
.*                                  IS BEING INVOKED INTERNALLY    @T1A 01750000
.*                                  BY THE RACROUTE MACRO.         @T1A 01800000
         GBLC  &ICHL                THIS IS USED TO COMMUNICATE    @T1A 01850000
.*                                  TO THE RACROUTE MACRO THE      @T1A 01900000
.*                                  ADDRESS OF THE PARAMETER LIST  @T1A 01950000
         GBLC  &ICHRLVL             SYMBOL TO BRANCH TO            @L7A 02000000
.*                                  WHEN INCOMPATIBLE MACRO LEVEL. @L7A 02050000
&PARMLEN SETA  32                   LENGTH OF PARAMETER LIST       @L3A 02075000
.******* PERFORM PARAMETER DIAGNOSIS                                    02100000
.* VALIDATE RELEASE PARAMETER                                      @L7A 02150000
         AIF   (&ICHRAC).RLVL01                                    @L7A 02200000
&ICHRLVL SETC  'ICH&SYSNDX.X'      CREATE LABEL FOR SVC BYPASS     @L7A 02250000
.RLVL01  ANOP                                                      @L7A 02300000
         AIF   ('&RELEASE' EQ '').ERLVL1R                          @L7A 02350000
         AIF   (N'&RELEASE GT 2).ERLVL1  INVALID RELEASE PARAMETER @L7A 02400000
         AIF   ('&RELEASE(1)' EQ '1.6' OR                              X02421400
               '&RELEASE(1)' EQ '1.7' OR                               X02442800
               '&RELEASE(1)' EQ '1.8').NOERLVL                     @P2C 02464200
         AIF   ('&RELEASE(1)' NE '1.8.1' AND                           X02485600
               '&RELEASE(1)' NE '1.9' AND                              X02499800
               '&RELEASE(1)' NE '1.9.2' AND                            X02509300
               '&RELEASE(1)' NE '2.1'    AND                           X02515600
               '&RELEASE(1)' NE '2.2'    AND                           X02519800
               '&RELEASE(1)' NE '2.3'    AND                           X02522600
               '&RELEASE(1)' NE '2.4'    AND                           X02524500
               '&RELEASE(1)' NE '2.6'    AND                           X02525800
               '&RELEASE(1)' NE '2608' AND                             X02526600
               '&RELEASE(1)' NE '7703').ERLVL1                     @M5A 02527400
.NOERLVL ANOP                                                      @P2A 02528400
         AIF   (N'&RELEASE EQ 1).NORLVLC CHECK NOT SPECIFIED       @L7A 02550000
         AIF   ('&RELEASE(2)' NE 'CHECK').ERLVL1 INVALID VALUE     @L7A 02600000
         AIF   ('&MF(1)' NE 'E').ERLVL2 CHECK ONLY WITH MF=E       @L7A 02650000
.ERLVL2R ANOP                                                      @L7A 02700000
.NORLVLC ANOP                                                      @L7A 02750000
.ERLVL1R ANOP                                                      @L7A 02800000
.******* IF RELEASE IS 1.9 OR GREATER MUST USE RACROUTE            @PAA 02808300
         AIF   ('&RELEASE(1)' EQ '').NOERR01                       @PAA 02816600
         AIF   ('&RELEASE(1)'(1,3) GE '1.9' AND NOT &ICHRAC).ERR08 @PAA 02824900
.NOERR01 ANOP                                                      @PAA 02833200
.ERTN08  ANOP                                                      @PAA 02841500
.******* CHECK SPECIFIED PARAMETERS AGAINST RACF LEVEL             @L7A 02850000
         AIF   ('&RELEASE' GE '1.7').ERLVLAR                       @L8A 02900000
         AIF   (T'&LOC NE 'O').ERLVLA                              @L7A 02950000
.ERLVLAR ANOP                                                      @L7A 03000000
.******* VALIDATE MF PARAMETER                                          03050000
         AIF   ('&MF' NE 'S' AND '&MF' NE 'L' AND '&MF'(1,2) NE        -03100000
               '(E' AND '&MF'(1,2) NE '(M').ERR06                  @L1A 03150000
.ERTN06  ANOP                                                           03200000
.******* VALIDATE ENVIR PARAMETER                                       03250000
         AIF   ('&ENVIR' EQ '').ERTN05                                  03300000
         AIF   ('&ENVIR' NE 'CREATE' AND '&ENVIR' NE 'DELETE').ERR05    03350000
.ERTN05  ANOP                                                           03400000
.******* VALIDATE OWNER PARAMETER                                       03450000
         AIF   ('&OWNER' EQ '').ERTN04                                  03500000
         AIF   ('&OWNER' NE 'NO' AND '&OWNER' NE 'YES').ERR04           03550000
.ERTN04  ANOP                                                           03600000
.******* VALIDATE LOC PARAMETER                                    @L7A 03650000
         AIF   ('&LOC' EQ '').ERTN11                               @L7A 03700000
         AIF   ('&LOC' NE 'ANY' AND '&LOC' NE 'ABOVE').ERR11       @L7A 03750000
.ERTN11  ANOP                                                      @L7A 03800000
.**      LOC ALLOWED ONLY IF RACLIST INVOKED FROM RACROUTE         @L7A 03850000
         AIF   ('&LOC' NE '' AND NOT &ICHRAC).ERR12                @L7A 03900000
.ERTN12  ANOP                                                      @L7A 03950000
.******* VALIDATE FILTER PARAMETER                                 @L1A 03952300
         AIF   ('&FILTER' EQ '').ERTN15                            @L1A 03954600
.**      RELEASE MUST BE 1.9 OR GREATER TO SPECIFY FILTER          @L1A 03956900
         AIF   ('&RELEASE(1)' EQ '').ERR13                         @P4A 03958400
         AIF   ('&RELEASE(1)'(1,3) LT '1.9').ERR13                 @P4C 03959900
.ERTN13  ANOP                                                      @L1A 03961500
.**      MUST BE CALLED BY RACROUTE MACRO TO SPECIFY FILTER        @L1A 03963800
         AIF   (NOT &ICHRAC).ERR14                                 @L1A 03966100
.ERTN14  ANOP                                                      @L1A 03968400
.**      LIST KEYWORD MUST NOT BE SPECIFIED CONCURRENTLY WITH FILTER    03970700
         AIF   ('&LIST' NE '').ERR15                               @L1A 03973000
.ERTN15  ANOP                                                      @L1A 03975300
.******* VALIDATE GLOBAL PARAMETER                                 @L3A 03975400
         AIF   ('&GLOBAL' EQ '').ERTN22                            @L3A 03975500
         AIF   ('&GLOBAL' NE 'YES' AND '&GLOBAL' NE 'NO' ).ERR19   @L3A 03975600
.ERTN19  ANOP                                                      @L3A 03975700
         AIF   ('&GLOBAL' NE '' AND NOT &ICHRAC ).ERR20            @L3A 03975800
.ERTN20  ANOP                                                      @L3A 03975900
.****************************************************              @L3A 03976000
.*  VALIDATE THAT FILTER OR LIST ARE NOT SPECIFIED                 @L3A 03976100
.*  IF GLOBAL=YES                                                  @L3A 03976200
.****************************************************              @L3A 03976300
         AIF   ('&GLOBAL' EQ 'NO').ERTN22                          @L3A 03976400
         AIF   ('&FILTER' NE '').ERR21                             @L3A 03976500
.ERTN21  ANOP                                                      @L3A 03976600
         AIF   ('&LIST' NE '').ERR22                               @L3A 03977000
.ERTN22  ANOP                                                      @L3A 03977200
         AIF   ('&MF'(1,2) NE '(M').ERTN17                         @L1A 03977600
.******* MF=M ONLY ALLOWED IF RACLIST INVOKED FROM RACROUTE AND RELEASE 03979900
.******* IS GREATER OR EQUAL TO 1.9                                @L1A 03982200
.**      TEST FOR SUPPORTING RELEASE                               @L1A 03984500
         AIF   ('&RELEASE(1)' EQ '').ERR16                         @P4A 03986000
         AIF   ('&RELEASE(1)'(1,3) LT '1.9').ERR16                 @P4C 03987500
.ERTN16  ANOP                                                      @L1A 03989100
.**      TEST FOR RACROUTE INVOKATION                              @L1A 03991400
         AIF   (NOT &ICHRAC).ERR17                                 @L1A 03993700
.ERTN17  ANOP                                                      @L1A 03996000
.* --------------------------------------------------------------- @P3A 03996100
.* VALIDATE SUBPOOL PARAMETER                                      @P3A 03996200
.* IF SUBPOOL IS SPECIFIED AND IS NOT IN THE REGISTER FORMAT       @P3A 03996300
.* THEN WE VERIFY THAT THE LITERAL IS A NUMERAL AND IS LESS        @P3A 03996400
.* THAN 255.                                                       @P3A 03996500
.* IF ERROR IS FOUND IN FIRST SUBPOOL STRING, THE SECOND STRING    @P3A 03996600
.* IS NOT LOOKED INTO.                                             @P3A 03996700
.* --------------------------------------------------------------- @P3A 03996800
         AIF   ('&SUBPOOL' EQ '').NOSPCHK                          @P3A 03996900
         AIF   ('&SUBPOOL(1)' EQ '').ERR18                         @P3A 03997000
         AIF   ('&SUBPOOL(1)'(1,1) EQ '(').SP2CHK                  @P3A 03997100
         AIF   (T'&SUBPOOL(1) NE 'N').SP2CHK                       @P3A 03997200
         AIF   ('&SUBPOOL(1)' GT '255').ERR18                      @P3A 03997300
.SP2CHK  ANOP                                                      @P3A 03997400
         AIF   ('&SUBPOOL(2)' EQ '').NOSPCHK                       @P3A 03997500
         AIF   ('&SUBPOOL(2)'(1,1) EQ '(').NOSPCHK                 @P3A 03997600
         AIF   (T'&SUBPOOL(2) NE 'N').NOSPCHK                      @P3A 03997700
         AIF   ('&SUBPOOL(2)' GT '255').ERR18                      @P3A 03997800
.ERTN18  ANOP                                                      @P3A 03997900
.NOSPCHK ANOP                                                      @P3A 03998000
.*                                                                 @P3A 03998100
.******* REGISTER SPECIFICATION OF ADDR TYPE PARAMETERS NOT ALLOWED     04000000
.******* WITH LIST FORM                                                 04050000
         AIF   ('&MF' NE 'L').RLST070                                   04100000
         AIF   ('&CLASS' EQ '').RLST010                                 04150000
         AIF   ('&CLASS'(1,1) EQ '(').ERR07                             04200000
.RLST010 ANOP                                                           04250000
         AIF   ('&LIST' EQ '').RLST020                                  04300000
         AIF   ('&LIST'(1,1) EQ '(').ERR07                              04350000
.RLST020 ANOP                                                           04400000
         AIF   ('&ACEE' EQ '').RLST030                                  04450000
         AIF   ('&ACEE'(1,1) EQ '(').ERR07                              04500000
.RLST030 ANOP                                                           04550000
         AIF   ('&INSTLN(1)' NE '&INSTLN').ERR07                        04600000
.RLST040 ANOP                                                           04650000
         AIF   ('&APPL' EQ '').RLST050                                  04700000
         AIF   ('&APPL'(1,1) EQ '(').ERR07                              04750000
.RLST050 ANOP                                                           04800000
         AIF   ('&SUBPOOL(1)' EQ '').RLST060                            04850000
         AIF   ('&SUBPOOL(1)'(1,1) EQ '(').ERR07                        04900000
.RLST060 ANOP                                                           04950000
         AIF   ('&SUBPOOL(2)' EQ '').RLST061                            05000000
         AIF   ('&SUBPOOL(2)'(1,1) EQ '(').ERR07                        05050000
.RLST061 ANOP                                                           05100000
         AIF   ('&FILTER' EQ '').RLST065                           @L1A 05112500
         AIF   ('&FILTER'(1,1) EQ '(').ERR07                       @L1A 05125000
.RLST065 ANOP                                                      @L1A 05137500
.ERTN07  ANOP                                                           05150000
.RLST070 ANOP                                                           05200000
.******* PRODUCE LIST FORM IF REQUESTED                                 05250000
         AIF   ('&MF' NE 'L').RLST430                                   05300000
&SYMBOL  DS    0F                                                       05350000
.RLST100 ANOP                                                           05400000
ICH0&SYSNDX EQU *                                                       05450000
.******* PRODUCE BYTE INFORMATION                                       05500000
         AIF   ('&SUBPOOL(1)' EQ '').RLST110                            05550000
         AIF   ('&SUBPOOL(1)'(1,1) EQ '(').RLST110                      05600000
&ICHL    DC    AL1(&SUBPOOL(1))        INDEX SUBPOOL NUMBER        @T1A 05650000
         AGO   .RLST115                                                 05700000
.RLST110 ANOP                                                           05750000
&ICHL    DC    AL1(255)                INDEX SUBPOOL NUMBER        @T1A 05800000
.RLST115 ANOP                                                           05850000
         AIF   ('&SUBPOOL(2)' EQ '').RLST120                            05900000
         AIF   ('&SUBPOOL(2)'(1,1) EQ '(').RLST120                      05950000
         DC    AL1(&SUBPOOL(2)) -      PROFILE SUBPOOL NUMBER           06000000
         AGO   .RLST125                                                 06050000
.RLST120 ANOP                                                           06100000
         DC    AL1(255)         -      PROFILE SUBPOOL NUMBER           06150000
.RLST125 ANOP                                                           06200000
         DC    AL1(2)           -      RACLIST ENTRY CODE               06250000
.******* PRODUCE FLAG BYTE 1 FROM ENVIR/OWNER/LOC PARAMETER        @L7A 06300000
&BIT0    SETA  0                       0 AND 1 INDICATE CREATE    @L4C  06350000
&BIT1    SETA  0                       '00' OR DELETE '01'        @L4C  06400000
&BIT2    SETA  0                       ADD OWNER W/ALTER          @L4C  06450000
&BIT3    SETA  0                       LOC = ABOVE SPECIFIED      @L4C  06462500
&BIT4    SETA  0                       1.8|1.8.1 SPECIFIED (WARNING    X06468300
                                        MODE SUPPORTED)           @P8C  06474100
&BIT5    SETA  0                       1.9 SPECIFIED (FILTER           X06479900
                                        SUPPORTED)                @P8C  06485700
&BIT6    SETA  0                       1.9.2 SPECIFIED            @L4A  06491600
.****** &BIT7 SHOULD NEVER BE TOUCHED, IT IS USED BY AN APAR      @L4A  06495700
         AIF   ('&ENVIR' EQ '' OR '&ENVIR' EQ 'CREATE').RLST130         06500000
         AIF   ('&ENVIR' NE 'DELETE').RLST130                           06550000
&BIT0    SETA  1                                                        06600000
.RLST130 ANOP                                                           06650000
         AIF   ('&OWNER' EQ '' OR '&OWNER' EQ 'NO').RLST131             06700000
         AIF   ('&OWNER' NE 'YES').RLST131                              06750000
&BIT2    SETA  1                                                        06800000
.RLST131 ANOP                                                           06850000
         AIF   ('&LOC' EQ '' OR '&LOC' EQ 'ANY').RLST132           @L7A 06900000
         AIF   ('&LOC' NE 'ABOVE').RLST132                         @L7A 06950000
&BIT3    SETA  1                                                   @L7A 07000000
.RLST132 ANOP                                                      @L7A 07050000
         AIF   ('&RELEASE(1)' EQ '').RLST135                       @02A 07055500
