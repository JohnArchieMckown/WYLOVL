.********************************************************************   00188400
.***AC.PLV01  PROCESS PLISTVER                                      *   00188500
.********************************************************************   00188600
&XPLISTVER      SETA  0                                                 00188700
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00188800
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00188900
&XPLISTVER      SETA  2                                                 00189000
.ZPLV0          ANOP  ,                                                 00189100
.*                                                                      00189200
                AIF   ('&LESSCONNACTION' EQ '').ZPLV1E                  00189300
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00189400
&XPLISTVER      SETA  1                                                 00189500
.ZPLV1E         ANOP  ,                                                 00189600
.*                                                                      00189700
                AIF   ('&LESSCONNACTION' EQ '').ZPLV2E                  00189800
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00189900
&XPLISTVER      SETA  1                                                 00190000
.ZPLV2E         ANOP  ,                                                 00190100
.*                                                                      00190200
                AIF   ('&POPULATECF' EQ '').ZPLV3E                      00190300
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00190400
&XPLISTVER      SETA  2                                                 00190500
.ZPLV3E         ANOP  ,                                                 00190600
.*                                                                      00190700
                AIF   ('&POPULATECF' EQ '').ZPLV4E                      00190800
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00190900
&XPLISTVER      SETA  2                                                 00191000
.ZPLV4E         ANOP  ,                                                 00191100
.*                                                                      00191200
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00191300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00191400
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00191500
.*-------------------------------------------------------------------   00191600
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00191700
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00191800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00191900
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00192000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00192100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00192200
                AGO   .ZPVCK2                                           00192300
.ZPVCK3         ANOP  ,                                                 00192400
.*-------------------------------------------------------------------   00192500
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00192600
