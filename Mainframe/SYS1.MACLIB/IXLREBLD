.*******************************************************************    00577700
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00577800
.*******************************************************************    00577900
                      ACTR  10000                                       00578000
.*                                                                      00578100
                      LCLA  &ZLC           COLUMN POSITION              00578200
                      LCLA  &ZOPX          LIST TABLE INDEX             00578300
                      LCLC  &ZLL           LABEL                        00578400
                      LCLC  &ZLOP          OPERATION                    00578500
                      LCLC  &ZLV           VALUE                        00578600
                      LCLC  &ZLD           DESCRIPTION                  00578700
.*                                                                      00578800
&ZOPX                 SETA  1                                           00578900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00579000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00579100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00579200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00579300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00579400
                      AIF   (K'&ZLD LE 0).ZL5                           00579500
&ZLC                  SETA  35             COMMENT COLUMN               00579600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00579700
&ZLC                  SETA  33             COMMMENT COLUMN              00579800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00579900
&ZLV                  SETC  '&ZLV'.' '                                  00580000
                      AGO   .ZL3                                        00580100
.*                                                                      00580200
.ZL4                  ANOP  ,                                           00580300
&ZLV                  SETC  '&ZLV'.' '                                  00580400
.ZL5                  ANOP  ,                                           00580500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00580600
.*                                                                      00580700
&ZLL     &ZLOP &ZLV                                                     00580800
.*                                                                      00580900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00581000
                      AGO   .ZL2           CONTINUE LOOPING             00581100
.*                                                                      00581200
.ZLX                  ANOP  ,                                           00581300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00581400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00581500
         L     14,16(0,0)              GET CVT ADDRESS                  00581600
         L     14,772(14,0)            GET SFT ADDRESS                  00581700
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00581800
         LA    15,47                   GET EX FOR SERVICE ROUTINE       00581900
         OR    14,15                   GET PC NUMBER                    00582000
         PC    0(14)                                                    00582100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00582200
.ZLINKX               ANOP  ,                                           00582300
.*******************************************************************    00582400
.***AP.X01  FORMAT GENERATED CODE                                       00582500
