.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00525200
&ZPLLEN               SETA 96                                           00525300
.*                                                                      00525400
.******************************************************************     00525500
.***AI.L01 IXLREBLD FMT=LIST FILL IN PARAMETER AREA                     00525600
.******************************************************************     00525700
                      ACTR  10000                                       00525800
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00525900
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00526000
&ZCVAL                SETC  'LAE'          XAX MODE                     00526100
.ZIMFXA               ANOP ,                                            00526200
.*                                                                      00526300
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00526400
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00526500
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00526600
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00526700
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00526800
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00526900
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00527000
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00527100
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00527200
&ZOPN                 SETA  &ZOPN+1                                     00527300
&ZOPL(&ZOPN)          SETC  ' '                                         00527400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00527500
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00527600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00527700
                      AGO   .ZIMFNX                                     00527800
.*                                                                      00527900
.ZIMFN2               ANOP ,                                            00528000
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00528100
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00528200
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00528300
&ZOPN                 SETA  &ZOPN+1                                     00528400
&ZOPL(&ZOPN)          SETC  ' '                                         00528500
&ZOPO(&ZOPN)          SETC  'LAE'                                       00528600
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00528700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00528800
                      AGO   .ZIMFNX                                     00528900
.ZIMFNX1              ANOP ,                                            00529000
&ZOPN                 SETA  &ZOPN+1                                     00529100
&ZOPL(&ZOPN)          SETC  ' '                                         00529200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00529300
