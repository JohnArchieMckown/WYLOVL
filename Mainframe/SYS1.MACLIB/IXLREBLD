.***AO.X02  RETURN CODE PROCESSING                                      00571200
.*******************************************************************    00571300
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00571400
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00571500
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00571600
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00571700
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00571800
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00571900
&ZOPN                 SETA  &ZOPN+1                                     00572000
&ZOPL(&ZOPN)          SETC  ' '                                         00572100
&ZOPO(&ZOPN)          SETC  'ST'                                        00572200
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00572300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00572400
                      AGO   .ZMRETX                                     00572500
.*                                                                      00572600
.ZMRET2               ANOP  ,                                           00572700
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00572800
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00572900
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00573000
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00573100
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00573200
&ZOPN                 SETA  &ZOPN+1                                     00573300
&ZOPL(&ZOPN)          SETC  ' '                                         00573400
&ZOPO(&ZOPN)          SETC  'LR'                                        00573500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00573600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00573700
.ZMRETX               ANOP  ,                                           00573800
.*******************************************************************    00573900
.***AO.X03  REASON CODE PROCESSING                                      00574000
.*******************************************************************    00574100
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00574200
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00574300
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00574400
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00574500
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00574600
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00574700
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00574800
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00574900
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00575000
&ZOPN                 SETA  &ZOPN+1                                     00575100
