.***AO.X04  LABEL FOR MF=L PROCESSING                                   01539500
.*******************************************************************    01539600
.ZMFLX                ANOP  ,              MF=L PROCESSING              01539700
.*******************************************************************    01539800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               01539900
.*******************************************************************    01540000
                      ACTR  10000                                       01540100
.*                                                                      01540200
                      LCLA  &ZLC           COLUMN POSITION              01540300
                      LCLA  &ZOPX          LIST TABLE INDEX             01540400
                      LCLC  &ZLL           LABEL                        01540500
                      LCLC  &ZLOP          OPERATION                    01540600
                      LCLC  &ZLV           VALUE                        01540700
                      LCLC  &ZLD           DESCRIPTION                  01540800
.*                                                                      01540900
&ZOPX                 SETA  1                                           01541000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            01541100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01541200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01541300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01541400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01541500
                      AIF   (K'&ZLD LE 0).ZL5                           01541600
&ZLC                  SETA  35             COMMENT COLUMN               01541700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    01541800
&ZLC                  SETA  33             COMMMENT COLUMN              01541900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   01542000
&ZLV                  SETC  '&ZLV'.' '                                  01542100
                      AGO   .ZL3                                        01542200
.*                                                                      01542300
.ZL4                  ANOP  ,                                           01542400
&ZLV                  SETC  '&ZLV'.' '                                  01542500
.ZL5                  ANOP  ,                                           01542600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01542700
.*                                                                      01542800
&ZLL     &ZLOP &ZLV                                                     01542900
.*                                                                      01543000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01543100
                      AGO   .ZL2           CONTINUE LOOPING             01543200
.*                                                                      01543300
.ZLX                  ANOP  ,                                           01543400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01543500
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01543600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01543700
         AIF   ('&REQUEST' EQ 'QUERY').FPLINK                      @L1A 01543800
         AIF   ('&REQUEST' NE 'CALL').NOTFP                        @L1A 01543900
         AIF   ('&FASTPATH' EQ 'YES').FPLINK                       @L1A 01544000
.NOTFP   ANOP                          Not CALL FASTPATH=YES       @L1A 01544100
         AIF   ('&LINKAGE' NE 'BRANCH').PCLINK                     @L1A 01544200
         L     15,16(0,0)              GET CVT ADDRESS             @L1A 01544300
         L     15,140(15,0)            GET ECVT ADDRESS            @L1A 01544400
         L     15,228(15,0)                                        @L1A 01544500
         AIF   ('&LINKTEST(1)' NE 'YES').DOBASSM0                  @L1A 01544600
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 01544700
         BZ    C&SYSNDX                NO, DO NOT CALL             @L1A 01544800
         L     15,32(15,0)             GET SERVICE ROUTINE ADDR    @L1A 01544900
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 01545000
         BNZ   A&SYSNDX                YES, CALL IT                @L1A 01545100
C&SYSNDX DS    0H                      FUNCTION NOT AVAILABLE      @L1A 01545200
         LA    15,12                   INDICATE FUNCTION NOT AVAIL @L1A 01545300
         LA    0,X'0C01'               REASON CODE                 @L1A 01545400
         B     B&SYSNDX                                            @L1A 01545500
A&SYSNDX DS    0H                      NEED TO CALL                @L1A 01545600
         BASSM 14,15                   CALL ROUTINE                @L1A 01545700
B&SYSNDX DS    0H                                                  @L1A 01545800
         AGO   .ENDLINK                                            @L1A 01545900
.DOBASSM0 ANOP                                                     @L1A 01546000
         L     15,32(15,0)             GET SERVICE ROUTINE ADDR    @L1A 01546100
         BASSM 14,15                   CALL ROUTINE                @L1A 01546200
         AGO   .ENDLINK                                            @L1A 01546300
.PCLINK  ANOP                                                      @L1A 01546400
         L     14,16(0,0)              GET CVT ADDRESS             @L1A 01546500
         AIF   ('&LINKTEST(1)' NE 'YES').DOPC                      @L1A 01546600
         L     15,140(14,0)            GET ECVT ADDRESS            @L1A 01546700
         L     15,228(15,0)                                        @L1A 01546800
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 01546900
         BZ    C&SYSNDX                NO, NOT AVAILABLE           @L1A 01547000
         AIF   ('&SYSASCE' EQ 'P').ASCPRIM                         @L1A 01547100
         LAE   15,0(15,0)              SET AR for compare          @L1A 01547200
.ASCPRIM ANOP                                                      @L1A 01547300
         CLI   24(15),0                IS PC ROUTINE AVAILABLE     @L1A 01547400
         BNZ   A&SYSNDX                YES, CALL ROUTINE           @L1A 01547500
C&SYSNDX DS    0H                      NOT AVAILABLE               @L1A 01547600
         LA    15,12                   INDICATE FUNCTION NOT AVAIL @L1A 01547700
         LA    0,X'0C01'               REASON CODE                 @L1A 01547800
         B     B&SYSNDX                                            @L1A 01547900
A&SYSNDX DS    0H                      NEED TO CALL                @L1A 01548000
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 01548100
         L     14,368(14,0)            GET LX/EX FOR SERVICE RTN   @L1A 01548200
         PC    0(14)                   CALL ROUTINE                @L1A 01548300
B&SYSNDX DS    0H                                                  @L1A 01548400
         AGO   .ENDLINK                                            @L1A 01548500
.DOPC    ANOP                                                      @L1A 01548600
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 01548700
         L     14,368(14,0)            GET LX/EX FOR SERVICE RTN   @L1A 01548800
         PC    0(14)                                               @L1A 01548900
         AGO   .ENDLINK                                            @L1A 01549000
.FPLINK  ANOP                                                      @L1A 01549100
         L     15,16(0,0)              GET CVT ADDRESS             @L1A 01549200
         L     15,140(15,0)            GET ECVT ADDRESS            @L1A 01549300
         L     15,228(15,0)                                        @L1A 01549400
         AIF   ('&LINKTEST(1)' NE 'YES').DOBASSM                   @L1A 01549500
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 01549600
         BZ    C&SYSNDX                NO, DO NOT CALL             @L1A 01549700
         L     15,28(15,0)             GET EXIT FASTPATH CALL ADDR @L1A 01549800
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 01549900
         BNZ   A&SYSNDX                YES, CALL IT                @L1A 01550000
C&SYSNDX DS    0H                      NO NEED TO CALL             @L1A 01550100
         LA    15,12                   INDICATE FUNCTION NOT AVAIL @L1A 01550200
         LA    0,X'0C01'               REASON CODE                 @L1A 01550300
         B     B&SYSNDX                                            @L1A 01550400
A&SYSNDX DS    0H                      NEED TO CALL                @L1A 01550500
         BASSM 14,15                   CALL ROUTINE                @L1A 01550600
B&SYSNDX DS    0H                                                  @L1A 01550700
         AGO   .ENDLINK                                            @L1A 01550800
.DOBASSM ANOP                                                      @L1A 01550900
         L     15,28(15,0)             GET EXIT FASTPATH CALL ADDR @L1A 01551000
         BASSM 14,15                   CALL ROUTINE                @L1A 01551100
.ENDLINK ANOP                                                      @L1A 01551200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01551300
.ZLINKX               ANOP  ,                                           01551400
