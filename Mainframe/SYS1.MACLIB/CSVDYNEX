                  AIF   (&ZMACRET NE 0).ZLX2    ERROR DETECTED          01266300
.*******************************************************************    01266400
.***AE.01  START OF CSVDYNEX MACRO CODE                                 01266500
.*******************************************************************    01266600
                  LCLC  &M0M0001                 NAME                   01266700
&M0M0001          SETC  '&XLABEL'                SET NAME               01266800
                  AIF   (K'&XLABEL NE 0).XLAB2                          01266900
&M0M0001          SETC  'M00M&SYSNDX'            DEFAULT NAME           01267000
.*                                                                      01267100
.XLAB2            ANOP  ,                                               01267200
                  AIF   ('&XMF' EQ 'L').XLAB3                           01267300
&M0M0001 DS    0H                             CSVDYNEX-0                01267400
.XLAB3            ANOP  ,                                               01267500
.*******************************************************************    01267600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              01267700
.*******************************************************************    01267800
              PUSH  PRINT                                               01267900
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      01268000
                  PRINT OFF         ++ PR01                             01268100
                  AGO   .ZPRNTXX                                        01268200
.ZPRINTX          PRINT ON                                              01268300
.ZPRNTXX          ANOP  ,                                               01268400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01268500
         AIF   ('&XMFSCK' NE 'NOCHECK').NEXTT_OK                   @L2A 01268600
         AIF   ('&NEXTTOKEN' EQ '').NEXTT_OK                       @L2A 01268700
         AIF   ('&REQUEST' EQ '').NEXTT_OK                         @L2A 01268800
         AIF   ('&REQUEST' EQ 'RECOVER').NEXTT_OK                  @L2A 01268900
&ZXNEXTTOKEN SETC 'INOUT'                                          @L2A 01269000
.NEXTT_OK ANOP                                                     @L2A 01269100
         AIF   ('&ANYKEY' NE 'YES').ANYKEYOK                       @01A 01269200
         AIF   ('&REENTRANT' EQ 'REQ').RENTOK                      @01A 01269300
&ZMS     SETC  'ANYKEY=YES REQUIRES REENTRANT=REQ'                 @01A 01269400
         MNOTE 8,'&ZMS'                                            @01A 01269500
         MEXIT                                                     @01A 01269600
.RENTOK  ANOP                                                      @01A 01269700
         AIF   ('&FASTPATH' NE 'YES').ANYKEYOK                     @01A 01269800
         AIF   ('&ABENDCONSEC' NE 'YES').ANYKEYOK                  @01A 01269900
&ZMS     SETC  'ANYKEY=YES IS NOT VALID WITH FASTPATH=YES AND'     @01A 01270000
&ZMS     SETC  '&ZMS. ABENDCONSEC=YES'                             @01A 01270100
         MNOTE 8,'&ZMS'                                            @01A 01270200
         MEXIT                                                     @01A 01270300
.ANYKEYOK ANOP                                                     @01A 01270400
         AIF   ('&REQUEST' NE 'ACTIVATE').NOTACTIVATE              @L1A 01270500
&ZMS     SETC  'REQUEST=ACTIVATE MAY NOT BE SPECIFIED'             @L1A 01270600
         MNOTE 8,'&ZMS'                                            @L1A 01270700
         MEXIT                                                     @L1A 01270800
.NOTACTIVATE ANOP                                                  @L1A 01270900
         AIF   ('&REQUEST' NE 'PROCESSDP').NOTPROCESSDP            @L1A 01271000
&ZMS     SETC  'REQUEST=PROCESSDP MAY NOT BE SPECIFIED'            @L1A 01271100
         MNOTE 8,'&ZMS'                                            @L1A 01271200
         MEXIT                                                     @L1A 01271300
.NOTPROCESSDP ANOP                                                 @L1A 01271400
         AIF   ('&CMDINFO' EQ '').NOCMDINFO                        @L1A 01271500
&ZMS     SETC  'CMDINFO MAY NOT BE SPECIFIED'                      @L1A 01271600
         MNOTE 8,'&ZMS'                                            @L1A 01271700
         MEXIT                                                     @L1A 01271800
.NOCMDINFO ANOP                                                    @L1A 01271900
         AIF   ('&LINKAGE' EQ '').NOLINKAGE                        @L1A 01272000
         AIF   ('&LINKAGE(1)' NE 'INTERNAL').NOLINKAGE1            @L1A 01272100
&ZMS     SETC  'LINKAGE=INTERNAL MAY NOT BE SPECIFIED'             @L1A 01272200
         MNOTE 8,'&ZMS'                                            @L1A 01272300
         MEXIT                                                     @L1A 01272400
.NOLINKAGE1 ANOP                                                   @L1A 01272500
         AIF   ('&LINKAGE(1)' NE 'BRANCH').NOLINKAGE2              @L1A 01272600
         AIF   ('&SYSASCE' EQ 'P').NOLINKAGE2                      @L1A 01272700
&ZMS     SETC  'BRANCH MAY NOT BE SPECIFIED IN AR MODE'            @L1A 01272800
         MNOTE 8,'&ZMS'                                            @L1A 01272900
         MEXIT                                                     @L1A 01273000
.NOLINKAGE2 ANOP                                                   @L1A 01273100
         AIF   ('&XMF' NE 'M').NOLINKAGE                           @L1A 01273200
&ZMS     SETC  'LINKAGE MAY NOT BE SPECIFIED WITH MF=M'            @L1A 01273300
         MNOTE 8,'&ZMS'                                            @L1A 01273400
         MEXIT                                                     @L1A 01273500
.NOLINKAGE ANOP                                                    @L1A 01273600
         AIF   ('&LINKTEST' EQ '').NOLINKTEST                      @L1A 01273700
         AIF   ('&XMF' NE 'M').NOLINKTEST                          @L1A 01273800
&ZMS     SETC  'LINKTEST MAY NOT BE SPECIFIED WITH MF=M'           @L1A 01273900
         MNOTE 8,'&ZMS'                                            @L1A 01274000
         MEXIT                                                     @L1A 01274100
.NOLINKTEST ANOP                                                   @L1A 01274200
         AIF   ('&FASTPATH' EQ '').NOFASTPATH                      @L1A 01274300
         AIF   ('&REQUEST(1)' NE 'CALL').NOFASTPATH                @L1A 01274400
         AIF   ('&XMF' NE 'M').NOFASTPATH                          @L1A 01274500
&ZMS  SETC 'FASTPATH MAY NOT BE SPECIFIED WITH MF=M FOR REQUEST=CALL'   01274600
         MNOTE 8,'&ZMS'                                            @L1A 01274700
         MEXIT                                                     @L1A 01274800
.NOFASTPATH ANOP                                                   @L1A 01274900
         AIF   ('&REQUEST' NE '').ISREQUEST                        @L1A 01275000
         AIF   ('&XMF' NE 'E').ISREQUEST                           @L1A 01275100
&ZMS     SETC  'REQUEST IS REQUIRED WITH MF=E'                     @L1A 01275200
         MNOTE 8,'&ZMS'                                            @L1A 01275300
         MEXIT                                                     @L1A 01275400
.ISREQUEST ANOP                                                    @L1A 01275500
         AIF   ('&XMF' EQ 'M').AROK                                @L1A 01275600
         AIF   ('&SYSASCE' EQ 'P').AROK                            @L1A 01275700
         AIF   ('&REQUEST' EQ 'QUERY').ARBAD                       @L1A 01275800
         AIF   ('&REQUEST' NE 'CALL').AROK                         @L1A 01275900
         AIF   ('&FASTPATH' NE 'YES').AROK                         @L1A 01276000
.ARBAD   ANOP                                                      @L1A 01276100
&ZMS     SETC  'REQUEST=CALL WITH FASTPATH=YES AND REQUEST=QUERY'  @L1A 01276200
