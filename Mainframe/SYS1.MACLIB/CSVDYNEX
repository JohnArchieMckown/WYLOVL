.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00287900
.ZG306            ANOP  ,                                               00288000
&ZTLC             SETC 'S'                                              00288100
                  AIF   ('&MF(1)' NE '&ZTLC').ZG307                     00288200
&XMF              SETC  'S'         SET KEYWORD                         00288300
&XMFSCK           SETC  'COMPLETE'  SET XMFSCK                          00288400
                  AIF   (N'&MF LE 1).ZG306A     CK NUM                  00288500
&ZMS              SETC  '"MF='      *KPOSA* MNOTE                       00288600
&ZMS              SETC  '&ZMS.&MF"'                                     00288700
&ZMS              SETC  '&ZMS "MF"'                                     00288800
                  MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'             00288900
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00289000
.ZG306A           ANOP  ,                                               00289100
.*-------------------------------------------------------------------   00289200
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00289300
.ZG307            ANOP  ,                                               00289400
&ZTLC             SETC 'L'                                              00289500
                  AIF   ('&MF(1)' NE '&ZTLC').ZG314                     00289600
&XMF              SETC  'L'         SET KEYWORD                         00289700
                  AIF   (N'&MF LE 3).ZG307A     CK NUM                  00289800
&ZMS              SETC  '"MF='      *KPOSA* MNOTE                       00289900
&ZMS              SETC  '&ZMS.&MF"'                                     00290000
&ZMS              SETC  '&ZMS "MF"'                                     00290100
                  MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'            00290200
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00290300
.ZG307A           ANOP  ,                                               00290400
.*-------------------------------------------------------------------   00290500
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00290600
                  AIF   (N'&SYSLIST NE 0).ZG307B  POSITIONALS?          00290700
                  AIF   ('&REQUEST' NE '').ZG307B  KEYS?                00290800
                  AIF   ('&EXITNAME' NE '').ZG307B  KEYS?               00290900
                  AIF   ('&AMODE' NE '').ZG307B  KEYS?                  00291000
                  AIF   ('&REENTRANT' NE '').ZG307B  KEYS?              00291100
