.***AC.PLV01  PROCESS PLISTVER                                      *   00281300
.********************************************************************   00281400
&XPLISTVER      SETA  0                                                 00281500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00281600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00281700
&XPLISTVER      SETA  0                                                 00281800
.ZPLV0          ANOP  ,                                                 00281900
.*                                                                      00282000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00282100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00282200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00282300
.*-------------------------------------------------------------------   00282400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00282500
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00282600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00282700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00282800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00282900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00283000
                AGO   .ZPVCK2                                           00283100
.ZPVCK3         ANOP  ,                                                 00283200
.*-------------------------------------------------------------------   00283300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00283400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00283500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00283600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00283700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00283800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00283900
                AGO   .ZPVCKE                                           00284000
.ZPVCK2         ANOP  ,                                                 00284100
.*-------------------------------------------------------------------   00284200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00284300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00284400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00284500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00284600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00284700
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00284800
.ZPVCK6         ANOP  ,                                                 00284900
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00285000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00285100
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00285200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00285300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00285400
&XPLISTVER      SETA  0                                                 00285500
                AGO   .ZPVCKE                                           00285600
.*-------------------------------------------------------------------   00285700
.ZPVCK4A        ANOP  ,                                                 00285800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00285900
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00286000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00286100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00286200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00286300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00286400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00286500
                AGO   .ZPVCK5                                           00286600
.ZPVCK4         ANOP  ,                                                 00286700
&XPLISTVER      SETA  &PLISTVER(1)                                      00286800
.ZPVCK5         ANOP  ,                                                 00286900
.ZPVCKE         ANOP  ,                                                 00287000
.********************************************************************   00287100
.***AG.MF01 PROCESS MF MACRO FORMS                                      00287200
.********************************************************************   00287300
                  ACTR  10000                                           00287400
