.***AI.L01 CSVDYNEX FMT=LIST FILL IN PARAMETER AREA                     01366400
.******************************************************************     01366500
                      ACTR  10000                                       01366600
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01366700
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01366800
&ZCVAL                SETC  'LAE'          XAX MODE                     01366900
.ZIMFXA               ANOP ,                                            01367000
.*                                                                      01367100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01367200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01367300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01367400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01367500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01367600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01367700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01367800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01367900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01368000
&ZOPN                 SETA  &ZOPN+1                                     01368100
&ZOPL(&ZOPN)          SETC  ' '                                         01368200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01368300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01368400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01368500
                      AGO   .ZIMFNX                                     01368600
.*                                                                      01368700
.ZIMFN2               ANOP ,                                            01368800
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01368900
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01369000
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01369100
&ZOPN                 SETA  &ZOPN+1                                     01369200
&ZOPL(&ZOPN)          SETC  ' '                                         01369300
&ZOPO(&ZOPN)          SETC  'LAE'                                       01369400
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01369500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01369600
                      AGO   .ZIMFNX                                     01369700
.ZIMFNX1              ANOP ,                                            01369800
&ZOPN                 SETA  &ZOPN+1                                     01369900
