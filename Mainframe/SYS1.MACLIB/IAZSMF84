SMF84GS  DSECT                                                          02350000
R84CPUSC DS    F            CPU BUSY SAMPLE COUNT                       02360000
R84NPA   DS    F            IATNUC POSTED-ACTIVE COUNT;                 02370000
*                           % OF IATNUC POSTED-ACTIVE = IATNUC POSTED-  02380000
*                           ACTIVE COUNT / CPU BUSY SAMPLE COUNT * 100  02390000
R84APA   DS    F            IATAUX POSTED-ACTIVE COUNT;                 02400000
*                           % OF IATAUX POSTED-ACTIVE = IATAUX POSTED-  02410000
*                           ACTIVE COUNT / CPU BUSY SAMPLE COUNT * 100  02420000
R84NPNA  DS    F            IATNUC POSTED--NOT-ACTIVE COUNT;            02430000
*                           % OF IATNUC POSTED-NOT-ACTIVE = IATNUC      02440000
*                           POSTED-NOT-ACTIVE COUNT / CPU BUSY SAMPLE   02450000
*                           COUNT * 100                                 02460000
R84APNA  DS    F            IATAUX POSTED--NOT-ACTIVE COUNT;            02470000
*                           % OF IATAUX POSTED-NOT-ACTIVE = IATAUX      02480000
*                           POSTED-NOT-ACTIVE COUNT / CPU BUSY SAMPLE   02490000
*                           COUNT * 100                                 02500000
R84NNP   DS    F            IATNUC NOT-POSTED COUNT;                    02510000
*                           % OF IATNUC NOT-POSTED = IATNUC NOT-POSTED  02520000
*                           COUNT / CPU BUSY SAMPLE COUNT * 100         02530000
R84ANP   DS    F            IATAUX NOT-POSTED COUNT;                    02540000
*                           % OF IATAUX NOT-POSTED = IATAUX NOT-POSTED  02550000
*                           COUNT / CPU BUSY SAMPLE COUNT * 100         02560000
R84NNW   DS    F            IATNUC NONSTANDARD-WAIT COUNT;              02570000
*                           % OF IATNUC NONSTANDARD-WAIT = IATNUC NON-  02580000
*                           STANDARD WAIT COUNT / CPU BUSY SAMPLE COUNT 02590000
*                           * 100                                       02600000
R84ANW   DS    F            IATAUX NONSTANDARD-WAIT COUNT;              02610000
*                           % OF IATAUX NONSTANDARD-WAIT = IATAUX NON-  02620000
*                           STANDARD-WAIT COUNT / CPU BUSY SAMPLE COUNT 02630000
*                           * 100                                       02640000
R84NSLLR DS    F            IATNUC SUSPENDED-LOCAL-LOCK-REQUEST COUNT;  02650000
*                           % OF IATNUC SUSPENDED-LOCAL-LOCK-REQUEST =  02660000
*                           IATNUC SUSPENDED-LOCAL-LOCK-REQUEST COUNT / 02670000
*                           CPU BUSY SAMPLE COUNT * 100                 02680000
R84ASLLR DS    F            IATAUX SUSPENDED-LOCAL-LOCK-REQUEST COUNT;  02690000
*                           % OF IATAUX SUSPENDED-LOCAL-LOCK-REQUEST =  02700000
*                           IATAUX SUSPENDED-LOCAL-LOCK-REQUEST COUNT   02710000
*                           / CPU BUSY SAMPLE COUNT * 100               02720000
R84NSO   DS    F            IATNUC SUSPENDED-OTHER COUNT;               02730000
*                           % OF IATNUC SUSPENDED-OTHER = IATNUC        02740000
*                           SUSPENDED-OTHER COUNT / CPU BUSY SAMPLE     02750000
*                           COUNT * 100                                 02760000
R84ASO   DS    F            IATAUX SUSPENDED-OTHER COUNT;               02770000
*                           % OF IATAUX SUSPENDED-OTHER = IATAUX        02780000
*                           SUSPENDED-OTHER COUNT / CPU BUSY SAMPLE     02790000
*                           COUNT * 100                                 02800000
R84GISZ1 EQU   *-R84CPUSC   SIZE OF JMF GENERAL INFO SECTION            02810000
         EJECT                                                          02820000
         AIF   ('&SUBTYPE' EQ '1').SUBTYP1                     #695@MEA 02830000
         AIF   ('&SUBTYPE' EQ '2').SUBTYP2                     #695@MEA 02840000
         AIF   ('&SUBTYPE' EQ '3').SUBTYP3                              02850000
         AIF   ('&SUBTYPE' EQ '4').SUBTYP4                              02860000
         AIF   ('&SUBTYPE' EQ '5').SUBTYP5                              02870000
         AIF   ('&SUBTYPE' EQ '6').SUBTYP6                              02880000
         AIF   ('&SUBTYPE' EQ '7').SUBTYP7                              02890000
         AIF   ('&SUBTYPE' EQ '8').SUBTYP8                              02900000
         AIF   ('&SUBTYPE' EQ '9').SUBTYP9                              02910000
         AIF   ('&SUBTYPE' EQ '10').SUBTY10                        @RWA 02911000
         AIF   ('&SUBTYPE' EQ 'ALL').SUBTYP1                   #695@MEA 02920000
         MEXIT                                                 #695@MEA 02930000
.SUBTYP1 ANOP                                                           02940000
