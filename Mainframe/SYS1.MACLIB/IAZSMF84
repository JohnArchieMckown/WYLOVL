*             SMF 84 HEADER                                       *     01120000
*                                                                 *     01130000
*******************************************************************     01140000
SMF84HDR DSECT                                                          01150000
SMF84PTR EQU   *                                                        01160000
SMF84LEN DS    H            RECORD LENGTH                               01170000
SMF84SEG DS    H            SEGMENT DESCRIPTOR                          01180000
SMF84FLG DS    X            SYSTEM INDICATOR                            01190000
*        EQU   X'80'        RESERVED                           #569@MEA 01200000
SMF84STI EQU   X'40'        SUBTYPE INDICATOR INDICATES WHICH  #0733MEC 01210000
*                            SUBSYSTEM IS BEING USED           #0733MEA 01220000
*        EQU   X'20'        RESERVED                           #569@MEA 01230000
*        EQU   X'10'        RESERVED                           #569@MEA 01240000
*        EQU   X'08'        RESERVED                           #569@MEA 01250000
*        EQU   X'04'        MVS/XA                            #0733@MEC 01260000
*        EQU   X'02'        VS2                                #569@MEA 01270000
*        EQU   X'01'        VS1                                #569@MEA 01280000
SMF84RTY DS    X            RECORD TYPE (84 OR X'54')         #0275@MEC 01290000
SMF84TME DS    PL4          TIME, IN HUNDREDS OF A SECOND WHEN#0275@MEC 01300000
*                           RECORD WAS WRITTEN  (SET BY SMF)  #0733@MEC 01310000
SMF84DTE DS    PL4          DATE RECORD WAS WRITTEN, IN THE FORM        01320000
*                           00YYDDDF WHERE F IS THE SIGN      #0275@MEC 01330000
*                           (SET BY SMF)                      #0733@MEA 01340000
SMF84SID DS    XL4          SYSTEM IDENTIFICATION (SET BY SMF)#0733@MEC 01350000
SMF84SBS DS    H            SUBSYSTEM IDENTIFICATION --                 01360000
*                            X'0005' SIGNIFIES JES3                     01370000
SMF84SGN DS    H            SEGMENT NUMBER                     #569@MEC 01380000
SMF84FL1 DS    X            FLAG BYTE 1 FOR SMF TYPE 84        #569@MEA 01390000
SMF84LST EQU   X'80'        LAST SMF SEGMENT INDICATOR         #569@MEA 01400000
SMF84VER DS    X            SMF84 VERSION NUMBER               @OY34777 01410000
*                           DEFAULT IS '00'                    @OY34777 01413000
SMF84V2  EQU   X'02'        TEMPORARY FIX INDICATOR            @OY34777 01416000
SMF84V3  EQU   X'03'        OS390 2.4.0                            @RJA 01418000
SMF84STY DS    H            RECORD SUBTYPE                              01420000
SMF84TRN DS    H            NUMBER OF TRIPLETS IN THIS RECORD           01430000
*                                                              #569@MED 01440000
SMF84PRS DS    F            OFFSET TO JMF PRODUCT SECTION               01450000
SMF84PRL DS    H            LENGTH OF JMF PRODUCT SECTION               01460000
SMF84PRN DS    H            NUMBER OF JMF PRODUCT SECTIONS    #0733@MEC 01470000
SMF84GNS DS    F            OFFSET TO JMF GENERAL SECTION               01480000
SMF84GNL DS    H            LENGTH OF JMF GENERAL SECTION               01490000
SMF84GNN DS    H            NUMBER OF JMF GENERAL SECTIONS              01500000
SMF84J1O DS    F            OFFSET TO JMF DATA SECTION                  01510000
SMF84J1L DS    H            LENGTH OF JMF DATA SECTION                  01520000
SMF84J1N DS    H            NUMBER OF JMF DATA SECTIONS                 01530000
SMF84SZ1 EQU   *-SMF84LEN   SIZE OF SMF HEADER                          01540000
         SPACE ,                                                        01550000
         AIF   ('&SUBTYPE' EQ 'ALL').PROSEC                             01560000
         AIF   ('&SUBTYPE' EQ '0').PROSEC                               01570000
         AIF   ('&SUBTYPE' EQ '1').PROSEC                               01580000
         AIF   ('&SUBTYPE' EQ '2').PROSEC                               01590000
         AIF   ('&SUBTYPE' EQ '3').PROSEC                               01600000
         AIF   ('&SUBTYPE' EQ '4').PROSEC                               01610000
         AIF   ('&SUBTYPE' EQ '5').PROSEC                               01620000
         AIF   ('&SUBTYPE' EQ '6').PROSEC                               01630000
         AIF   ('&SUBTYPE' EQ '7').PROSEC                               01640000
         AIF   ('&SUBTYPE' EQ '8').PROSEC                               01650000
         AIF   ('&SUBTYPE' EQ '9').PROSEC                               01660000
         AIF   ('&SUBTYPE' EQ '10').PROSEC                         @RWA 01661000
         SPACE 3                                                        01670000
.ERROR   MNOTE 8, &SUBTYPE INVALID RECORD SUBTYPE SPECIFIED             01680000
.CONTB   ANOP                                                           01690000
         MEXIT                                                          01700000
         SPACE 1                                                        01710000
