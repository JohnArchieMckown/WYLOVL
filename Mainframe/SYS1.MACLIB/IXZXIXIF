.***AC.PLV01  PROCESS PLISTVER                                      *   00062800
.********************************************************************   00062900
&XPLISTVER      SETA  2                                                 00063000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00063100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00063200
&XPLISTVER      SETA  2                                                 00063300
.ZPLV0          ANOP  ,                                                 00063400
.*                                                                      00063500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00063600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00063700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00063800
.*-------------------------------------------------------------------   00063900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00064000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00064100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00064300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00064400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064500
                AGO   .ZPVCK2                                           00064600
.ZPVCK3         ANOP  ,                                                 00064700
.*-------------------------------------------------------------------   00064800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00064900
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00065000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00065200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00065300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065400
                AGO   .ZPVCKE                                           00065500
.ZPVCK2         ANOP  ,                                                 00065600
.*-------------------------------------------------------------------   00065700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00065800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00065900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00066000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00066100
                AIF   (&ZN LT 2).ZPVCK6    TOO LOW                      00066200
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00066300
.ZPVCK6         ANOP  ,                                                 00066400
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00066500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066600
&ZMS            SETC  '&ZMS THE RANGE IS "2-2"'                         00066700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00066800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066900
&XPLISTVER      SETA  2                                                 00067000
                AGO   .ZPVCKE                                           00067100
.*-------------------------------------------------------------------   00067200
.ZPVCK4A        ANOP  ,                                                 00067300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00067400
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00067500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00067600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00067700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00067800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00067900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068000
                AGO   .ZPVCK5                                           00068100
.ZPVCK4         ANOP  ,                                                 00068200
&XPLISTVER      SETA  &PLISTVER(1)                                      00068300
.ZPVCK5         ANOP  ,                                                 00068400
.ZPVCKE         ANOP  ,                                                 00068500
.********************************************************************   00068600
.***AG.MF01 PROCESS MF MACRO FORMS                                      00068700
.********************************************************************   00068800
                ACTR  10000                                             00068900
.*------------------------------------------------------------------    00069000
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00069100
&XMF            SETC  ''          INITIALIZE XVARIABLE                  00069200
.*-------------------------------------------------------------------   00069300
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00069400
.ZG51           ANOP  ,                                                 00069500
&ZTLC           SETC 'S'                                                00069600
                AIF   ('&MF(1)' NE '&ZTLC').ZG52                        00069700
&XMF            SETC  'S'         SET KEYWORD                           00069800
                AIF   (N'&MF LE 1).ZG51A        CK NUM                  00069900
&ZMS            SETC  '"MF='      *KPOSA* MNOTE                         00070000
&ZMS            SETC  '&ZMS.&MF"'                                       00070100
&ZMS            SETC  '&ZMS "MF"'                                       00070200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00070300
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00070400
.ZG51A          ANOP  ,                                                 00070500
.*-------------------------------------------------------------------   00070600
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00070700
.ZG52           ANOP  ,                                                 00070800
&ZTLC           SETC 'L'                                                00070900
                AIF   ('&MF(1)' NE '&ZTLC').ZG59                        00071000
&XMF            SETC  'L'         SET KEYWORD                           00071100
                AIF   (N'&MF LE 3).ZG52A        CK NUM                  00071200
&ZMS            SETC  '"MF='      *KPOSA* MNOTE                         00071300
&ZMS            SETC  '&ZMS.&MF"'                                       00071400
&ZMS            SETC  '&ZMS "MF"'                                       00071500
                MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'              00071600
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00071700
.ZG52A          ANOP  ,                                                 00071800
.*-------------------------------------------------------------------   00071900
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00072000
                AIF   (N'&SYSLIST NE 0).ZG52B   POSITIONALS?            00072100
                AIF   ('&GROUPTOKEN' NE '').ZG52B  KEYS?                00072200
                AIF   ('&GROUPNAME' NE '').ZG52B  KEYS?                 00072300
                AIF   ('&INFOLVL' NE '').ZG52B  KEYS?                   00072400
                AIF   ('&STATE' NE '').ZG52B    KEYS?                   00072500
                AIF   ('&SYSTEM' NE '').ZG52B   KEYS?                   00072600
                AIF   ('&POLYJES' NE '').ZG52B  KEYS?                   00072700
                AIF   ('&FUNCTION' NE '').ZG52B  KEYS?                  00072800
                AIF   ('&REQMBOX' NE '').ZG52B  KEYS?                   00072900
                AIF   ('&REQTOKEN' NE '').ZG52B  KEYS?                  00073000
                AIF   ('&ANSAREA' NE '').ZG52B  KEYS?                   00073100
                AIF   ('&ANSLEN' NE '').ZG52B   KEYS?                   00073200
                AIF   ('&RTNCODE' NE '').ZG52B  KEYS?                   00073300
                AIF   ('&RSNCODE' NE '').ZG52B  KEYS?                   00073400
                AGO    .ZG52C     INVOCATION OKAY                       00073500
.ZG52B          ANOP  ,                                                 00073600
&ZMS            SETC  'THE "L" FORM OF "MF"'    *MFLA* MNOTE            00073700
&ZMS            SETC  '&ZMS CAN ONLY HAVE'                              00073800
&ZMS            SETC  '&ZMS THE "PLISTVER" KEY'                         00073900
                MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'            00074000
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00074100
.ZG52C          ANOP  ,                                                 00074200
.*-------------------------------------------------------------------   00074300
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00074400
&ZXMFCTRL       SETC  'INPUT'     SET IN/OUTPUT                         00074500
&XMFCTRL        SETC  '&MF(2,1)'  SET XVARIABLE                         00074600
                AIF   (N'&MF(2) GT 1).ZG54B     MULTI PARMS             00074700
&XMFCTRL        SETC  '&MF(2)'    SET XVARIABLE                         00074800
.ZG54B          AIF   (K'&MF(2) GT 0).ZG54C                             00074900
&ZXMFCTRL       SETC  ''          RESET IN/OUTPUT                       00075000
&ZMS            SETC  'WHEN "L" IS SPECIFIED'   *KRQDA*                 00075100
&ZMS            SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'               00075200
                MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                      00075300
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00075400
.ZG54C          ANOP  ,                                                 00075500
.*-------------------------------------------------------------------   00075600
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00075700
                AIF   (K'&ZXMFCTRL LE 0).ZG54P  NOT I/O                 00075800
                AIF   ('&XMFCTRL'(1,1) NE '(').ZG54P                    00075900
&ZXMFCTRL       SETC  ''          RESET IN/OUTPUT                       00076000
&ZMS            SETC  '"MF='      *KGPRA* MNOTE                         00076100
&ZMS            SETC  '&ZMS.&MF"'                                       00076200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00076300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 2.'                 00076400
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00076500
.ZG54P          ANOP  ,                                                 00076600
.*-------------------------------------------------------------------   00076700
.***AG.KXVAR13 VALUE, OPT INPUT, POS(3) -XVARIABLE                      00076800
&ZXMFATTR       SETC  'INPUT'     SET IN/OUTPUT                         00076900
&XMFATTR        SETC  '0D'                                              00077000
                AIF   (K'&MF(3) LE 0).ZG57B     PARM                    00077100
&XMFATTR        SETC  '&MF(3,1)'  SET XVARIABLE                         00077200
                AIF   (N'&MF(3) GT 1).ZG57B     MULTI PARMS             00077300
&XMFATTR        SETC  '&MF(3)'    SET XVARIABLE                         00077400
.ZG57B          ANOP  ,                                                 00077500
.*-------------------------------------------------------------------   00077600
.***AG.KCK01 VALUE, OPT INPUT, POS(3) -GPR NOT ALLOWED                  00077700
                AIF   (K'&ZXMFATTR LE 0).ZG57P  NOT I/O                 00077800
