                MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                      00082100
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00082200
.ZG61C          ANOP  ,                                                 00082300
.*-------------------------------------------------------------------   00082400
.***AG.KCK02 NAME, OPT RQD INPUT, POS(2) -GPR OUT OF RANGE              00082500
                AIF   (K'&ZXMFCTRL LE 0).ZG61R  NOT I/O                 00082600
                AIF   ('&XMFCTRL'(1,1) NE '(').ZG61R                    00082700
&ZCGPR          SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)                        00082800
                AIF   ('&ZCGPR'(1,1) NE '0').ZG61P                      00082900
                AIF   (K'&ZCGPR GT 2).ZG61Q                             00083000
                AIF   ('&ZCGPR' EQ '0').ZG61Q                           00083100
&ZCGPR          SETC  '&ZCGPR'(2,1)                                     00083200
                AIF   (T'&ZCGPR NE 'N').ZG61Q   INVALID REG NAME        00083300
.ZG61P          ANOP  ,                                                 00083400
                AIF   (T'&ZCGPR NE 'N').ZG61R   NON NUMERIC             00083500
                AIF   ('&ZCGPR' LT '1').ZG61Q                           00083600
                AIF   ('&ZCGPR' LE '12').ZG61R                          00083700
.ZG61Q          ANOP  ,                                                 00083800
&ZMS            SETC  '"MF='      *KGPRA* MNOTE                         00083900
&ZMS            SETC  '&ZMS.&MF"'                                       00084000
&ZMS            SETC  '&ZMS ARGUMENT 2 MUST SPECIFY A REGISTER'         00084100
                MNOTE 8,' &ZMS IN THE RANGE (1)-(12).'                  00084200
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00084300
.ZG61R          ANOP  ,                                                 00084400
.*-------------------------------------------------------------------   00084500
.***AG.KKWRD02 VALUE, OPT INPUT, ME KEYWORD -SINGLE KEYWORD             00084600
