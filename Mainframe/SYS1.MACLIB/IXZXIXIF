                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00236100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00236200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00236300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00236400
