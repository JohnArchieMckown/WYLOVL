.******************************************************************     00185400
.***AI.L01 IXZXIXIF FMT=LIST FILL IN PARAMETER AREA                     00185500
.******************************************************************     00185600
                      ACTR  10000                                       00185700
&ZCVAL                SETC  'LA'            XA MODE                     00185800
.*                                                                      00185900
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00186000
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00186100
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00186200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00186300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00186400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00186500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00186600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00186700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00186800
&ZOPN                 SETA  &ZOPN+1                                     00186900
&ZOPL(&ZOPN)          SETC  ' '                                         00187000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00187100
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00187200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00187300
                      AGO   .ZIMFNX                                     00187400
.*                                                                      00187500
.ZIMFN2               ANOP ,                                            00187600
&ZOPN                 SETA  &ZOPN+1                                     00187700
&ZOPL(&ZOPN)          SETC  ' '                                         00187800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00187900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00188000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00188100
.ZIMFNX               ANOP ,                                            00188200
