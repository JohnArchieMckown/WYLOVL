                      LCLC  &ZOPL(4095)    LABEL                        00157400
                      LCLC  &ZOPO(4095)    OPERATION                    00157500
                      LCLC  &ZOPV(4095)    VALUE                        00157600
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00157700
.*                                                                      00157800
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00157900
.******************************************************************     00158000
.***AL.L10 IXZXIXIF FMT=LIST MF=S|L PARAMETERS                          00158100
.******************************************************************     00158200
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00158300
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00158400
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00158500
&XMFATTR              SETC  '0D'                                        00158600
.*                                                                      00158700
&ZOPN                 SETA  &ZOPN+1                                     00158800
