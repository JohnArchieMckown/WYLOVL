.*------------------------------------------------------------------    00086500
.***AG.KKCTL04 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00086600
.ZG67           ANOP  ,                                                 00086700
&ZXMF           SETC  'INPUT'     SET IN/OUTPUT                         00086800
                AIF   (K'&XMF GT 0).ZG67E       VALID ARG               00086900
                AIF   (T'&MF EQ 'O').ZG67D      OMITTED                 00087000
&ZASYSLNDX      SETA  0                                                 00087100
.ZG67A          ANOP  ,           POSITIONAL MACRO KEY                  00087200
&ZASYSLNDX      SETA  &ZASYSLNDX+1                                      00087300
                AIF   (&ZASYSLNDX GT N'&SYSLIST).ZG67C                  00087400
&ZTLC           SETC 'MF'                                               00087500
                AIF   ('&SYSLIST(&ZASYSLNDX)' NE '&ZTLC').ZG67A         00087600
.ZG67B          ANOP  ,                                                 00087700
&ZXMF           SETC  ''          RESET IN/OUTPUT                       00087800
&ZMS            SETC  'THE "MF" KEY'            *KOPTA* MNOTE           00087900
&ZMS            SETC  '&ZMS MUST CONTAIN ONE'                           00088000
                MNOTE 8,' &ZMS (OR MORE) ARGUMENT(S).'                  00088100
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00088200
                AGO   .ZG67E                                            00088300
.ZG67C          ANOP  ,                                                 00088400
&ZXMF           SETC  ''          RESET IN/OUTPUT                       00088500
&ZMS            SETC  '"MF='      *KUKWA* MNOTE                         00088600
&ZMS            SETC  '&ZMS.&MF"'                                       00088700
