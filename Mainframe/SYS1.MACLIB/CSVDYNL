&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00672200
.******************************************************************     00672300
.***AL.L10 CSVDYNL FMT=LIST MF=S|L PARAMETERS                           00672400
.******************************************************************     00672500
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00672600
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00672700
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00672800
&XMFATTR              SETC  '0D'                                        00672900
.*                                                                      00673000
&ZOPN                 SETA  &ZOPN+1                                     00673100
&ZOPL(&ZOPN)          SETC  ' '                                         00673200
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00673300
&ZOPO(&ZOPN)          SETC  'B'                                         00673400
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00673500
                      AGO   .ZALVL6B                                    00673600
.ZALVL6A              ANOP                                              00673700
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00673800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00673900
.ZALVL6B              ANOP                                              00674000
&ZOPD(&ZOPN)          SETC  '++ CSVDYNL ARG PARMS'                      00674100
.*                                                                      00674200
&ZOPN                 SETA  &ZOPN+1                                     00674300
