&ZOPD(&ZOPN)          SETC  '++ &ZXLISTLNKNAME XLISTLNKNAME'            00794300
.*                                                                      00794400
.ZI50E                ANOP  ,                                           00794500
.*-------------------------------------------------------------------   00794600
&ZOPN1                SETA &ZOPN                                        00794700
.*-------------------------------------------------------------------   00794800
.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              00794900
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         00795000
.*******************************************************************    00795100
.***  PROCESS CSVDYNL OUTPUT PARAMETERS                                 00795200
.*******************************************************************    00795300
                      ACTR  10000                                       00795400
.*******************************************************************    00795500
.***AO.X02  RETURN CODE PROCESSING                                      00795600
.*******************************************************************    00795700
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00795800
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00795900
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00796000
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00796100
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00796200
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00796300
&ZOPN                 SETA  &ZOPN+1                                     00796400
&ZOPL(&ZOPN)          SETC  ' '                                         00796500
&ZOPO(&ZOPN)          SETC  'ST'                                        00796600
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00796700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00796800
                      AGO   .ZMRETX                                     00796900
.*                                                                      00797000
.ZMRET2               ANOP  ,                                           00797100
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00797200
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00797300
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00797400
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00797500
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00797600
&ZOPN                 SETA  &ZOPN+1                                     00797700
&ZOPL(&ZOPN)          SETC  ' '                                         00797800
&ZOPO(&ZOPN)          SETC  'LR'                                        00797900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00798000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00798100
.ZMRETX               ANOP  ,                                           00798200
.*******************************************************************    00798300
.***AO.X03  REASON CODE PROCESSING                                      00798400
.*******************************************************************    00798500
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00798600
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00798700
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00798800
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00798900
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00799000
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00799100
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00799200
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00799300
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00799400
&ZOPN                 SETA  &ZOPN+1                                     00799500
&ZOPL(&ZOPN)          SETC  ' '                                         00799600
&ZOPO(&ZOPN)          SETC  'ST'                                        00799700
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00799800
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00799900
                      AGO   .ZMRSNX                                     00800000
.*                                                                      00800100
.ZMRSN2               ANOP  ,                                           00800200
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00800300
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00800400
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00800500
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00800600
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00800700
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00800800
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00800900
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00801000
&ZOPN                 SETA  &ZOPN+1                                     00801100
&ZOPL(&ZOPN)          SETC  ' '                                         00801200
&ZOPO(&ZOPN)          SETC  'LR'                                        00801300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00801400
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00801500
.ZMRSNX               ANOP  ,                                           00801600
.*******************************************************************    00801700
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00801800
.*******************************************************************    00801900
.ZMFLX                ANOP  ,              MF=L PROCESSING              00802000
.*******************************************************************    00802100
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00802200
.*******************************************************************    00802300
                      ACTR  10000                                       00802400
.*                                                                      00802500
                      LCLA  &ZLC           COLUMN POSITION              00802600
                      LCLA  &ZOPX          LIST TABLE INDEX             00802700
                      LCLC  &ZLL           LABEL                        00802800
                      LCLC  &ZLOP          OPERATION                    00802900
                      LCLC  &ZLV           VALUE                        00803000
                      LCLC  &ZLD           DESCRIPTION                  00803100
.*                                                                      00803200
&ZOPX                 SETA  1                                           00803300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00803400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00803500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00803600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00803700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00803800
                      AIF   (K'&ZLD LE 0).ZL5                           00803900
&ZLC                  SETA  35             COMMENT COLUMN               00804000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00804100
&ZLC                  SETA  33             COMMMENT COLUMN              00804200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00804300
&ZLV                  SETC  '&ZLV'.' '                                  00804400
                      AGO   .ZL3                                        00804500
.*                                                                      00804600
.ZL4                  ANOP  ,                                           00804700
&ZLV                  SETC  '&ZLV'.' '                                  00804800
.ZL5                  ANOP  ,                                           00804900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00805000
.*                                                                      00805100
&ZLL     &ZLOP &ZLV                                                     00805200
.*                                                                      00805300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00805400
                      AGO   .ZL2           CONTINUE LOOPING             00805500
.*                                                                      00805600
.ZLX                  ANOP  ,                                           00805700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00805800
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00805900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00806000
         L     14,16(0,0)              GET CVT ADDRESS             @L1A 00806100
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 00806200
         L     14,448(14,0)            GET LX/EX FOR SERVICE RTN   @L1A 00806300
         PC    0(14)                                               @L1A 00806400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00806500
.ZLINKX               ANOP  ,                                           00806600
.*******************************************************************    00806700
.***AP.X01  FORMAT GENERATED CODE                                       00806800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00806900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00807000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00807100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00807200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00807300
                      AIF   (K'&ZLD LE 0).ZL51                          00807400
&ZLC                  SETA  35             COMMENT COLUMN               00807500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00807600
&ZLC                  SETA  33             COMMENT COLUMN               00807700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00807800
&ZLV                  SETC  '&ZLV'.' '                                  00807900
                      AGO   .ZL31                                       00808000
.*                                                                      00808100
.ZL41                 ANOP  ,                                           00808200
&ZLV                  SETC  '&ZLV'.' '                                  00808300
.ZL51                 ANOP  ,                                           00808400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00808500
.*                                                                      00808600
&ZLL     &ZLOP &ZLV                                                     00808700
.*                                                                      00808800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00808900
                      AGO   .ZL21          CONTINUE LOOPING             00809000
.*                                                                      00809100
.ZLX1                 ANOP  ,                                           00809200
.*******************************************************************    00809300
.***AP.X02  END OF CSVDYNL MACRO                                        00809400
