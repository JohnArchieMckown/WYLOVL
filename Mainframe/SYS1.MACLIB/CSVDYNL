.***AE.01  START OF CSVDYNL MACRO CODE                                  00661300
.*******************************************************************    00661400
                 LCLC  &M0M0001                 NAME                    00661500
&M0M0001         SETC  '&XLABEL'                SET NAME                00661600
                 AIF   (K'&XLABEL NE 0).XLAB2                           00661700
&M0M0001         SETC  'M00M&SYSNDX'            DEFAULT NAME            00661800
.*                                                                      00661900
.XLAB2           ANOP  ,                                                00662000
                 AIF   ('&XMF' EQ 'L').XLAB3                            00662100
&M0M0001 DS    0H                             CSVDYNL-0                 00662200
.XLAB3           ANOP  ,                                                00662300
.*******************************************************************    00662400
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00662500
.*******************************************************************    00662600
              PUSH  PRINT                                               00662700
                 AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON       00662800
                 PRINT OFF         ++ PR01                              00662900
                 AGO   .ZPRNTXX                                         00663000
.ZPRINTX         PRINT ON                                               00663100
.ZPRNTXX         ANOP  ,                                                00663200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00663300
         LCLC  &T2,&T1                                             @L1A 00663400
         LCLA  &A1                                                 @L1A 00663500
         AIF   ('&REQUEST' NE 'QUERYDYN').NOQD                     @L1A 00663600
 AIF ('&MF' EQ '' AND '&RETCODE' EQ '' AND '&RSNCODE' EQ '').OKRET @L1A 00663700
&T1      SETC  'MF, RETCODE AND RSNCODE ARE NOT VALID WITH '       @L1A 00663800
&T1      SETC  '&T1.REQUEST=QUERYDYN'                              @L1A 00663900
         MNOTE 8,'&T1'                                             @L1A 00664000
.OKRET   ANOP                                                      @L1A 00664100
         AIF   ('&DYNFUNC'(1,1) NE '(').LREG Not a reg, branch     @L1A 00664200
&T1      SETC  '&DYNFUNC'(2,K'&DYNFUNC-2) Isolate reg number       @L1A 00664300
&T1      SETC  '0(1,&T1.)'             Target expression           @L1A 00664400
&T2      SETC  '0&DYNFUNC'             Target expression           @L1A 00664500
         AGO   .MREG                                               @L1A 00664600
.LREG    ANOP                          Not a reg                   @L1A 00664700
&T2      SETC  '&DYNFUNC(1)'           Target expression           @L1A 00664800
&A1      SETA  K'&DYNFUNC              Length of expression        @L1A 00664900
         AIF   ('&DYNFUNC'(&A1,1) NE ')').NOTRX Branch if not RX   @L1A 00665000
&A2      SETA  2                                                   @L1A 00665100
.OREG    ANOP                          Loop to find open paren     @L1A 00665200
         AIF   (&A2 GE &A1).NOTRX      Reached end, non-RX         @L1A 00665300
         AIF   ('&DYNFUNC'(&A2,1) NE '(').NREG Not "(", branch     @L1A 00665400
&T1      SETC  '&DYNFUNC'(1,&A2).'1'.','   Found paren             @L1A 00665500
&T1      SETC  '&T1'.'&DYNFUNC'(&A2+1,&A1-&A2) Create expression   @L1A 00665600
         AGO   .MREG                                               @L1A 00665700
.NREG    ANOP                          Continue loop               @L1A 00665800
&A2      SETA  &A2+1                   Next character              @L1A 00665900
         AGO   .OREG                   Repeat                      @L1A 00666000
.NOTRX   ANOP                          Not an RX-address           @L1A 00666100
&T1      SETC  '&DYNFUNC(1).(1)'       Get expression              @L1A 00666200
.MREG    ANOP                          Produce resulting statement @L1A 00666300
         L     15,16(0,0)              GET CVT                     @L1A 00666400
         AIF   ('&SYSASCE' EQ 'P').NOAR                            @L1A 00666500
         LAE   15,0(15,0)              CLEAR AR15                  @L1A 00666600
.NOAR    ANOP                                                      @L1A 00666700
         TM    X'4F1'(15),X'04'        Is MVS release correct      @L1A 00666800
         BZ    C&SYSNDX                No, no dynamic LNKLST       @L1A 00666900
         ICM   15,15,X'3E0'(15)        Check security product      @L1A 00667000
         BZ    A&SYSNDX                No security product, OK     @L1A 00667100
         TM    X'19A'(15),X'10'        Is dynamic LNKLST supported @L1A 00667200
         BNZ   A&SYSNDX                Yes, OK                     @L1A 00667300
C&SYSNDX DS    0H                      Function not available      @L1A 00667400
         MVI   &T2,0                   Function not available      @L1A 00667500
         B     B&SYSNDX                Skip other path             @L1A 00667600
A&SYSNDX DS    0H                      Function possibly available @L1A 00667700
         L     15,16(0,0)              GET CVT                     @L1A 00667800
         L     15,140(15,0)            GET ECVT                    @L1A 00667900
         L     15,228(15,0)                                        @L1A 00668000
         MVC   &T1,27(15)              GET AVAILABILITY VALUE      @L1A 00668100
B&SYSNDX DS    0H                      End of QUERYDYN             @L1A 00668200
         POP   PRINT                                               @01A 00668300
         MEXIT                                                     @L1A 00668400
.NOQD    ANOP                                                      @L1A 00668500
         AIF   ('&LINKAGE' EQ '').NOLINKAGE                        @L1A 00668600
&ZMS     SETC  'LINKAGE MAY NOT BE SPECIFIED'                      @L1A 00668700
         MNOTE 8,'&ZMS'                                            @L1A 00668800
         POP   PRINT                                               @01A 00668900
         MEXIT                                                     @L1A 00669000
.NOLINKAGE ANOP                                                    @L1A 00669100
         AIF   ('&REQUEST' NE '').ISREQUEST                        @L1A 00669200
         AIF   ('&XMF' NE 'E').ISREQUEST                           @L1A 00669300
&ZMS     SETC  'REQUEST IS REQUIRED WITH MF=E'                     @L1A 00669400
         MNOTE 8,'&ZMS'                                            @L1A 00669500
         POP   PRINT                                               @01A 00669600
         MEXIT                                                     @L1A 00669700
.ISREQUEST ANOP                                                    @L1A 00669800
         AIF   ('&REQUEST(1)' NE 'INTERNAL').REQUESTOK             @L1A 00669900
&ZMS     SETC  'REQUEST=INTERNAL MAY NOT BE SPECIFIED'             @L1A 00670000
         MNOTE 8,'&ZMS'                                            @L1A 00670100
         POP   PRINT                                               @01A 00670200
         MEXIT                                                     @L1A 00670300
.REQUESTOK ANOP                                                    @L1A 00670400
         AIF   ('&REQUEST(1)' NE 'ACTIVATE').REQUESTOK2            @L1A 00670500
&ZMS     SETC  'REQUEST=ACTIVATE MAY NOT BE SPECIFIED'             @L1A 00670600
         MNOTE 8,'&ZMS'                                            @L1A 00670700
         POP   PRINT                                               @01A 00670800
         MEXIT                                                     @L1A 00670900
.REQUESTOK2 ANOP                                                   @L1A 00671000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00671100
.********************************************************************   00671200
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00671300
