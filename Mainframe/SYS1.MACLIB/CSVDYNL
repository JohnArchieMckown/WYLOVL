.***AE.KC00 INITIALIZE KEYWORD CONTROL VARIABLES                        00501200
.********************************************************************   00501300
&XREQUEST        SETC  ''          INITIALIZE XVARIABLE                 00501400
&XCHECKSYS1      SETC  ''          INITIALIZE XVARIABLE                 00501500
&XPOS            SETC  ''          INITIALIZE XVARIABLE                 00501600
&XCHECKCONCAT    SETC  ''          INITIALIZE XVARIABLE                 00501700
&XSEARCH         SETC  ''          INITIALIZE XVARIABLE                 00501800
&XUSERINFO       SETC  ''          INITIALIZE XVARIABLE                 00501900
&XWHICHAS        SETC  ''          INITIALIZE XVARIABLE                 00502000
&XLINKAGE        SETC  ''          INITIALIZE XVARIABLE                 00502100
.*-------------------------------------------------------------------   00502200
.***AE.KKCTL02 VALUE, RQD INPUT, ME KYWRDCTL -KEYWORD CONTROL           00502300
&ZTLC            SETC 'DEFINE'                                          00502400
                 AIF   ('&REQUEST(1)' NE '&ZTLC').ZE3A                  00502500
&XREQUEST        SETC  'DEFINE'    SET KEYWORD                          00502600
                 AIF   (N'&REQUEST LE 1).ZE3A   CK NUM                  00502700
&ZMS             SETC  '"REQUEST='              *KPOSA* MNOTE           00502800
&ZMS             SETC  '&ZMS.&REQUEST"'                                 00502900
&ZMS             SETC  '&ZMS "REQUEST"'                                 00503000
                 MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'              00503100
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00503200
.ZE3A            ANOP  ,                                                00503300
.*----------------------------------------------------------------***   00503400
.***AE.KXVAR03 NAME, RQD INPUT, KEYS -XVAR KEY                          00503500
                 AIF   (N'&LNKLSTNAME LE 1).ZE5A  CK NUM                00503600
&ZMS             SETC  '"LNKLSTNAME='           *KPOSA* MNOTE           00503700
&ZMS             SETC  '&ZMS.&LNKLSTNAME"'                              00503800
&ZMS             SETC  '&ZMS "LNKLSTNAME"'                              00503900
                 MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'              00504000
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00504100
.ZE5A            ANOP  ,                                                00504200
.*-------------------------------------------------------------------   00504300
.***AE.KXVAR04 NAME, RQD INPUT, KEYS -XVAR ARGUMENT                     00504400
                 AIF   (K'&LNKLSTNAME(1) LE 0).ZE5B NO PRM              00504500
&ZXLNKLSTNAME    SETC  'INPUT'     SET IN/OUTPUT                        00504600
&XLNKLSTNAME     SETC  '&LNKLSTNAME(1)'         SET XVARIABLE           00504700
                 AIF   (N'&LNKLSTNAME GT 1).ZE5B                        00504800
.*               ANOP              MULTIPLE PARMS                       00504900
&XLNKLSTNAME     SETC  '&LNKLSTNAME'            SET XVARIABLE           00505000
.ZE5B            ANOP  ,                                                00505100
.*-------------------------------------------------------------------   00505200
.***AE.KCK02 NAME, RQD INPUT, KEYS -GPR OUT OF RANGE                    00505300
                 AIF   (K'&ZXLNKLSTNAME LE 0).ZE5R  NOT I/O             00505400
                 AIF   ('&XLNKLSTNAME'(1,1) NE '(').ZE5R                00505500
&ZCGPR           SETC  '&XLNKLSTNAME'(2,K'&XLNKLSTNAME-2)               00505600
                 AIF   ('&ZCGPR'(1,1) NE '0').ZE5P                      00505700
                 AIF   (K'&ZCGPR GT 2).ZE5Q                             00505800
                 AIF   ('&ZCGPR' EQ '0').ZE5Q                           00505900
&ZCGPR           SETC  '&ZCGPR'(2,1)                                    00506000
                 AIF   (T'&ZCGPR NE 'N').ZE5Q   INVALID REG NAME        00506100
.ZE5P            ANOP  ,                                                00506200
                 AIF   (T'&ZCGPR NE 'N').ZE5R   NON NUMERIC             00506300
                 AIF   ('&ZCGPR' LT '2').ZE5Q                           00506400
                 AIF   ('&ZCGPR' LE '12').ZE5R                          00506500
.ZE5Q            ANOP  ,                                                00506600
&ZMS             SETC  '"LNKLSTNAME='           *KGPRA* MNOTE           00506700
&ZMS             SETC  '&ZMS.&LNKLSTNAME"'                              00506800
&ZMS             SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'        00506900
                 MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                 00507000
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00507100
.ZE5R            ANOP  ,                                                00507200
