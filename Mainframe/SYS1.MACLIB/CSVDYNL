.********************************************************************   00218100
.***AC.PLV01  PROCESS PLISTVER                                      *   00218200
.********************************************************************   00218300
&XPLISTVER      SETA  0                                                 00218400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00218500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00218600
&XPLISTVER      SETA  0                                                 00218700
.ZPLV0          ANOP  ,                                                 00218800
.*                                                                      00218900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00219000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00219100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00219200
.*-------------------------------------------------------------------   00219300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00219400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00219500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00219600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00219700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00219800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00219900
                AGO   .ZPVCK2                                           00220000
.ZPVCK3         ANOP  ,                                                 00220100
.*-------------------------------------------------------------------   00220200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00220300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00220400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00220500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00220600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00220700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00220800
                AGO   .ZPVCKE                                           00220900
.ZPVCK2         ANOP  ,                                                 00221000
.*-------------------------------------------------------------------   00221100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00221200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00221300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00221400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00221500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00221600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00221700
.ZPVCK6         ANOP  ,                                                 00221800
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00221900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00222000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00222100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00222200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00222300
&XPLISTVER      SETA  0                                                 00222400
                AGO   .ZPVCKE                                           00222500
.*-------------------------------------------------------------------   00222600
.ZPVCK4A        ANOP  ,                                                 00222700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00222800
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00222900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00223000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00223100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00223200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00223300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00223400
                AGO   .ZPVCK5                                           00223500
.ZPVCK4         ANOP  ,                                                 00223600
&XPLISTVER      SETA  &PLISTVER(1)                                      00223700
.ZPVCK5         ANOP  ,                                                 00223800
.ZPVCKE         ANOP  ,                                                 00223900
.********************************************************************   00224000
.***AG.MF01 PROCESS MF MACRO FORMS                                      00224100
.********************************************************************   00224200
                 ACTR  10000                                            00224300
.*------------------------------------------------------------------    00224400
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00224500
&XMF             SETC  ''          INITIALIZE XVARIABLE                 00224600
.*-------------------------------------------------------------------   00224700
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00224800
.ZG132           ANOP  ,                                                00224900
&ZTLC            SETC 'S'                                               00225000
                 AIF   ('&MF(1)' NE '&ZTLC').ZG133                      00225100
&XMF             SETC  'S'         SET KEYWORD                          00225200
&XMFSCK          SETC  'COMPLETE'  SET XMFSCK                           00225300
                 AIF   (N'&MF LE 1).ZG132A      CK NUM                  00225400
&ZMS             SETC  '"MF='      *KPOSA* MNOTE                        00225500
&ZMS             SETC  '&ZMS.&MF"'                                      00225600
&ZMS             SETC  '&ZMS "MF"'                                      00225700
                 MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'              00225800
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00225900
.ZG132A          ANOP  ,                                                00226000
.*-------------------------------------------------------------------   00226100
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00226200
.ZG133           ANOP  ,                                                00226300
&ZTLC            SETC 'L'                                               00226400
                 AIF   ('&MF(1)' NE '&ZTLC').ZG140                      00226500
&XMF             SETC  'L'         SET KEYWORD                          00226600
                 AIF   (N'&MF LE 3).ZG133A      CK NUM                  00226700
&ZMS             SETC  '"MF='      *KPOSA* MNOTE                        00226800
&ZMS             SETC  '&ZMS.&MF"'                                      00226900
&ZMS             SETC  '&ZMS "MF"'                                      00227000
                 MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'             00227100
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00227200
.ZG133A          ANOP  ,                                                00227300
.*-------------------------------------------------------------------   00227400
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00227500
                 AIF   (N'&SYSLIST NE 0).ZG133B  POSITIONALS?           00227600
                 AIF   ('&REQUEST' NE '').ZG133B  KEYS?                 00227700
                 AIF   ('&LNKLSTNAME' NE '').ZG133B  KEYS?              00227800
                 AIF   ('&COPYFROM' NE '').ZG133B  KEYS?                00227900
                 AIF   ('&CHECKSYS1' NE '').ZG133B  KEYS?               00228000
                 AIF   ('&DSNAME' NE '').ZG133B  KEYS?                  00228100
                 AIF   ('&VOLUME' NE '').ZG133B  KEYS?                  00228200
                 AIF   ('&POS' NE '').ZG133B    KEYS?                   00228300
                 AIF   ('&AFTERDSNAME' NE '').ZG133B  KEYS?             00228400
                 AIF   ('&CHECKCONCAT' NE '').ZG133B  KEYS?             00228500
                 AIF   ('&PROBDSNAME' NE '').ZG133B  KEYS?              00228600
                 AIF   ('&MODNAME' NE '').ZG133B  KEYS?                 00228700
                 AIF   ('&FOUNDDSNAME' NE '').ZG133B  KEYS?             00228800
                 AIF   ('&ANSAREA' NE '').ZG133B  KEYS?                 00228900
                 AIF   ('&ANSLEN' NE '').ZG133B  KEYS?                  00229000
                 AIF   ('&SEARCH' NE '').ZG133B  KEYS?                  00229100
                 AIF   ('&LISTLNKNAME' NE '').ZG133B  KEYS?             00229200
                 AIF   ('&USERINFO' NE '').ZG133B  KEYS?                00229300
                 AIF   ('&JOBNAME' NE '').ZG133B  KEYS?                 00229400
                 AIF   ('&ASID' NE '').ZG133B   KEYS?                   00229500
                 AIF   ('&WHICHAS' NE '').ZG133B  KEYS?                 00229600
                 AIF   ('&DYNFUNC' NE '').ZG133B  KEYS?                 00229700
                 AIF   ('&LINKAGE' NE '').ZG133B  KEYS?                 00229800
                 AIF   ('&CMDINFO' NE '').ZG133B  KEYS?                 00229900
                 AIF   ('&RETCODE' NE '').ZG133B  KEYS?                 00230000
                 AIF   ('&RSNCODE' NE '').ZG133B  KEYS?                 00230100
                 AGO    .ZG133C    INVOCATION OKAY                      00230200
.ZG133B          ANOP  ,                                                00230300
&ZMS             SETC  'THE "L" FORM OF "MF"'   *MFLA* MNOTE            00230400
&ZMS             SETC  '&ZMS CAN ONLY HAVE'                             00230500
&ZMS             SETC  '&ZMS THE "PLISTVER" KEY'                        00230600
                 MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'           00230700
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00230800
.ZG133C          ANOP  ,                                                00230900
.*-------------------------------------------------------------------   00231000
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00231100
&ZXMFCTRL        SETC  'INPUT'     SET IN/OUTPUT                        00231200
&XMFCTRL         SETC  '&MF(2,1)'  SET XVARIABLE                        00231300
                 AIF   (N'&MF(2) GT 1).ZG135B   MULTI PARMS             00231400
&XMFCTRL         SETC  '&MF(2)'    SET XVARIABLE                        00231500
.ZG135B          AIF   (K'&MF(2) GT 0).ZG135C                           00231600
&ZXMFCTRL        SETC  ''          RESET IN/OUTPUT                      00231700
&ZMS             SETC  'WHEN "L" IS SPECIFIED'  *KRQDA*                 00231800
&ZMS             SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'              00231900
                 MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                     00232000
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00232100
.ZG135C          ANOP  ,                                                00232200
.*-------------------------------------------------------------------   00232300
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00232400
                 AIF   (K'&ZXMFCTRL LE 0).ZG135P  NOT I/O               00232500
                 AIF   ('&XMFCTRL'(1,1) NE '(').ZG135P                  00232600
&ZXMFCTRL        SETC  ''          RESET IN/OUTPUT                      00232700
