.***AC.PLV01  PROCESS PLISTVER                                      *   00090100
.********************************************************************   00090200
&XPLISTVER      SETA  0                                                 00090300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00090400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00090500
&XPLISTVER      SETA  1                                                 00090600
.ZPLV0          ANOP  ,                                                 00090700
.*                                                                      00090800
                AIF   ('&EMCSTGPCT' EQ '').ZPLV1E                       00090900
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00091000
&XPLISTVER      SETA  1                                                 00091100
.ZPLV1E         ANOP  ,                                                 00091200
.*                                                                      00091300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00091400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00091500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00091600
.*-------------------------------------------------------------------   00091700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00091800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00091900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00092000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00092100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00092200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00092300
                AGO   .ZPVCK2                                           00092400
.ZPVCK3         ANOP  ,                                                 00092500
.*-------------------------------------------------------------------   00092600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00092700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00092800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00092900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00093000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00093100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00093200
                AGO   .ZPVCKE                                           00093300
.ZPVCK2         ANOP  ,                                                 00093400
.*-------------------------------------------------------------------   00093500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00093600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00093700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00093800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00093900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00094000
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00094100
.ZPVCK6         ANOP  ,                                                 00094200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00094300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00094400
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00094500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00094600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00094700
