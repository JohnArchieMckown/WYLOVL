&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00240800
                      AIF   (K'&ZLD LE 0).ZL51                          00240900
&ZLC                  SETA  35             COMMENT COLUMN               00241000
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00241100
&ZLC                  SETA  33             COMMENT COLUMN               00241200
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00241300
&ZLV                  SETC  '&ZLV'.' '                                  00241400
                      AGO   .ZL31                                       00241500
.*                                                                      00241600
.ZL41                 ANOP  ,                                           00241700
&ZLV                  SETC  '&ZLV'.' '                                  00241800
.ZL51                 ANOP  ,                                           00241900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00242000
.*                                                                      00242100
&ZLL     &ZLOP &ZLV                                                     00242200
.*                                                                      00242300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00242400
                      AGO   .ZL21          CONTINUE LOOPING             00242500
.*                                                                      00242600
.ZLX1                 ANOP  ,                                           00242700
.*******************************************************************    00242800
.***AP.X02  END OF IXLALTER MACRO                                       00242900
&ZIXLALTER            SETC  'YES'          SET IXLALTER USED            00243000
         POP   PRINT                                                    00243100
*                                            IXLALTER-1                 00243200
         SPACE 1                                                        00243300
.ZLX2                 ANOP  ,                                           00243400
         MEND                                                           00243500
**********************************************************************/ 00243600
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00243700
*%IXLALTER: MACRO KEYS(STRNAME,REQUEST,SIZE,STRSIZE,RATIO,ENTRYRATIO,   00243800
