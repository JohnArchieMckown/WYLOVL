                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01050600
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01050700
&ZOPN                 SETA  &ZOPN+1                                     01050800
&ZOPL(&ZOPN)          SETC  ' '                                         01050900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01051000
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01051100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01051200
                      AGO   .ZIMFNX                                     01051300
.*                                                                      01051400
.ZIMFN2               ANOP ,                                            01051500
&ZOPN                 SETA  &ZOPN+1                                     01051600
&ZOPL(&ZOPN)          SETC  ' '                                         01051700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01051800
