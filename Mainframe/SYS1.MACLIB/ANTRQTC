.***AC.PLV01  PROCESS PLISTVER                                      *   00279700
.********************************************************************   00279800
&XPLISTVER      SETA  0                                                 00279900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00280000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00280100
&XPLISTVER      SETA  4                                                 00280200
.ZPLV0          ANOP  ,                                                 00280300
.*                                                                      00280400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00280500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00280600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00280700
.*-------------------------------------------------------------------   00280800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00280900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00281000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00281100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00281200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00281300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00281400
                AGO   .ZPVCK2                                           00281500
.ZPVCK3         ANOP  ,                                                 00281600
.*-------------------------------------------------------------------   00281700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00281800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00281900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00282000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00282100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00282200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00282300
                AGO   .ZPVCKE                                           00282400
.ZPVCK2         ANOP  ,                                                 00282500
.*-------------------------------------------------------------------   00282600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00282700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00282800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00282900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00283000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00283100
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00283200
.ZPVCK6         ANOP  ,                                                 00283300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00283400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00283500
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00283600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00283700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00283800
&XPLISTVER      SETA  4                                                 00283900
                AGO   .ZPVCKE                                           00284000
.*-------------------------------------------------------------------   00284100
.ZPVCK4A        ANOP  ,                                                 00284200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00284300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00284400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00284500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00284600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00284700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00284800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00284900
                AGO   .ZPVCK5                                           00285000
.ZPVCK4         ANOP  ,                                                 00285100
&XPLISTVER      SETA  &PLISTVER(1)                                      00285200
.ZPVCK5         ANOP  ,                                                 00285300
.ZPVCKE         ANOP  ,                                                 00285400
.********************************************************************   00285500
.***AG.MF01 PROCESS MF MACRO FORMS                                      00285600
.********************************************************************   00285700
