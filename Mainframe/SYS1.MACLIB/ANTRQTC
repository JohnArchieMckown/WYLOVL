*               ORG   XRETINFO                                        * 00242700
*      RTC      DS    F                                               * 00242800
*      RSC      DS    F                                               * 00242900
*               ORG   XRETINFO+100                                    * 00243000
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 00243100
*               ORG   *+1-(*-@DATD)/(*-@DATD)                         * 00243200
*      @ENDDATD DS    0X                                              * 00243300
*      @DYNSIZE EQU   ((@ENDDATD-@DATD+7)/8)*8                        * 00243400
*               END   TESTER                                          * 00243500
*                                                                     * 00243600
*                                                                     * 00243700
*    Operation:  Getting a list of SnapShot subsystems                * 00243800
*    in PL/X.                                                         * 00243900
*                                                                     * 00244000
*       @PROCESS                                                      * 00244100
*         NOMSOURCE                                                   * 00244200
*         GENMARGINS(1,72)                                            * 00244300
*         OPTIMIZE(ADV,48)                                            * 00244400
*         ENVIRONMENT(MVS/ESA)                                        * 00244500
*         SOURCE(SEGMENT)                                             * 00244600
*         TITLE('TESTER: Test ANTRQST');                              * 00244700
*                                                                     * 00244800
*        TESTER: PROCEDURE                                            * 00244900
*                 OPTIONS(                                            * 00245000
*                         AMODE(31)                                   * 00245100
*                         CODEREG(12)                                 * 00245200
*                         DATAREG(13)                                 * 00245300
*                         REENTRANT                                   * 00245400
*                         RMODE(ANY)                                  * 00245500
*                         );                                          * 00245600
*                                                                     * 00245700
*       DCL                                                           * 00245800
*         1 XRETINFO CHAR(100) BDY(WORD),                             * 00245900
*           3 RTC FIXED(32),                                          * 00246000
*           3 RSC FIXED(32);                                          * 00246100
*       DCL XQRYSIZE FIXED(16) INITIAL(LENGTH(XQRYINFO));             * 00246200
*       DCL XQRYINFO CHAR(512);                                       * 00246300
*       DCL RTCD FIXED(31);                                           * 00246400
*       DCL RSCD FIXED(31);                                           * 00246500
*       DCL RTNCD FIXED(31);                                          * 00246600
*       DCL RSNCD FIXED(31);                                          * 00246700
*                                                                     * 00246800
*         ?ANTRQST REQUEST(SQRYSSYS)                                  * 00246900
*                  QRYSIZE(XQRYSIZE)                                  * 00247000
*                  QRYINFO(XQRYINFO)                                  * 00247100
*                  HLQ(XHLQ)                                          * 00247200
*                  RETINFO(XRETINFO)                                  * 00247300
*                  RETCODE(RTNCD)                                     * 00247400
*                  RSNCODE(RSNCD)                                     * 00247500
*                  MF(E,PARMLIST);                                    * 00247600
*                                                                     * 00247700
*         IF RTNCD ^= 0 THEN                                          * 00247800
*           DO;                                                       * 00247900
*             RTCD = RTNCD;                                           * 00248000
*             RSCD = RSNCD;                                           * 00248100
*           END;                                                      * 00248200
*         ELSE                                                        * 00248300
*           DO;                                                       * 00248400
*             RTCD = RTC;                                             * 00248500
*             RSCD = RSC;                                             * 00248600
*           END;                                                      * 00248700
*                                                                     * 00248800
*         IF RTCD = RQST_PC_NUMBER_ZERO THEN                          * 00248900
*           ?WTO ('ANTAS000 NOT ACTIVE');                             * 00249000
*                                                                     * 00249100
*         RETURN CODE(RTCD,RSCD);                                     * 00249200
*                                                                     * 00249300
*         ?ANTRQSTL NAME(PARMLIST);                                   * 00249400
*                                                                     * 00249500
*         ?EPILOG;                                                    * 00249600
*                                                                     * 00249700
*       END TESTER;                                                   * 00249800
*                                                                     * 00249900
*                                                                     * 00250000
*                                                                     * 00250100
*01* MACRO USAGE NOTES:                                               * 00250200
*                                                                     * 00250300
*                                                                     * 00250400
*    1.  Printing of the macro expansion is controlled by the ZPRINT  * 00250500
*        global macro variable.  Any value other than 'NO' will       * 00250600
*        result in the macro expansion being printed.  The default is * 00250700
*        'YES'.  The following examples illustrate how the ZPRINT     * 00250800
*        variable should be set.                                      * 00250900
*                                                                     * 00251000
*                    GBLC &ZPRINT                                     * 00251100
*            &ZPRINT SETC 'YES'          &ZPRINT SETC 'NO'            * 00251200
*                                                                     * 00251300
*                                                                     * 00251400
*    2.  When using the NOCHECK option in conjunction with the Modify * 00251500
*        and/or  Execute  forms,  be  sure  that  it is preceded by a * 00251600
*        Modify   or  Execute  form  invocation  which  specifies  or * 00251700
*        defaults  to  the  COMPLETE  option.   This insures that the * 00251800
*        parameter  list  is  initialized completely.   The parameter * 00251900
*        list  field which identifies the version number of the macro * 00252000
*        is only set when the Standard form or the Complete option of * 00252100
*        the  Execute/Modify  form  is  used,  or  when  PLISTVER  is * 00252200
*        explicitly  specified.  Be sure that the resulting parameter * 00252300
*        list version number covers all the keys that you use.        * 00252400
*                                                                     * 00252500
*                                                                     * 00252600
*    3.  The  ANTRQTC macro supports multiple versions. The macro key * 00252700
*        list  below  contains the version level in which the key was * 00252800
*        first  introduced.  Positionals  are  identified by the word * 00252900
*        'Positional'  followed by the numeric value of its position. * 00253000
*        Nested  positionals  are  preceded  by their outer key.  The * 00253100
*        lowest value represents the original version and the highest * 00253200
*        value  represents  the  most  recent  version of the ANTRQTC * 00253300
*        macro.    When  specifying  PLISTVER,  be sure that it is as * 00253400
*        least  as high as the highest version number of all the keys * 00253500
*        being used.                                                  * 00253600
*                                                                     * 00253700
*        VERSION   KEY                                                * 00253800
*                                                                     * 00253900
*              0      ACCESS                                          * 00254000
*                     ALET                                            * 00254100
*                     ASYNCH                                          * 00254200
*                     BASICINFO                                       * 00254300
*                     BASICSIZE                                       * 00254400
*                     BEGINCYL0                                       * 00254500
*                     DEVN                                            * 00254600
*                     DEVTYPE                                         * 00254700
*                     ECB                                             * 00254800
*                     EXTINFO                                         * 00254900
*                     EXTNBR                                          * 00255000
*                     EXTNDINFO                                       * 00255100
*                     EXTNDSIZE                                       * 00255200
*                     HIGHDEVN                                        * 00255300
*                     ILK                                             * 00255400
*                     LOWDEVN                                         * 00255500
*                     PARTITION                                       * 00255600
*                     QRYINFO                                         * 00255700
*                     QRYSIZE                                         * 00255800
*                     REQUEST                                         * 00255900
*                     RETINFO                                         * 00256000
*                     SCTARGET                                        * 00256100
*                     SDEVN                                           * 00256200
*                     SEXTINFO                                        * 00256300
*                     SEXTNBR                                         * 00256400
*                     SSFILTER                                        * 00256500
*                     SSFSIZE                                         * 00256600
*                     SUCBPTR                                         * 00256700
*                     SVOLSER                                         * 00256800
*                     TDEVN                                           * 00256900
*                     TEXTINFO                                        * 00257000
*                     TEXTNBR                                         * 00257100
*                     TRELOCATE                                       * 00257200
*                     TUCBPTR                                         * 00257300
*                     TVOLSER                                         * 00257400
*                     UCBPTR                                          * 00257500
*                     VLFILTER                                        * 00257600
*                     VLFSIZE                                         * 00257700
*                     VOLSER                                          * 00257800
*                     WAITTIME                                        * 00257900
*                                                                     * 00258000
*    4.  Character Filtering.  The rules for a filter pattern are:    * 00258100
*                                                                     * 00258200
*                                                                     * 00258300
*        o   An  alphanumeric  character represents itself.   CASE of * 00258400
*            the character MATTERS.                                   * 00258500
*                                                                     * 00258600
*        o   A question mark ("?") represents any character.          * 00258700
*                                                                     * 00258800
*        o   An  asterik  ("*")  represents  zero or more characters. * 00258900
*            Two or more consecutive asteriks are redundant.          * 00259000
*                                                                     * 00259100
*        o   Characters  in  square  brackets  ("["  (X'AD')  and "]" * 00259200
*            (X'BD')) represent 'one of' the enclosed characters.  If * 00259300
*            the  first  charcter  in  brackets is a logical not ("^" * 00259400
*            (X'5A'))  the  meaning  changes  to  'not  one  of'  the * 00259500
*            enclosed characters.                                     * 00259600
*                                                                     * 00259700
*        o   A back-slash ("\") allows one of the above characters or * 00259800
*            itself to be used in a pattern.                          * 00259900
*                                                                     * 00260000
*                                                                     * 00260100
*                                                                     * 00260200
*                                                                     * 00260300
*                                                                     * 00260400
*01* CHANGE ACTIVITY:                                                 * 00260500
*                                                                     * 00260600
*   Flag LineItem  FMID    Date   ID    Comment                       * 00260700
*                                                                     * 00260800
*    $00=OW30320  HDZ11E0 970531 TUCRNC:                              * 00260900
*    $01=OW29881  HDZ11E0 980309 TUCRNC:                              * 00261000
*    $02=OW35368  HDZ11E0 981026 TUCHAD:                              * 00261100
*    $03=OW33608  HDZ11E0 980901 TUCRNC:                              * 00261200
*    $04=OW37501  HDZ11E0 990210 TUCHAD:                              * 00261300
*    $05=OWxxxxx  HDZ11E0 990629 TUCRNC:                              * 00261400
****** END OF SPECIFICATIONS *****************************************/ 00261500
*/* TLINE 1 *********************************************************** 00261600
         MACRO                                                          00261700
