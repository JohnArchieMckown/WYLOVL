               AIF   (N'&RSNCODE GT 1).ZE193B                           00992900
.*             ANOP              MULTIPLE PARMS                         00993000
&XRSNCODE      SETC  '&RSNCODE'  SET XVARIABLE                          00993100
.ZE193B        ANOP  ,                                                  00993200
.*-------------------------------------------------------------------   00993300
.***AE.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00993400
               AIF   (K'&ZXRSNCODE LE 0).ZE193R  NOT I/O                00993500
               AIF   ('&XRSNCODE'(1,1) NE '(').ZE193R                   00993600
&ZCGPR         SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                       00993700
               AIF   ('&ZCGPR'(1,1) NE '0').ZE193P                      00993800
               AIF   (K'&ZCGPR GT 2).ZE193Q                             00993900
               AIF   ('&ZCGPR' EQ '0').ZE193Q                           00994000
&ZCGPR         SETC  '&ZCGPR'(2,1)                                      00994100
               AIF   (T'&ZCGPR NE 'N').ZE193Q   INVALID REG NAME        00994200
.ZE193P        ANOP  ,                                                  00994300
               AIF   (T'&ZCGPR NE 'N').ZE193R   NON NUMERIC             00994400
               AIF   ('&ZCGPR' LT '2').ZE193Q                           00994500
               AIF   ('&ZCGPR' LE '12').ZE193R                          00994600
.ZE193Q        ANOP  ,                                                  00994700
&ZMS           SETC  '"RSNCODE='                *KGPRA* MNOTE           00994800
&ZMS           SETC  '&ZMS.&RSNCODE"'                                   00994900
&ZMS           SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'          00995000
               MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                   00995100
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00995200
.ZE193R        ANOP  ,                                                  00995300
.******************************************************************     00995400
.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00995500
.******************************************************************     00995600
               ACTR  10000                                              00995700
.*----------------------------------------------------------------***   00995800
.***AF.P02 EXTRA POSITIONALS                                            00995900
               AIF   (N'&SYSLIST LE 0).ZELISTX  NO EXTRAS               00996000
&ZCVAL         SETC  ''          ERROR FIELD                            00996100
&ZAWRK         SETA  0           POSITIONAL NUMBER                      00996200
.ZELIST2       ANOP  ,                                                  00996300
&ZAWRK         SETA  &ZAWRK+1    NEXT POSITIONAL                        00996400
               AIF   (&ZAWRK GT N'&SYSLIST).ZELIST3  DONE               00996500
&ZCVAL         SETC '&ZCVAL,&SYSLIST(&ZAWRK)'   EXTRA                   00996600
               AGO   .ZELIST2                                           00996700
.*                                                                      00996800
.ZELIST3       ANOP  ,                                                  00996900
&ZCVAL         SETC '&ZCVAL'(2,K'&ZCVAL-1)      STRIP COMMA             00997000
&ZMS           SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE           00997100
&ZMS           SETC  '&ZMS "&ZCVAL"'                                    00997200
&ZMS           SETC  '&ZMS CANNOT BE USED.'                             00997300
&ZMS           SETC  '&ZMS NO POSITIONAL ARGUMENTS'                     00997400
               MNOTE 8,' &ZMS ARE ALLOWED.'                             00997500
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00997600
.ZELISTX       ANOP  ,                                                  00997700
