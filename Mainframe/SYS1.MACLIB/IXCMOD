&ZOPN1                SETA &ZOPN                                        00105800
.*******************************************************************    00105900
.***  PROCESS IXCMOD OUTPUT PARAMETERS                                  00106000
.*******************************************************************    00106100
                      ACTR  10000                                       00106200
.*******************************************************************    00106300
.***AO.X02  RETURN CODE PROCESSING                                      00106400
.*******************************************************************    00106500
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00106600
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00106700
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00106800
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00106900
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00107000
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00107100
&ZOPN                 SETA  &ZOPN+1                                     00107200
&ZOPL(&ZOPN)          SETC  ' '                                         00107300
&ZOPO(&ZOPN)          SETC  'ST'                                        00107400
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00107500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00107600
                      AGO   .ZMRETX                                     00107700
.*                                                                      00107800
.ZMRET2               ANOP  ,                                           00107900
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00108000
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00108100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00108200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00108300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00108400
&ZOPN                 SETA  &ZOPN+1                                     00108500
&ZOPL(&ZOPN)          SETC  ' '                                         00108600
&ZOPO(&ZOPN)          SETC  'LR'                                        00108700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00108800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00108900
.ZMRETX               ANOP  ,                                           00109000
.*******************************************************************    00109100
.***AO.X03  REASON CODE PROCESSING                                      00109200
.*******************************************************************    00109300
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00109400
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00109500
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00109600
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00109700
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00109800
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00109900
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00110000
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00110100
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00110200
&ZOPN                 SETA  &ZOPN+1                                     00110300
&ZOPL(&ZOPN)          SETC  ' '                                         00110400
&ZOPO(&ZOPN)          SETC  'ST'                                        00110500
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00110600
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00110700
                      AGO   .ZMRSNX                                     00110800
.*                                                                      00110900
.ZMRSN2               ANOP  ,                                           00111000
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00111100
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00111200
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00111300
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00111400
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00111500
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00111600
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00111700
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00111800
&ZOPN                 SETA  &ZOPN+1                                     00111900
&ZOPL(&ZOPN)          SETC  ' '                                         00112000
&ZOPO(&ZOPN)          SETC  'LR'                                        00112100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00112200
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00112300
.ZMRSNX               ANOP  ,                                           00112400
.*******************************************************************    00112500
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00112600
.*******************************************************************    00112700
.ZMFLX                ANOP  ,              MF=L PROCESSING              00112800
.*******************************************************************    00112900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00113000
.*******************************************************************    00113100
                      ACTR  10000                                       00113200
.*                                                                      00113300
                      LCLA  &ZLC           COLUMN POSITION              00113400
                      LCLA  &ZOPX          LIST TABLE INDEX             00113500
                      LCLC  &ZLL           LABEL                        00113600
                      LCLC  &ZLOP          OPERATION                    00113700
                      LCLC  &ZLV           VALUE                        00113800
                      LCLC  &ZLD           DESCRIPTION                  00113900
.*                                                                      00114000
&ZOPX                 SETA  1                                           00114100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00114200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00114300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00114400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00114500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00114600
                      AIF   (K'&ZLD LE 0).ZL5                           00114700
&ZLC                  SETA  35             COMMENT COLUMN               00114800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00114900
&ZLC                  SETA  33             COMMMENT COLUMN              00115000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00115100
&ZLV                  SETC  '&ZLV'.' '                                  00115200
                      AGO   .ZL3                                        00115300
.*                                                                      00115400
.ZL4                  ANOP  ,                                           00115500
&ZLV                  SETC  '&ZLV'.' '                                  00115600
.ZL5                  ANOP  ,                                           00115700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00115800
.*                                                                      00115900
&ZLL     &ZLOP &ZLV                                                     00116000
.*                                                                      00116100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00116200
                      AGO   .ZL2           CONTINUE LOOPING             00116300
.*                                                                      00116400
.ZLX                  ANOP  ,                                           00116500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00116600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00116700
         L     14,16(0,0)              GET CVT ADDRESS                  00116800
         L     14,772(14,0)            GET SFT ADDRESS                  00116900
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00117000
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00117100
         OR    14,15                   GET PC NUMBER                    00117200
         PC    0(14)                                                    00117300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00117400
.ZLINKX               ANOP  ,                                           00117500
.*******************************************************************    00117600
.***AP.X01  FORMAT GENERATED CODE                                       00117700
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00117800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00117900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00118000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00118100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00118200
                      AIF   (K'&ZLD LE 0).ZL51                          00118300
&ZLC                  SETA  35             COMMENT COLUMN               00118400
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00118500
&ZLC                  SETA  33             COMMENT COLUMN               00118600
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00118700
&ZLV                  SETC  '&ZLV'.' '                                  00118800
                      AGO   .ZL31                                       00118900
.*                                                                      00119000
.ZL41                 ANOP  ,                                           00119100
&ZLV                  SETC  '&ZLV'.' '                                  00119200
.ZL51                 ANOP  ,                                           00119300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00119400
.*                                                                      00119500
&ZLL     &ZLOP &ZLV                                                     00119600
.*                                                                      00119700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00119800
                      AGO   .ZL21          CONTINUE LOOPING             00119900
.*                                                                      00120000
.ZLX1                 ANOP  ,                                           00120100
.*******************************************************************    00120200
.***AP.X02  END OF IXCMOD MACRO                                         00120300
&ZIXCMOD              SETC  'YES'          SET IXCMOD USED              00120400
         POP   PRINT                                                    00120500
*                                            IXCMOD-0                   00120600
         SPACE 1                                                        00120700
.ZLX2                 ANOP  ,                                           00120800
         MEND                                                           00120900
