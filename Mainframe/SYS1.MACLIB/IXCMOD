.*******************************************************************    00035800
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00035900
.*******************************************************************    00036000
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00036100
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00036200
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00036300
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00036400
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00036500
              LCLA  &ZAWRK      USED AS A WORK AREA                     00036600
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00036700
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00036800
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00036900
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00037000
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00037100
              LCLA  &ZMACRET    MACRO RETURN CODE                       00037200
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00037300
.*                                                                      00037400
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00037500
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00037600
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00037700
              LCLC  &ZENDBASEPL                 End of base PL          00037800
.********************************************************************   00037900
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00038000
.********************************************************************   00038100
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00038200
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00038300
.*                                                                      00038400
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00038500
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00038600
              SPLEVEL  TEST                                             00038700
.ZSYS2        ANOP  ,                                                   00038800
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00038900
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00039000
              SYSSTATE TEST                                             00039100
              AGO   .ZSYS4                                              00039200
.ZSYS3        ANOP  ,                                                   00039300
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00039400
.ZSYS4        ANOP  ,                                                   00039500
.*******************************************************************    00039600
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00039700
.*******************************************************************    00039800
              LCLC  &XTARGET    ++ NAME                                 00039900
              LCLC  &XINTERVAL  ++ NAME                                 00040000
              LCLC  &XRETCODE   ++ NAME                                 00040100
              LCLC  &XRSNCODE   ++ NAME                                 00040200
              LCLC  &XMF        ++ VALUE                                00040300
              LCLC  &XMFCTRL    ++ NAME                                 00040400
              LCLC  &XMFATTR    ++ VALUE                                00040500
              LCLC  &XMFSCK     ++ VALUE                                00040600
.*******************************************************************    00040700
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00040800
.*******************************************************************    00040900
              LCLC  &ZXTARGET   ++ INPUT NAME                           00041000
              LCLC  &ZXINTERVAL                 ++ INPUT NAME           00041100
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00041200
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00041300
              LCLC  &ZXMF       ++ INPUT VALUE                          00041400
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00041500
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00041600
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00041700
.********************************************************************   00041800
.***AG.MF01 PROCESS MF MACRO FORMS                                      00041900
.********************************************************************   00042000
              ACTR  10000                                               00042100
.*------------------------------------------------------------------    00042200
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00042300
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00042400
.*-------------------------------------------------------------------   00042500
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00042600
.ZG15         ANOP  ,                                                   00042700
&ZTLC         SETC 'S'                                                  00042800
              AIF   ('&MF(1)' NE '&ZTLC').ZG16                          00042900
&XMF          SETC  'S'         SET KEYWORD                             00043000
              AIF   (N'&MF LE 1).ZG15A          CK NUM                  00043100
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00043200
&ZMS          SETC  '&ZMS.&MF"'                                         00043300
&ZMS          SETC  '&ZMS "MF"'                                         00043400
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00043500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00043600
.ZG15A        ANOP  ,                                                   00043700
.*-------------------------------------------------------------------   00043800
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00043900
.ZG16         ANOP  ,                                                   00044000
&ZTLC         SETC 'L'                                                  00044100
              AIF   ('&MF(1)' NE '&ZTLC').ZG23                          00044200
&XMF          SETC  'L'         SET KEYWORD                             00044300
              AIF   (N'&MF LE 3).ZG16A          CK NUM                  00044400
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00044500
&ZMS          SETC  '&ZMS.&MF"'                                         00044600
&ZMS          SETC  '&ZMS "MF"'                                         00044700
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00044800
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00044900
.ZG16A        ANOP  ,                                                   00045000
.*-------------------------------------------------------------------   00045100
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00045200
              AIF   (N'&SYSLIST NE 0).ZG16B     POSITIONALS?            00045300
              AIF   ('&TARGET' NE '').ZG16B     KEYS?                   00045400
              AIF   ('&INTERVAL' NE '').ZG16B   KEYS?                   00045500
              AIF   ('&RETCODE' NE '').ZG16B    KEYS?                   00045600
              AIF   ('&RSNCODE' NE '').ZG16B    KEYS?                   00045700
              AGO    .ZG16C     INVOCATION OKAY                         00045800
.ZG16B        ANOP  ,                                                   00045900
&ZMS          SETC  'THE "L" FORM OF "MF" CANNOT'  *MFLA* MNOTE         00046000
&ZMS          SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                   00046100
&ZMS          SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'              00046200
              MNOTE 8,' &ZMS ON THE INVOCATION.'                        00046300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00046400
.ZG16C        ANOP  ,                                                   00046500
.*-------------------------------------------------------------------   00046600
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00046700
&ZXMFCTRL     SETC  'INPUT'     SET IN/OUTPUT                           00046800
&XMFCTRL      SETC  '&MF(2,1)'  SET XVARIABLE                           00046900
              AIF   (N'&MF(2) GT 1).ZG18B       MULTI PARMS             00047000
&XMFCTRL      SETC  '&MF(2)'    SET XVARIABLE                           00047100
.ZG18B        AIF   (K'&MF(2) GT 0).ZG18C                               00047200
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00047300
&ZMS          SETC  'WHEN "L" IS SPECIFIED'     *KRQDA*                 00047400
&ZMS          SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'                 00047500
              MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                        00047600
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00047700
.ZG18C        ANOP  ,                                                   00047800
.*-------------------------------------------------------------------   00047900
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00048000
              AIF   (K'&ZXMFCTRL LE 0).ZG18P    NOT I/O                 00048100
              AIF   ('&XMFCTRL'(1,1) NE '(').ZG18P                      00048200
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00048300
&ZMS          SETC  '"MF='      *KGPRA* MNOTE                           00048400
&ZMS          SETC  '&ZMS.&MF"'                                         00048500
