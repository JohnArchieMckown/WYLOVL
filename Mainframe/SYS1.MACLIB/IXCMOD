.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00095100
&ZPLLEN               SETA 24                                           00095200
.*                                                                      00095300
.******************************************************************     00095400
.***AI.L01 IXCMOD FMT=LIST FILL IN PARAMETER AREA                       00095500
.******************************************************************     00095600
                      ACTR  10000                                       00095700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00095800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00095900
&ZCVAL                SETC  'LAE'          XAX MODE                     00096000
.ZIMFXA               ANOP ,                                            00096100
.*                                                                      00096200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00096300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00096400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00096500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00096600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00096700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00096800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00096900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00097000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00097100
&ZOPN                 SETA  &ZOPN+1                                     00097200
&ZOPL(&ZOPN)          SETC  ' '                                         00097300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00097400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00097500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00097600
                      AGO   .ZIMFNX                                     00097700
.*                                                                      00097800
.ZIMFN2               ANOP ,                                            00097900
&ZOPN                 SETA  &ZOPN+1                                     00098000
&ZOPL(&ZOPN)          SETC  ' '                                         00098100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00098200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00098300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00098400
.ZIMFNX               ANOP ,                                            00098500
.********************************************************************   00098600
.***AI.L02 IXCMOD FMT=LIST INITIALIZE PARAMETER AREA                    00098700
