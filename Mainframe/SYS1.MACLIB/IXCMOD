&ZOPL(&ZOPN)          SETC  ' '                                         00087100
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00087200
&ZOPV(&ZOPN)          SETC  '1'                                         00087300
&ZOPD(&ZOPN)          SETC  ' '                                         00087400
.*                                                                      00087500
.ZLMFSL2              ANOP  ,                                           00087600
.*-------------------------------------------------------------------   00087700
.***AM.L01  IXCMOD FMT=LIST PARAMETERS                                  00087800
