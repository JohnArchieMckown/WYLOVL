.*                                                                      00102900
.ZI4E                 ANOP  ,                                           00103000
.*-------------------------------------------------------------------   00103100
.***AI.L25  XINTERVAL F                                                 00103200
                      AIF   ('&ZXINTERVAL' EQ '').ZI5E  NOT USED        00103300
                      AIF   ('&ZXINTERVAL' EQ 'OUTPUT').ZI5E            00103400
&ZCGPR                SETC  '&XINTERVAL'                                00103500
.*                                                                      00103600
                      AIF   ('&XINTERVAL'(1,1) NE '(').ZI5B             00103700
&ZCGPR   SETC  '0('.'&XINTERVAL'(2,K'&XINTERVAL-2).')'                  00103800
                      AGO   .ZI5D                                       00103900
.ZI5B                 ANOP  ,                                           00104000
.*                                                                      00104100
&ZC                   SETC  '&XINTERVAL'(1,1)                           00104200
                      AIF   (T'&ZC NE 'N').ZI5D                         00104300
&ZC                   SETC  '&XINTERVAL'                                00104400
