.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00120300
.*******************************************************************    00120400
                      ACTR  10000                                       00120500
.*                                                                      00120600
                      LCLA  &ZLC           COLUMN POSITION              00120700
                      LCLA  &ZOPX          LIST TABLE INDEX             00120800
                      LCLC  &ZLL           LABEL                        00120900
                      LCLC  &ZLOP          OPERATION                    00121000
                      LCLC  &ZLV           VALUE                        00121100
                      LCLC  &ZLD           DESCRIPTION                  00121200
.*                                                                      00121300
&ZOPX                 SETA  1                                           00121400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00121500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00121600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00121700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00121800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00121900
                      AIF   (K'&ZLD LE 0).ZL5                           00122000
&ZLC                  SETA  35             COMMENT COLUMN               00122100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00122200
&ZLC                  SETA  33             COMMMENT COLUMN              00122300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00122400
&ZLV                  SETC  '&ZLV'.' '                                  00122500
                      AGO   .ZL3                                        00122600
.*                                                                      00122700
.ZL4                  ANOP  ,                                           00122800
&ZLV                  SETC  '&ZLV'.' '                                  00122900
.ZL5                  ANOP  ,                                           00123000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00123100
.*                                                                      00123200
&ZLL     &ZLOP &ZLV                                                     00123300
.*                                                                      00123400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00123500
                      AGO   .ZL2           CONTINUE LOOPING             00123600
.*                                                                      00123700
.ZLX                  ANOP  ,                                           00123800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00123900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00124000
         L     14,16(0,0)         Get CVT address                       00124100
         L     14,140(0,14)       Get ECVT address                      00124200
         L     14,284(0,14)       Get JCCT address                      00124300
         L     14,20(0,14)        Get PC Number for IXZXIXCL service    00124400
*                                  routine                              00124500
         PC    0(14)              Call the IXZXIXCL service routine     00124600
         AIF   ('&XMFCTRL'(1,1) NE '(').IXCL200                         00124700
         LR    1,&XMFCTRL         Restore the parameter list address    00124800
         AGO   .IXCL250                                                 00124900
.IXCL200 ANOP                                                           00125000
         LA    1,&XMFCTRL         Restore the parameter list address    00125100
.IXCL250 ANOP                                                           00125200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00125300
.ZLINKX               ANOP  ,                                           00125400
.*******************************************************************    00125500
.***AP.X01  FORMAT GENERATED CODE                                       00125600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00125700
