&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00125900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00126000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00126100
                      AIF   (K'&ZLD LE 0).ZL51                          00126200
&ZLC                  SETA  35             COMMENT COLUMN               00126300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00126400
&ZLC                  SETA  33             COMMENT COLUMN               00126500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00126600
&ZLV                  SETC  '&ZLV'.' '                                  00126700
                      AGO   .ZL31                                       00126800
.*                                                                      00126900
.ZL41                 ANOP  ,                                           00127000
&ZLV                  SETC  '&ZLV'.' '                                  00127100
.ZL51                 ANOP  ,                                           00127200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00127300
