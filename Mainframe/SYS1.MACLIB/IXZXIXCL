&ZCGPR          SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                      00083900
                AIF   ('&ZCGPR'(1,1) NE '0').ZG11P                      00084000
                AIF   (K'&ZCGPR GT 2).ZG11Q                             00084100
                AIF   ('&ZCGPR' EQ '0').ZG11Q                           00084200
&ZCGPR          SETC  '&ZCGPR'(2,1)                                     00084300
                AIF   (T'&ZCGPR NE 'N').ZG11Q   INVALID REG NAME        00084400
.ZG11P          ANOP  ,                                                 00084500
                AIF   (T'&ZCGPR NE 'N').ZG11R   NON NUMERIC             00084600
                AIF   ('&ZCGPR' LT '2').ZG11Q                           00084700
                AIF   ('&ZCGPR' LE '12').ZG11R                          00084800
.ZG11Q          ANOP  ,                                                 00084900
&ZMS            SETC  '"RSNCODE='               *KGPRA* MNOTE           00085000
&ZMS            SETC  '&ZMS.&RSNCODE"'                                  00085100
&ZMS            SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'         00085200
                MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                  00085300
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00085400
.ZG11R          ANOP  ,                                                 00085500
.******************************************************************     00085600
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00085700
.******************************************************************     00085800
                ACTR  10000                                             00085900
.*----------------------------------------------------------------***   00086000
.***AH.P02 EXTRA POSITIONALS                                            00086100
                AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS              00086200
                AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL              00086300
                AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL          00086400
                AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL           00086500
.ZGLISTX1       ANOP  ,                                                 00086600
&ZCVAL          SETC  ''          ERROR FIELD                           00086700
&ZAWRK          SETA  0           POSITIONAL NUMBER                     00086800
.ZGLIST2        ANOP  ,                                                 00086900
&ZAWRK          SETA  &ZAWRK+1    NEXT POSITIONAL                       00087000
                AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE              00087100
