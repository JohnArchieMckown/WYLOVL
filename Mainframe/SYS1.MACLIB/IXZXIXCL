.***AB.07  DEFINE ALL MACRO VARIABLES                                   00040300
.*******************************************************************    00040400
                LCLC  &XGROUPTOKEN              ++ NAME                 00040500
                LCLC  &XFAILEDSYS               ++ NAME                 00040600
                LCLC  &XRTNCODE   ++ NAME                               00040700
                LCLC  &XRSNCODE   ++ NAME                               00040800
                LCLA  &XPLISTVER  ++ VALUE                              00040900
                LCLC  &XMF        ++ VALUE                              00041000
                LCLC  &XMFCTRL    ++ NAME                               00041100
                LCLC  &XMFATTR    ++ VALUE                              00041200
                LCLC  &XMFSCK     ++ VALUE                              00041300
.*******************************************************************    00041400
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00041500
.*******************************************************************    00041600
                LCLC  &ZXGROUPTOKEN             ++ INPUT NAME           00041700
                LCLC  &ZXFAILEDSYS              ++ INPUT NAME           00041800
                LCLC  &ZXRTNCODE  ++ OUTPUT NAME                        00041900
                LCLC  &ZXRSNCODE  ++ OUTPUT NAME                        00042000
                LCLC  &ZXPLISTVER               ++ INPUT VALUE          00042100
                LCLC  &ZXMF       ++ INPUT VALUE                        00042200
                LCLC  &ZXMFCTRL   ++ INPUT NAME                         00042300
                LCLC  &ZXMFATTR   ++ INPUT VALUE                        00042400
                LCLC  &ZXMFSCK    ++ INPUT VALUE                        00042500
.********************************************************************   00042600
.***AC.PLV01  PROCESS PLISTVER                                      *   00042700
.********************************************************************   00042800
&XPLISTVER      SETA  1                                                 00042900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00043000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00043100
&XPLISTVER      SETA  1                                                 00043200
.ZPLV0          ANOP  ,                                                 00043300
.*                                                                      00043400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00043500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00043600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00043700
.*-------------------------------------------------------------------   00043800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00043900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00044000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00044100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00044200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00044300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00044400
                AGO   .ZPVCK2                                           00044500
.ZPVCK3         ANOP  ,                                                 00044600
.*-------------------------------------------------------------------   00044700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00044800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00044900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00045000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00045100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00045200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00045300
                AGO   .ZPVCKE                                           00045400
.ZPVCK2         ANOP  ,                                                 00045500
.*-------------------------------------------------------------------   00045600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00045700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00045800
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00045900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00046000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00046100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00046200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00046300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00046400
                AGO   .ZPVCK5                                           00046500
.ZPVCK4         ANOP  ,                                                 00046600
&XPLISTVER      SETA  &PLISTVER(1)                                      00046700
.ZPVCK5         ANOP  ,                                                 00046800
.*-------------------------------------------------------------------   00046900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00047000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00047100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00047200
                AIF   (&XPLISTVER LT 1).ZPVCK6  TOO LOW                 00047300
                AIF   (&XPLISTVER LE 1).ZPVCKE  NOT HIGH                00047400
.ZPVCK6         ANOP   ,                                                00047500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00047600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00047700
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00047800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00047900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00048000
.ZPVCKE         ANOP   ,                                                00048100
.********************************************************************   00048200
.***AG.MF01 PROCESS MF MACRO FORMS                                      00048300
.********************************************************************   00048400
                ACTR  10000                                             00048500
.*------------------------------------------------------------------    00048600
