.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00711300
.******************************************************************     00711400
                     ACTR  10000                                        00711500
.*-------------------------------------------------------------------   00711600
.***AF.P03 EXTRA POSITIONALS AMONG NON-X KEYS                           00711700
&ZCVAL               SETC  ''          ERROR FIELD                      00711800
&ZAWRK               SETA  0           POSITIONAL COUNTER               00711900
&ZASYSLNDX           SETA  0           NON-X KEY COUNTER                00712000
.ZELIST2             ANOP  ,                                            00712100
&ZASYSLNDX           SETA  &ZASYSLNDX+1         NEXT NON-X KEY          00712200
         AIF   (&ZASYSLNDX GT N'&SYSLIST).ZELIST3  DONE                 00712300
         AIF   ('&ZCSYSLIST(&ZASYSLNDX)' EQ 'NONXKEY').ZELIST2          00712400
&ZAWRK               SETA  &ZAWRK+1    NEXT POSITIONAL                  00712500
                     AIF   (&ZAWRK LE 0).ZELIST2                        00712600
&ZCVAL               SETC '&ZCVAL,&SYSLIST(&ZASYSLNDX)'  EXTRA          00712700
                     AGO   .ZELIST2                                     00712800
.*                                                                      00712900
.ZELIST3  AIF  (K'&ZCVAL LE 0).ZELISTX                                  00713000
&ZCVAL               SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA           00713100
&ZMS                 SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE     00713200
&ZMS                 SETC  '&ZMS "&ZCVAL"'                              00713300
&ZMS                 SETC  '&ZMS CANNOT BE USED.'                       00713400
