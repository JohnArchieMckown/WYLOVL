.***AO.X02  RETURN CODE PROCESSING                                      00875000
.*******************************************************************    00875100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00875200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00875300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00875400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00875500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00875600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00875700
&ZOPN                 SETA  &ZOPN+1                                     00875800
&ZOPL(&ZOPN)          SETC  ' '                                         00875900
&ZOPO(&ZOPN)          SETC  'ST'                                        00876000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00876100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00876200
                      AGO   .ZMRETX                                     00876300
.*                                                                      00876400
.ZMRET2               ANOP  ,                                           00876500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00876600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00876700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00876800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00876900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00877000
&ZOPN                 SETA  &ZOPN+1                                     00877100
&ZOPL(&ZOPN)          SETC  ' '                                         00877200
&ZOPO(&ZOPN)          SETC  'LR'                                        00877300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00877400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00877500
.ZMRETX               ANOP  ,                                           00877600
.*******************************************************************    00877700
.***AO.X03  REASON CODE PROCESSING                                      00877800
.*******************************************************************    00877900
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00878000
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00878100
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00878200
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00878300
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00878400
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00878500
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00878600
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00878700
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00878800
&ZOPN                 SETA  &ZOPN+1                                     00878900
&ZOPL(&ZOPN)          SETC  ' '                                         00879000
&ZOPO(&ZOPN)          SETC  'ST'                                        00879100
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00879200
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00879300
                      AGO   .ZMRSNX                                     00879400
.*                                                                      00879500
.ZMRSN2               ANOP  ,                                           00879600
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00879700
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00879800
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00879900
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00880000
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00880100
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00880200
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00880300
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00880400
&ZOPN                 SETA  &ZOPN+1                                     00880500
&ZOPL(&ZOPN)          SETC  ' '                                         00880600
&ZOPO(&ZOPN)          SETC  'LR'                                        00880700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00880800
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00880900
.ZMRSNX               ANOP  ,                                           00881000
.*******************************************************************    00881100
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00881200
.*******************************************************************    00881300
.ZMFLX                ANOP  ,              MF=L PROCESSING              00881400
.*******************************************************************    00881500
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00881600
.*******************************************************************    00881700
                      ACTR  10000                                       00881800
.*                                                                      00881900
                      LCLA  &ZLC           COLUMN POSITION              00882000
                      LCLA  &ZOPX          LIST TABLE INDEX             00882100
                      LCLC  &ZLL           LABEL                        00882200
                      LCLC  &ZLOP          OPERATION                    00882300
                      LCLC  &ZLV           VALUE                        00882400
                      LCLC  &ZLD           DESCRIPTION                  00882500
.*                                                                      00882600
&ZOPX                 SETA  1                                           00882700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00882800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00882900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00883000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00883100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00883200
                      AIF   (K'&ZLD LE 0).ZL5                           00883300
&ZLC                  SETA  35             COMMENT COLUMN               00883400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00883500
&ZLC                  SETA  33             COMMMENT COLUMN              00883600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00883700
&ZLV                  SETC  '&ZLV'.' '                                  00883800
                      AGO   .ZL3                                        00883900
.*                                                                      00884000
.ZL4                  ANOP  ,                                           00884100
&ZLV                  SETC  '&ZLV'.' '                                  00884200
.ZL5                  ANOP  ,                                           00884300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00884400
.*                                                                      00884500
&ZLL     &ZLOP &ZLV                                                     00884600
.*                                                                      00884700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00884800
                      AGO   .ZL2           CONTINUE LOOPING             00884900
.*                                                                      00885000
.ZLX                  ANOP  ,                                           00885100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00885200
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00885300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00885400
         L     15,16(0,0)           GET CVT ADDRESS                     00885500
         L     15,140(15,0)         GET ECVT ADDRESS                    00885600
         L     15,288(15,0)         GET LSAB ADDRESS                    00885700
         L     15,124(15,0)         GET ENTRY POINT ADDRESS             00885800
         LA    0,5                  EX OF BROWSE SERVICE                00885900
         BALR  14,15                INVOKE SERVICE ROUTINE              00886000
         L     1,0(,1)              RESTORE PARMLIST ADDRESS            00886100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00886200
.ZLINKX               ANOP  ,                                           00886300
.*******************************************************************    00886400
.***AP.X01  FORMAT GENERATED CODE                                       00886500
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00886600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00886700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00886800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00886900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00887000
                      AIF   (K'&ZLD LE 0).ZL51                          00887100
&ZLC                  SETA  35             COMMENT COLUMN               00887200
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00887300
&ZLC                  SETA  33             COMMENT COLUMN               00887400
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00887500
&ZLV                  SETC  '&ZLV'.' '                                  00887600
                      AGO   .ZL31                                       00887700
.*                                                                      00887800
.ZL41                 ANOP  ,                                           00887900
&ZLV                  SETC  '&ZLV'.' '                                  00888000
.ZL51                 ANOP  ,                                           00888100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00888200
.*                                                                      00888300
&ZLL     &ZLOP &ZLV                                                     00888400
.*                                                                      00888500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00888600
                      AGO   .ZL21          CONTINUE LOOPING             00888700
.*                                                                      00888800
.ZLX1                 ANOP  ,                                           00888900
.*******************************************************************    00889000
.***AP.X02  END OF IXGBRWSE MACRO                                       00889100
&ZIXGBRWSE            SETC  'YES'          SET IXGBRWSE USED            00889200
         POP   PRINT                                                    00889300
*                                            IXGBRWSE-2                 00889400
         SPACE 1                                                        00889500
.ZLX2                 ANOP  ,                                           00889600
         MEND                                                           00889700
**********************************************************************/ 00889800
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00889900
*%IXGBRWSE: MACRO KEYS(STREAMTOKEN,REQUEST,OLDEST,YOUNGEST,             00890000
*                 STARTBLOCKID,SEARCH,GMT,VIEW,DIAG,BROWSETOKEN,        00890100
*                 DIRECTION,BUFFER,BUFFLEN,BUFFKEY,BUFFALET,BLKSIZE,    00890200
*                 MULTIBLOCK,RETBLOCKID,TIMESTAMP,RETBLOCKINFO,         00890300
*                 MAXNUMLOGBLOCKS,BLOCKID,POSITION,MODE,ECB,REQDATA,    00890400
*                 ANSAREA,ANSLEN,RETCODE,RSNCODE,PLISTVER,MF);          00890500
*  ANS('?'||MACLABEL||' '||'IXGBRWSP '||MACLIST||                       00890600
*         ' '||MACKEYS||';') COL(MACLMAR);                              00890700
*%END IXGBRWSE;                                                         00890800
