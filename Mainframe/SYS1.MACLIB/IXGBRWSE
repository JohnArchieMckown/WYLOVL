&ZOPD(&ZOPN)          SETC  '++ XREQUEST.'                              00725100
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).READCURSOR'                   00725200
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00725300
.*                                                                      00725400
&ZOPN                 SETA  &ZOPN+1                                     00725500
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._XREQUEST_READBLOCK'              00725600
&ZOPO(&ZOPN)          SETC  'EQU'                                       00725700
