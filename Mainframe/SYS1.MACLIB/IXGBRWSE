                     LCLC  &ZXRETBLOCKINFO      ++ INPUT VALUE          00237800
                     LCLC  &ZXMAXNUMLOGBLOCKS   ++ INPUT NAME           00237900
                     LCLC  &ZXBLKSRCH  ++ INPUT NAME                    00238000
                     LCLC  &ZXBLOCKID  ++ INPUT NAME                    00238100
                     LCLC  &ZXPOSITION          ++ INPUT VALUE          00238200
                     LCLC  &ZXMODE     ++ INPUT VALUE                   00238300
                     LCLC  &ZXECB      ++ INPUT NAME                    00238400
                     LCLC  &ZXREQDATA  ++ INPUT NAME                    00238500
                     LCLC  &ZXANSAREA  ++ INPUT NAME                    00238600
                     LCLC  &ZXANSLEN   ++ INPUT NAME                    00238700
                     LCLC  &ZXRETCODE  ++ OUTPUT NAME                   00238800
                     LCLC  &ZXRSNCODE  ++ OUTPUT NAME                   00238900
                     LCLC  &ZXPLISTVER          ++ INPUT VALUE          00239000
                     LCLC  &ZXMF       ++ INPUT VALUE                   00239100
                     LCLC  &ZXMFCTRL   ++ INPUT NAME                    00239200
                     LCLC  &ZXMFATTR   ++ INPUT VALUE                   00239300
                     LCLC  &ZXMFSCK    ++ INPUT VALUE                   00239400
.********************************************************************   00239500
.***AC.PLV01  PROCESS PLISTVER                                      *   00239600
.********************************************************************   00239700
&XPLISTVER      SETA  0                                                 00239800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00239900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00240000
&XPLISTVER      SETA  2                                                 00240100
.ZPLV0          ANOP  ,                                                 00240200
.*                                                                      00240300
                AIF   ('&DIAG' EQ '').ZPLV1E                            00240400
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00240500
&XPLISTVER      SETA  1                                                 00240600
.ZPLV1E         ANOP  ,                                                 00240700
.*                                                                      00240800
                AIF   ('&MULTIBLOCK' EQ '').ZPLV2E                      00240900
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00241000
&XPLISTVER      SETA  2                                                 00241100
.ZPLV2E         ANOP  ,                                                 00241200
.*                                                                      00241300
                AIF   ('&RETBLOCKINFO' EQ '').ZPLV3E                    00241400
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00241500
&XPLISTVER      SETA  2                                                 00241600
.ZPLV3E         ANOP  ,                                                 00241700
.*                                                                      00241800
                AIF   ('&MAXNUMLOGBLOCKS' EQ '').ZPLV4E                 00241900
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00242000
&XPLISTVER      SETA  2                                                 00242100
.ZPLV4E         ANOP  ,                                                 00242200
.*                                                                      00242300
                AIF   ('&REQDATA' EQ '').ZPLV5E                         00242400
                AIF   (&XPLISTVER GE 1).ZPLV5E                          00242500
&XPLISTVER      SETA  1                                                 00242600
.ZPLV5E         ANOP  ,                                                 00242700
.*                                                                      00242800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00242900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00243000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00243100
.*-------------------------------------------------------------------   00243200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00243300
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00243400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00243500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00243600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00243700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00243800
                AGO   .ZPVCK2                                           00243900
.ZPVCK3         ANOP  ,                                                 00244000
.*-------------------------------------------------------------------   00244100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00244200
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00244300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00244400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00244500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00244600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00244700
                AGO   .ZPVCKE                                           00244800
.ZPVCK2         ANOP  ,                                                 00244900
.*-------------------------------------------------------------------   00245000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00245100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00245200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00245300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00245400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00245500
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00245600
.ZPVCK6         ANOP  ,                                                 00245700
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00245800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00245900
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00246000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00246100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00246200
&XPLISTVER      SETA  2                                                 00246300
                AGO   .ZPVCKE                                           00246400
.*-------------------------------------------------------------------   00246500
.ZPVCK4A        ANOP  ,                                                 00246600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00246700
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00246800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00246900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00247000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00247100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00247200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00247300
                AGO   .ZPVCK5                                           00247400
.ZPVCK4         ANOP  ,                                                 00247500
&XPLISTVER      SETA  &PLISTVER(1)                                      00247600
.ZPVCK5         ANOP  ,                                                 00247700
.ZPVCKE         ANOP  ,                                                 00247800
.********************************************************************   00247900
