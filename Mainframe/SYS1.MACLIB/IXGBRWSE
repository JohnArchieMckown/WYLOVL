.*******************************************************************    00714700
.***AE.01  START OF IXGBRWSE MACRO CODE                                 00714800
.*******************************************************************    00714900
                     LCLC  &M0M0001                 NAME                00715000
&M0M0001             SETC  '&XLABEL'                SET NAME            00715100
                     AIF   (K'&XLABEL NE 0).XLAB2                       00715200
&M0M0001             SETC  'M00M&SYSNDX'            DEFAULT NAME        00715300
.*                                                                      00715400
.XLAB2               ANOP  ,                                            00715500
                     AIF   ('&XMF' EQ 'L').XLAB3                        00715600
&M0M0001 DS    0H                             IXGBRWSE-2                00715700
.XLAB3               ANOP  ,                                            00715800
.*******************************************************************    00715900
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00716000
.*******************************************************************    00716100
              PUSH  PRINT                                               00716200
                     AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON   00716300
                     PRINT OFF         ++ PR01                          00716400
                     AGO   .ZPRNTXX                                     00716500
.ZPRINTX             PRINT ON                                           00716600
.ZPRNTXX             ANOP  ,                                            00716700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00716800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00716900
.********************************************************************   00717000
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00717100
.********************************************************************   00717200
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00717300
                      LCLA  &ZOPN1         INVOCATION INDEX             00717400
                      LCLC  &ZOPL(4095)    LABEL                        00717500
                      LCLC  &ZOPO(4095)    OPERATION                    00717600
                      LCLC  &ZOPV(4095)    VALUE                        00717700
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00717800
.*                                                                      00717900
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00718000
.******************************************************************     00718100
.***AL.L10 IXGBRWSE FMT=LIST MF=S|L PARAMETERS                          00718200
.******************************************************************     00718300
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00718400
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00718500
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00718600
&XMFATTR              SETC  '0D'                                        00718700
.*                                                                      00718800
&ZOPN                 SETA  &ZOPN+1                                     00718900
&ZOPL(&ZOPN)          SETC  ' '                                         00719000
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00719100
&ZOPO(&ZOPN)          SETC  'B'                                         00719200
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00719300
                      AGO   .ZALVL6B                                    00719400
.ZALVL6A              ANOP                                              00719500
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00719600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00719700
.ZALVL6B              ANOP                                              00719800
&ZOPD(&ZOPN)          SETC  '++ IXGBRWSE ARG PARMS'                     00719900
.*                                                                      00720000
&ZOPN                 SETA  &ZOPN+1                                     00720100
&ZOPL(&ZOPN)          SETC  ' '                                         00720200
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00720300
&ZOPV(&ZOPN)          SETC  '1'                                         00720400
&ZOPD(&ZOPN)          SETC  ' '                                         00720500
.*                                                                      00720600
.ZLMFSL2              ANOP  ,                                           00720700
.*-------------------------------------------------------------------   00720800
.***AM.L00  IXGBRWSE FMT=LIST name                                      00720900
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00721000
&ZOPN                 SETA &ZOPN+1                                      00721100
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00721200
&ZOPO(&ZOPN)          SETC 'EQU'                                        00721300
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00721400
&ZOPD(&ZOPN)          SETC '++ IXGBRWSE NAME'                           00721500
.ZLMFSL2M             ANOP  ,                                           00721600
.*-------------------------------------------------------------------   00721700
.***AM.L01  IXGBRWSE FMT=LIST PARAMETERS                                00721800
