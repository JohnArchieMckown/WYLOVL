.*                                                                      00114600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00114700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00114800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00114900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00115000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00115100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00115200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00115300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00115400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00115500
&ZOPN                 SETA  &ZOPN+1                                     00115600
