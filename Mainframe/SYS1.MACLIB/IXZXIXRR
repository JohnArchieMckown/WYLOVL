.***AC.PLV01  PROCESS PLISTVER                                      *   00045000
.********************************************************************   00045100
&XPLISTVER      SETA  1                                                 00045200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00045300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00045400
&XPLISTVER      SETA  1                                                 00045500
.ZPLV0          ANOP  ,                                                 00045600
.*                                                                      00045700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00045800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00045900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00046000
.*-------------------------------------------------------------------   00046100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00046200
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00046300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00046400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00046500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00046600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00046700
                AGO   .ZPVCK2                                           00046800
.ZPVCK3         ANOP  ,                                                 00046900
.*-------------------------------------------------------------------   00047000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00047100
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00047200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00047300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00047400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00047500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00047600
                AGO   .ZPVCKE                                           00047700
.ZPVCK2         ANOP  ,                                                 00047800
.*-------------------------------------------------------------------   00047900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00048000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00048100
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00048200
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00048300
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00048400
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00048500
                MNOTE 8,' &ZMS IS REQUIRED.'                            00048600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00048700
                AGO   .ZPVCK5                                           00048800
.ZPVCK4         ANOP  ,                                                 00048900
&XPLISTVER      SETA  &PLISTVER(1)                                      00049000
.ZPVCK5         ANOP  ,                                                 00049100
.*-------------------------------------------------------------------   00049200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00049300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00049400
