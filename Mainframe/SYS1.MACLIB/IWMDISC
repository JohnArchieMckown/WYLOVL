                      LCLC  &ZLV           VALUE                        00138000
                      LCLC  &ZLD           DESCRIPTION                  00138100
.*                                                                      00138200
&ZOPX                 SETA  1                                           00138300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00138400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00138500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00138600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00138700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00138800
                      AIF   (K'&ZLD LE 0).ZL5                           00138900
&ZLC                  SETA  35             COMMENT COLUMN               00139000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00139100
&ZLC                  SETA  33             COMMMENT COLUMN              00139200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00139300
&ZLV                  SETC  '&ZLV'.' '                                  00139400
                      AGO   .ZL3                                        00139500
.*                                                                      00139600
.ZL4                  ANOP  ,                                           00139700
&ZLV                  SETC  '&ZLV'.' '                                  00139800
.ZL5                  ANOP  ,                                           00139900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00140000
.*                                                                      00140100
&ZLL     &ZLOP &ZLV                                                     00140200
.*                                                                      00140300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00140400
                      AGO   .ZL2           CONTINUE LOOPING             00140500
.*                                                                      00140600
.ZLX                  ANOP  ,                                           00140700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00140800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00140900
         L     14,16(0,0)              GET CVT ADDRESS                  00141000
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00141100
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00141200
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00141300
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00141400
         B     END&SYSNDX              BRANCH TO END LABEL              00141500
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00141600
         L     14,772(14,0)            GET SFT ADDRESS                  00141700
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00141800
         LA    15,6                    GET EX FOR SERVICE ROUTINE       00141900
         OR    14,15                   GET PC NUMBER                    00142000
         PC    0(14)                                                    00142100
END&SYSNDX DS    0H                    END LABEL                        00142200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00142300
.ZLINKX               ANOP  ,                                           00142400
