              AIF   ('&XMF' EQ 'L').XLAB3                               00109500
&M0M0001 DS    0H                             IWMDISC-0                 00109600
.XLAB3        ANOP  ,                                                   00109700
.*******************************************************************    00109800
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00109900
.*******************************************************************    00110000
              PUSH  PRINT                                               00110100
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00110200
              PRINT OFF         ++ PR01                                 00110300
              AGO   .ZPRNTXX                                            00110400
.ZPRINTX      PRINT ON                                                  00110500
.ZPRNTXX      ANOP  ,                                                   00110600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00110700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00110800
.********************************************************************   00110900
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00111000
.********************************************************************   00111100
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00111200
                      LCLA  &ZOPN1         INVOCATION INDEX             00111300
                      LCLC  &ZOPL(4095)    LABEL                        00111400
                      LCLC  &ZOPO(4095)    OPERATION                    00111500
                      LCLC  &ZOPV(4095)    VALUE                        00111600
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00111700
.*                                                                      00111800
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00111900
.******************************************************************     00112000
.***AL.L10 IWMDISC FMT=LIST MF=S|L PARAMETERS                           00112100
.******************************************************************     00112200
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00112300
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00112400
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00112500
&XMFATTR              SETC  '0D'                                        00112600
.*                                                                      00112700
&ZOPN                 SETA  &ZOPN+1                                     00112800
&ZOPL(&ZOPN)          SETC  ' '                                         00112900
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00113000
&ZOPO(&ZOPN)          SETC  'B'                                         00113100
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00113200
                      AGO   .ZALVL6B                                    00113300
.ZALVL6A              ANOP                                              00113400
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00113500
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00113600
.ZALVL6B              ANOP                                              00113700
&ZOPD(&ZOPN)          SETC  '++ IWMDISC ARG PARMS'                      00113800
.*                                                                      00113900
&ZOPN                 SETA  &ZOPN+1                                     00114000
&ZOPL(&ZOPN)          SETC  ' '                                         00114100
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00114200
&ZOPV(&ZOPN)          SETC  '1'                                         00114300
&ZOPD(&ZOPN)          SETC  ' '                                         00114400
.*                                                                      00114500
.ZLMFSL2              ANOP  ,                                           00114600
.*-------------------------------------------------------------------   00114700
