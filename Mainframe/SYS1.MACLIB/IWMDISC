.***AC.PLV01  PROCESS PLISTVER                                      *   00066200
.********************************************************************   00066300
&XPLISTVER      SETA  0                                                 00066400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00066500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00066600
&XPLISTVER      SETA  0                                                 00066700
.ZPLV0          ANOP  ,                                                 00066800
.*                                                                      00066900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00067000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00067100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00067200
.*-------------------------------------------------------------------   00067300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00067400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00067500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00067600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00067700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00067800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067900
                AGO   .ZPVCK2                                           00068000
.ZPVCK3         ANOP  ,                                                 00068100
.*-------------------------------------------------------------------   00068200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00068300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00068400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00068600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00068700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068800
                AGO   .ZPVCKE                                           00068900
.ZPVCK2         ANOP  ,                                                 00069000
.*-------------------------------------------------------------------   00069100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00069200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00069300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00069400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00069500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00069600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00069700
.ZPVCK6         ANOP  ,                                                 00069800
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00069900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00070000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00070100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00070200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070300
&XPLISTVER      SETA  0                                                 00070400
                AGO   .ZPVCKE                                           00070500
.*-------------------------------------------------------------------   00070600
.ZPVCK4A        ANOP  ,                                                 00070700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00070800
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00070900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00071000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00071100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00071200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00071300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071400
                AGO   .ZPVCK5                                           00071500
.ZPVCK4         ANOP  ,                                                 00071600
&XPLISTVER      SETA  &PLISTVER(1)                                      00071700
.ZPVCK5         ANOP  ,                                                 00071800
.ZPVCKE         ANOP  ,                                                 00071900
.********************************************************************   00072000
