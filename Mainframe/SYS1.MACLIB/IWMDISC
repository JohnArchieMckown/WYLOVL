&ZCGPR                SETC  '&XCONNTKN'                                 00127700
.*                                                                      00127800
                      AIF   ('&XCONNTKN'(1,1) NE '(').ZI3B              00127900
&ZCGPR   SETC  '0('.'&XCONNTKN'(2,K'&XCONNTKN-2).')'                    00128000
                      AGO   .ZI3D                                       00128100
.ZI3B                 ANOP  ,                                           00128200
.*                                                                      00128300
&ZC                   SETC  '&XCONNTKN'(1,1)                            00128400
                      AIF   (T'&ZC NE 'N').ZI3D                         00128500
&ZC                   SETC  '&XCONNTKN'                                 00128600
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI3D                00128700
&ZCGPR                SETC  '=BL4''&XCONNTKN'''                         00128800
.*                                                                      00128900
.ZI3D                 ANOP  ,                                           00129000
.*                                                                      00129100
&ZOPN                 SETA  &ZOPN+1                                     00129200
&ZOPL(&ZOPN)          SETC  ' '                                         00129300
&ZOPO(&ZOPN)          SETC  'MVC'                                       00129400
&ZOPV(&ZOPN)          SETC  '4(4,1),&ZCGPR'                             00129500
