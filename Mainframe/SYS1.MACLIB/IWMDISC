.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00121600
&ZPLLEN               SETA 12                                           00121700
.*                                                                      00121800
.******************************************************************     00121900
.***AI.L01 IWMDISC FMT=LIST FILL IN PARAMETER AREA                      00122000
.******************************************************************     00122100
                      ACTR  10000                                       00122200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00122300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00122400
&ZCVAL                SETC  'LAE'          XAX MODE                     00122500
.ZIMFXA               ANOP ,                                            00122600
.*                                                                      00122700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00122800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00122900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00123000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00123100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00123200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00123300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00123400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00123500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00123600
&ZOPN                 SETA  &ZOPN+1                                     00123700
&ZOPL(&ZOPN)          SETC  ' '                                         00123800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00123900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00124000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00124100
                      AGO   .ZIMFNX                                     00124200
.*                                                                      00124300
.ZIMFN2               ANOP ,                                            00124400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00124500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00124600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00124700
&ZOPN                 SETA  &ZOPN+1                                     00124800
&ZOPL(&ZOPN)          SETC  ' '                                         00124900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00125000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00125100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00125200
                      AGO   .ZIMFNX                                     00125300
.ZIMFNX1              ANOP ,                                            00125400
&ZOPN                 SETA  &ZOPN+1                                     00125500
