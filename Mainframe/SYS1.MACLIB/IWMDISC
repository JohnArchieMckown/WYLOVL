.*                                                                      00129700
.ZI3E                 ANOP  ,                                           00129800
.*-------------------------------------------------------------------   00129900
&ZOPN1                SETA &ZOPN                                        00130000
.*******************************************************************    00130100
.***  PROCESS IWMDISC OUTPUT PARAMETERS                                 00130200
.*******************************************************************    00130300
                      ACTR  10000                                       00130400
.*******************************************************************    00130500
.***AO.X02  RETURN CODE PROCESSING                                      00130600
.*******************************************************************    00130700
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00130800
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00130900
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00131000
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00131100
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00131200
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00131300
&ZOPN                 SETA  &ZOPN+1                                     00131400
&ZOPL(&ZOPN)          SETC  ' '                                         00131500
&ZOPO(&ZOPN)          SETC  'ST'                                        00131600
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00131700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00131800
                      AGO   .ZMRETX                                     00131900
.*                                                                      00132000
.ZMRET2               ANOP  ,                                           00132100
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00132200
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00132300
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00132400
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00132500
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00132600
&ZOPN                 SETA  &ZOPN+1                                     00132700
&ZOPL(&ZOPN)          SETC  ' '                                         00132800
&ZOPO(&ZOPN)          SETC  'LR'                                        00132900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00133000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00133100
.ZMRETX               ANOP  ,                                           00133200
.*******************************************************************    00133300
.***AO.X03  REASON CODE PROCESSING                                      00133400
.*******************************************************************    00133500
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00133600
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00133700
