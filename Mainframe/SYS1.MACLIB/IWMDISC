                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00134100
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00134200
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00134300
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00134400
&ZOPN                 SETA  &ZOPN+1                                     00134500
