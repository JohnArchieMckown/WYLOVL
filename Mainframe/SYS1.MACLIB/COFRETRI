.***AC.PLV01  PROCESS PLISTVER                                      *   00054300
.********************************************************************   00054400
&XPLISTVER      SETA  1                                                 00054500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00054600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00054700
&XPLISTVER      SETA  1                                                 00054800
.ZPLV0          ANOP  ,                                                 00054900
.*                                                                      00055000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00055100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00055200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00055300
.*-------------------------------------------------------------------   00055400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00055500
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00055600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00055800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00055900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056000
                AGO   .ZPVCK2                                           00056100
.ZPVCK3         ANOP  ,                                                 00056200
.*-------------------------------------------------------------------   00056300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00056400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00056500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00056600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00056700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00056800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056900
                AGO   .ZPVCKE                                           00057000
.ZPVCK2         ANOP  ,                                                 00057100
.*-------------------------------------------------------------------   00057200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00057300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00057400
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00057500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00057600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00057700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00057800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00057900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00058000
                AGO   .ZPVCK5                                           00058100
.ZPVCK4         ANOP  ,                                                 00058200
&XPLISTVER      SETA  &PLISTVER(1)                                      00058300
.ZPVCK5         ANOP  ,                                                 00058400
.*-------------------------------------------------------------------   00058500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00058600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00058700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00058800
                AIF   (&XPLISTVER LT 1).ZPVCK6  TOO LOW                 00058900
                AIF   (&XPLISTVER LE 1).ZPVCKE  NOT HIGH                00059000
.ZPVCK6         ANOP   ,                                                00059100
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00059200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00059300
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00059400
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00059500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059600
.ZPVCKE         ANOP   ,                                                00059700
.********************************************************************   00059800
