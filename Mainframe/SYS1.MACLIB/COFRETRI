.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00171600
.*******************************************************************    00171700
                      ACTR  10000                                       00171800
.*                                                                      00171900
                      LCLA  &ZLC           COLUMN POSITION              00172000
                      LCLA  &ZOPX          LIST TABLE INDEX             00172100
                      LCLC  &ZLL           LABEL                        00172200
                      LCLC  &ZLOP          OPERATION                    00172300
                      LCLC  &ZLV           VALUE                        00172400
                      LCLC  &ZLD           DESCRIPTION                  00172500
.*                                                                      00172600
&ZOPX                 SETA  1                                           00172700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00172800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00172900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00173000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00173100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00173200
                      AIF   (K'&ZLD LE 0).ZL5                           00173300
&ZLC                  SETA  35             COMMENT COLUMN               00173400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00173500
&ZLC                  SETA  33             COMMMENT COLUMN              00173600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00173700
&ZLV                  SETC  '&ZLV'.' '                                  00173800
                      AGO   .ZL3                                        00173900
.*                                                                      00174000
.ZL4                  ANOP  ,                                           00174100
&ZLV                  SETC  '&ZLV'.' '                                  00174200
.ZL5                  ANOP  ,                                           00174300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00174400
.*                                                                      00174500
&ZLL     &ZLOP &ZLV                                                     00174600
.*                                                                      00174700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00174800
                      AGO   .ZL2           CONTINUE LOOPING             00174900
.*                                                                      00175000
.ZLX                  ANOP  ,                                           00175100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00175200
