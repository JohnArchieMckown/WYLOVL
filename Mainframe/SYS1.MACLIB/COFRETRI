.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00114900
.******************************************************************     00115000
              ACTR  10000                                               00115100
.*----------------------------------------------------------------***   00115200
.***AH.P02 EXTRA POSITIONALS                                            00115300
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00115400
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00115500
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00115600
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00115700
.ZGLISTX1     ANOP  ,                                                   00115800
&ZCVAL        SETC  ''          ERROR FIELD                             00115900
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00116000
.ZGLIST2      ANOP  ,                                                   00116100
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00116200
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00116300
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00116400
              AGO   .ZGLIST2                                            00116500
.*                                                                      00116600
.ZGLIST3      ANOP  ,                                                   00116700
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00116800
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00116900
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00117000
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00117100
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00117200
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00117300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00117400
.ZGLISTX      ANOP  ,                                                   00117500
&ZASYSLNDX    SETA  0                                                   00117600
