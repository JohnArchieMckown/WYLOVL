         PUSH  PRINT                                               @D6A 00198900
         AIF   ('&LIST' EQ 'YES').ASCBLST                          @D6A 00199000
         PRINT OFF                                                 @D6A 00199100
.ASCBLST ANOP                                                      @D6A 00199200
         SPACE 1                                                        00199300
         AIF ('&DSECT' EQ 'NO').ASCB10                                  00199400
ASCB     DSECT                                                          00200002
         AGO .ASCB20                                                    00210002
.ASCB10  ANOP                                                           00220002
         DS    0D                                                       00222002
ASCB     EQU   *                                                        00230002
.ASCB20  ANOP                                                           00240002
ASCBEGIN DS    0D -                BEGINNING OF ASCB                    00250002
ASCBASCB DS    CL4 -               ACRONYM IN EBCDIC -ASCB-             00300002
