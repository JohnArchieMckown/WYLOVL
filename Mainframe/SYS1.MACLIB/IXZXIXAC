.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00067900
.ZG44           ANOP  ,                                                 00068000
&ZTLC           SETC 'S'                                                00068100
                AIF   ('&MF(1)' NE '&ZTLC').ZG45                        00068200
&XMF            SETC  'S'         SET KEYWORD                           00068300
                AIF   (N'&MF LE 1).ZG44A        CK NUM                  00068400
&ZMS            SETC  '"MF='      *KPOSA* MNOTE                         00068500
&ZMS            SETC  '&ZMS.&MF"'                                       00068600
&ZMS            SETC  '&ZMS "MF"'                                       00068700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00068800
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00068900
.ZG44A          ANOP  ,                                                 00069000
.*-------------------------------------------------------------------   00069100
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00069200
.ZG45           ANOP  ,                                                 00069300
&ZTLC           SETC 'L'                                                00069400
                AIF   ('&MF(1)' NE '&ZTLC').ZG52                        00069500
&XMF            SETC  'L'         SET KEYWORD                           00069600
                AIF   (N'&MF LE 3).ZG45A        CK NUM                  00069700
&ZMS            SETC  '"MF='      *KPOSA* MNOTE                         00069800
&ZMS            SETC  '&ZMS.&MF"'                                       00069900
&ZMS            SETC  '&ZMS "MF"'                                       00070000
                MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'              00070100
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00070200
.ZG45A          ANOP  ,                                                 00070300
.*-------------------------------------------------------------------   00070400
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00070500
                AIF   (N'&SYSLIST NE 0).ZG45B   POSITIONALS?            00070600
                AIF   ('&MSGTOKEN' NE '').ZG45B  KEYS?                  00070700
                AIF   ('&USERRC' NE '').ZG45B   KEYS?                   00070800
                AIF   ('&DATA' NE '').ZG45B     KEYS?                   00070900
                AIF   ('&DATALEN' NE '').ZG45B  KEYS?                   00071000
                AIF   ('&GROUPTOKEN' NE '').ZG45B  KEYS?                00071100
                AIF   ('&SYSRC' NE '').ZG45B    KEYS?                   00071200
                AIF   ('&SYSRSN' NE '').ZG45B   KEYS?                   00071300
                AIF   ('&MSGATTR' NE '').ZG45B  KEYS?                   00071400
                AIF   ('&STB' NE '').ZG45B      KEYS?                   00071500
                AIF   ('&RTNCODE' NE '').ZG45B  KEYS?                   00071600
                AIF   ('&RSNCODE' NE '').ZG45B  KEYS?                   00071700
                AGO    .ZG45C     INVOCATION OKAY                       00071800
.ZG45B          ANOP  ,                                                 00071900
&ZMS            SETC  'THE "L" FORM OF "MF"'    *MFLA* MNOTE            00072000
&ZMS            SETC  '&ZMS CAN ONLY HAVE'                              00072100
&ZMS            SETC  '&ZMS THE "PLISTVER" KEY'                         00072200
                MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'            00072300
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00072400
.ZG45C          ANOP  ,                                                 00072500
.*-------------------------------------------------------------------   00072600
