.***AC.PLV01  PROCESS PLISTVER                                      *   00061600
.********************************************************************   00061700
&XPLISTVER      SETA  1                                                 00061800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00061900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00062000
&XPLISTVER      SETA  1                                                 00062100
.ZPLV0          ANOP  ,                                                 00062200
.*                                                                      00062300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00062400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00062500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00062600
.*-------------------------------------------------------------------   00062700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00062800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00062900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00063100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00063200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063300
                AGO   .ZPVCK2                                           00063400
.ZPVCK3         ANOP  ,                                                 00063500
.*-------------------------------------------------------------------   00063600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00063700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00063800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00064000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00064100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064200
                AGO   .ZPVCKE                                           00064300
.ZPVCK2         ANOP  ,                                                 00064400
.*-------------------------------------------------------------------   00064500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00064600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00064700
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00064800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00064900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00065000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00065100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00065200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065300
                AGO   .ZPVCK5                                           00065400
.ZPVCK4         ANOP  ,                                                 00065500
&XPLISTVER      SETA  &PLISTVER(1)                                      00065600
.ZPVCK5         ANOP  ,                                                 00065700
.*-------------------------------------------------------------------   00065800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00065900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00066000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00066100
                AIF   (&XPLISTVER LT 1).ZPVCK6  TOO LOW                 00066200
                AIF   (&XPLISTVER LE 1).ZPVCKE  NOT HIGH                00066300
.ZPVCK6         ANOP   ,                                                00066400
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00066500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066600
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00066700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00066800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066900
.ZPVCKE         ANOP   ,                                                00067000
.********************************************************************   00067100
