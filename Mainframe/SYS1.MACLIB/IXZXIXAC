.***AO.X02  RETURN CODE PROCESSING                                      00200100
.*******************************************************************    00200200
                      AIF   (N'&RTNCODE LE 0).ZMRETX                    00200300
                      AIF   ('&XRTNCODE'(1,1) EQ '(').ZMRET2            00200400
                      AIF   ('&XRTNCODE' EQ 'GPR15').ZMRETX             00200500
                      AIF   ('&XRTNCODE' EQ 'REG15').ZMRETX             00200600
                      AIF   ('&XRTNCODE' EQ 'R15').ZMRETX               00200700
                      AIF   ('&XRTNCODE' EQ '15').ZMRETX                00200800
&ZOPN                 SETA  &ZOPN+1                                     00200900
&ZOPL(&ZOPN)          SETC  ' '                                         00201000
&ZOPO(&ZOPN)          SETC  'ST'                                        00201100
&ZOPV(&ZOPN)          SETC  '15,&XRTNCODE'                              00201200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00201300
                      AGO   .ZMRETX                                     00201400
.*                                                                      00201500
.ZMRET2               ANOP  ,                                           00201600
&ZCGPR                SETC  '&XRTNCODE'(2,K'&XRTNCODE-2)                00201700
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00201800
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00201900
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00202000
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00202100
&ZOPN                 SETA  &ZOPN+1                                     00202200
&ZOPL(&ZOPN)          SETC  ' '                                         00202300
&ZOPO(&ZOPN)          SETC  'LR'                                        00202400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00202500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00202600
.ZMRETX               ANOP  ,                                           00202700
.*******************************************************************    00202800
.***AO.X03  REASON CODE PROCESSING                                      00202900
.*******************************************************************    00203000
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00203100
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00203200
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00203300
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00203400
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00203500
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00203600
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00203700
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00203800
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00203900
&ZOPN                 SETA  &ZOPN+1                                     00204000
&ZOPL(&ZOPN)          SETC  ' '                                         00204100
&ZOPO(&ZOPN)          SETC  'ST'                                        00204200
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00204300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00204400
                      AGO   .ZMRSNX                                     00204500
.*                                                                      00204600
.ZMRSN2               ANOP  ,                                           00204700
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00204800
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00204900
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00205000
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00205100
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00205200
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00205300
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00205400
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00205500
&ZOPN                 SETA  &ZOPN+1                                     00205600
&ZOPL(&ZOPN)          SETC  ' '                                         00205700
&ZOPO(&ZOPN)          SETC  'LR'                                        00205800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00205900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00206000
.ZMRSNX               ANOP  ,                                           00206100
