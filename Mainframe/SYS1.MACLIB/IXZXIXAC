.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00206700
.*******************************************************************    00206800
                      ACTR  10000                                       00206900
.*                                                                      00207000
                      LCLA  &ZLC           COLUMN POSITION              00207100
                      LCLA  &ZOPX          LIST TABLE INDEX             00207200
                      LCLC  &ZLL           LABEL                        00207300
                      LCLC  &ZLOP          OPERATION                    00207400
                      LCLC  &ZLV           VALUE                        00207500
                      LCLC  &ZLD           DESCRIPTION                  00207600
.*                                                                      00207700
&ZOPX                 SETA  1                                           00207800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00207900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00208000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00208100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00208200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00208300
                      AIF   (K'&ZLD LE 0).ZL5                           00208400
&ZLC                  SETA  35             COMMENT COLUMN               00208500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00208600
&ZLC                  SETA  33             COMMMENT COLUMN              00208700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00208800
&ZLV                  SETC  '&ZLV'.' '                                  00208900
                      AGO   .ZL3                                        00209000
.*                                                                      00209100
.ZL4                  ANOP  ,                                           00209200
&ZLV                  SETC  '&ZLV'.' '                                  00209300
.ZL5                  ANOP  ,                                           00209400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00209500
.*                                                                      00209600
&ZLL     &ZLOP &ZLV                                                     00209700
.*                                                                      00209800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00209900
                      AGO   .ZL2           CONTINUE LOOPING             00210000
.*                                                                      00210100
.ZLX                  ANOP  ,                                           00210200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00210300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00210400
         GBLC  &ANVIRON           JES2 environment variable             00210500
         GBLB  &J3MN              JES3 Main Task environment            00210600
         GBLB  &J3ST              JES3 Subtask environment              00210700
         GBLB  &J3FSS             JES3 FSS environment                  00210800
         GBLB  &JSXCF             JESXCF environment                    00210900
         AIF   ('&ANVIRON' EQ 'JES2').IXAC050  Are we running under     00211000
.*                                JES2 Main task                        00211100
         AIF   ('&ANVIRON' EQ 'SUBTASK').IXAC050 Are we running under   00211200
.*                                JES2 Subtask                          00211300
         AIF   (&J3MN).IXAC050    Are we running under JES3 Main task   00211400
         AIF   (&J3ST).IXAC050    Are we running under JES3 Sub task    00211500
         AIF   (&J3FSS).IXAC050   Are we running under JES3 FSS         00211600
         MNOTE 8,'IXZ0001 Running in an invalid environment'            00211700
.IXAC050 ANOP                                                           00211800
         AIF   ('&GROUPTOKEN'(1,1) NE '(').IXAC100                      00211900
         LR    14,&GROUPTOKEN     Load the address of the group token   00212000
         L     14,0(14)           Load the group token                  00212100
         AGO   .IXAC150                                                 00212200
.IXAC100 ANOP                                                           00212300
         L     14,&GROUPTOKEN     Load the group token                  00212400
.IXAC150 ANOP                                                           00212500
         L     14,60(14)          Load the PC number of the             00212600
*                                 routine to be called                  00212700
         PC    0(14)              Call the acknowledge message          00212800
*                                 routine                               00212900
         AIF   ('&XMFCTRL'(1,1) NE '(').IXAC200                         00213000
         LR    1,&XMFCTRL         Restore the parameter list address    00213100
         AGO   .IXAC250                                                 00213200
.IXAC200 ANOP                                                           00213300
         LA    1,&XMFCTRL         Restore the parameter list address    00213400
.IXAC250 ANOP                                                           00213500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00213600
.ZLINKX               ANOP  ,                                           00213700
