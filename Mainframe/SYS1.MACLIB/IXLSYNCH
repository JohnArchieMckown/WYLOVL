.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00116700
.*******************************************************************    00116800
                      ACTR  10000                                       00116900
.*                                                                      00117000
                      LCLA  &ZLC           COLUMN POSITION              00117100
                      LCLA  &ZOPX          LIST TABLE INDEX             00117200
                      LCLC  &ZLL           LABEL                        00117300
                      LCLC  &ZLOP          OPERATION                    00117400
                      LCLC  &ZLV           VALUE                        00117500
                      LCLC  &ZLD           DESCRIPTION                  00117600
.*                                                                      00117700
&ZOPX                 SETA  1                                           00117800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00117900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00118000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00118100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00118200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00118300
                      AIF   (K'&ZLD LE 0).ZL5                           00118400
&ZLC                  SETA  35             COMMENT COLUMN               00118500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00118600
&ZLC                  SETA  33             COMMMENT COLUMN              00118700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00118800
&ZLV                  SETC  '&ZLV'.' '                                  00118900
                      AGO   .ZL3                                        00119000
.*                                                                      00119100
.ZL4                  ANOP  ,                                           00119200
&ZLV                  SETC  '&ZLV'.' '                                  00119300
.ZL5                  ANOP  ,                                           00119400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00119500
.*                                                                      00119600
&ZLL     &ZLOP &ZLV                                                     00119700
.*                                                                      00119800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00119900
                      AGO   .ZL2           CONTINUE LOOPING             00120000
.*                                                                      00120100
.ZLX                  ANOP  ,                                           00120200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00120300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00120400
         L     15,16(0,0)              GET CVT ADDRESS             @D3C 00120500
         L     15,140(15,0)            GET ECVT ADDRESS            @D3C 00120600
         L     15,256(15,0)            GET XRAT ADDRESS            @D3C 00120700
         L     15,28(15,0)             IXLSYNCH Service Routine    @D3C 00120800
         AIF   ('&SYSASCE' NE 'P').BR1 CHECK FOR AR MODE                00120900
         LAE   1,0(1,0)                MAKE SURE AR1 IS CLEARED         00121000
.BR1     ANOP                                                           00121100
         BALR  14,15                   BRANCH TO ROUTINE           @D3C 00121200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00121300
.ZLINKX               ANOP  ,                                           00121400
.*******************************************************************    00121500
.***AP.X01  FORMAT GENERATED CODE                                       00121600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00121700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00121800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00121900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00122000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00122100
                      AIF   (K'&ZLD LE 0).ZL51                          00122200
&ZLC                  SETA  35             COMMENT COLUMN               00122300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00122400
&ZLC                  SETA  33             COMMENT COLUMN               00122500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00122600
&ZLV                  SETC  '&ZLV'.' '                                  00122700
                      AGO   .ZL31                                       00122800
.*                                                                      00122900
.ZL41                 ANOP  ,                                           00123000
&ZLV                  SETC  '&ZLV'.' '                                  00123100
.ZL51                 ANOP  ,                                           00123200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00123300
.*                                                                      00123400
&ZLL     &ZLOP &ZLV                                                     00123500
.*                                                                      00123600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00123700
                      AGO   .ZL21          CONTINUE LOOPING             00123800
.*                                                                      00123900
.ZLX1                 ANOP  ,                                           00124000
.*******************************************************************    00124100
.***AP.X02  END OF IXLSYNCH MACRO                                       00124200
