.ZIMFXA               ANOP ,                                            00103000
.*                                                                      00103100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00103200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00103300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00103400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00103500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00103600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00103700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00103800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00103900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00104000
&ZOPN                 SETA  &ZOPN+1                                     00104100
&ZOPL(&ZOPN)          SETC  ' '                                         00104200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00104300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00104400
