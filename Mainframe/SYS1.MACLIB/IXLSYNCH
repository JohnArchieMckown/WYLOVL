&XRSNCODE     SETC  '&RSNCODE'  SET XVARIABLE                           00085500
.ZG8C         ANOP  ,                                                   00085600
.*-------------------------------------------------------------------   00085700
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00085800
              AIF   (K'&ZXRSNCODE LE 0).ZG8R    NOT I/O                 00085900
              AIF   ('&XRSNCODE'(1,1) NE '(').ZG8R                      00086000
&ZCGPR        SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                        00086100
              AIF   ('&ZCGPR'(1,1) NE '0').ZG8P                         00086200
              AIF   (K'&ZCGPR GT 2).ZG8Q                                00086300
              AIF   ('&ZCGPR' EQ '0').ZG8Q                              00086400
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00086500
              AIF   (T'&ZCGPR NE 'N').ZG8Q      INVALID REG NAME        00086600
.ZG8P         ANOP  ,                                                   00086700
              AIF   (T'&ZCGPR NE 'N').ZG8R      NON NUMERIC             00086800
              AIF   ('&ZCGPR' LT '2').ZG8Q                              00086900
              AIF   ('&ZCGPR' LE '12').ZG8R                             00087000
.ZG8Q         ANOP  ,                                                   00087100
&ZMS          SETC  '"RSNCODE='                 *KGPRA* MNOTE           00087200
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00087300
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00087400
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00087500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00087600
.ZG8R         ANOP  ,                                                   00087700
.******************************************************************     00087800
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00087900
.******************************************************************     00088000
              ACTR  10000                                               00088100
.*----------------------------------------------------------------***   00088200
.***AH.P02 EXTRA POSITIONALS                                            00088300
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00088400
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00088500
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00088600
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00088700
.ZGLISTX1     ANOP  ,                                                   00088800
&ZCVAL        SETC  ''          ERROR FIELD                             00088900
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00089000
.ZGLIST2      ANOP  ,                                                   00089100
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00089200
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00089300
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00089400
              AGO   .ZGLIST2                                            00089500
.*                                                                      00089600
.ZGLIST3      ANOP  ,                                                   00089700
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00089800
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00089900
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00090000
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00090100
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00090200
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00090300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00090400
.ZGLISTX      ANOP  ,                                                   00090500
&ZASYSLNDX    SETA  0                                                   00090600
