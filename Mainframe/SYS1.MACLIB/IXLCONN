.***AH.P03 EXTRA POSITIONALS AMONG NON-X KEYS                           00659200
&ZCVAL              SETC  ''          ERROR FIELD                       00659300
&ZAWRK              SETA  0           POSITIONAL COUNTER                00659400
&ZASYSLNDX          SETA  0           NON-X KEY COUNTER                 00659500
.ZGLIST2            ANOP  ,                                             00659600
&ZASYSLNDX          SETA  &ZASYSLNDX+1          NEXT NON-X KEY          00659700
         AIF   (&ZASYSLNDX GT N'&SYSLIST).ZGLIST3  DONE                 00659800
         AIF   ('&ZCSYSLIST(&ZASYSLNDX)' EQ 'NONXKEY').ZGLIST2          00659900
&ZAWRK              SETA  &ZAWRK+1    NEXT POSITIONAL                   00660000
                    AIF   (&ZAWRK LE 0).ZGLIST2                         00660100
&ZCVAL              SETC '&ZCVAL,&SYSLIST(&ZASYSLNDX)'  EXTRA           00660200
                    AGO   .ZGLIST2                                      00660300
.*                                                                      00660400
.ZGLIST3  AIF  (K'&ZCVAL LE 0).ZGLISTX                                  00660500
&ZCVAL              SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA            00660600
&ZMS                SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE      00660700
&ZMS                SETC  '&ZMS "&ZCVAL"'                               00660800
&ZMS                SETC  '&ZMS CANNOT BE USED.'                        00660900
&ZMS                SETC  '&ZMS NO POSITIONAL ARGUMENTS'                00661000
                    MNOTE 8,' &ZMS ARE ALLOWED.'                        00661100
&ZMACRET            SETA  8           SET MACRO ERROR RETURN CODE       00661200
.ZGLISTX            ANOP  ,                                             00661300
&ZASYSLNDX          SETA  0                                             00661400
