.***  PROCESS IXLCONN OUTPUT PARAMETERS                                 00906700
.*******************************************************************    00906800
                      ACTR  10000                                       00906900
.*******************************************************************    00907000
.***AO.X02  RETURN CODE PROCESSING                                      00907100
.*******************************************************************    00907200
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00907300
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00907400
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00907500
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00907600
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00907700
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00907800
&ZOPN                 SETA  &ZOPN+1                                     00907900
&ZOPL(&ZOPN)          SETC  ' '                                         00908000
&ZOPO(&ZOPN)          SETC  'ST'                                        00908100
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00908200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00908300
                      AGO   .ZMRETX                                     00908400
.*                                                                      00908500
.ZMRET2               ANOP  ,                                           00908600
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00908700
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00908800
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00908900
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00909000
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00909100
&ZOPN                 SETA  &ZOPN+1                                     00909200
&ZOPL(&ZOPN)          SETC  ' '                                         00909300
&ZOPO(&ZOPN)          SETC  'LR'                                        00909400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00909500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00909600
.ZMRETX               ANOP  ,                                           00909700
.*******************************************************************    00909800
.***AO.X03  REASON CODE PROCESSING                                      00909900
.*******************************************************************    00910000
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00910100
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00910200
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00910300
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00910400
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00910500
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00910600
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00910700
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00910800
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00910900
&ZOPN                 SETA  &ZOPN+1                                     00911000
&ZOPL(&ZOPN)          SETC  ' '                                         00911100
&ZOPO(&ZOPN)          SETC  'ST'                                        00911200
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00911300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00911400
                      AGO   .ZMRSNX                                     00911500
.*                                                                      00911600
.ZMRSN2               ANOP  ,                                           00911700
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00911800
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00911900
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00912000
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00912100
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00912200
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00912300
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00912400
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00912500
&ZOPN                 SETA  &ZOPN+1                                     00912600
&ZOPL(&ZOPN)          SETC  ' '                                         00912700
&ZOPO(&ZOPN)          SETC  'LR'                                        00912800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00912900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00913000
.ZMRSNX               ANOP  ,                                           00913100
.*******************************************************************    00913200
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00913300
.*******************************************************************    00913400
.ZMFLX                ANOP  ,              MF=L PROCESSING              00913500
.*******************************************************************    00913600
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00913700
.*******************************************************************    00913800
                      ACTR  10000                                       00913900
.*                                                                      00914000
                      LCLA  &ZLC           COLUMN POSITION              00914100
                      LCLA  &ZOPX          LIST TABLE INDEX             00914200
                      LCLC  &ZLL           LABEL                        00914300
                      LCLC  &ZLOP          OPERATION                    00914400
                      LCLC  &ZLV           VALUE                        00914500
                      LCLC  &ZLD           DESCRIPTION                  00914600
.*                                                                      00914700
&ZOPX                 SETA  1                                           00914800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00914900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00915000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00915100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00915200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00915300
                      AIF   (K'&ZLD LE 0).ZL5                           00915400
&ZLC                  SETA  35             COMMENT COLUMN               00915500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00915600
&ZLC                  SETA  33             COMMMENT COLUMN              00915700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00915800
&ZLV                  SETC  '&ZLV'.' '                                  00915900
                      AGO   .ZL3                                        00916000
.*                                                                      00916100
.ZL4                  ANOP  ,                                           00916200
&ZLV                  SETC  '&ZLV'.' '                                  00916300
.ZL5                  ANOP  ,                                           00916400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00916500
.*                                                                      00916600
&ZLL     &ZLOP &ZLV                                                     00916700
.*                                                                      00916800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00916900
                      AGO   .ZL2           CONTINUE LOOPING             00917000
.*                                                                      00917100
.ZLX                  ANOP  ,                                           00917200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00917300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00917400
         L     14,16(0,0)              GET CVT ADDRESS                  00917500
         L     14,772(14,0)            GET SFT ADDRESS                  00917600
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00917700
         LA    15,19                   GET EX FOR SERVICE ROUTINE       00917800
         OR    14,15                   GET PC NUMBER                    00917900
         PC    0(14)                                                    00918000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00918100
.ZLINKX               ANOP  ,                                           00918200
.*******************************************************************    00918300
.***AP.X01  FORMAT GENERATED CODE                                       00918400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00918500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00918600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00918700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00918800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00918900
                      AIF   (K'&ZLD LE 0).ZL51                          00919000
&ZLC                  SETA  35             COMMENT COLUMN               00919100
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00919200
&ZLC                  SETA  33             COMMENT COLUMN               00919300
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00919400
&ZLV                  SETC  '&ZLV'.' '                                  00919500
                      AGO   .ZL31                                       00919600
.*                                                                      00919700
.ZL41                 ANOP  ,                                           00919800
&ZLV                  SETC  '&ZLV'.' '                                  00919900
.ZL51                 ANOP  ,                                           00920000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00920100
.*                                                                      00920200
&ZLL     &ZLOP &ZLV                                                     00920300
.*                                                                      00920400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00920500
                      AGO   .ZL21          CONTINUE LOOPING             00920600
.*                                                                      00920700
.ZLX1                 ANOP  ,                                           00920800
.*******************************************************************    00920900
