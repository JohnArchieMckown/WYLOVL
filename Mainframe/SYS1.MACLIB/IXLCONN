.***AC.PLV01  PROCESS PLISTVER                                      *   00296900
.********************************************************************   00297000
&XPLISTVER      SETA  0                                                 00297100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00297200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00297300
&XPLISTVER      SETA  8                                                 00297400
.ZPLV0          ANOP  ,                                                 00297500
.*                                                                      00297600
                AIF   ('&ALLOWDUPREBLD' EQ '').ZPLV1E                   00297700
                AIF   (&XPLISTVER GE 5).ZPLV1E                          00297800
&XPLISTVER      SETA  5                                                 00297900
.ZPLV1E         ANOP  ,                                                 00298000
.*                                                                      00298100
                AIF   ('&ALLOWAUTO' EQ '').ZPLV2E                       00298200
                AIF   (&XPLISTVER GE 7).ZPLV2E                          00298300
&XPLISTVER      SETA  7                                                 00298400
.ZPLV2E         ANOP  ,                                                 00298500
.*                                                                      00298600
                AIF   ('&SUSPEND' EQ '').ZPLV3E                         00298700
                AIF   (&XPLISTVER GE 7).ZPLV3E                          00298800
&XPLISTVER      SETA  7                                                 00298900
.ZPLV3E         ANOP  ,                                                 00299000
.*                                                                      00299100
                AIF   ('&ALLOWALTER' EQ '').ZPLV4E                      00299200
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00299300
&XPLISTVER      SETA  1                                                 00299400
.ZPLV4E         ANOP  ,                                                 00299500
.*                                                                      00299600
                AIF   ('&RATIO' EQ '').ZPLV5E                           00299700
                AIF   (&XPLISTVER GE 1).ZPLV5E                          00299800
&XPLISTVER      SETA  1                                                 00299900
.ZPLV5E         ANOP  ,                                                 00300000
.*                                                                      00300100
                AIF   ('&MINENTRY' EQ '').ZPLV6E                        00300200
                AIF   (&XPLISTVER GE 1).ZPLV6E                          00300300
&XPLISTVER      SETA  1                                                 00300400
.ZPLV6E         ANOP  ,                                                 00300500
.*                                                                      00300600
                AIF   ('&MINELEMENT' EQ '').ZPLV7E                      00300700
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00300800
&XPLISTVER      SETA  1                                                 00300900
.ZPLV7E         ANOP  ,                                                 00301000
.*                                                                      00301100
                AIF   ('&MINEMC' EQ '').ZPLV8E                          00301200
                AIF   (&XPLISTVER GE 4).ZPLV8E                          00301300
&XPLISTVER      SETA  4                                                 00301400
.ZPLV8E         ANOP  ,                                                 00301500
.*                                                                      00301600
                AIF   ('&MINCFLEVEL' EQ '').ZPLV9E                      00301700
                AIF   (&XPLISTVER GE 8).ZPLV9E                          00301800
&XPLISTVER      SETA  8                                                 00301900
.ZPLV9E         ANOP  ,                                                 00302000
.*                                                                      00302100
                AIF   ('&CONNECTIVITY' EQ '').ZPLV10E                   00302200
                AIF   (&XPLISTVER GE 3).ZPLV10E                         00302300
&XPLISTVER      SETA  3                                                 00302400
.ZPLV10E        ANOP  ,                                                 00302500
.*                                                                      00302600
                AIF   ('&UDFORDER' EQ '').ZPLV11E                       00302700
                AIF   (&XPLISTVER GE 5).ZPLV11E                         00302800
&XPLISTVER      SETA  5                                                 00302900
.ZPLV11E        ANOP  ,                                                 00303000
.*                                                                      00303100
                AIF   ('&NAMECLASSMASK' EQ '').ZPLV12E                  00303200
                AIF   (&XPLISTVER GE 6).ZPLV12E                         00303300
&XPLISTVER      SETA  6                                                 00303400
.ZPLV12E        ANOP  ,                                                 00303500
.*                                                                      00303600
                AIF   ('&EMCSTGPCT' EQ '').ZPLV13E                      00303700
                AIF   (&XPLISTVER GE 2).ZPLV13E                         00303800
&XPLISTVER      SETA  2                                                 00303900
.ZPLV13E        ANOP  ,                                                 00304000
.*                                                                      00304100
                AIF   ('&KEYTYPE' EQ '').ZPLV14E                        00304200
                AIF   (&XPLISTVER GE 8).ZPLV14E                         00304300
&XPLISTVER      SETA  8                                                 00304400
.ZPLV14E        ANOP  ,                                                 00304500
.*                                                                      00304600
                AIF   ('&ENTRYIDTYPE' EQ '').ZPLV15E                    00304700
                AIF   (&XPLISTVER GE 8).ZPLV15E                         00304800
&XPLISTVER      SETA  8                                                 00304900
.ZPLV15E        ANOP  ,                                                 00305000
.*                                                                      00305100
                AIF   ('&RNAMELEN' EQ '').ZPLV16E                       00305200
                AIF   (&XPLISTVER GE 3).ZPLV16E                         00305300
&XPLISTVER      SETA  3                                                 00305400
.ZPLV16E        ANOP  ,                                                 00305500
.*                                                                      00305600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00305700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00305800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00305900
.*-------------------------------------------------------------------   00306000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00306100
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00306200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00306300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00306400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00306500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00306600
                AGO   .ZPVCK2                                           00306700
.ZPVCK3         ANOP  ,                                                 00306800
.*-------------------------------------------------------------------   00306900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00307000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00307100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00307200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00307300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00307400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00307500
                AGO   .ZPVCKE                                           00307600
.ZPVCK2         ANOP  ,                                                 00307700
.*-------------------------------------------------------------------   00307800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00307900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00308000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00308100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00308200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00308300
                AIF   (&ZN LE 8).ZPVCK4A   NOT HIGH                     00308400
.ZPVCK6         ANOP  ,                                                 00308500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00308600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00308700
&ZMS            SETC  '&ZMS THE RANGE IS "0-8"'                         00308800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00308900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00309000
&XPLISTVER      SETA  8                                                 00309100
                AGO   .ZPVCKE                                           00309200
.*-------------------------------------------------------------------   00309300
.ZPVCK4A        ANOP  ,                                                 00309400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00309500
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00309600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00309700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00309800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00309900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00310000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00310100
                AGO   .ZPVCK5                                           00310200
.ZPVCK4         ANOP  ,                                                 00310300
&XPLISTVER      SETA  &PLISTVER(1)                                      00310400
.ZPVCK5         ANOP  ,                                                 00310500
.ZPVCKE         ANOP  ,                                                 00310600
