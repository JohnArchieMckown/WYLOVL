.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00757200
&ZPLLEN               SETA 168                                          00757300
.*                                                                      00757400
.******************************************************************     00757500
.***AI.L01 IXLCONN FMT=LIST FILL IN PARAMETER AREA                      00757600
.******************************************************************     00757700
                      ACTR  10000                                       00757800
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00757900
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00758000
&ZCVAL                SETC  'LAE'          XAX MODE                     00758100
.ZIMFXA               ANOP ,                                            00758200
.*                                                                      00758300
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00758400
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00758500
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00758600
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00758700
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00758800
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00758900
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00759000
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00759100
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00759200
&ZOPN                 SETA  &ZOPN+1                                     00759300
&ZOPL(&ZOPN)          SETC  ' '                                         00759400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00759500
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00759600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00759700
                      AGO   .ZIMFNX                                     00759800
.*                                                                      00759900
.ZIMFN2               ANOP ,                                            00760000
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00760100
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00760200
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00760300
&ZOPN                 SETA  &ZOPN+1                                     00760400
&ZOPL(&ZOPN)          SETC  ' '                                         00760500
&ZOPO(&ZOPN)          SETC  'LAE'                                       00760600
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00760700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00760800
                      AGO   .ZIMFNX                                     00760900
.ZIMFNX1              ANOP ,                                            00761000
&ZOPN                 SETA  &ZOPN+1                                     00761100
&ZOPL(&ZOPN)          SETC  ' '                                         00761200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00761300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00761400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00761500
.ZIMFNX               ANOP ,                                            00761600
.********************************************************************   00761700
.***AI.L02 IXLCONN FMT=LIST INITIALIZE PARAMETER AREA                   00761800
