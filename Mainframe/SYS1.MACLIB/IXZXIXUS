&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00147400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00147500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00147600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00147700
                      AIF   (K'&ZLD LE 0).ZL51                          00147800
&ZLC                  SETA  35             COMMENT COLUMN               00147900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00148000
&ZLC                  SETA  33             COMMENT COLUMN               00148100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00148200
&ZLV                  SETC  '&ZLV'.' '                                  00148300
                      AGO   .ZL31                                       00148400
.*                                                                      00148500
