.*                                                                      00127200
.ZI5E                 ANOP  ,                                           00127300
.*-------------------------------------------------------------------   00127400
.***AI.L05 XUPDTYPE BL1                                                 00127500
&ZW                   SETC  ''                                          00127600
&ZCLNG                SETC  ''             RESET BIT ON FLAG            00127700
.*-------------------------------------------------------------------   00127800
.***AI.L08 XUPDTYPE "REPLACE"                                           00127900
.***      BL1'1XXXXXXX'                                                 00128000
         AIF   ('&XUPDTYPE' NE 'REPLACE').ZI7A                          00128100
&ZW                   SETC  '&ZW.1'                                     00128200
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00128300
                      AGO   .ZI7C                                       00128400
.*                                                                      00128500
.ZI7A                 ANOP  ,                                           00128600
&ZW                   SETC  '&ZW.0'                                     00128700
.ZI7C                 ANOP  ,                                           00128800
.*-------------------------------------------------------------------   00128900
.***AI.L08 XUPDTYPE "AND"                                               00129000
.***      BL1'X1XXXXXX'                                                 00129100
         AIF   ('&XUPDTYPE' NE 'AND').ZI8A                              00129200
&ZW                   SETC  '&ZW.1'                                     00129300
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00129400
                      AGO   .ZI8C                                       00129500
.*                                                                      00129600
.ZI8A                 ANOP  ,                                           00129700
&ZW                   SETC  '&ZW.0'                                     00129800
.ZI8C                 ANOP  ,                                           00129900
.*-------------------------------------------------------------------   00130000
.***AI.L08 XUPDTYPE "OR"                                                00130100
.***      BL1'XX1XXXXX'                                                 00130200
         AIF   ('&XUPDTYPE' NE 'OR').ZI9A                               00130300
&ZW                   SETC  '&ZW.100000'                                00130400
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00130500
                      AGO   .ZI9C                                       00130600
.*                                                                      00130700
.ZI9A                 ANOP  ,                                           00130800
&ZW                   SETC  '&ZW.000000'                                00130900
.ZI9C                 ANOP  ,                                           00131000
.*-------------------------------------------------------------------   00131100
.***AI.L11 XUPDTYPE BL1                                                 00131200
                      AIF   ('&ZCLNG' NE 'YES').ZI9E  ALL ZERO BITS     00131300
&ZOPN                 SETA  &ZOPN+1                                     00131400
&ZOPL(&ZOPN)          SETC  ' '                                         00131500
&ZOPO(&ZOPN)          SETC  'OI'                                        00131600
&ZOPV(&ZOPN)          SETC  '44(1),B''&ZW'''                            00131700
&ZOPD(&ZOPN)          SETC  '++ INPUT BL1'                              00131800
.ZI9E                 ANOP  ,                                           00131900
.*-------------------------------------------------------------------   00132000
&ZOPN1                SETA &ZOPN                                        00132100
.*******************************************************************    00132200
.***  PROCESS IXZXIXUS OUTPUT PARAMETERS                                00132300
.*******************************************************************    00132400
                      ACTR  10000                                       00132500
.*******************************************************************    00132600
.***AO.X01  LABEL TO BYPASS INVOCATION                                  00132700
.*******************************************************************    00132800
&ZOPN                 SETA  &ZOPN+1                                     00132900
&ZOPL(&ZOPN)          SETC  'AMGE&SYSNDX'                               00133000
&ZOPO(&ZOPN)          SETC  'DS'                                        00133100
&ZOPV(&ZOPN)          SETC  '0H'                                        00133200
&ZOPD(&ZOPN)          SETC  '++ BYPASS INVOCATION'                      00133300
.*******************************************************************    00133400
.***AO.X02  RETURN CODE PROCESSING                                      00133500
.*******************************************************************    00133600
                      AIF   (N'&RTNCODE LE 0).ZMRETX                    00133700
                      AIF   ('&XRTNCODE'(1,1) EQ '(').ZMRET2            00133800
                      AIF   ('&XRTNCODE' EQ 'GPR15').ZMRETX             00133900
                      AIF   ('&XRTNCODE' EQ 'REG15').ZMRETX             00134000
                      AIF   ('&XRTNCODE' EQ 'R15').ZMRETX               00134100
                      AIF   ('&XRTNCODE' EQ '15').ZMRETX                00134200
&ZOPN                 SETA  &ZOPN+1                                     00134300
&ZOPL(&ZOPN)          SETC  ' '                                         00134400
&ZOPO(&ZOPN)          SETC  'ST'                                        00134500
&ZOPV(&ZOPN)          SETC  '15,&XRTNCODE'                              00134600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00134700
                      AGO   .ZMRETX                                     00134800
.*                                                                      00134900
.ZMRET2               ANOP  ,                                           00135000
&ZCGPR                SETC  '&XRTNCODE'(2,K'&XRTNCODE-2)                00135100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00135200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00135300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00135400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00135500
&ZOPN                 SETA  &ZOPN+1                                     00135600
&ZOPL(&ZOPN)          SETC  ' '                                         00135700
&ZOPO(&ZOPN)          SETC  'LR'                                        00135800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00135900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00136000
.ZMRETX               ANOP  ,                                           00136100
.*******************************************************************    00136200
.***AO.X03  REASON CODE PROCESSING                                      00136300
.*******************************************************************    00136400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00136500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00136600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00136700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00136800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00136900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00137000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00137100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00137200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00137300
&ZOPN                 SETA  &ZOPN+1                                     00137400
&ZOPL(&ZOPN)          SETC  ' '                                         00137500
&ZOPO(&ZOPN)          SETC  'ST'                                        00137600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00137700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00137800
                      AGO   .ZMRSNX                                     00137900
.*                                                                      00138000
.ZMRSN2               ANOP  ,                                           00138100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00138200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00138300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00138400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00138500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00138600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00138700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00138800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00138900
&ZOPN                 SETA  &ZOPN+1                                     00139000
&ZOPL(&ZOPN)          SETC  ' '                                         00139100
&ZOPO(&ZOPN)          SETC  'LR'                                        00139200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00139300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00139400
.ZMRSNX               ANOP  ,                                           00139500
.*******************************************************************    00139600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00139700
.*******************************************************************    00139800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00139900
.*******************************************************************    00140000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00140100
.*******************************************************************    00140200
                      ACTR  10000                                       00140300
.*                                                                      00140400
                      LCLA  &ZLC           COLUMN POSITION              00140500
                      LCLA  &ZOPX          LIST TABLE INDEX             00140600
                      LCLC  &ZLL           LABEL                        00140700
                      LCLC  &ZLOP          OPERATION                    00140800
                      LCLC  &ZLV           VALUE                        00140900
                      LCLC  &ZLD           DESCRIPTION                  00141000
.*                                                                      00141100
&ZOPX                 SETA  1                                           00141200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00141300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00141400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00141500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00141600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00141700
                      AIF   (K'&ZLD LE 0).ZL5                           00141800
&ZLC                  SETA  35             COMMENT COLUMN               00141900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00142000
&ZLC                  SETA  33             COMMMENT COLUMN              00142100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00142200
&ZLV                  SETC  '&ZLV'.' '                                  00142300
                      AGO   .ZL3                                        00142400
.*                                                                      00142500
.ZL4                  ANOP  ,                                           00142600
&ZLV                  SETC  '&ZLV'.' '                                  00142700
.ZL5                  ANOP  ,                                           00142800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00142900
.*                                                                      00143000
&ZLL     &ZLOP &ZLV                                                     00143100
.*                                                                      00143200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00143300
                      AGO   .ZL2           CONTINUE LOOPING             00143400
.*                                                                      00143500
.ZLX                  ANOP  ,                                           00143600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00143700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00143800
         GBLC  &ANVIRON           JES2 environment variable             00143900
         GBLB  &J3MN              JES3 Main Task environment            00144000
         GBLB  &J3ST              JES3 Subtask environment              00144100
         GBLB  &J3FSS             JES3 FSS environment                  00144200
         GBLB  &JSXCF             JESXCF environment                    00144300
         AIF   ('&ANVIRON' EQ 'JES2').IXUS050  Are we running under     00144400
.*                                JES2 Main task                        00144500
         AIF   ('&ANVIRON' EQ 'SUBTASK').IXUS050 Are we running under   00144600
.*                                JES2 Subtask                          00144700
         AIF   (&J3MN).IXUS050    Are we running under JES3 Main task   00144800
         AIF   (&J3ST).IXUS050    Are we running under JES3 Sub task    00144900
         AIF   (&J3FSS).IXUS050    Are we running under JES3 FSS        00145000
         MNOTE 8,'IXZ0001 Running in an invalid environment'            00145100
.IXUS050 ANOP                                                           00145200
         AIF   ('&GROUPTOKEN'(1,1) NE '(').IXUS100                      00145300
         LR    14,&GROUPTOKEN     Load the address of the group token   00145400
         L     14,0(14)           Load the group token                  00145500
         AGO   .IXUS150                                                 00145600
.IXUS100 ANOP                                                           00145700
         L     14,&GROUPTOKEN     Load the group token                  00145800
.IXUS150 ANOP                                                           00145900
         L     14,76(14)          Load the PC number of the             00146000
*                                 routine to be called                  00146100
         PC    0(14)              Message reroute routine               00146200
         AIF   ('&XMFCTRL'(1,1) NE '(').IXUS200                         00146300
         LR    1,&XMFCTRL         Restore the parameter list address    00146400
         AGO   .IXUS250                                                 00146500
.IXUS200 ANOP                                                           00146600
         LA    1,&XMFCTRL         Restore the parameter list address    00146700
.IXUS250 ANOP                                                           00146800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00146900
.ZLINKX               ANOP  ,                                           00147000
.*******************************************************************    00147100
.***AP.X01  FORMAT GENERATED CODE                                       00147200
