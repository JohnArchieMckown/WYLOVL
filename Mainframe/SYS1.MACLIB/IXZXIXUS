&ZOPO(&ZOPN)          SETC  'MVC'                                       00124300
&ZOPV(&ZOPN)          SETC  '8(32,1),&ZCGPR'                            00124400
&ZOPD(&ZOPN)          SETC  '++ &ZXUSTATE XUSTATE CL32'                 00124500
.*                                                                      00124600
.ZI4E                 ANOP  ,                                           00124700
.*-------------------------------------------------------------------   00124800
.***AI.L25  XGROUPTOKEN F                                               00124900
                      AIF   ('&ZXGROUPTOKEN' EQ '').ZI5E  NOT USED      00125000
                      AIF   ('&ZXGROUPTOKEN' EQ 'OUTPUT').ZI5E          00125100
&ZCGPR                SETC  '&XGROUPTOKEN'                              00125200
.*                                                                      00125300
                      AIF   ('&XGROUPTOKEN'(1,1) NE '(').ZI5B           00125400
&ZCGPR   SETC  '0('.'&XGROUPTOKEN'(2,K'&XGROUPTOKEN-2).')'              00125500
                      AGO   .ZI5D                                       00125600
.ZI5B                 ANOP  ,                                           00125700
.*                                                                      00125800
&ZC                   SETC  '&XGROUPTOKEN'(1,1)                         00125900
                      AIF   (T'&ZC NE 'N').ZI5D                         00126000
&ZC                   SETC  '&XGROUPTOKEN'                              00126100
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI5D                00126200
&ZCGPR                SETC  '=FL4''&XGROUPTOKEN'''                      00126300
.*                                                                      00126400
.ZI5D                 ANOP  ,                                           00126500
.*                                                                      00126600
&ZOPN                 SETA  &ZOPN+1                                     00126700
&ZOPL(&ZOPN)          SETC  ' '                                         00126800
&ZOPO(&ZOPN)          SETC  'MVC'                                       00126900
&ZOPV(&ZOPN)          SETC  '40(4,1),&ZCGPR'                            00127000
