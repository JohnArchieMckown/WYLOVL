.***AC.PLV01  PROCESS PLISTVER                                      *   00059200
.********************************************************************   00059300
&XPLISTVER      SETA  0                                                 00059400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00059500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00059600
&XPLISTVER      SETA  0                                                 00059700
.ZPLV0          ANOP  ,                                                 00059800
.*                                                                      00059900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00060000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00060100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00060200
.*-------------------------------------------------------------------   00060300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00060400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00060500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00060600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00060700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00060800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00060900
                AGO   .ZPVCK2                                           00061000
.ZPVCK3         ANOP  ,                                                 00061100
.*-------------------------------------------------------------------   00061200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00061300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00061400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00061500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00061600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00061700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00061800
                AGO   .ZPVCKE                                           00061900
.ZPVCK2         ANOP  ,                                                 00062000
.*-------------------------------------------------------------------   00062100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00062200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00062300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00062400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00062500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00062600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00062700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00062800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062900
                AGO   .ZPVCK5                                           00063000
.ZPVCK4         ANOP  ,                                                 00063100
&XPLISTVER      SETA  &PLISTVER(1)                                      00063200
.ZPVCK5         ANOP  ,                                                 00063300
.*-------------------------------------------------------------------   00063400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00063500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00063600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00063700
                AIF   (&XPLISTVER LT 0).ZPVCK6  TOO LOW                 00063800
                AIF   (&XPLISTVER LE 0).ZPVCKE  NOT HIGH                00063900
.ZPVCK6         ANOP   ,                                                00064000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00064100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064200
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00064300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00064400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064500
.ZPVCKE         ANOP   ,                                                00064600
.********************************************************************   00064700
