.ZIMFXA               ANOP ,                                            00119400
.*                                                                      00119500
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00119600
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00119700
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00119800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00119900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00120000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00120100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00120200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00120300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00120400
&ZOPN                 SETA  &ZOPN+1                                     00120500
