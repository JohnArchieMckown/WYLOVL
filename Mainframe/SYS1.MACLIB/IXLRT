                   LCLC  &ZXVERCONNAME          ++ INPUT NAME           00142900
                   LCLC  &ZXOUTRDATATYPE        ++ OUTPUT NAME          00143000
                   LCLC  &ZXENTRYIDLIST         ++ INPUT NAME           00143100
                   LCLC  &ZXFIRSTELEM           ++ INPUT NAME           00143200
                   LCLC  &ZXLASTELEM            ++ INPUT NAME           00143300
                   LCLC  &ZXANSAREA  ++ OUTPUT NAME                     00143400
                   LCLC  &ZXANSLEN   ++ INPUT NAME                      00143500
                   LCLC  &ZXRETCODE  ++ OUTPUT NAME                     00143600
                   LCLC  &ZXRSNCODE  ++ OUTPUT NAME                     00143700
                   LCLC  &ZXPLISTVER            ++ INPUT VALUE          00143800
                   LCLC  &ZXMF       ++ INPUT VALUE                     00143900
                   LCLC  &ZXMFCTRL   ++ INPUT NAME                      00144000
                   LCLC  &ZXMFATTR   ++ INPUT VALUE                     00144100
                   LCLC  &ZXMFSCK    ++ INPUT VALUE                     00144200
.********************************************************************   00144300
.***AC.PLV01  PROCESS PLISTVER                                      *   00144400
.********************************************************************   00144500
&XPLISTVER      SETA  0                                                 00144600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00144700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00144800
&XPLISTVER      SETA  4                                                 00144900
.ZPLV0          ANOP  ,                                                 00145000
.*                                                                      00145100
                AIF   ('&RDATATYPE' EQ '').ZPLV1E                       00145200
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00145300
&XPLISTVER      SETA  2                                                 00145400
.ZPLV1E         ANOP  ,                                                 00145500
.*                                                                      00145600
                AIF   ('&EXTRESTOKEN' EQ '').ZPLV2E                     00145700
                AIF   (&XPLISTVER GE 3).ZPLV2E                          00145800
&XPLISTVER      SETA  3                                                 00145900
.ZPLV2E         ANOP  ,                                                 00146000
.*                                                                      00146100
                AIF   ('&MRTDLEVEL' EQ '').ZPLV3E                       00146200
                AIF   (&XPLISTVER GE 4).ZPLV3E                          00146300
&XPLISTVER      SETA  4                                                 00146400
.ZPLV3E         ANOP  ,                                                 00146500
.*                                                                      00146600
                AIF   ('&RDATATYPE' EQ '').ZPLV4E                       00146700
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00146800
&XPLISTVER      SETA  2                                                 00146900
.ZPLV4E         ANOP  ,                                                 00147000
.*                                                                      00147100
                AIF   ('&RDATATYPE' EQ '').ZPLV5E                       00147200
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00147300
&XPLISTVER      SETA  2                                                 00147400
.ZPLV5E         ANOP  ,                                                 00147500
.*                                                                      00147600
                AIF   ('&EXTRESTOKEN' EQ '').ZPLV6E                     00147700
                AIF   (&XPLISTVER GE 3).ZPLV6E                          00147800
&XPLISTVER      SETA  3                                                 00147900
.ZPLV6E         ANOP  ,                                                 00148000
.*                                                                      00148100
                AIF   ('&FASTRESTOKEN' EQ '').ZPLV7E                    00148200
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00148300
&XPLISTVER      SETA  1                                                 00148400
.ZPLV7E         ANOP  ,                                                 00148500
