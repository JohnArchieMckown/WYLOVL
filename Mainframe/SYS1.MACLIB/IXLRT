&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00410000
                      AGO   .ZALVL6B                                    00410100
.ZALVL6A              ANOP                                              00410200
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00410300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00410400
.ZALVL6B              ANOP                                              00410500
&ZOPD(&ZOPN)          SETC  '++ IXLRT ARG PARMS'                        00410600
.*                                                                      00410700
&ZOPN                 SETA  &ZOPN+1                                     00410800
&ZOPL(&ZOPN)          SETC  ' '                                         00410900
