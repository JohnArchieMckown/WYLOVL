.ZPLV12E        ANOP  ,                                                 00151000
.*                                                                      00151100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00151200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00151300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00151400
.*-------------------------------------------------------------------   00151500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00151600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00151700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00151800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00151900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00152000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00152100
                AGO   .ZPVCK2                                           00152200
.ZPVCK3         ANOP  ,                                                 00152300
.*-------------------------------------------------------------------   00152400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00152500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00152600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00152700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00152800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00152900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00153000
                AGO   .ZPVCKE                                           00153100
.ZPVCK2         ANOP  ,                                                 00153200
.*-------------------------------------------------------------------   00153300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00153400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00153500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00153600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00153700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00153800
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00153900
.ZPVCK6         ANOP  ,                                                 00154000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00154100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00154200
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00154300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00154400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00154500
&XPLISTVER      SETA  4                                                 00154600
                AGO   .ZPVCKE                                           00154700
.*-------------------------------------------------------------------   00154800
.ZPVCK4A        ANOP  ,                                                 00154900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00155000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00155100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00155200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00155300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00155400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00155500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00155600
                AGO   .ZPVCK5                                           00155700
.ZPVCK4         ANOP  ,                                                 00155800
&XPLISTVER      SETA  &PLISTVER(1)                                      00155900
.ZPVCK5         ANOP  ,                                                 00156000
.ZPVCKE         ANOP  ,                                                 00156100
