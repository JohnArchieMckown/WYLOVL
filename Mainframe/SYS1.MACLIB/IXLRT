.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00559200
.*******************************************************************    00559300
                      ACTR  10000                                       00559400
.*                                                                      00559500
                      LCLA  &ZLC           COLUMN POSITION              00559600
                      LCLA  &ZOPX          LIST TABLE INDEX             00559700
                      LCLC  &ZLL           LABEL                        00559800
                      LCLC  &ZLOP          OPERATION                    00559900
                      LCLC  &ZLV           VALUE                        00560000
                      LCLC  &ZLD           DESCRIPTION                  00560100
.*                                                                      00560200
&ZOPX                 SETA  1                                           00560300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00560400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00560500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00560600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00560700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00560800
                      AIF   (K'&ZLD LE 0).ZL5                           00560900
&ZLC                  SETA  35             COMMENT COLUMN               00561000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00561100
&ZLC                  SETA  33             COMMMENT COLUMN              00561200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00561300
&ZLV                  SETC  '&ZLV'.' '                                  00561400
                      AGO   .ZL3                                        00561500
.*                                                                      00561600
.ZL4                  ANOP  ,                                           00561700
&ZLV                  SETC  '&ZLV'.' '                                  00561800
.ZL5                  ANOP  ,                                           00561900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00562000
.*                                                                      00562100
&ZLL     &ZLOP &ZLV                                                     00562200
.*                                                                      00562300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00562400
                      AGO   .ZL2           CONTINUE LOOPING             00562500
.*                                                                      00562600
.ZLX                  ANOP  ,                                           00562700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00562800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00562900
