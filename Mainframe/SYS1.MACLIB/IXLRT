.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00407800
.********************************************************************   00407900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00408000
                      LCLA  &ZOPN1         INVOCATION INDEX             00408100
                      LCLC  &ZOPL(4095)    LABEL                        00408200
                      LCLC  &ZOPO(4095)    OPERATION                    00408300
                      LCLC  &ZOPV(4095)    VALUE                        00408400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00408500
.*                                                                      00408600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00408700
.******************************************************************     00408800
.***AL.L10 IXLRT FMT=LIST MF=S|L PARAMETERS                             00408900
.******************************************************************     00409000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00409100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00409200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00409300
&XMFATTR              SETC  '0D'                                        00409400
.*                                                                      00409500
&ZOPN                 SETA  &ZOPN+1                                     00409600
&ZOPL(&ZOPN)          SETC  ' '                                         00409700
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00409800
