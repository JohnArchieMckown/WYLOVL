&ZOPO(&ZOPN)          SETC  'SPACE'                                     00097000
&ZOPV(&ZOPN)          SETC  '1'                                         00097100
&ZOPD(&ZOPN)          SETC  ' '                                         00097200
.*                                                                      00097300
.ZLMFSL2              ANOP  ,                                           00097400
.*-------------------------------------------------------------------   00097500
.***AM.L01  IXCLEAVE FMT=LIST PARAMETERS                                00097600
