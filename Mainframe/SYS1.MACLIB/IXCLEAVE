.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00129900
.*******************************************************************    00130000
                      ACTR  10000                                       00130100
.*                                                                      00130200
                      LCLA  &ZLC           COLUMN POSITION              00130300
                      LCLA  &ZOPX          LIST TABLE INDEX             00130400
                      LCLC  &ZLL           LABEL                        00130500
                      LCLC  &ZLOP          OPERATION                    00130600
                      LCLC  &ZLV           VALUE                        00130700
                      LCLC  &ZLD           DESCRIPTION                  00130800
.*                                                                      00130900
&ZOPX                 SETA  1                                           00131000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00131100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00131200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00131300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00131400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00131500
                      AIF   (K'&ZLD LE 0).ZL5                           00131600
&ZLC                  SETA  35             COMMENT COLUMN               00131700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00131800
&ZLC                  SETA  33             COMMMENT COLUMN              00131900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00132000
&ZLV                  SETC  '&ZLV'.' '                                  00132100
                      AGO   .ZL3                                        00132200
.*                                                                      00132300
.ZL4                  ANOP  ,                                           00132400
&ZLV                  SETC  '&ZLV'.' '                                  00132500
.ZL5                  ANOP  ,                                           00132600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00132700
.*                                                                      00132800
&ZLL     &ZLOP &ZLV                                                     00132900
.*                                                                      00133000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00133100
                      AGO   .ZL2           CONTINUE LOOPING             00133200
.*                                                                      00133300
.ZLX                  ANOP  ,                                           00133400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00133500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00133600
         L     14,16(0,0)              GET CVT ADDRESS                  00133700
