.*******************************************************************    00123200
.***AO.X02  RETURN CODE PROCESSING                                      00123300
.*******************************************************************    00123400
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00123500
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00123600
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00123700
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00123800
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00123900
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00124000
&ZOPN                 SETA  &ZOPN+1                                     00124100
&ZOPL(&ZOPN)          SETC  ' '                                         00124200
&ZOPO(&ZOPN)          SETC  'ST'                                        00124300
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00124400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00124500
                      AGO   .ZMRETX                                     00124600
