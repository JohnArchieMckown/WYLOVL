.ZG11R        ANOP  ,                                                   00085100
.*----------------------------------------------------------------***   00085200
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00085300
              AIF   (N'&RSNCODE LE 1).ZG14A     CK NUM                  00085400
&ZMS          SETC  '"RSNCODE='                 *KPOSA* MNOTE           00085500
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00085600
&ZMS          SETC  '&ZMS "RSNCODE"'                                    00085700
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00085800
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00085900
.ZG14A        ANOP  ,                                                   00086000
.*-------------------------------------------------------------------   00086100
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00086200
              AIF   (K'&RSNCODE(1) LE 0).ZG14C  NO PARM                 00086300
&ZXRSNCODE    SETC  'OUTPUT'    SET OUTPUT                              00086400
&XRSNCODE     SETC  '&RSNCODE(1)'               SET XVARIABLE           00086500
              AIF   (N'&RSNCODE GT 1).ZG14C                             00086600
&XRSNCODE     SETC  '&RSNCODE'  SET XVARIABLE                           00086700
.ZG14C        ANOP  ,                                                   00086800
.*-------------------------------------------------------------------   00086900
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00087000
              AIF   (K'&ZXRSNCODE LE 0).ZG14R   NOT I/O                 00087100
              AIF   ('&XRSNCODE'(1,1) NE '(').ZG14R                     00087200
&ZCGPR        SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                        00087300
              AIF   ('&ZCGPR'(1,1) NE '0').ZG14P                        00087400
              AIF   (K'&ZCGPR GT 2).ZG14Q                               00087500
              AIF   ('&ZCGPR' EQ '0').ZG14Q                             00087600
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00087700
              AIF   (T'&ZCGPR NE 'N').ZG14Q     INVALID REG NAME        00087800
.ZG14P        ANOP  ,                                                   00087900
              AIF   (T'&ZCGPR NE 'N').ZG14R     NON NUMERIC             00088000
              AIF   ('&ZCGPR' LT '2').ZG14Q                             00088100
              AIF   ('&ZCGPR' LE '12').ZG14R                            00088200
.ZG14Q        ANOP  ,                                                   00088300
&ZMS          SETC  '"RSNCODE='                 *KGPRA* MNOTE           00088400
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00088500
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00088600
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00088700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00088800
.ZG14R        ANOP  ,                                                   00088900
.******************************************************************     00089000
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00089100
.******************************************************************     00089200
              ACTR  10000                                               00089300
.*----------------------------------------------------------------***   00089400
.***AH.P02 EXTRA POSITIONALS                                            00089500
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00089600
&ZCVAL        SETC  ''          ERROR FIELD                             00089700
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00089800
.ZGLIST2      ANOP  ,                                                   00089900
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00090000
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00090100
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00090200
              AGO   .ZGLIST2                                            00090300
.*                                                                      00090400
.ZGLIST3      ANOP  ,                                                   00090500
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00090600
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00090700
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00090800
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00090900
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00091000
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00091100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00091200
.ZGLISTX      ANOP  ,                                                   00091300
&ZASYSLNDX    SETA  0                                                   00091400
.*-------------------------------------------------------------------   00091500
.***AG.MF04 MF=(SLE)                                                    00091600
.ZGMFX        ANOP  ,           END OF FULL SYNTAX CHECK                00091700
.*******************************************************************    00091800
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00091900
.*******************************************************************    00092000
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          00092100
.*******************************************************************    00092200
.***AE.01  START OF IXCLEAVE MACRO CODE                                 00092300
.*******************************************************************    00092400
              LCLC  &M0M0001                 NAME                       00092500
&M0M0001      SETC  '&XLABEL'                SET NAME                   00092600
              AIF   (K'&XLABEL NE 0).XLAB2                              00092700
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00092800
.*                                                                      00092900
.XLAB2        ANOP  ,                                                   00093000
&M0M0001 DS    0H                             IXCLEAVE-0                00093100
.*******************************************************************    00093200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00093300
.*******************************************************************    00093400
              PUSH  PRINT                                               00093500
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00093600
              PRINT OFF         ++ PR01                                 00093700
              AGO   .ZPRNTXX                                            00093800
.ZPRINTX      PRINT ON                                                  00093900
.ZPRNTXX      ANOP  ,                                                   00094000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00094100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00094200
.********************************************************************   00094300
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00094400
.********************************************************************   00094500
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00094600
                      LCLA  &ZOPN1         INVOCATION INDEX             00094700
                      LCLC  &ZOPL(4095)    LABEL                        00094800
                      LCLC  &ZOPO(4095)    OPERATION                    00094900
                      LCLC  &ZOPV(4095)    VALUE                        00095000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00095100
.*                                                                      00095200
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00095300
.******************************************************************     00095400
.***AL.L10 IXCLEAVE FMT=LIST MF=S|L PARAMETERS                          00095500
.******************************************************************     00095600
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00095700
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00095800
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00095900
&XMFATTR              SETC  '0D'                                        00096000
.*                                                                      00096100
&ZOPN                 SETA  &ZOPN+1                                     00096200
&ZOPL(&ZOPN)          SETC  ' '                                         00096300
&ZOPO(&ZOPN)          SETC  'B'                                         00096400
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00096500
&ZOPD(&ZOPN)          SETC  '++ IXCLEAVE ARG PARMS'                     00096600
.*                                                                      00096700
