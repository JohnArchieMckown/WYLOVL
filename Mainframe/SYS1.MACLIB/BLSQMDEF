         MACRO                                                          00050000
&NAME    BLSQMDEF &OPT1,                                               *00100000
               &BASELBL=,&CBLEN=,&MAINTLV=,&ACROFF=,&ACROLEN=,         *00150000
               &ACROLBL=,&ACRONYM=,&PREFIX=3,&OFFSETS=PRINT,           *00200000
               &STRTCOL=0,&LBLSPC=0,&HEADER=,&VIEWMATCH=           @L1C 00250000
*        MACDATE 03/03/87                                               00300000
         LCLA  &TEMP                                                    01150000
         LCLB  &B1,&B2,&B3                                         @L1C 01200000
         LCLC  &ACRLEN                                                  01225000
         LCLC  &DEFLBL                                                  01250000
         LCLC  &SMAINL,&SACRYN                                          01300000
         GBLA  &GPREFIX                                                 01350000
         GBLA  &LLBL                   MAXIMUM LABEL LENGTH        @L1C 01400000
         GBLB  &FIRSTSW                                            @L1C 01450000
         GBLB  &VARYSPC                                                 01500000
         GBLB  &ARAYOPN                                                 01550000
         GBLC  &FIRTFLD                                                 01600000
         GBLC  &OFFLBL                                                  01650000
         GBLC  &LEVEL                                                   01700000
         GBLC  &VARYLBL                                                 01750000
         GBLC  &VARYVAL                                                 01800000
         GBLC  &VALULLN                USED TO SET MAX LBL LEN IN HDR   01850000
         GBLC  &GSRCNDX                SOURCE INDEX                @L1A 01875000
         AIF   ('&OPT1' EQ '').LBL015                                   01900000
         AIF   ('&OPT1' EQ 'END').LBL005                                01950000
         MNOTE 4,'INVALID PARAMETER SPECIFIED - ''&OPT1'''              02000000
         AGO   .LBL015                                                  02050000
.LBL005  ANOP                                                           02100000
         AIF   (&FIRSTSW NE 1).MSG3                                     02150000
&VARYVAL SETC  'X''00'''                                                02200000
         AIF   (&VARYSPC EQ 0).LBL010                                   02250000
&VARYVAL SETC  'X''40'''                                                02300000
.LBL010  ANOP                                                           02350000
&B2      SETB  0                       ARRAY BIT                   @L1A 02366600
&OFFLBL  SETC  ''                      RESET LABEL OFFSET          @L1C 02383200
&VARYLBL EQU   &VARYVAL                                                 02400000
&FIRSTSW SETB  0                                                        02450000
&LEVEL   EQU   1                       MODEL LEVEL                 @L1C 02500000
&VALULLN EQU   &LLBL                   MAXIMUM LABEL LENGTH        @L1C 02550000
         DC    AL1(0)                  END OF MODEL                @L1C 02600000
         AIF   (&ARAYOPN EQ 0).LBL011                                   02650000
         MNOTE 8,'ARRAY DEFINITION NOT CLOSED'                          02700000
         MNOTE *,'ARRAY=END MUST BE SPECIFIED ON BLSQMFLD MACRO'        02750000
.LBL011  ANOP                                                           02800000
&ARAYOPN SETB  0                                                        02825000
         MEXIT                                                          02850000
.LBL015  ANOP                                                           02900000
         AIF   (&FIRSTSW NE 0).MSG4                                     02950000
&FIRTFLD SETC  'FLD1&SYSNDX'                                            03000000
&LEVEL   SETC  'LVL&SYSNDX'                                             03050000
&VARYLBL SETC  'VARY&SYSNDX'                                            03100000
&VARYVAL SETC  'X''00'''                                                03150000
&VALULLN SETC  'LLBL&SYSNDX'                                            03200000
         AIF   ('&CBLEN' NE '').PCK010                                  03250000
         MNOTE 8,'''CBLEN'' MUST BE SPECIFIED'                          03300000
.PCK010  ANOP                                                           03400000
&GSRCNDX SETC  '0'                     SOURCE INDEX DEFAULT        @L1A 03425000
&GPREFIX SETA  3                       DEFAULT PREFIX VALUE             03450000
&LLBL    SETA  5                       DEFAULT MAXIMUM LABEL LENGTH     03500000
         AIF   (T'&PREFIX(1) NE 'N').MSG1 '                        @L1C 03550000
&TEMP    SETA  &PREFIX(1)                                               03583300
         AIF   (&TEMP GT 8).MSG1                                        03616600
&GPREFIX SETA  &PREFIX(1)+1                                             03650000
&LLBL    SETA  8-&PREFIX(1)            MAXIMUM LABEL LENGTH             03700000
.PCK020  ANOP                                                           03750000
&B1      SETB  1                                                        03800000
         AIF   ('&OFFSETS' NE 'PRINT' AND '&OFFSETS' NE 'NOPRINT').MSG2 03850000
         AIF   ('&OFFSETS' EQ 'PRINT').PCK025                           03900000
&B1      SETB  0                                                        03950000
.PCK025  ANOP                                                           04000000
&B3      SETB  1             ASSUME VALUE MATCH                   @L1A  04007100
         AIF   ('&VIEWMATCH' NE '' AND '&VIEWMATCH' NE 'VALUE').MSG5    04014200
.*                                                                      04021300
         AIF   ('&VIEWMATCH' EQ 'VALUE').PCK027                   @L1A  04028400
&B3      SETB  0             BIT MATCH                            @L1A  04035500
.PCK027  ANOP                                                     @L1A  04042600
&OFFLBL  SETC  ''                                                       04050000
         AIF   ('&BASELBL' EQ '').PCK30                                 04100000
&OFFLBL  SETC  '&BASELBL'                                               04150000
.PCK30   ANOP                                                           04200000
         AIF   ('&ACROLEN' EQ '').PCK35                                 04206200
&ACRLEN  SETC  '&ACROLEN'                                               04212400
         AIF   (T'&ACROLEN(1) NE 'N').PCK35 '                      @L1C 04218600
&TEMP    SETA  &ACROLEN(1)                                              04223200
         AIF   (&TEMP LE 8).PCK35                                       04227800
         MNOTE 4,'INVALID VALUE SPECIFIED FOR ACROLEN'                  04232400
&ACRLEN  SETC  '0'                                                      04237200
.PCK35   ANOP                                                           04243400
.*                                                                      04250000
.*       PARAMETER CHECKING COMPLETE                                    04300000
.*                                                                      04350000
&SMAINL  SETC  '        '                                               04400000
         AIF   ('&MAINTLV' EQ '').LBL030                                04450000
&SMAINL  SETC  '&MAINTLV'                                               04500000
         AIF   (K'&MAINTLV LE 8).LBL030 '                          @L1C 04516600
         MNOTE 4,'MAINTLV VALUE TOO LONG. TRUNCATED TO 8 CHARACTERS'    04533200
.LBL030  ANOP  ,                                                   @L1C 04550000
&SACRYN  SETC  'CL8'' '''                                          @L1C 04600000
         AIF   ('&ACRONYM' EQ '').LBL040                                04650000
         AIF   ('&ACRONYM'(1,2) NE 'C''').LBL031                   @L1A 04654100
&SACRYN  SETC  'CL8'.'&ACRONYM'(2,K'&ACRONYM-1) '                  @L1A 04658200
         AIF   ('&ACROLEN' NE '').LBL041                           @L1M 04662300
&TEMP    SETA  K'&ACRONYM-3 '          Length of acronym           @L1A 04666400
         AGO   .LBL034                 Check length of acronym     @L1A 04670500
.LBL031  AIF   ('&ACRONYM'(1,2) NE 'X''').LBL033                   @L1A 04674600
&TEMP    SETA  (K'&ACRONYM-2)/2 '      Length of acronym           @L1A 04678700
&SACRYN  SETC  '&ACRONYM'(3,K'&ACRONYM-3) '                        @L1A 04682800
&SACRYN  SETC  '&SACRYN'.'0000000000000000'(1,16-(2*&TEMP)) '      @L1A 04686900
&SACRYN  SETC  'XL8''&SACRYN'''                                    @L1A 04691000
         AIF   ('&ACROLEN' NE '').LBL041                           @L1M 04695100
         AGO   .LBL034                 Check length of acronym     @L1A 04699200
.LBL033  ANOP  ,                                                   @L1A 04703300
&SACRYN  SETC  'CL8''&ACRONYM'''                                   @L1A 04707400
         AIF   ('&ACROLEN' NE '').LBL041                           @L1M 04711500
&TEMP    SETA  K'&ACRONYM '            Length of acronym           @L1A 04715600
.LBL034  AIF   (&TEMP LE 8).LBL035     Check length of acronym     @L1A 04719700
         MNOTE 4,'ACRONYM VALUE TOO LONG. TRUNCATED TO 8 CHARACTERS'    04724000
&TEMP    SETA  8                                                        04726000
.LBL035  ANOP                                                           04728000
&ACRLEN  SETC  '&TEMP'                                                  04730000
         AGO   .LBL041                                                  04740000
.LBL040  ANOP                                                           04750000
&ACRLEN  SETC  '0'                                                      04766600
.*                                                                      04772100
.*       BEGIN DEFINING A MODEL                                         04777600
.*                                                                      04783100
.LBL041  ANOP  ,                       Begin defining a model      @P1C 04788600
&NAME    DS    0H                      Align new model             @P1C 04794100
&DEFLBL  SETC  'DEF&SYSNDX'                                             04800000
&DEFLBL  DC    &SACRYN                 EBCDIC CTRL BLK ACRONYM     @L1C 04850000
         AIF   ('&HEADER' EQ '').LBL045                                 04900000
         AIF   (K'&HEADER LE 8).LBL043                           ' @L1C 04912500
         MNOTE 4,'HEADER VALUE TOO LONG. TRUNCATED TO 8 CHARACTERS'     04925000
.LBL043  ANOP                                                           04937500
         DC    CL8'&HEADER'            OUTPUT HEADER               @L1C 04950000
         AGO   .LBL046                                                  05000000
.LBL045  ANOP                                                           05050000
         DC    CL8' '                  OUTPUT HEADER (NONE)        @L1C 05100000
.LBL046  ANOP                                                           05150000
         DC    CL8'&SMAINL'            MAINTENANCE LEVEL OF CONTROL    *05175000
                                       BLOCK                       @L1C 05200000
         AIF   ('&CBLEN' EQ '').LBL048                                  05225000
         DC    AL2(&CBLEN)             LENGTH OF CONTROL BLOCK     @L1C 05250000
         AGO   .LBL049                                                  05260000
.LBL048  ANOP                                                           05270000
         DC    AL2(0)                  LENGTH OF CONTROL BLOCK     @L1C 05280000
.LBL049  ANOP                                                           05290000
         AIF   ('&ACROFF' EQ '').LBL050                                 05300000
         DC    AL2(&ACROFF)            OFFSET TO ACRONYM IN CONTROL    *05333300
                                       BLOCK                       @L1C 05366600
         AGO   .LBL053                                                  05400000
.LBL050  ANOP                                                           05450000
         AIF   ('&ACROLBL' EQ '').LBL052                                05483300
         AIF   ('&OFFLBL' NE '').LBL051                                 05516600
         DC    AL2(0)                  OFFSET TO ACRONYM IN CONTROL    *05538800
                                       BLOCK                       @L1C 05561000
         MNOTE 8,'ACROFF PARAMETER REQUIRED BUT NOT SPECIFIED'          05583200
         AGO   .LBL053                                                  05616500
.LBL051  ANOP                                                           05650000
         DC    AL2(&ACROLBL-&OFFLBL)   OFFSET TO ACRONYM IN CONTROL    *05672200
                                       BLOCK                       @L1C 05694400
         AGO   .LBL053                                                  05716600
.LBL052  ANOP                                                           05750000
         DC    AL2(0)                  OFFSET TO ACRONYM           @L1C 05783300
.LBL053  ANOP                                                           05816600
         DC    AL1(&ACRLEN)            LENGTH OF ACRONYM           @L1C 05859900
         DC    AL1(&VALULLN)           LABEL LENGTH                @L1C 05903200
         DC    AL1(&STRTCOL)           START COLUMN OR LEFT MARGIN @L1C 05946500
         DC    AL1(&LBLSPC)            NUMBER OF COLUMNS BETWEEN       *05989800
                                       LABELS                      @L1C 06033100
         DC    AL1(B'&B1&B2&B3.00000'+&VARYLBL)  FLAGS             @L1C 06076400
         DC    AL1(&FIRTFLD-&DEFLBL)   OFFSET TO FIRST FIELD DESC  @L1C 06119700
         DC    AL1(&LEVEL)             MODEL LEVEL                 @L1C 06163000
         DC    X'00'                   RESERVED                    @L1C 06206300
&FIRSTSW SETB  1                                                        06250000
         MEXIT                                                          06300000
.*                                                                      06350000
.MSG1    ANOP                                                           06400000
         MNOTE 4,'INVALID VALUE SPECIFIED FOR PREFIX'                   06450000
         AGO   .PCK020                                                  06500000
.MSG2    ANOP                                                           06550000
         MNOTE 4,'INVALID VALUE SPECIFIED FOR OFFSETS'                  06600000
         AGO   .PCK025                                                  06650000
.MSG5    ANOP                                                      @L1A 06662500
         MNOTE 4,'INVALID VALUE SPECIFIED FOR VIEWMATCH'           @L1A 06675000
         AGO   .PCK027                                             @L1A 06687500
.MSG3    ANOP                                                           06700000
    MNOTE 8,'NO DEFINITION CURRENTLY IN PROGRESS - TERMINATION IGNORED' 06750000
         MEXIT                                                          06800000
.MSG4    ANOP                                                           06850000
    MNOTE 8,'DEFINITION CURRENTLY IN PROGRESS - NEW DEFINITION IGNORED' 06900000
         MEXIT                                                          06950000
         MEND  ,                                                     */ 06983300
