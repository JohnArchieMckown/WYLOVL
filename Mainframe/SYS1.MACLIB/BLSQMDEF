*%BLSQMDEF:                         /* Define model header       @L2C*/ 07050000
*   MACRO KEYS(ACROFF,ACROLBL,ACROLEN,ACRONYM,BASELBL,CBLEN,HEADER,     07100000
*       LBLSPC,MAINTLV,OFFSETS,PREFIX,STRTCOL,VIEWMATCH);               07150000
*                                                                       07200000
*/* ================================================================ */ 07250000
*/* Define Macro-Variables                                           */ 07300000
*/* ================================================================ */ 07350000
*                                                                       07400000
*   DECLARE                         /* Macro variables               */ 07450000
*     ASAXMAC_IsItPLX CHARACTER EXTERNAL, /* ASAXMAC interface   @H1A*/ 07475000
*     ELSQMDEFFLAG CHARACTER EXTERNAL, /* Flag constant base     @L1A*/ 07500000
*     ELSQMDEFNAME CHARACTER EXTERNAL, /* Unique name base       @L1A*/ 07550000
*     ELSQMDEFOFFL CHARACTER EXTERNAL, /* Offset label base      @L1A*/ 07600000
*     ELSQMDEFSRCI CHARACTER EXTERNAL, /* Multiple source index  @L1A*/ 07650000
*     ELSQMDEFLLEN FIXED EXTERNAL,  /* Maximum label length      @L1A*/ 07700000
*     ELSQMDEFPLEN FIXED EXTERNAL,  /* Prefix length + 1         @L1A*/ 07750000
*     CLMETRIC CHARACTER INTERNAL,  /* Length metric             @H1A*/ 07775000
*     F1       FIXED INTERNAL;      /* Utility                   @L1P*/ 07800000
*   DEACTIVATE                      /* Prevent accidents         @L1A*/ 07850000
*       CLMETRIC,                   /*                           @H1C*/ 07875000
*       ELSQMDEFFLAG,ELSQMDEFNAME,ELSQMDEFOFFL,ELSQMDEFSRCI;            07900000
*   ANSWER('?ASAXMAC ISITPLX(1,4,1);'); /*                       @H1A*/ 07908300
*   IF ASAXMAC_IsItPLX = 'YES' THEN /* Use SIZE                  @H1A*/ 07916600
*     CLMETRIC = 'SIZE(';           /*                           @H1A*/ 07924900
*   ELSE                            /* Use LENGTH                    */ 07933200
*     CLMETRIC = 'LENGTH(';         /*                           @H1A*/ 07941500
*                                                                       07950000
*/* ================================================================ */ 08000000
*/* Produce Declaration                                              */ 08050000
*/* ================================================================ */ 08100000
*                                                                       08150000
*   IF TRANSLATE(MACLIST(1)) = 'END' THEN /* Mark end of model   @L3C*/ 08200000
*                                                                       08250000
*     /***************************************************************/ 08300000
*     /*                                                             */ 08350000
*     /* Mark end of model                                           */ 08400000
*     /*                                                             */ 08450000
*     /***************************************************************/ 08500000
*                                                                       08550000
*     DO;                           /* Mark end of model         @L1A*/ 08600000
*       ANSWER('2 END#MODEL BIT(8) INITIAL(''00''X),/*BLSQMDEF'||       08650000
*           MACLIST||' '||MACKEYS||'*/') COLUMN(8); /*           @L1A*/ 08700000
*       ANSWER(ELSQMDEFNAME||'FLAG BIT(8) CONSTANT('''||ELSQMDEFFLAG||  08750000
*           '''B),/*Model header flags*/') COLUMN(7); /*         @L1A*/ 08800000
*       ANSWER(ELSQMDEFNAME||'LLEN CONSTANT('||CHAR(ELSQMDEFLLEN)||     08850000
*           ');/*Maximum label length*/') COLUMN(7); /*          @L1A*/ 08883300
*       ANSWER('?BLRDEFEP;');       /* Process deferred DCLs     @L2P*/ 08916600
*     END;                          /* Mark end of model         @L1A*/ 08950000
*   ELSE                            /* Begin model               @L1A*/ 09000000
*                                                                       09050000
*     /***************************************************************/ 09100000
*     /*                                                             */ 09150000
*     /* Begin a model                                               */ 09200000
*     /*                                                             */ 09250000
*     /***************************************************************/ 09300000
*                                                                       09350000
*     DO;                           /* Begin model               @L1A*/ 09400000
*       ANSWER('?BLRDEFEP;');       /* Process deferred DCLs     @L2P*/ 09425000
*       ELSQMDEFNAME = 'BLSQMDEF' || MACINDEX; /* Unique name    @L1A*/ 09450000
*       ELSQMDEFOFFL = BASELBL(1);  /* Offset label base         @L1A*/ 09500000
*       ELSQMDEFSRCI = '0';         /* Multiple source index     @L1A*/ 09550000
*                                                                       09600000
*       /*************************************************************/ 09650000
*       /*                                                           */ 09700000
*       /* Process prefix length                                     */ 09750000
*       /*                                                           */ 09800000
*       /*************************************************************/ 09850000
*                                                                       09900000
*       ELSQMDEFLLEN = 5;           /* Maximum label length      @L1A*/ 09950000
*       ELSQMDEFPLEN = 4;           /* Prefix length + 1         @L1A*/ 10000000
*       IF LENGTH(PREFIX(1)) > 0 THEN /* Explicit prefix length  @L1A*/ 10050000
*         DO;                       /* Explicit prefix length    @L1A*/ 10100000
*           F1 = FIXED(PREFIX(1));  /* Prefix length             @L1A*/ 10150000
*           ELSQMDEFLLEN = 8 - F1;  /* Maximum label length      @L1A*/ 10200000
*           ELSQMDEFPLEN = F1 + 1;  /* Prefix length + 1         @L1A*/ 10250000
*         END;                      /* Explicit prefix length    @L1A*/ 10300000
*                                                                       10350000
*       /*************************************************************/ 10400000
*       /*                                                           */ 10450000
*       /* Declare prefix length                                     */ 10500000
*       /*                                                           */ 10550000
*       /*************************************************************/ 10600000
*                                                                       10650000
*       ANSWER('DECLARE/*BLSQMDEF #MD97344*/'); /*               @H1C*/ 10685700
*       ANSWER('/*BLSQMDEF'||MACLIST||' '||MACKEYS||'*/') COLUMN(7);    10721400
*                                   /*                           @L2A*/ 10757100
*       ANSWER('1 '||MACLIST(1)||' '||MACLIST(2)||                      10792800
*           ' STATIC LOCAL BOUNDARY(WORD),/*BLSQMDEF #MD97344*/')       10828500
*           COLUMN(7);              /*                           @H1C*/ 10864200
*       ANSWER('2 MODEL#HEADER BOUNDARY(WORD),/*Model header*/') COLUMN 10900000
*           (9);                    /*                           @L1A*/ 10950000
*                                                                       11000000
*       /*************************************************************/ 11050000
*       /*                                                           */ 11100000
*       /* Process acronym                                           */ 11150000
*       /*                                                           */ 11200000
*       /*************************************************************/ 11250000
*                                                                       11300000
*       ANSWER('3 ACRONYM CHARACTER(8) INITIAL(') COLUMN(9); /*  @L1A*/ 11350000
*       IF LENGTH(ACRONYM(1)) > 0 THEN /*                        @L1A*/ 11400000
*         ANSWER(ACRONYM(1));       /*                           @L1A*/ 11450000
*       ELSE                                                            11500000
*         ANSWER(''' ''');          /*                           @L1A*/ 11550000
*       ANSWER('),/*Control block acronym*/'); /*                @L1A*/ 11600000
*                                                                       11650000
*       /*************************************************************/ 11700000
*       /*                                                           */ 11750000
*       /* Process label                                             */ 11800000
*       /*                                                           */ 11850000
*       /*************************************************************/ 11900000
*                                                                       11950000
*       ANSWER('3 OUTPUT#HEADER CHARACTER(8) INITIAL(') COLUMN(9); /*   12000000
*                                                                @L1A*/ 12050000
*       IF LENGTH(HEADER(1)) > 0 THEN /*                         @L1A*/ 12100000
*         ANSWER(HEADER(1));        /*                           @L1A*/ 12150000
*       ELSE                                                            12200000
*         ANSWER(''' ''');          /*                           @L1A*/ 12250000
*       ANSWER('),/*Formatted output header*/'); /*              @L1A*/ 12300000
*                                                                       12350000
*       /*************************************************************/ 12400000
*       /*                                                           */ 12450000
*       /* Process maintenance level                                 */ 12500000
*       /*                                                           */ 12550000
*       /*************************************************************/ 12600000
*                                                                       12650000
*       ANSWER('3 MODEL#MAINTENANCE#LEVEL CHARACTER(8) INITIAL(')       12700000
