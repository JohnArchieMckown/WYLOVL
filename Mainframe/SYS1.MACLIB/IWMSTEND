.***AC.PLV01  PROCESS PLISTVER                                      *   00058700
.********************************************************************   00058800
&XPLISTVER      SETA  0                                                 00058900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00059000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00059100
&XPLISTVER      SETA  0                                                 00059200
.ZPLV0          ANOP  ,                                                 00059300
.*                                                                      00059400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00059500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00059600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00059700
.*-------------------------------------------------------------------   00059800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00059900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00060000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00060100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00060200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00060300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00060400
                AGO   .ZPVCK2                                           00060500
.ZPVCK3         ANOP  ,                                                 00060600
.*-------------------------------------------------------------------   00060700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00060800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00060900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00061000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00061100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00061200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00061300
                AGO   .ZPVCKE                                           00061400
.ZPVCK2         ANOP  ,                                                 00061500
.*-------------------------------------------------------------------   00061600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00061700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00061800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00061900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00062000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00062100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00062200
.ZPVCK6         ANOP  ,                                                 00062300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00062400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00062500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00062600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00062700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062800
&XPLISTVER      SETA  0                                                 00062900
                AGO   .ZPVCKE                                           00063000
.*-------------------------------------------------------------------   00063100
.ZPVCK4A        ANOP  ,                                                 00063200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00063300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00063400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00063500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00063600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00063700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00063800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063900
                AGO   .ZPVCK5                                           00064000
.ZPVCK4         ANOP  ,                                                 00064100
&XPLISTVER      SETA  &PLISTVER(1)                                      00064200
.ZPVCK5         ANOP  ,                                                 00064300
.ZPVCKE         ANOP  ,                                                 00064400
.********************************************************************   00064500
