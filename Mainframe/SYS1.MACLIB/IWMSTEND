&ZOPD(&ZOPN)          SETC '++ &ZXWLMEUTKN  XWLMEUTKN'                  00111600
.*-------------------------------------------------------------------   00111700
.***AM.L10 END OF IWMSTEND LIST PARAMETERS                              00111800
&ZOPN                 SETA &ZOPN+1                                      00111900
&ZOPL(&ZOPN)          SETC '&XMFCTRL.L'                                 00112000
&ZOPO(&ZOPN)          SETC 'EQU'                                        00112100
&ZOPV(&ZOPN)          SETC '*-&XMFCTRL'                                 00112200
&ZOPD(&ZOPN)          SETC '++ LENGTH OF PLIST'                         00112300
.*                                                                      00112400
&ZOPN                 SETA &ZOPN+1                                      00112500
&ZOPL(&ZOPN)          SETC ' '                                          00112600
&ZOPO(&ZOPN)          SETC 'SPACE'                                      00112700
&ZOPV(&ZOPN)          SETC '1'                                          00112800
&ZOPD(&ZOPN)          SETC ' '                                          00112900
.*-------------------------------------------------------------------   00113000
.***AL.L12 IWMSTEND FMT=LIST MF=S|L PARAMETERS                          00113100
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00113200
.*                                                                      00113300
&ZOPN                 SETA  &ZOPN+1                                     00113400
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00113500
&ZOPO(&ZOPN)          SETC  'DC'                                        00113600
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00113700
&ZOPD(&ZOPN)          SETC  '++ IWMSTEND ARG PARMS'                     00113800
.*                                                                      00113900
.ZLMFSLX              ANOP  ,                                           00114000
.*-------------------------------------------------------------------   00114100
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00114200
&ZPLLEN               SETA 16                                           00114300
.*                                                                      00114400
.******************************************************************     00114500
.***AI.L01 IWMSTEND FMT=LIST FILL IN PARAMETER AREA                     00114600
.******************************************************************     00114700
                      ACTR  10000                                       00114800
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00114900
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00115000
&ZCVAL                SETC  'LAE'          XAX MODE                     00115100
.ZIMFXA               ANOP ,                                            00115200
.*                                                                      00115300
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00115400
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00115500
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00115600
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00115700
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00115800
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00115900
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00116000
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00116100
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00116200
&ZOPN                 SETA  &ZOPN+1                                     00116300
&ZOPL(&ZOPN)          SETC  ' '                                         00116400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00116500
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00116600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00116700
                      AGO   .ZIMFNX                                     00116800
.*                                                                      00116900
.ZIMFN2               ANOP ,                                            00117000
&ZOPN                 SETA  &ZOPN+1                                     00117100
&ZOPL(&ZOPN)          SETC  ' '                                         00117200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00117300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00117400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00117500
.ZIMFNX               ANOP ,                                            00117600
.********************************************************************   00117700
.***AI.L02 IWMSTEND FMT=LIST INITIALIZE PARAMETER AREA                  00117800
.********************************************************************   00117900
.***AI.L03 MF=(SLE)                                                     00118000
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00118100
.*                                                                      00118200
&ZOPN                 SETA  &ZOPN+1                                     00118300
&ZOPL(&ZOPN)          SETC  ' '                                         00118400
