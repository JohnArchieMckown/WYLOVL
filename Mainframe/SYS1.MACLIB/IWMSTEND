              AIF   ('&ZCGPR' LE '12').ZG2R                             00089100
.ZG2Q         ANOP  ,                                                   00089200
&ZMS          SETC  '"WLMEUTKN='                *KGPRA* MNOTE           00089300
&ZMS          SETC  '&ZMS.&WLMEUTKN"'                                   00089400
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00089500
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00089600
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00089700
.ZG2R         ANOP  ,                                                   00089800
.*----------------------------------------------------------------***   00089900
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00090000
              AIF   (N'&RETCODE LE 1).ZG5A      CK NUM                  00090100
&ZMS          SETC  '"RETCODE='                 *KPOSA* MNOTE           00090200
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00090300
&ZMS          SETC  '&ZMS "RETCODE"'                                    00090400
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00090500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00090600
.ZG5A         ANOP  ,                                                   00090700
.*-------------------------------------------------------------------   00090800
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00090900
              AIF   (K'&RETCODE(1) LE 0).ZG5C   NO PARM                 00091000
&ZXRETCODE    SETC  'OUTPUT'    SET OUTPUT                              00091100
&XRETCODE     SETC  '&RETCODE(1)'               SET XVARIABLE           00091200
              AIF   (N'&RETCODE GT 1).ZG5C                              00091300
&XRETCODE     SETC  '&RETCODE'  SET XVARIABLE                           00091400
.ZG5C         ANOP  ,                                                   00091500
.*-------------------------------------------------------------------   00091600
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00091700
              AIF   (K'&ZXRETCODE LE 0).ZG5R    NOT I/O                 00091800
              AIF   ('&XRETCODE'(1,1) NE '(').ZG5R                      00091900
&ZCGPR        SETC  '&XRETCODE'(2,K'&XRETCODE-2)                        00092000
              AIF   ('&ZCGPR'(1,1) NE '0').ZG5P                         00092100
              AIF   (K'&ZCGPR GT 2).ZG5Q                                00092200
              AIF   ('&ZCGPR' EQ '0').ZG5Q                              00092300
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00092400
              AIF   (T'&ZCGPR NE 'N').ZG5Q      INVALID REG NAME        00092500
.ZG5P         ANOP  ,                                                   00092600
              AIF   (T'&ZCGPR NE 'N').ZG5R      NON NUMERIC             00092700
              AIF   ('&ZCGPR' LT '2').ZG5Q                              00092800
              AIF   ('&ZCGPR' LE '12').ZG5R                             00092900
.ZG5Q         ANOP  ,                                                   00093000
&ZMS          SETC  '"RETCODE='                 *KGPRA* MNOTE           00093100
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00093200
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00093300
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00093400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00093500
.ZG5R         ANOP  ,                                                   00093600
.*----------------------------------------------------------------***   00093700
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00093800
              AIF   (N'&RSNCODE LE 1).ZG8A      CK NUM                  00093900
&ZMS          SETC  '"RSNCODE='                 *KPOSA* MNOTE           00094000
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00094100
&ZMS          SETC  '&ZMS "RSNCODE"'                                    00094200
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00094300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00094400
.ZG8A         ANOP  ,                                                   00094500
.*-------------------------------------------------------------------   00094600
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00094700
              AIF   (K'&RSNCODE(1) LE 0).ZG8C   NO PARM                 00094800
&ZXRSNCODE    SETC  'OUTPUT'    SET OUTPUT                              00094900
&XRSNCODE     SETC  '&RSNCODE(1)'               SET XVARIABLE           00095000
              AIF   (N'&RSNCODE GT 1).ZG8C                              00095100
&XRSNCODE     SETC  '&RSNCODE'  SET XVARIABLE                           00095200
.ZG8C         ANOP  ,                                                   00095300
.*-------------------------------------------------------------------   00095400
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00095500
              AIF   (K'&ZXRSNCODE LE 0).ZG8R    NOT I/O                 00095600
              AIF   ('&XRSNCODE'(1,1) NE '(').ZG8R                      00095700
&ZCGPR        SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                        00095800
              AIF   ('&ZCGPR'(1,1) NE '0').ZG8P                         00095900
              AIF   (K'&ZCGPR GT 2).ZG8Q                                00096000
              AIF   ('&ZCGPR' EQ '0').ZG8Q                              00096100
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00096200
              AIF   (T'&ZCGPR NE 'N').ZG8Q      INVALID REG NAME        00096300
.ZG8P         ANOP  ,                                                   00096400
              AIF   (T'&ZCGPR NE 'N').ZG8R      NON NUMERIC             00096500
              AIF   ('&ZCGPR' LT '2').ZG8Q                              00096600
              AIF   ('&ZCGPR' LE '12').ZG8R                             00096700
.ZG8Q         ANOP  ,                                                   00096800
&ZMS          SETC  '"RSNCODE='                 *KGPRA* MNOTE           00096900
