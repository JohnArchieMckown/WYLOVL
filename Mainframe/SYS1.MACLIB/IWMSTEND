              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00097200
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00097300
.ZG8R         ANOP  ,                                                   00097400
.******************************************************************     00097500
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00097600
.******************************************************************     00097700
              ACTR  10000                                               00097800
.*----------------------------------------------------------------***   00097900
.***AH.P02 EXTRA POSITIONALS                                            00098000
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00098100
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00098200
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00098300
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00098400
.ZGLISTX1     ANOP  ,                                                   00098500
&ZCVAL        SETC  ''          ERROR FIELD                             00098600
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00098700
.ZGLIST2      ANOP  ,                                                   00098800
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00098900
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00099000
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00099100
              AGO   .ZGLIST2                                            00099200
.*                                                                      00099300
.ZGLIST3      ANOP  ,                                                   00099400
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00099500
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00099600
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00099700
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00099800
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00099900
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00100000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00100100
.ZGLISTX      ANOP  ,                                                   00100200
&ZASYSLNDX    SETA  0                                                   00100300
.*-------------------------------------------------------------------   00100400
.***AG.MF04 MF=(SLE)                                                    00100500
.ZGMFX        ANOP  ,           END OF FULL SYNTAX CHECK                00100600
.*******************************************************************    00100700
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00100800
.*******************************************************************    00100900
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          00101000
.*******************************************************************    00101100
.***AE.01  START OF IWMSTEND MACRO CODE                                 00101200
.*******************************************************************    00101300
              LCLC  &M0M0001                 NAME                       00101400
&M0M0001      SETC  '&XLABEL'                SET NAME                   00101500
              AIF   (K'&XLABEL NE 0).XLAB2                              00101600
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00101700
.*                                                                      00101800
.XLAB2        ANOP  ,                                                   00101900
              AIF   ('&XMF' EQ 'L').XLAB3                               00102000
&M0M0001 DS    0H                             IWMSTEND-0                00102100
.XLAB3        ANOP  ,                                                   00102200
.*******************************************************************    00102300
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00102400
.*******************************************************************    00102500
              PUSH  PRINT                                               00102600
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00102700
              PRINT OFF         ++ PR01                                 00102800
              AGO   .ZPRNTXX                                            00102900
.ZPRINTX      PRINT ON                                                  00103000
.ZPRNTXX      ANOP  ,                                                   00103100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00103200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00103300
.********************************************************************   00103400
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00103500
.********************************************************************   00103600
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00103700
                      LCLA  &ZOPN1         INVOCATION INDEX             00103800
                      LCLC  &ZOPL(4095)    LABEL                        00103900
                      LCLC  &ZOPO(4095)    OPERATION                    00104000
                      LCLC  &ZOPV(4095)    VALUE                        00104100
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00104200
.*                                                                      00104300
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00104400
.******************************************************************     00104500
.***AL.L10 IWMSTEND FMT=LIST MF=S|L PARAMETERS                          00104600
.******************************************************************     00104700
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00104800
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00104900
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00105000
&XMFATTR              SETC  '0D'                                        00105100
.*                                                                      00105200
&ZOPN                 SETA  &ZOPN+1                                     00105300
&ZOPL(&ZOPN)          SETC  ' '                                         00105400
&ZOPO(&ZOPN)          SETC  'B'                                         00105500
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00105600
&ZOPD(&ZOPN)          SETC  '++ IWMSTEND ARG PARMS'                     00105700
.*                                                                      00105800
&ZOPN                 SETA  &ZOPN+1                                     00105900
&ZOPL(&ZOPN)          SETC  ' '                                         00106000
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00106100
&ZOPV(&ZOPN)          SETC  '1'                                         00106200
&ZOPD(&ZOPN)          SETC  ' '                                         00106300
.*                                                                      00106400
.ZLMFSL2              ANOP  ,                                           00106500
.*-------------------------------------------------------------------   00106600
.***AM.L00  IWMSTEND FMT=LIST name                                      00106700
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00106800
&ZOPN                 SETA &ZOPN+1                                      00106900
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00107000
&ZOPO(&ZOPN)          SETC 'EQU'                                        00107100
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00107200
&ZOPD(&ZOPN)          SETC '++ IWMSTEND NAME'                           00107300
.ZLMFSL2M             ANOP  ,                                           00107400
.*-------------------------------------------------------------------   00107500
.***AM.L01  IWMSTEND FMT=LIST PARAMETERS                                00107600
&ZOPN                 SETA &ZOPN+1                                      00107700
&ZOPL(&ZOPN)          SETC '&XMFCTRL'                                   00107800
&ZOPO(&ZOPN)          SETC 'DS'                                         00107900
&ZOPV(&ZOPN)          SETC '&XMFATTR'                                   00108000
&ZOPD(&ZOPN)          SETC '++ IWMSTEND PARM LIST'                      00108100
.*-------------------------------------------------------------------   00108200
.***AM.L02 XVERSION XL1                                                 00108300
