*01* EXAMPLES:                                                        * 03221000
*                                                                     * 03222000
*                                                                     * 03223000
*      Operation:  Getting the status for device                      * 03224000
*      0F40 in a PPRC session in assembler.                           * 03225000
*                                                                     * 03226000
*                                                                     * 03227000
*               TITLE 'TESTER: Test ANTRQST'                          * 03228000
*      TESTER   CSECT ,                                               * 03229000
*               USING *,15                                            * 03230000
*               B     @PROLOG                                         * 03231000
*               DC    AL1(16)                                         * 03232000
*               DC    C'TESTER    99.193'                             * 03233000
*               DROP  15                                              * 03234000
*      @PROLOG  STM   14,12,12(13)                                    * 03235000
*               LR    12,15                                           * 03236000
*      @PSTART  EQU   TESTER                                          * 03237000
*               USING @PSTART,12                                      * 03238000
*               LA    15,0                                            * 03239000
*               L     0,@SIZDATD+4                                    * 03240000
*               GETMAIN  RU,LV=(0),SP=(15)                            * 03241000
*               LR    15,13                                           * 03242000
*               LR    13,01                                           * 03243000
*               USING @DATD,13                                        * 03244000
*               ST    15,4(,13)                                       * 03245000
*               ST    13,8(,15)                                       * 03246000
*               LM    15,01,16(15)                                    * 03247000
*               MVC   XDEVN,=XL2'0F40'                                * 03248000
*               MVC   XQRYSIZE,=AL2(512)                              * 03249000
*               ANTRQST ILK=PPRC,                    X                * 03250000
*                     REQUEST=PQUERY,                X                * 03251000
*                     DEVN=XDEVN,                    X                * 03252000
*                     QRYSIZE=XQRYSIZE,              X                * 03253000
*                     QRYINFO=XQRYINFO,              X                * 03254000
*                     RETINFO=XRETINFO,              X                * 03255000
*                     RETCODE=RTNCD,                 X                * 03256000
*                     RSNCODE=RSNCD,                 X                * 03257000
*                     MF=(E,P_LIST)                                   * 03258000
*               CLC   RTNCD,=F'0'                                     * 03259000
*               BE    SKIP1                                           * 03260000
*               MVC   RTCD,RTNCD                                      * 03261000
*               MVC   RSCD,RSNCD                                      * 03262000
*               B     SKIP2                                           * 03263000
*      SKIP1    DS    0H                                              * 03264000
*               MVC   RTCD,RTC                                        * 03265000
*               MVC   RSCD,RSC                                        * 03266000
*      SKIP2    DS    0H                                              * 03267000
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 03268000
*               BNE   SKIP3                                           * 03269000
*               WTO   'ANTAS000 NOT ACTIVE'                           * 03270000
*      SKIP3    DS    0H                                              * 03271000
*               L     2,RTCD                                          * 03272000
*               L     3,RSCD                                          * 03273000
*               LR    1,13                                            * 03274000
*               L     13,4(,13)                                       * 03275000
*               LA    15,0                                            * 03276000
*               L     0,@SIZDATD+4                                    * 03277000
*               FREEMAIN RU,LV=(0),A=(1),SP=(15)                      * 03278000
*               LR    15,2                                            * 03279000
*               LR    0,3                                             * 03280000
*               L     14,12(,13)                                      * 03281000
*               LM    1,12,24(13)                                     * 03282000
*               BR    14                                              * 03283000
*               LTORG                                                 * 03284000
*      @SIZDATD DS    0A                                              * 03285000
*               DC    AL1(0)                                          * 03286000
*               DC    AL3(@DYNSIZE)                                   * 03287000
*      @DATD    DSECT                                                 * 03288000
*               DS    18F                                             * 03289000
*      RTNCD    DS    F                                               * 03290000
*      RSNCD    DS    F                                               * 03291000
*      RTCD     DS    F                                               * 03292000
*      RSCD     DS    F                                               * 03293000
*      XDEVN    DS    XL2                                             * 03294000
*      XQRYSIZE DS    XL2                                             * 03295000
*      XQRYINFO DS    CL512                                           * 03296000
*      XRETINFO DS    0F,CL100                                        * 03297000
*               ORG   XRETINFO                                        * 03298000
*      RTC      DS    F                                               * 03299000
*      RSC      DS    F                                               * 03300000
*               ORG   XRETINFO+100                                    * 03301000
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 03302000
*               ORG   *+1-(*-@DATD)/(*-@DATD)                         * 03303000
*      @ENDDATD DS    0X                                              * 03304000
*      @DYNSIZE EQU   ((@ENDDATD-@DATD+7)/8)*8                        * 03305000
*               END   TESTER                                          * 03306000
*                                                                     * 03307000
*                                                                     * 03308000
*                                                                     * 03309000
*                                                                     * 03310000
*      Operation:  Getting the status for device 0F40                 * 03311000
*      in a PPRC session in assembler and using the                   * 03312000
*      ASYNCH parameter.                                              * 03313000
*                                                                     * 03314000
*                                                                     * 03315000
*               TITLE 'TESTER: Test ANTRQST'                          * 03316000
*      TESTER   CSECT ,                                               * 03317000
*               USING *,15                                            * 03318000
*               B     @PROLOG                                         * 03319000
*               DC    AL1(16)                                         * 03320000
*               DC    C'TESTER    99.193'                             * 03321000
*               DROP  15                                              * 03322000
*      @PROLOG  STM   14,12,12(13)                                    * 03323000
*               LR    12,15                                           * 03324000
*      @PSTART  EQU   TESTER                                          * 03325000
*               USING @PSTART,12                                      * 03326000
*               LA    15,0                                            * 03327000
*               L     0,@SIZDATD+4                                    * 03328000
*               GETMAIN  RU,LV=(0),SP=(15)                            * 03329000
*               LR    15,13                                           * 03330000
*               LR    13,01                                           * 03331000
*               USING @DATD,13                                        * 03332000
*               ST    15,4(,13)                                       * 03333000
*               ST    13,8(,15)                                       * 03334000
*               LM    15,01,16(15)                                    * 03335000
*               MVC   XDEVN,=XL2'0F40'                                * 03336000
*               MVC   XQRYSIZE,=AL2(512)                              * 03337000
*               MVC   XASYNCH,=CL3'YES'                               * 03338000
*               XC    XECB,XECB                                       * 03339000
*               ANTRQST ILK=PPRC,                    X                * 03340000
*                     REQUEST=PQUERY,                X                * 03341000
*                     DEVN=XDEVN,                    X                * 03342000
*                     QRYSIZE=XQRYSIZE,              X                * 03343000
*                     QRYINFO=XQRYINFO,              X                * 03344000
*                     RETINFO=XRETINFO,              X                * 03345000
*                     ASYNCH=XASYNCH,                X                * 03346000
*                     ECB=XECB,                      X                * 03347000
*                     RETCODE=RTNCD,                 X                * 03348000
*                     RSNCODE=RSNCD,                 X                * 03349000
*                     MF=(E,P_LIST)                                   * 03350000
*               CLC   RTNCD,=F'0'                                     * 03351000
*               BE    SKIP1                                           * 03352000
*               MVC   RTCD,RTNCD                                      * 03353000
*               MVC   RSCD,RSNCD                                      * 03354000
*               B     SKIP2                                           * 03355000
*      SKIP1    DS    0H                                              * 03356000
*               WAIT  ECB=XECB                                        * 03357000
*               NC    XECB,=XL4'3FFFFFFF'                             * 03358000
*               CLC   XECB,=F'0'                                      * 03359000
*               BE    SKIP1A                                          * 03360000
*               MVC   RTCD,XECB                                       * 03361000
*               MVC   RSCD,=F'0'                                      * 03362000
*               B     SKIP2                                           * 03363000
*      SKIP1A   DS    0H                                              * 03364000
*               MVC   RTCD,RTC                                        * 03365000
*               MVC   RSCD,RSC                                        * 03366000
*      SKIP2    DS    0H                                              * 03367000
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 03368000
*               BNE   SKIP3                                           * 03369000
*               WTO   'ANTAS000 NOT ACTIVE'                           * 03370000
*      SKIP3    DS    0H                                              * 03371000
*               L     2,RTCD                                          * 03372000
*               L     3,RSCD                                          * 03373000
*               LR    1,13                                            * 03374000
*               L     13,4(,13)                                       * 03375000
*               LA    15,0                                            * 03376000
*               L     0,@SIZDATD+4                                    * 03377000
*               FREEMAIN RU,LV=(0),A=(1),SP=(15)                      * 03378000
*               LR    15,2                                            * 03379000
*               LR    0,3                                             * 03380000
*               L     14,12(,13)                                      * 03381000
*               LM    1,12,24(13)                                     * 03382000
*               BR    14                                              * 03383000
*               LTORG                                                 * 03384000
*      @SIZDATD DS    0A                                              * 03385000
*               DC    AL1(0)                                          * 03386000
*               DC    AL3(@DYNSIZE)                                   * 03387000
*      @DATD    DSECT                                                 * 03388000
*               DS    18F                                             * 03389000
*      RTNCD    DS    F                                               * 03390000
*      RSNCD    DS    F                                               * 03391000
*      RTCD     DS    F                                               * 03392000
*      RSCD     DS    F                                               * 03393000
*      XECB     DS    F                                               * 03394000
*      XASYNCH  DS    CL3                                             * 03395000
*      XDEVN    DS    XL2                                             * 03396000
*      XQRYSIZE DS    XL2                                             * 03397000
*      XQRYINFO DS    CL512                                           * 03398000
*      XRETINFO DS    0F,CL100                                        * 03399000
*               ORG   XRETINFO                                        * 03400000
*      RTC      DS    F                                               * 03401000
*      RSC      DS    F                                               * 03402000
*               ORG   XRETINFO+100                                    * 03403000
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 03404000
*               ORG   *+1-(*-@DATD)/(*-@DATD)                         * 03405000
*      @ENDDATD DS    0X                                              * 03406000
*      @DYNSIZE EQU   ((@ENDDATD-@DATD+7)/8)*8                        * 03407000
*               END   TESTER                                          * 03408000
*                                                                     * 03409000
