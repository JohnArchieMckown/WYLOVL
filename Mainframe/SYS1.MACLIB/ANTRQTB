*                   |   |     ,SCCA=xscca                             * 00131000
*                   |   |     ,SSERIAL=xsserial                       * 00132000
*                   |   |     ,SSSID=xsssid                           * 00133000
*                   |   |     ,RETINFO=xretinfo                       * 00134000
*                   |   |    [,PLSS=xplss|NO_PLSS]                  * 00135000
*                   |   |    [,SLSS=xslss|NO_SLSS]                  * 00136000
*                   |   |    [,ASYNCH=xasynch|NO                    * 00137000
*                   |   |        [,ECB=xecb|NO_ECB]]                * 00138000
*                   |   |    [,CRIT=xcrit|NO]                       * 00139000
*                   |   |    [,MODE=xmode|COPY]                     * 00140000
*                   |   |    [,MSGREQ=xmsgreq|NO]                   * 00141000
*                   |   |    [,ONLINSEC=xonlinsec|NO]               * 00142000
*                   |   |    [,PACE=xpace|DEFAULT]                  * 00143000
*                   |   |    [,WAITTIME=xwaittime|0]                * 00144000
*                   |    ,REQUEST=PESTPATH                           * 00145000
*                   |   |     ,DEVN=xdevn                             * 00146000
*                   |   |    ,LINKS=xlinks                           * 00147000
*                   |   |    ,LINK1=xlink1                           * 00148000
*                   |   |        [,LINK2=xlink2|0                   * 00149000
*                   |   |            [,LINK3=xlink3|0               * 00150000
*                   |   |                [,LINK4=xlink4|0]]]        * 00151000
*                   |   |     ,PSERIAL=xpserial                       * 00152000
*                   |   |     ,PSSID=xpssid                           * 00153000
*                   |   |     ,SSERIAL=xsserial                       * 00154000
*                   |   |     ,SSSID=xsssid                           * 00155000
*                   |   |     ,RETINFO=xretinfo                       * 00156000
*                   |   |    [,CGROUP=xcgroup|NO]                   * 00157000
*                   |   |    [,PLSS=xplss|NO_PLSS]                  * 00158000
*                   |   |    [,RESETHP=xresethp|NO]                 * 00159000
*                   |   |    [,SLSS=xslss|NO_SLSS]                  * 00160000
