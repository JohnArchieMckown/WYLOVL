*                   |   |     ,SSSID=xsssid                           * 00169000
*                   |   |     ,RETINFO=xretinfo                       * 00170000
*                   |   |    [,PLSS=xplss|NO_PLSS]                  * 00171000
*                   |   |    [,SLSS=xslss|NO_SLSS]                  * 00172000
*                   |   |    [,ASYNCH=xasynch|NO                    * 00173000
*                   |   |        [,ECB=xecb|NO_ECB]]                * 00174000
*                   |   |    [,WAITTIME=xwaittime|0]                * 00175000
*                   |    ,REQUEST=PQUERY                             * 00176000
*                   |   |     ,DEVN=xdevn                             * 00177000
*                   |   |     ,QRYSIZE=xqrysize                       * 00178000
*                   |   |     ,QRYINFO=xqryinfo                       * 00179000
*                   |   |     ,RETINFO=xretinfo                       * 00180000
*                   |   |    [,ALET=xalet|0]                        * 00181000
*                   |   |    [,ASYNCH=xasynch|NO                    * 00182000
*                   |   |        [,ECB=xecb|NO_ECB]]                * 00183000
*                   |   |    [,BITMAP=xbitmap|YES]                  * 00184000
*                   |   |    [,PATHS=xpaths|NO]                     * 00185000
*                   |   |    [,WAITTIME=xwaittime|0]                * 00186000
*                   |    ,REQUEST=PRECOVER                           * 00187000
*                   |   |     ,DEVN=xdevn                             * 00188000
*                   |   |     ,PCCA=xpcca                             * 00189000
*                   |   |     ,PSERIAL=xpserial                       * 00190000
*                   |   |     ,PSSID=xpssid                           * 00191000
*                   |   |     ,SCCA=xscca                             * 00192000
