.***AC.PLV01  PROCESS PLISTVER                                      *   03662000
.********************************************************************   03663000
&XPLISTVER      SETA  0                                                 03664000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       03665000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   03666000
&XPLISTVER      SETA  4                                                 03667000
.ZPLV0          ANOP  ,                                                 03668000
.*                                                                      03669000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      03670000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      03671000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  03672000
.*-------------------------------------------------------------------   03673000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 03674000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                03675000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 03676000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 03677000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               03678000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                03679000
                AGO   .ZPVCK2                                           03680000
.ZPVCK3         ANOP  ,                                                 03681000
.*-------------------------------------------------------------------   03682000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  03683000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                03684000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 03685000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         03686000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 03687000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                03688000
                AGO   .ZPVCKE                                           03689000
.ZPVCK2         ANOP  ,                                                 03690000
.*-------------------------------------------------------------------   03691000
&ZCVAL          SETC  '&PLISTVER(1)'                                    03692000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             03693000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             03694000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        03695000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      03696000
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     03697000
.ZPVCK6         ANOP  ,                                                 03698000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                03699000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 03700000
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         03701000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         03702000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                03703000
&XPLISTVER      SETA  4                                                 03704000
                AGO   .ZPVCKE                                           03705000
.*-------------------------------------------------------------------   03706000
.ZPVCK4A        ANOP  ,                                                 03707000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        03708000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                03709000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               03710000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          03711000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 03712000
                MNOTE 8,' &ZMS IS REQUIRED.'                            03713000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                03714000
                AGO   .ZPVCK5                                           03715000
.ZPVCK4         ANOP  ,                                                 03716000
&XPLISTVER      SETA  &PLISTVER(1)                                      03717000
.ZPVCK5         ANOP  ,                                                 03718000
.ZPVCKE         ANOP  ,                                                 03719000
.********************************************************************   03720000
