*01* EXAMPLES:                                                        * 00362100
*                                                                     * 00362200
*    Operation:  Getting the current consistency time for             * 00362300
*    XRC session TUCSON in assembler.                                 * 00362400
*                                                                     * 00362500
*               TITLE 'TESTER: Test ANTRQST'                          * 00362600
*      TESTER   CSECT ,                                               * 00362700
*               USING *,15                                            * 00362800
*               B     @PROLOG                                         * 00362900
*               DC    AL1(16)                                         * 00363000
*               DC    C'TESTER    97.133'                             * 00363100
*               DROP  15                                              * 00363200
*      @PROLOG  STM   14,12,12(13)                                    * 00363300
*               LR    12,15                                           * 00363400
*      @PSTART  EQU   TESTER                                          * 00363500
*               USING @PSTART,12                                      * 00363600
*               LA    15,0                                            * 00363700
*               L     0,@SIZDATD+4                                    * 00363800
*               GETMAIN  RU,LV=(0),SP=(15)                            * 00363900
*               LR    15,13                                           * 00364000
*               LR    13,01                                           * 00364100
*               USING @DATD,13                                        * 00364200
*               ST    15,4(,13)                                       * 00364300
*               ST    13,8(,15)                                       * 00364400
*               LM    15,01,16(15)                                    * 00364500
*               MVC   XRCSID,=CL8'TUCSON  '                           * 00364600
*               ANTRQST ILK=XRC,                     X                * 00364700
*                     REQUEST=XCONTIME,              X                * 00364800
*                     SID=XRCSID,                    X                * 00364900
*                     TIME=CONTIME,                  X                * 00365000
*                     RETINFO=XRETINFO,              X                * 00365100
*                     RETCODE=RTNCD,                 X                * 00365200
*                     RSNCODE=RSNCD,                 X                * 00365300
*                     MF=(E,P_LIST)                                   * 00365400
*               CLC   RTNCD,=F'0'                                     * 00365500
*               BE    SKIP1                                           * 00365600
*               MVC   RTCD,RTNCD                                      * 00365700
*               MVC   RSCD,RSNCD                                      * 00365800
*               B     SKIP2                                           * 00365900
*      SKIP1    DS    0H                                              * 00366000
*               MVC   RTCD,RTC                                        * 00366100
*               MVC   RSCD,RSC                                        * 00366200
*      SKIP2    DS    0H                                              * 00366300
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 00366400
*               BNE   SKIP3                                           * 00366500
*               WTO   'ANTAS000 NOT ACTIVE'                           * 00366600
*      SKIP3    DS    0H                                              * 00366700
*               L     2,RTCD                                          * 00366800
*               L     3,RSCD                                          * 00366900
*               LR    1,13                                            * 00367000
*               L     13,4(,13)                                       * 00367100
*               LA    15,0                                            * 00367200
*               L     0,@SIZDATD+4                                    * 00367300
*               FREEMAIN RU,LV=(0),A=(1),SP=(15)                      * 00367400
*               LR    15,2                                            * 00367500
*               LR    0,3                                             * 00367600
*               L     14,12(,13)                                      * 00367700
*               LM    1,12,24(13)                                     * 00367800
*               BR    14                                              * 00367900
*               LTORG                                                 * 00368000
*      @SIZDATD DS    0A                                              * 00368100
*               DC    AL1(0)                                          * 00368200
*               DC    AL3(@DYNSIZE)                                   * 00368300
*      @DATD    DSECT                                                 * 00368400
*               DS    18F                                             * 00368500
*      RTNCD    DS    F                                               * 00368600
*      RSNCD    DS    F                                               * 00368700
*      RTCD     DS    F                                               * 00368800
*      RSCD     DS    F                                               * 00368900
*      XRCSID   DS    CL8                                             * 00369000
*      CONTIME  DS    CL8                                             * 00369100
*      XRETINFO DS    0F,CL100                                        * 00369200
*               ORG   XRETINFO                                        * 00369300
*      RTC      DS    F                                               * 00369400
*      RSC      DS    F                                               * 00369500
*               ORG   XRETINFO+100                                    * 00369600
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 00369700
*               ORG   *+1-(*-@DATD)/(*-@DATD)                         * 00369800
*      @ENDDATD DS    0X                                              * 00369900
*      @DYNSIZE EQU   ((@ENDDATD-@DATD+7)/8)*8                        * 00370000
*               END   TESTER                                          * 00370100
*                                                                     * 00370200
*                                                                     * 00370300
*    Operation:  Getting the current consistency time for             * 00370400
*    XRC session TUCSON in assembler and using the ASYNCH parameter.  * 00370500
*                                                                     * 00370600
*               TITLE 'TESTER: Test ANTRQST'                          * 00370700
*      TESTER   CSECT ,                                               * 00370800
*               USING *,15                                            * 00370900
*               B     @PROLOG                                         * 00371000
*               DC    AL1(16)                                         * 00371100
*               DC    C'TESTER    97.133'                             * 00371200
*               DROP  15                                              * 00371300
*      @PROLOG  STM   14,12,12(13)                                    * 00371400
*               LR    12,15                                           * 00371500
*      @PSTART  EQU   TESTER                                          * 00371600
*               USING @PSTART,12                                      * 00371700
*               LA    15,0                                            * 00371800
*               L     0,@SIZDATD+4                                    * 00371900
*               GETMAIN  RU,LV=(0),SP=(15)                            * 00372000
*               LR    15,13                                           * 00372100
*               LR    13,01                                           * 00372200
*               USING @DATD,13                                        * 00372300
*               ST    15,4(,13)                                       * 00372400
*               ST    13,8(,15)                                       * 00372500
*               LM    15,01,16(15)                                    * 00372600
*               MVC   XRCSID,=CL8'TUCSON  '                           * 00372700
*               MVC   XASYNCH,=CL3'YES'                               * 00372800
*               XC    XECB,XECB                                       * 00372900
*               ANTRQST ILK=XRC,                     X                * 00373000
*                     REQUEST=XCONTIME,              X                * 00373100
*                     SID=XRCSID,                    X                * 00373200
*                     TIME=CONTIME,                  X                * 00373300
*                     RETINFO=XRETINFO,              X                * 00373400
*                     ASYNCH=XASYNCH,                X                * 00373500
*                     ECB=XECB,                      X                * 00373600
*                     RETCODE=RTNCD,                 X                * 00373700
*                     RSNCODE=RSNCD,                 X                * 00373800
*                     MF=(E,P_LIST)                                   * 00373900
*               CLC   RTNCD,=F'0'                                     * 00374000
*               BE    SKIP1                                           * 00374100
*               MVC   RTCD,RTNCD                                      * 00374200
*               MVC   RSCD,RSNCD                                      * 00374300
*               B     SKIP2                                           * 00374400
*      SKIP1    DS    0H                                              * 00374500
*               WAIT  ECB=XECB                                        * 00374600
*               NC    XECB,=XL4'3FFFFFFF'                             * 00374700
*               CLC   XECB,=F'0'                                      * 00374800
*               BE    SKIP1A                                          * 00374900
*               MVC   RTCD,XECB                                       * 00375000
*               MVC   RSCD,=F'0'                                      * 00375100
*               B     SKIP2                                           * 00375200
*      SKIP1A   DS    0H                                              * 00375300
*               MVC   RTCD,RTC                                        * 00375400
*               MVC   RSCD,RSC                                        * 00375500
*      SKIP2    DS    0H                                              * 00375600
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 00375700
*               BNE   SKIP3                                           * 00375800
*               WTO   'ANTAS000 NOT ACTIVE'                           * 00375900
*      SKIP3    DS    0H                                              * 00376000
*               L     2,RTCD                                          * 00376100
*               L     3,RSCD                                          * 00376200
*               LR    1,13                                            * 00376300
*               L     13,4(,13)                                       * 00376400
*               LA    15,0                                            * 00376500
*               L     0,@SIZDATD+4                                    * 00376600
*               FREEMAIN RU,LV=(0),A=(1),SP=(15)                      * 00376700
*               LR    15,2                                            * 00376800
*               LR    0,3                                             * 00376900
*               L     14,12(,13)                                      * 00377000
*               LM    1,12,24(13)                                     * 00377100
*               BR    14                                              * 00377200
*               LTORG                                                 * 00377300
*      @SIZDATD DS    0A                                              * 00377400
*               DC    AL1(0)                                          * 00377500
*               DC    AL3(@DYNSIZE)                                   * 00377600
*      @DATD    DSECT                                                 * 00377700
*               DS    18F                                             * 00377800
*      RTNCD    DS    F                                               * 00377900
*      RSNCD    DS    F                                               * 00378000
*      RTCD     DS    F                                               * 00378100
*      RSCD     DS    F                                               * 00378200
*      XECB     DS    F                                               * 00378300
*      XASYNCH  DS    CL3                                             * 00378400
*      XRCSID   DS    CL8                                             * 00378500
*      CONTIME  DS    CL8                                             * 00378600
*      XRETINFO DS    0F,CL100                                        * 00378700
*               ORG   XRETINFO                                        * 00378800
*      RTC      DS    F                                               * 00378900
*      RSC      DS    F                                               * 00379000
*               ORG   XRETINFO+100                                    * 00379100
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 00379200
*               ORG   *+1-(*-@DATD)/(*-@DATD)                         * 00379300
*      @ENDDATD DS    0X                                              * 00379400
*      @DYNSIZE EQU   ((@ENDDATD-@DATD+7)/8)*8                        * 00379500
*               END   TESTER                                          * 00379600
*                                                                     * 00379700
*                                                                     * 00379800
*    Operation:  Getting the current consistency time for             * 00379900
*    XRC session TUCSON in PL/X.                                      * 00380000
*                                                                     * 00380100
*       @PROCESS                                                      * 00380200
*         NOMSOURCE                                                   * 00380300
*         GENMARGINS(1,72)                                            * 00380400
*         OPTIMIZE(ADV,48)                                            * 00380500
*         ENVIRONMENT(MVS/ESA)                                        * 00380600
*         SOURCE(SEGMENT)                                             * 00380700
*         TITLE('TESTER: Test ANTRQST');                              * 00380800
*                                                                     * 00380900
*        TESTER: PROCEDURE                                            * 00381000
*                 OPTIONS(                                            * 00381100
*                         AMODE(31)                                   * 00381200
*                         CODEREG(12)                                 * 00381300
*                         DATAREG(13)                                 * 00381400
*                         REENTRANT                                   * 00381500
*                         RMODE(ANY)                                  * 00381600
*                         );                                          * 00381700
*                                                                     * 00381800
*       DCL                                                           * 00381900
*         1 XRETINFO CHAR(100) BDY(WORD),                             * 00382000
*           3 RTC FIXED(32),                                          * 00382100
*           3 RSC FIXED(32);                                          * 00382200
*       DCL XSID CHAR(8) CONSTANT('TUCSON  ');                        * 00382300
*       DCL XHLQ CHAR(8) CONSTANT('SYS1    ');                        * 00382400
*       DCL XTIME CHAR(8);                                            * 00382500
*       DCL RTCD FIXED(31);                                           * 00382600
*       DCL RSCD FIXED(31);                                           * 00382700
*       DCL RTNCD FIXED(31);                                          * 00382800
*       DCL RSNCD FIXED(31);                                          * 00382900
*                                                                     * 00383000
*         ?ANTRQST ILK(XRC)                                           * 00383100
*                  REQUEST(XCONTIME)                                  * 00383200
*                  SID(XSID)                                          * 00383300
*                  TIME(XTIME)                                        * 00383400
*                  HLQ(XHLQ)                                          * 00383500
*                  RETINFO(XRETINFO)                                  * 00383600
*                  RETCODE(RTNCD)                                     * 00383700
*                  RSNCODE(RSNCD)                                     * 00383800
*                  MF(E,PARMLIST);                                    * 00383900
*                                                                     * 00384000
*         IF RTNCD ^= 0 THEN                                          * 00384100
*           DO;                                                       * 00384200
*             RTCD = RTNCD;                                           * 00384300
*             RSCD = RSNCD;                                           * 00384400
*           END;                                                      * 00384500
*         ELSE                                                        * 00384600
*           DO;                                                       * 00384700
*             RTCD = RTC;                                             * 00384800
*             RSCD = RSC;                                             * 00384900
*           END;                                                      * 00385000
*                                                                     * 00385100
*         IF RTCD = RQST_PC_NUMBER_ZERO THEN                          * 00385200
*           ?WTO ('ANTAS000 NOT ACTIVE');                             * 00385300
*                                                                     * 00385400
*         RETURN CODE(RTCD,RSCD);                                     * 00385500
*                                                                     * 00385600
*         ?ANTRQSTL NAME(PARMLIST);                                   * 00385700
*                                                                     * 00385800
*         ?EPILOG;                                                    * 00385900
*                                                                     * 00386000
*       END TESTER;                                                   * 00386100
*                                                                     * 00386200
*                                                                     * 00386300
*                                                                     * 00386400
*01* MACRO USAGE NOTES:                                               * 00386500
*                                                                     * 00386600
*                                                                     * 00386700
*    1.  Printing of the macro expansion is controlled by the ZPRINT  * 00386800
*        global macro variable.  Any value other than 'NO' will       * 00386900
*        result in the macro expansion being printed.  The default is * 00387000
*        'YES'.  The following examples illustrate how the ZPRINT     * 00387100
*        variable should be set.                                      * 00387200
*                                                                     * 00387300
*                    GBLC &ZPRINT                                     * 00387400
*            &ZPRINT SETC 'YES'          &ZPRINT SETC 'NO'            * 00387500
*                                                                     * 00387600
*                                                                     * 00387700
*    2.  When using the NOCHECK option in conjunction with the Modify * 00387800
*        and/or  Execute  forms,  be  sure  that  it is preceded by a * 00387900
*        Modify   or  Execute  form  invocation  which  specifies  or * 00388000
*        defaults  to  the  COMPLETE  option.   This insures that the * 00388100
*        parameter  list  is  initialized completely.   The parameter * 00388200
*        list  field which identifies the version number of the macro * 00388300
*        is only set when the Standard form or the Complete option of * 00388400
*        the  Execute/Modify  form  is  used,  or  when  PLISTVER  is * 00388500
*        explicitly  specified.  Be sure that the resulting parameter * 00388600
*        list version number covers all the keys that you use.        * 00388700
*                                                                     * 00388800
*                                                                     * 00388900
*    3.  The  ANTRQTA macro supports multiple versions. The macro key * 00389000
*        list  below  contains the version level in which the key was * 00389100
*        first  introduced.  Positionals  are  identified by the word * 00389200
*        'Positional'  followed by the numeric value of its position. * 00389300
*        Nested  positionals  are  preceded  by their outer key.  The * 00389400
*        lowest value represents the original version and the highest * 00389500
*        value  represents  the  most  recent  version of the ANTRQTA * 00389600
*        macro.    When  specifying  PLISTVER,  be sure that it is at * 00389700
*        least  as high as the highest version number of all the keys * 00389800
*        being used.                                                  * 00389900
*                                                                     * 00390000
*        VERSION   KEY                                                * 00390100
*                                                                     * 00390200
*              0      ALET                                            * 00390300
*                     ASYNCH                                          * 00390400
*                     ATTIME                                          * 00390500
*                     COPY                                            * 00390600
*                     DONOTBLOCK                                      * 00390700
*                     ECB                                             * 00390800
*                     ERRLVL                                          * 00390900
*                     ETYPE                                           * 00391000
*                     HLQ                                             * 00391100
*                     ILK                                             * 00391200
*                     MESSAGES                                        * 00391300
*                     MHLQ                                            * 00391400
*                     MID                                             * 00391500
*                     ONLINE                                          * 00391600
*                     PAGEFIX                                         * 00391700
*                     PRIORITY                                        * 00391800
*                     PVOLSER                                         * 00391900
*                     REQUEST                                         * 00392000
*                     RETINFO                                         * 00392100
*                     RFREQUENCY                                      * 00392200
*                     RTRACKS                                         * 00392300
*                     SCSESSION                                       * 00392400
*                     SCSYNCHP                                        * 00392500
*                     SCSYNCHS                                        * 00392600
*                     SID                                             * 00392700
*                     SSID                                            * 00392800
*                     STYPE                                           * 00392900
*                     SUSPENDED                                       * 00393000
*                     SVOLSER                                         * 00393100
*                     SYNCH                                           * 00393200
*                     TIME                                            * 00393300
*                     TIMEOUT                                         * 00393400
*                     UTILITY                                         * 00393500
*                     WAITTIME                                        * 00393600
*                                                                     * 00393700
*    4.  ANTRQST  Message  Buffer.   The format of an ANTRQST message * 00393800
*        buffer is:                                                   * 00393900
*                                                                     * 00394000
*                                                                     * 00394100
*        o   Bytes  1-4  contain  the  size  of  the  ANTRQST message * 00394200
*            buffer.                                                  * 00394300
*                                                                     * 00394400
*        o   Byte  5  contains  the  subpool  of  the ANTRQST message * 00394500
*            buffer.                                                  * 00394600
*                                                                     * 00394700
*        o   Byte 6 is not used.                                      * 00394800
*                                                                     * 00394900
*        o   Bytes  7-8  contain  the number of messages in the array * 00395000
*            that follows.                                            * 00395100
*                                                                     * 00395200
*        o   The  rest of the message buffer is an array of messages. * 00395300
*            Each message has the following format:                   * 00395400
*                                                                     * 00395500
*                                                                     * 00395600
*            -   Bytes  1-2  contain the size of the message in bytes * 00395700
*                3-x.                                                 * 00395800
*                                                                     * 00395900
*            -   Bytes 3-x contain the message.                       * 00396000
*                                                                     * 00396100
*                                                                     * 00396200
*                                                                     * 00396300
*                                                                     * 00396400
*                                                                     * 00396500
*        IT  IS  THE  RESPONSIBILITY  OF  THE CALLER TO FREEMAIN THIS * 00396600
*        BUFFER.                                                      * 00396700
*                                                                     * 00396800
*                                                                     * 00396900
*                                                                     * 00397000
*01* CHANGE ACTIVITY:                                                 * 00397100
*                                                                     * 00397200
*   Flag LineItem  FMID    Date   ID    Comment                       * 00397300
*                                                                     * 00397400
*    $00=OW30320  HDZ11C0 970531 TUCRNC:                              * 00397500
*    $01=OW29881  HDZ11C0 980309 TUCRNC:                              * 00397600
*    $02=OW35368  HDZ11C0 981026 TUCHAD:                              * 00397700
*    $03=OW33608  HDZ11C0 980901 TUCRNC:                              * 00397800
*    $04=OW37501  HDZ11C0 990210 TUCHAD:                              * 00397900
*    $05=OW42971  HDZ11C0 990517 TUCRNC:                              * 00398000
*    $06=OW43316  HDZ11C0 000711 TUCRNC:                              * 00398100
****** END OF SPECIFICATIONS *****************************************/ 00398200
*/* TLINE 1 *********************************************************** 00398300
         MACRO                                                          00398400
&XLABEL  ANTRQTA &ILK=,&REQUEST=,&RETINFO=,&SID=,&PVOLSER=,&SVOLSER=,  *00398500
               &ERRLVL=,&SCSESSION=,&DONOTBLOCK=,&COPY=,&SUSPENDED=,   *00398600
               &MESSAGES=,&ASYNCH=,&ECB=,&WAITTIME=,&HLQ=,&ONLINE=,    *00398700
               &TIME=,&ALET=,&ETYPE=,&MHLQ=,&MID=,&ATTIME=,&UTILITY=,  *00398800
               &PAGEFIX=,&SCSYNCHP=,&SCSYNCHS=,&SYNCH=,&TIMEOUT=,      *00398900
               &SSID=,&RFREQUENCY=,&RTRACKS=,&PRIORITY=,&STYPE=,       *00399000
               &RETCODE=,&RSNCODE=,&PLISTVER=,&MF=                      00399100
.*                                                                      00399200
*                MACDATE -03/19/01-<4>                                  00399300
.*                                                                      00399400
.*******************************************************************    00399500
