                AIF   ('&SID' NE '').ZG378B     KEYS?                   00424100
                AIF   ('&PVOLSER' NE '').ZG378B  KEYS?                  00424200
                AIF   ('&SVOLSER' NE '').ZG378B  KEYS?                  00424300
                AIF   ('&ERRLVL' NE '').ZG378B  KEYS?                   00424400
                AIF   ('&SCSESSION' NE '').ZG378B  KEYS?                00424500
                AIF   ('&DONOTBLOCK' NE '').ZG378B  KEYS?               00424600
                AIF   ('&COPY' NE '').ZG378B    KEYS?                   00424700
                AIF   ('&SUSPENDED' NE '').ZG378B  KEYS?                00424800
                AIF   ('&MESSAGES' NE '').ZG378B  KEYS?                 00424900
                AIF   ('&ASYNCH' NE '').ZG378B  KEYS?                   00425000
                AIF   ('&ECB' NE '').ZG378B     KEYS?                   00425100
                AIF   ('&WAITTIME' NE '').ZG378B  KEYS?                 00425200
                AIF   ('&HLQ' NE '').ZG378B     KEYS?                   00425300
                AIF   ('&ONLINE' NE '').ZG378B  KEYS?                   00425400
                AIF   ('&TIME' NE '').ZG378B    KEYS?                   00425500
                AIF   ('&ALET' NE '').ZG378B    KEYS?                   00425600
                AIF   ('&ETYPE' NE '').ZG378B   KEYS?                   00425700
                AIF   ('&MHLQ' NE '').ZG378B    KEYS?                   00425800
                AIF   ('&MID' NE '').ZG378B     KEYS?                   00425900
                AIF   ('&ATTIME' NE '').ZG378B  KEYS?                   00426000
                AIF   ('&UTILITY' NE '').ZG378B  KEYS?                  00426100
                AIF   ('&PAGEFIX' NE '').ZG378B  KEYS?                  00426200
                AIF   ('&SCSYNCHP' NE '').ZG378B  KEYS?                 00426300
                AIF   ('&SCSYNCHS' NE '').ZG378B  KEYS?                 00426400
                AIF   ('&SYNCH' NE '').ZG378B   KEYS?                   00426500
                AIF   ('&TIMEOUT' NE '').ZG378B  KEYS?                  00426600
                AIF   ('&SSID' NE '').ZG378B    KEYS?                   00426700
                AIF   ('&RFREQUENCY' NE '').ZG378B  KEYS?               00426800
                AIF   ('&RTRACKS' NE '').ZG378B  KEYS?                  00426900
                AIF   ('&PRIORITY' NE '').ZG378B  KEYS?                 00427000
                AIF   ('&STYPE' NE '').ZG378B   KEYS?                   00427100
