.*******************************************************************    01578400
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       01578500
.*******************************************************************    01578600
                AIF   (&ZMACRET NE 0).ZLX2      ERROR DETECTED          01578700
.*******************************************************************    01578800
.***AE.01  START OF ANTRQTA MACRO CODE                                  01578900
.*******************************************************************    01579000
                LCLC  &M0M0001                 NAME                     01579100
&M0M0001        SETC  '&XLABEL'                SET NAME                 01579200
                AIF   (K'&XLABEL NE 0).XLAB2                            01579300
&M0M0001        SETC  'M00M&SYSNDX'            DEFAULT NAME             01579400
.*                                                                      01579500
.XLAB2          ANOP  ,                                                 01579600
                AIF   ('&XMF' EQ 'L').XLAB3                             01579700
&M0M0001 DS    0H                             ANTRQTA-4                 01579800
.XLAB3          ANOP  ,                                                 01579900
.*******************************************************************    01580000
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              01580100
.*******************************************************************    01580200
              PUSH  PRINT                                               01580300
                AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON        01580400
                PRINT OFF         ++ PR01                               01580500
                AGO   .ZPRNTXX                                          01580600
.ZPRINTX        PRINT ON                                                01580700
.ZPRNTXX        ANOP  ,                                                 01580800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01580900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01581000
.********************************************************************   01581100
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     01581200
.********************************************************************   01581300
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       01581400
                      LCLA  &ZOPN1         INVOCATION INDEX             01581500
                      LCLC  &ZOPL(4095)    LABEL                        01581600
                      LCLC  &ZOPO(4095)    OPERATION                    01581700
                      LCLC  &ZOPV(4095)    VALUE                        01581800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  01581900
.*                                                                      01582000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     01582100
.******************************************************************     01582200
.***AL.L10 ANTRQTA FMT=LIST MF=S|L PARAMETERS                           01582300
.******************************************************************     01582400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     01582500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     01582600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     01582700
&XMFATTR              SETC  '0D'                                        01582800
.*                                                                      01582900
&ZOPN                 SETA  &ZOPN+1                                     01583000
