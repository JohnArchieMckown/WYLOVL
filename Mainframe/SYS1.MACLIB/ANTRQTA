.*-------------------------------------------------------------------   01645400
.***AL.L12 ANTRQTA FMT=LIST MF=S|L PARAMETERS                           01645500
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         01645600
.*                                                                      01645700
&ZOPN                 SETA  &ZOPN+1                                     01645800
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                01645900
&ZOPO(&ZOPN)          SETC  'DC'                                        01646000
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   01646100
&ZOPD(&ZOPN)          SETC  '++ ANTRQTA ARG PARMS'                      01646200
.*                                                                      01646300
.ZLMFSLX              ANOP  ,                                           01646400
.*-------------------------------------------------------------------   01646500
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          01646600
&ZPLLEN               SETA 512                                          01646700
.*                                                                      01646800
.******************************************************************     01646900
.***AI.L01 ANTRQTA FMT=LIST FILL IN PARAMETER AREA                      01647000
.******************************************************************     01647100
                      ACTR  10000                                       01647200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01647300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01647400
&ZCVAL                SETC  'LAE'          XAX MODE                     01647500
.ZIMFXA               ANOP ,                                            01647600
.*                                                                      01647700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01647800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01647900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01648000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01648100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01648200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01648300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01648400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01648500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01648600
&ZOPN                 SETA  &ZOPN+1                                     01648700
&ZOPL(&ZOPN)          SETC  ' '                                         01648800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01648900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01649000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01649100
                      AGO   .ZIMFNX                                     01649200
.*                                                                      01649300
.ZIMFN2               ANOP ,                                            01649400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01649500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01649600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01649700
&ZOPN                 SETA  &ZOPN+1                                     01649800
&ZOPL(&ZOPN)          SETC  ' '                                         01649900
&ZOPO(&ZOPN)          SETC  'LAE'                                       01650000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01650100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01650200
                      AGO   .ZIMFNX                                     01650300
.ZIMFNX1              ANOP ,                                            01650400
&ZOPN                 SETA  &ZOPN+1                                     01650500
&ZOPL(&ZOPN)          SETC  ' '                                         01650600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01650700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                01650800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01650900
.ZIMFNX               ANOP ,                                            01651000
.********************************************************************   01651100
.***AI.L02 ANTRQTA FMT=LIST INITIALIZE PARAMETER AREA                   01651200
.********************************************************************   01651300
.***AI.L03 MF=(SLME)                                                    01651400
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      01651500
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            01651600
&ZAWRK                SETA  0                                           01651700
&ZN                   SETA  &ZPLLEN                                     01651800
.ZCLRCONT             AIF   (&ZN LE 256).ZCLRX                          01651900
.*                                                                      01652000
&ZOPN                 SETA  &ZOPN+1                                     01652100
&ZOPL(&ZOPN)          SETC  ' '                                         01652200
&ZOPO(&ZOPN)          SETC  'XC'                                        01652300
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(256,1),&ZAWRK.(1)'                 01652400
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             01652500
.*                                                                      01652600
&ZAWRK                SETA &ZAWRK+256                                   01652700
&ZN                   SETA  &ZN-256                                     01652800
                      AGO   .ZCLRCONT                                   01652900
.*                                                                      01653000
.ZCLRX                ANOP  ,                                           01653100
.*                                                                      01653200
&ZOPN                 SETA  &ZOPN+1                                     01653300
&ZOPL(&ZOPN)          SETC  ' '                                         01653400
&ZOPO(&ZOPN)          SETC  'XC'                                        01653500
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(&ZN,1),&ZAWRK.(1)'                 01653600
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             01653700
.ZIMFLX               ANOP  ,                                           01653800
.*-------------------------------------------------------------------   01653900
.***AI.L60 XEYECATCH CL8                                                01654000
&ZOPN                 SETA  &ZOPN+1                                     01654100
