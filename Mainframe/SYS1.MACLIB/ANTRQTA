&ZOPL(&ZOPN)          SETC  ' '                                         01773200
&ZOPO(&ZOPN)          SETC  'MVI'                                       01773300
&ZOPV(&ZOPN)          SETC  '511(1),B''10111100'''                      01773400
&ZOPD(&ZOPN)          SETC  '++ XL1 CONSTANT'                           01773500
.*-------------------------------------------------------------------   01773600
&ZOPN1                SETA &ZOPN                                        01773700
.*-------------------------------------------------------------------   01773800
.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              01773900
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         01774000
.*******************************************************************    01774100
.***  PROCESS ANTRQTA OUTPUT PARAMETERS                                 01774200
.*******************************************************************    01774300
                      ACTR  10000                                       01774400
.*******************************************************************    01774500
.***AO.X02  RETURN CODE PROCESSING                                      01774600
.*******************************************************************    01774700
                      AIF   (N'&RETCODE LE 0).ZMRETX                    01774800
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            01774900
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             01775000
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             01775100
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               01775200
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                01775300
&ZOPN                 SETA  &ZOPN+1                                     01775400
&ZOPL(&ZOPN)          SETC  ' '                                         01775500
&ZOPO(&ZOPN)          SETC  'ST'                                        01775600
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              01775700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01775800
                      AGO   .ZMRETX                                     01775900
.*                                                                      01776000
.ZMRET2               ANOP  ,                                           01776100
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                01776200
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                01776300
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                01776400
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  01776500
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   01776600
&ZOPN                 SETA  &ZOPN+1                                     01776700
&ZOPL(&ZOPN)          SETC  ' '                                         01776800
&ZOPO(&ZOPN)          SETC  'LR'                                        01776900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 01777000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01777100
.ZMRETX               ANOP  ,                                           01777200
.*******************************************************************    01777300
.***AO.X03  REASON CODE PROCESSING                                      01777400
.*******************************************************************    01777500
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    01777600
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            01777700
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              01777800
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             01777900
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              01778000
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             01778100
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                01778200
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 01778300
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                01778400
&ZOPN                 SETA  &ZOPN+1                                     01778500
&ZOPL(&ZOPN)          SETC  ' '                                         01778600
&ZOPO(&ZOPN)          SETC  'ST'                                        01778700
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               01778800
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        01778900
                      AGO   .ZMRSNX                                     01779000
.*                                                                      01779100
.ZMRSN2               ANOP  ,                                           01779200
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                01779300
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 01779400
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                01779500
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 01779600
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                01779700
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   01779800
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    01779900
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   01780000
&ZOPN                 SETA  &ZOPN+1                                     01780100
&ZOPL(&ZOPN)          SETC  ' '                                         01780200
&ZOPO(&ZOPN)          SETC  'LR'                                        01780300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  01780400
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        01780500
.ZMRSNX               ANOP  ,                                           01780600
.*******************************************************************    01780700
.***AO.X04  LABEL FOR MF=L PROCESSING                                   01780800
.*******************************************************************    01780900
.ZMFLX                ANOP  ,              MF=L PROCESSING              01781000
.*******************************************************************    01781100
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               01781200
.*******************************************************************    01781300
                      ACTR  10000                                       01781400
.*                                                                      01781500
                      LCLA  &ZLC           COLUMN POSITION              01781600
                      LCLA  &ZOPX          LIST TABLE INDEX             01781700
                      LCLC  &ZLL           LABEL                        01781800
                      LCLC  &ZLOP          OPERATION                    01781900
                      LCLC  &ZLV           VALUE                        01782000
                      LCLC  &ZLD           DESCRIPTION                  01782100
.*                                                                      01782200
&ZOPX                 SETA  1                                           01782300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            01782400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01782500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01782600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01782700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01782800
                      AIF   (K'&ZLD LE 0).ZL5                           01782900
&ZLC                  SETA  35             COMMENT COLUMN               01783000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    01783100
&ZLC                  SETA  33             COMMMENT COLUMN              01783200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   01783300
&ZLV                  SETC  '&ZLV'.' '                                  01783400
                      AGO   .ZL3                                        01783500
.*                                                                      01783600
.ZL4                  ANOP  ,                                           01783700
&ZLV                  SETC  '&ZLV'.' '                                  01783800
.ZL5                  ANOP  ,                                           01783900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01784000
.*                                                                      01784100
&ZLL     &ZLOP &ZLV                                                     01784200
.*                                                                      01784300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01784400
                      AGO   .ZL2           CONTINUE LOOPING             01784500
.*                                                                      01784600
.ZLX                  ANOP  ,                                           01784700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01784800
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01784900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01785000
         L     14,16(,0)                                                01785100
         L     14,1216(,14)                                             01785200
         L     14,44(,14)                                               01785300
         L     14,8(,14)                                                01785400
         L     14,88(,14)                                               01785500
         ICM   14,15,324(14)                                            01785600
         BNZ   A&SYSNDX                                                 01785700
         L     15,=A(RQST_PC_NUMBER_ZERO)                               01785800
         SLR   0,0                                                      01785900
         B     B&SYSNDX                                                 01786000
A&SYSNDX DS    0H                                                       01786100
         PC    0(14)                                                    01786200
B&SYSNDX DS    0H                                                       01786300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01786400
.ZLINKX               ANOP  ,                                           01786500
.*******************************************************************    01786600
.***AP.X01  FORMAT GENERATED CODE                                       01786700
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            01786800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01786900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01787000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01787100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01787200
                      AIF   (K'&ZLD LE 0).ZL51                          01787300
&ZLC                  SETA  35             COMMENT COLUMN               01787400
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   01787500
&ZLC                  SETA  33             COMMENT COLUMN               01787600
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  01787700
&ZLV                  SETC  '&ZLV'.' '                                  01787800
                      AGO   .ZL31                                       01787900
.*                                                                      01788000
.ZL41                 ANOP  ,                                           01788100
&ZLV                  SETC  '&ZLV'.' '                                  01788200
.ZL51                 ANOP  ,                                           01788300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01788400
.*                                                                      01788500
&ZLL     &ZLOP &ZLV                                                     01788600
.*                                                                      01788700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01788800
                      AGO   .ZL21          CONTINUE LOOPING             01788900
.*                                                                      01789000
.ZLX1                 ANOP  ,                                           01789100
.*******************************************************************    01789200
