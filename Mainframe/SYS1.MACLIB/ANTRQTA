*%ANTRQTA: MACRO KEYS(ILK,REQUEST,RETINFO,SID,PVOLSER,SVOLSER,ERRLVL,   01790200
*                 SCSESSION,DONOTBLOCK,COPY,SUSPENDED,MESSAGES,ASYNCH,  01790300
*                 ECB,WAITTIME,HLQ,ONLINE,TIME,ALET,ETYPE,MHLQ,MID,     01790400
*                 ATTIME,UTILITY,PAGEFIX,SCSYNCHP,SCSYNCHS,SYNCH,       01790500
*                 TIMEOUT,SSID,RFREQUENCY,RTRACKS,PRIORITY,STYPE,       01790600
*                 RETCODE,RSNCODE,PLISTVER,MF);                         01790700
