.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00428300
&ZXMFCTRL       SETC  'INPUT'     SET IN/OUTPUT                         00428400
&XMFCTRL        SETC  '&MF(2,1)'  SET XVARIABLE                         00428500
                AIF   (N'&MF(2) GT 1).ZG380B    MULTI PARMS             00428600
&XMFCTRL        SETC  '&MF(2)'    SET XVARIABLE                         00428700
.ZG380B         AIF   (K'&MF(2) GT 0).ZG380C                            00428800
&ZXMFCTRL       SETC  ''          RESET IN/OUTPUT                       00428900
&ZMS            SETC  'WHEN "L" IS SPECIFIED'   *KRQDA*                 00429000
&ZMS            SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'               00429100
                MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                      00429200
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00429300
.ZG380C         ANOP  ,                                                 00429400
.*-------------------------------------------------------------------   00429500
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00429600
                AIF   (K'&ZXMFCTRL LE 0).ZG380P  NOT I/O                00429700
                AIF   ('&XMFCTRL'(1,1) NE '(').ZG380P                   00429800
&ZXMFCTRL       SETC  ''          RESET IN/OUTPUT                       00429900
