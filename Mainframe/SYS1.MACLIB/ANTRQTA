.***AC.PLV01  PROCESS PLISTVER                                      *   00414300
.********************************************************************   00414400
&XPLISTVER      SETA  0                                                 00414500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00414600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00414700
&XPLISTVER      SETA  4                                                 00414800
.ZPLV0          ANOP  ,                                                 00414900
.*                                                                      00415000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00415100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00415200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00415300
.*-------------------------------------------------------------------   00415400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00415500
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00415600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00415700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00415800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00415900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00416000
                AGO   .ZPVCK2                                           00416100
.ZPVCK3         ANOP  ,                                                 00416200
.*-------------------------------------------------------------------   00416300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00416400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00416500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00416600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00416700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00416800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00416900
                AGO   .ZPVCKE                                           00417000
.ZPVCK2         ANOP  ,                                                 00417100
.*-------------------------------------------------------------------   00417200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00417300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00417400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00417500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00417600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00417700
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00417800
.ZPVCK6         ANOP  ,                                                 00417900
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00418000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00418100
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00418200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00418300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00418400
&XPLISTVER      SETA  4                                                 00418500
                AGO   .ZPVCKE                                           00418600
.*-------------------------------------------------------------------   00418700
.ZPVCK4A        ANOP  ,                                                 00418800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00418900
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00419000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00419100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00419200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00419300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00419400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00419500
                AGO   .ZPVCK5                                           00419600
.ZPVCK4         ANOP  ,                                                 00419700
&XPLISTVER      SETA  &PLISTVER(1)                                      00419800
.ZPVCK5         ANOP  ,                                                 00419900
.ZPVCKE         ANOP  ,                                                 00420000
.********************************************************************   00420100
