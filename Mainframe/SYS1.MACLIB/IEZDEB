*     2 DEB2XFG1 BIT(8),            /* FIRST FLAG BYTE           @16A*/ 71590600
*       3 DEB2XOSM  BIT(1),         /* JFCMEDIA SET BY OPEN      @16A*/ 71591800
*       3 DEB2XNLM  BIT(1),         /* NL TP DS OPENED FOR MOD   @LLA*/ 71592200
*       3 DEB2XTCO  BIT(1),         /* TCLOSE TAPE OUTPUT        @LLA*/ 71592600
*       3 DEB2XIBC  BIT(1),         /* INACCURATE BLOCK COUNT    @LLA*/ 71593000
*       3 DEB2XNL1  BIT(1),         /* NL TAPE FILE SEQ 1        @LLA*/ 71593400
*       3 *         BIT(3),         /* RESERVED                  @LLA*/ 71593800
*     2 DEB2XUOF FIXED(8),          /* TIOT DD OFFSET OF UNIT    @17C*/ 71594200
*     2 DEB2XLSQ BIT(16),           /* TAPE VOL SEQ FROM LABEL   @LGC*/ 71595400
*     2 DEB2XALV CHAR(1),           /* AL VERSION DURING OUTPUT  @LJA*/ 71595600
*     2 *        CHAR(1),           /* RESERVED                  @LJC*/ 71595800
*     2 DEB2XTIM PTR(31),           /* TIME STAMP POINTER        @18A*/ 71596000
*     2 * CHAR(3),                  /* RESERVED                  @LJA*/ 71596100
*     2 DEB2XCFG BIT(8),            /* ANSI V4 CCSID FLAGS       @LJA*/ 71596200
*       3 DEB2XIBM BIT(1),          /* IBM VERSION 4 CREATED TAPE@LJA*/ 71596300
*       3 DEB2XOUT BIT(1),          /* OPENED FOR OUTPUT NOT MOD @LJA*/ 71596400
*       3 DEB2XMOD BIT(1),          /* OPENED FOR OUTPUT DISP MOD@LJA*/ 71596500
*       3 DEB2XUDF BIT(1),          /* USER APPLICATION CCSID WAS       71596600
*                                      DEFAULTED                 @LJA*/ 71596700
*       3 DEB2XTDF BIT(1),          /* TAPE CCSID WAS DEFAULTED  @LJA*/ 71596800
*       3 DEB2XIGN BIT(1),          /* TAPE CCSID SPECIFIED BUT NO      71597000
*                                      CONVERSION DONE - CCSID WAS      71597200
*                                      IGNORED                   @LJA*/ 71597400
*     2 DEB2XUSR FIXED(32),         /* CCSID OF USER APPLICATION @LJA*/ 71597600
*     2 DEB2XTPE FIXED(32),         /* CCSID OF TAPE             @LJA*/ 71597800
*     2 DEB2XLBL FIXED(32),         /* CCSID IN EXISTING LABEL   @LJA*/ 71598000
*     2 DEB2XSDC PTR(31),           /* PTR TO SAM DATA CONVERSION       71598200
*                                       BLOCK                    @LJA*/ 71598400
*     2 DEB2XTBL FIXED(31),         /* ACCUMULATIVE BLKCNT ACROSS@LLA   71598500
*                                      VOLUMES                   @LLA*/ 71598600
*     2 DEB2XTCB FIXED(31),         /* DCB BLKCOUNT TCLOSE OUTPUT@LLA*/ 71598700
*     2 DEB2XSGT FIXED(8),          /* SG TYPE (GENERAL/SPECIFIC)@LMA*/ 71598820
*     2 *        CHAR(7),           /* RESERVED                  @LMC*/ 71598840
*     2 DEB2XEND CHAR(0);           /*                           @13A*/ 71598900
*%IF IKEPLS=''  %THEN                                                   71599000
