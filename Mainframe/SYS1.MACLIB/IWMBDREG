                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00136200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00136300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00136400
