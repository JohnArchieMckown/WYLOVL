                      AGO   .ZMRSNX                                     00137300
.*                                                                      00137400
.ZMRSN2               ANOP  ,                                           00137500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00137600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00137700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00137800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00137900
