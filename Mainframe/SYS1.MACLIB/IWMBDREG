&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00134100
                      AGO   .ZMRETX                                     00134200
.*                                                                      00134300
.ZMRET2               ANOP  ,                                           00134400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00134500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00134600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00134700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00134800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00134900
