.*******************************************************************    00139400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00139500
.*******************************************************************    00139600
                      ACTR  10000                                       00139700
.*                                                                      00139800
                      LCLA  &ZLC           COLUMN POSITION              00139900
                      LCLA  &ZOPX          LIST TABLE INDEX             00140000
                      LCLC  &ZLL           LABEL                        00140100
                      LCLC  &ZLOP          OPERATION                    00140200
                      LCLC  &ZLV           VALUE                        00140300
                      LCLC  &ZLD           DESCRIPTION                  00140400
.*                                                                      00140500
&ZOPX                 SETA  1                                           00140600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00140700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00140800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00140900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00141000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00141100
                      AIF   (K'&ZLD LE 0).ZL5                           00141200
&ZLC                  SETA  35             COMMENT COLUMN               00141300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00141400
&ZLC                  SETA  33             COMMMENT COLUMN              00141500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00141600
&ZLV                  SETC  '&ZLV'.' '                                  00141700
                      AGO   .ZL3                                        00141800
.*                                                                      00141900
.ZL4                  ANOP  ,                                           00142000
&ZLV                  SETC  '&ZLV'.' '                                  00142100
.ZL5                  ANOP  ,                                           00142200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00142300
.*                                                                      00142400
&ZLL     &ZLOP &ZLV                                                     00142500
.*                                                                      00142600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00142700
                      AGO   .ZL2           CONTINUE LOOPING             00142800
.*                                                                      00142900
.ZLX                  ANOP  ,                                           00143000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00143100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00143200
         L     14,16(0,0)              GET CVT ADDRESS                  00143300
         L     14,772(14,0)            GET SFT ADDRESS                  00143400
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00143500
         LA    15,60                   GET EX FOR SERVICE ROUTINE       00143600
         OR    14,15                   GET PC NUMBER                    00143700
         PC    0(14)                                                    00143800
END&SYSNDX DS    0H                    END LABEL                        00143900
