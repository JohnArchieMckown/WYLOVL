&LRD     SETB  ('&OPTCD(&I)' EQ 'LRD' OR &LRD)                  X04SVHS 03050000
&WAITX   SETB  ('&OPTCD(&I)' EQ 'WAITX' OR &WAITX)             @ZA07549 03100000
&NWAITX  SETB  ('&OPTCD(&I)' EQ 'NWAITX' OR &NWAITX)           @ZA07549 03105000
&NRI     SETB  ('&OPTCD(&I)' EQ 'NRI' OR &NRI)                     @L1A 03105800
&CR      SETB  ('&OPTCD(&I)' EQ 'CR' OR &CR)                       @L1A 03106600
&CRE     SETB  ('&OPTCD(&I)' EQ 'CRE' OR &CRE)                     @L1A 03107400
&KL      SETB  ('&OPTCD(&I)' EQ 'KL' OR &KL)                       @L1A 03108200
&NOKL    SETB  ('&OPTCD(&I)' EQ 'NOKL' OR &NOKL)                   @L1A 03109000
&RBA     SETB  ('&OPTCD(&I)' EQ 'RBA' OR &RBA)                     @EAA 03109300
&XRBA    SETB  ('&OPTCD(&I)' EQ 'XRBA' OR &XRBA)                   @EAA 03109600
&A       SETA  &KEY+&ADR+&CNV+&SEQ+&DIR+&SKP+&SYN+&ASY+&NUP+&UPD+&NSP+&*03110000
               KEQ+&KGE+&FKS+&GEN+&MVE+&LOC+&FWD+&BWD+&ARD+&LRD+&WAITX+*03130000
               &NWAITX+&NRI+&CR+&CRE+&KL+&NOKL+&RBA+&XRBA          @EAC 03145000
.*             &A CONTAINS THE NUMBER OF VSAM OPTIONS SPECIFIED         03160000
.*             &B, &C, AND &D CONTAIN THE NUMBER OF VTAM OPTIONS SPEC   03170000
.*             &ERR CONTAINS THE NUMBER OF ERRORS                       03180000
         AIF   (&A+&B+&C+&D+&ERR EQ &I).T000100                         03200000
.*             IF &A+&B+&C+&D+&ERR EQUAL &I, THE LAST OPTION CHECKED    03250000
.*             WAS A VALID VSAM OPTION.  ELSE CHECK FOR A VALID VTAM    03300000
.*             OPTION                                                   03310000
&CA      SETB  ('&OPTCD(&I)' EQ 'CA' OR &CA)                     X03004 03350000
&CS      SETB  ('&OPTCD(&I)' EQ 'CS' OR &CS)                     X03004 03400000
