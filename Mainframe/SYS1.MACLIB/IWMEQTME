.***AC.PLV01  PROCESS PLISTVER                                      *   21450000
.********************************************************************   21500000
&XPLISTVER      SETA  0                                                 21550000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       21600000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   21650000
&XPLISTVER      SETA  0                                                 21700000
.ZPLV0          ANOP  ,                                                 21750000
.*                                                                      21800000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      21850000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      21900000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  21950000
.*-------------------------------------------------------------------   22000000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 22050000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                22100000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 22150000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 22200000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               22250000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                22300000
                AGO   .ZPVCK2                                           22350000
.ZPVCK3         ANOP  ,                                                 22400000
.*-------------------------------------------------------------------   22450000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  22500000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                22550000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 22600000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         22650000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 22700000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                22750000
                AGO   .ZPVCKE                                           22800000
.ZPVCK2         ANOP  ,                                                 22850000
.*-------------------------------------------------------------------   22900000
&ZCVAL          SETC  '&PLISTVER(1)'                                    22950000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             23000000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             23050000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        23100000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      23150000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     23200000
.ZPVCK6         ANOP  ,                                                 23250000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                23300000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 23350000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         23400000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         23450000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                23500000
&XPLISTVER      SETA  0                                                 23550000
                AGO   .ZPVCKE                                           23600000
.*-------------------------------------------------------------------   23650000
.ZPVCK4A        ANOP  ,                                                 23700000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        23750000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                23800000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               23850000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          23900000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 23950000
                MNOTE 8,' &ZMS IS REQUIRED.'                            24000000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                24050000
                AGO   .ZPVCK5                                           24100000
.ZPVCK4         ANOP  ,                                                 24150000
&XPLISTVER      SETA  &PLISTVER(1)                                      24200000
.ZPVCK5         ANOP  ,                                                 24250000
.ZPVCKE         ANOP  ,                                                 24300000
.********************************************************************   24350000
