.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          78768500
&ZPLLEN               SETA 186                                          78794800
.*                                                                      78821100
.******************************************************************     78847400
.***AI.L01 IXGCONN FMT=LIST FILL IN PARAMETER AREA                      78873700
.******************************************************************     78900000
                      ACTR  10000                                       78926300
&ZCVAL                SETC  'LA'           ASSUME XA MODE               78952600
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    78978900
&ZCVAL                SETC  'LAE'          XAX MODE                     79005200
.ZIMFXA               ANOP ,                                            79031500
.*                                                                      79057800
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    79084100
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         79110400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 79136700
