*/*END PROPRIETARY V3 STATEMENT                                      */ 00001800
*/*                                                                  */ 00001900
*/* STATUS - OS/390 DFSMS V2R10                                      */ 00002000
*/*                                                                  */ 00002100
*/* FUNCTION = MAPS ESOTERIC NAME LIST BUILT BY IFG0JES3 AND         */ 00002200
*/*            PASSED TO JES3                                        */ 00002300
*/*                                                                  */ 00002400
*/* NOTES = NONE.                                                    */ 00002500
*/*                                                                  */ 00002600
*/*     DEPENDENCIES = NONE                                          */ 00002700
*/*                                                                  */ 00002800
*/*     RESTRICTIONS = CHANGES TO THIS MACRO MUST BE COORDINATED     */ 00002900
*/*                    WITH JES3 FUNCTION THAT INTERFACES WITH       */ 00003000
*/*                    IFG0JES3.                                     */ 00003100
*/*                                                                  */ 00003300
*/*     REGISTER CONVENTIONS = NOT APPLICABLE.                       */ 00003400
*/*                                                                  */ 00003500
*/* PATCH LABEL = NOT APPLICABLE.                                    */ 00003600
*/*                                                                  */ 00003700
*/* MODULE TYPE = MACRO.                                             */ 00003800
*/*                                                                  */ 00003900
*/*     PROCESSOR = ASSEMBLER LANGUAGE.                              */ 00004000
*/*                                                                  */ 00004100
*/*     MODULE SIZE = VARIABLE                                       */ 00004200
*/*                                                                  */ 00004300
*/*     ATTRIBUTES = PLIST FIELDS.                                   */ 00004400
*/*                                                                  */ 00004500
*/* ENTRY POINT = ENLPL IN BAL AND PLS VERSIONS                      */ 00004600
*/*                                                                  */ 00004700
*/*     PURPOSE = THIS MACRO IS TO BE USED TO MAP THE ESOTERIC       */ 00004800
*/*               NAME LIST THAT IFG0JES3 BUILDS AND PASSES TO       */ 00004900
*/*               JES3. IFG0JES3 IS CALLED VIA MACRO 'IFGXATL',      */ 00005010
*/*               DURING JES3 SMS TAPE PROCESSING. THE ESOTERIC      */ 00005020
*/*               NAME LIST IS BUILT FROM THE DEVICE POOL NAME       */ 00005030
*/*               LIST (DPN) THAT IS PASSED TO IFG0JES3 FROM         */ 00005100
*/*               SMS/OAM. THE DPN LIST IS ACTUALLY BUILT BY OAM     */ 00005200
*/*               BASED ON THE ASSOCIATED SMS STORAGE GROUP(S)).     */ 00005300
*/*                                                                  */ 00005600
*/*     LINKAGE = IFGENL FOR BAL CALL                                */ 00005700
*/*               % INCLUDE SYSLIB(IFGENL) IN PLS CALL               */ 00005800
*/*                                                                  */ 00005900
*/*     INPUT = NONE                                                 */ 00006000
*/*                                                                  */ 00006100
*/*                                                                  */ 00006200
*/*     OUTPUT = ESOTERIC NAME LIST.                                 */ 00006300
*/*                                                                  */ 00006400
*/* EXIT NORMAL = MEND STATEMENT                                     */ 00006500
*/*                                                                  */ 00006600
*/* EXIT ERROR = NONE                                                */ 00006700
*/*                                                                  */ 00006800
*/* EXTERNAL REFERENCES = NONE.                                      */ 00006900
*/*                                                                  */ 00007000
*/* TABLES = NONE.                                                   */ 00007100
*/*                                                                  */ 00007200
*/* MACROS = NONE                                                    */ 00007300
*/*                                                                  */ 00007400
*/* CHANGE ACTIVITY =                                                */ 00007500
*/*                                                                  */ 00007600
*/*$00=IFGENL,HDZ11F0,981231,SJPLJAL: CREATE                         */ 00007890
*/*$01=OW38428,HDZ11F0,990720,SJPLJAL: INCLUDE MACRO SOURCE          */ 00008080
*/*                                                                  */ 00008300
*/* **************************************************************** */ 00008400
*/*                                                                  */ 00008500
*/*                                                                  */ 00008600
*/*      THIS MACRO IS USED TO MAP THE ESOTERIC NAME LIST BUILT BY   */ 00008700
*/*      IFG0JES3 AND PASSED BACK TO JES3 CALLER.                    */ 00008800
*/*                                                                  */ 00009200
*/* **************************************************************** */ 00009300
*/*                                                                  */ 00009400
*/* **************************************************************** */ 00009500
*/*                                                                  */ 00009600
*%GOTO ENLL2;                                                           00009700
ENLPL    DSECT                                                          00009800
ENLLEN   DS    F                   TOTAL LENGTH OF ESOTTERIC NAME LIST  00010000
ENLBTHRS DS    F                   POINTER TO FIRST ESOTERIC NAME ENTRY 00011800
*                                    BELOW SCRATCH THRESHOLD            00011810
ENLNAME  DS    CL8                 ESOTERIC NAME ENTRY                  00011820
ENLEND   DS    F                   X'FFFFFFFF' MARKS END OF LIST        00011900
LISTSP   EQU   241                 ESOTERIC LIST SUBPOOL                00012000
         SPACE 1                                                        00014700
*                                                                       00014800
****************************************************************        00015300
*                                                                       00015400
*             END ESOTERIC NAME LIST PARAMETER LIST                     00015500
*                                                                       00015600
****************************************************************        00015700
         MEND                                                           00015800
*                                                                    */ 00015900
*/*                                                                  */ 00016300
*%ENLL2 : ;                                                    /*    */ 00016400
*DCL 1 ENLPL BASED(LIBLISTA) BDY(WORD),                                 00016500
*      2 ENLLEN   FIXED(31),            /* TOTAL LENGTH OF ESOTERIC     00016600
*                                          NAME LIST                 */ 00016700
*      2 ENLBTHRS PTR(31),              /* POINTER TO 1ST ESOTERIC      00018200
*      2 ENLNAME  CHAR(8),              /* ESOTERIC NAME ENTRY       */ 00018400
*      2 ENLEND   CHAR(4);              /* X'FFFFFFFF' MARKS END OF     00018500
*                                          LIST                      */ 00018600
*DCL LISTSP CONSTANT(241) FIXED(8);     /* ESOTERIC LIST SUBPOOL     */ 00018700
*/*                                                                  */ 00021100
*/* ************************************************************     */ 00021200
*/*                                                                  */ 00021300
*/*            END ESOTERIC NAME LIST PARAMETER LIST                 */ 00021400
*/*                                                                  */ 00021500
*/* ************************************************************     */ 00021600
