 %IFAL36: IF IFAR36='' %THEN %GOTO IFAL60;                    /* @L8A*/ 10201000
    DECLARE                                                             10202000
     1 SMFRCD36 BASED(SMF36PTR) BOUNDARY(WORD),                         10203000
     /* RECORD TYPE 36 - EXPORT ICF CATALOG WRITTEN WHENEVER            10204000
           AN ICF CATALOG IS SUCCESSFULLY EXPORTED               @L8A*/ 10205000
      2 SMF36HDR,                   /* SMF HEADER SECTION        @L8A*/ 10206000
       3 SMF36LEN FIXED(15),        /* RDW RECORD LENGTH         @L8A*/ 10207000
       3 SMF36SEG FIXED(15),        /* RDW SEGMENT DESCRIPTION   @L8A*/ 10208000
       3 SMF36SYS BIT(8),           /* OPERATING ENVIRONMENT     @L8A*/ 10209000
        4 SMF36RS0 BIT(1),          /* RESERVED                  @L8A*/ 10209200
        4 SMF36STU BIT(1),          /* SUB TYPE UTILIZED         @L8A*/ 10209400
        4 SMF36RS1 BIT(6),          /* RESERVED                  @L8A*/ 10209600
       3 SMF36RTY FIXED(8),         /* RECORD TYPE 36            @L8A*/ 10210000
       3 SMF36TME FIXED(31) BOUNDARY(BYTE), /*TIME RECORD WRITTEN@L8A*/ 10211000
       3 SMF36DTE CHARACTER(4),     /* DATE RECORD WRITTEN       @L8A*/ 10212000
       3 SMF36CPU CHARACTER(4),     /*SYSTEM ID FROM INSTALLATION@L8A*/ 10213000
        4 SMF36SID CHARACTER(2),    /*SYSTEM ID FROM INSTALLATION@L8A*/ 10214000
        4 SMF36MOD CHARACTER(2),    /* SYSTEM MODULE ID          @L8A*/ 10215000
       3 SMF36SBS CHARACTER(4),     /* SUBSYSTEM ID (NOT USED)   @L8A*/ 10216000
       3 SMF36SUB CHARACTER(2),     /* RECORD SUBTYPE            @L8A*/ 10217000
      2 SMF36SLF,                   /* SELF-DEFINING SECTION     @L8A*/ 10218000
       3 SMF36NOT FIXED(15),        /* NUMBER OF TRIPLETS        @L8A*/ 10219000
       3 *        FIXED(15),        /* RESERVED                  @L8A*/ 10220000
       3 SMF36POF POINTER(31),      /* OFFSET TO PRODUCT SECTION @L8A*/ 10221000
       3 SMF36PLN FIXED(15),        /* LENGTH OF PRODUCT SECTION @L8A*/ 10222000
       3 SMF36PNO FIXED(15),        /* NUMBER OF PRODUCT SECTIONS@L8A*/ 10223000
       3 SMF36DOF POINTER(31),      /* OFFSET TO DATA SECTION    @L8A*/ 10224000
       3 SMF36DLN FIXED(15),        /* LENGTH OF DATA SECTION    @L8A*/ 10225000
       3 SMF36DNO FIXED(15),        /* NUMBER OF DATA SECTIONS   @L8A*/ 10226000
     1 SMF36PRD BASED(SMF36PTH),    /* PRODUCT SECTION           @L8A*/ 10227000
      2 SMF36PVN CHARACTER(2),      /* PRODUCT VERSION           @L8A*/ 10228000
      2 SMF36PNM CHARACTER(8),      /* PRODUCT NAME              @L8A*/ 10229000
      2 SMF36PRL CHARACTER(2),      /* RECORD LEVEL              @L8A*/ 10230000
     1 SMF36DAT BASED(SMF36PTD),    /* DATA SECTION              @L8A*/ 10231000
      2 SMF36JNM CHARACTER(8),      /* JOB NAME                  @L8A*/ 10232000
      2 SMF36RST CHARACTER(4),      /* READER START TIME(.01 SEC @L8A*/ 10233000
      2 SMF36RDT CHARACTER(4),      /* READER START DATE         @L8A*/ 10234000
      2 SMF36UID CHARACTER(8),      /* USER ID FIELD             @L8A*/ 10235000
      2 SMF36PGM CHARACTER(8),      /* PROGRAM NAME              @L8A*/ 10236000
      2 SMF36CNM CHARACTER(44),     /* ICF CATALOG NAME          @L8A*/ 10237000
      2 SMF36CVS CHARACTER(6),      /* ICF CATALOG VOLUME SERIAL @L8A*/ 10238000
      2 SMF36CDT CHARACTER(4),      /* ICF CATALOG UCB DEVICE    @L8A*/ 10238600
                                    /* TYPE                      @L8A*/ 10239200
      2 SMF36EDT CHARACTER(8),      /* DATE OF EXPORT MM/DD/YY   @L8A*/ 10240000
      2 SMF36ETM CHARACTER(8),      /* TIME OF EXPORT HH:MM:SS   @L8A*/ 10241000
      2 SMF36PDS CHARACTER(44),     /* PORTABLE DATA SET NAME    @L8A*/ 10242000
      2 SMF36PVS CHARACTER(6),      /* PORTABLE DATA SET VOLSER  @L8A*/ 10242500
                                    /* (FIRST/ONLY VOLUME        @L8A*/ 10243000
      2 SMF36PDT CHARACTER(4),      /* PORTABLE DATA SET UCB     @L8A*/ 10243500
                                    /* DEVICE TYPE (ASSOCIATED   @L8A*/ 10244000
                                    /* WITH FIRST/ONLY VOLUME)   @L8A*/ 10244500
      2 SMF36EIN CHARACTER(2);      /* EXPORT INDICATOR          @L8A*/ 10245000
                                    /* 'AE' - ALIASES EXPORTED   @L8A*/ 10246000
                                    /* 'NE' - NO ALIASES EXPORTED@L8A*/ 10247000
 %IFAL60: IF IFAR60='' %THEN %GOTO IFAL61;                              10250000
    DECLARE                                                             10300000
      1 SMFRCD60 BASED(SMF60PTR) BOUNDARY(WORD),                        10350000
    /* RECORD TYPE 60 - VSAM VOLUME RECORD INSERTED, DELETED, OR        10400000
                        UPDATED                                         10450000
       WRITTEN WHENEVER A VSAM VOLUME RECORD INSERT, DELETE, OR         10500000
       UPDATE REQUEST IS MADE AGAINST A VSAM VOLUME DATA SET         */ 10550000
       2 SMF60HDR,                  /* SMF HEADER SECTION            */ 10600000
        3 SMF60LEN FIXED(15),       /* RDW RECORD LENGTH             */ 10650000
        3 SMF60SEG FIXED(15),       /* RDW SEGMENT DESCRIPTOR        */ 10700000
        3 SMF60SYS BIT(8),          /* OPERATING ENVIRONMENT         */ 10750000
         4 * BIT(6),                /* RESERVED                      */ 10800000
         4 SMF60VS2 BIT(1),         /* VS2                           */ 10850000
         4 SMF60VS1 BIT(1),         /* VS1                           */ 10900000
        3 SMF60RTY FIXED(8),        /* RECORD TYPE 60                */ 10950000
        3 SMF60TME FIXED(31) BOUNDARY(BYTE), /* TIME RECORD WRITTEN  */ 11000000
        3 SMF60DTE CHARACTER(4),    /* DATE RECORD WRITTEN           */ 11050000
        3 SMF60CPU CHARACTER(4),    /* SYSTEM ID FROM INSTALLATION   */ 11100000
