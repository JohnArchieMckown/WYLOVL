.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00143800
                   AIF   (N'&SYSLIST NE 0).ZG148B  POSITIONALS?         00143900
                   AIF   ('&WORKAREA' NE '').ZG148B  KEYS?              00144000
                   AIF   ('&UCBAREA' NE '').ZG148B  KEYS?               00144100
                   AIF   ('&CMXTAREA' NE '').ZG148B  KEYS?              00144200
                   AIF   ('&DEVNCHAR' NE '').ZG148B  KEYS?              00144300
                   AIF   ('&UCBPAREA' NE '').ZG148B  KEYS?              00144400
                   AIF   ('&DCEAREA' NE '').ZG148B  KEYS?               00144500
                   AIF   ('&DCELEN' NE '').ZG148B  KEYS?                00144600
                   AIF   ('&VOLSER' NE '').ZG148B  KEYS?                00144700
                   AIF   ('&UCBPTR' NE '').ZG148B  KEYS?                00144800
                   AIF   ('&TEXT' NE '').ZG148B  KEYS?                  00144900
                   AIF   ('&PTOKEN' NE '').ZG148B  KEYS?                00145000
