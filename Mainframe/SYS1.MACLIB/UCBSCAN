                      AGO   .ZMRSNX                                     00521800
.*                                                                      00521900
.ZMRSN2               ANOP  ,                                           00522000
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00522100
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00522200
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00522300
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00522400
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00522500
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00522600
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00522700
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00522800
&ZOPN                 SETA  &ZOPN+1                                     00522900
&ZOPL(&ZOPN)          SETC  ' '                                         00523000
&ZOPO(&ZOPN)          SETC  'LR'                                        00523100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00523200
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00523300
.ZMRSNX               ANOP  ,                                           00523400
.*******************************************************************    00523500
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00523600
.*******************************************************************    00523700
.ZMFLX                ANOP  ,              MF=L PROCESSING              00523800
.*******************************************************************    00523900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00524000
.*******************************************************************    00524100
                      ACTR  10000                                       00524200
.*                                                                      00524300
                      LCLA  &ZLC           COLUMN POSITION              00524400
                      LCLA  &ZOPX          LIST TABLE INDEX             00524500
                      LCLC  &ZLL           LABEL                        00524600
                      LCLC  &ZLOP          OPERATION                    00524700
                      LCLC  &ZLV           VALUE                        00524800
                      LCLC  &ZLD           DESCRIPTION                  00524900
.*                                                                      00525000
&ZOPX                 SETA  1                                           00525100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00525200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00525300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00525400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00525500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00525600
                      AIF   (K'&ZLD LE 0).ZL5                           00525700
&ZLC                  SETA  35             COMMENT COLUMN               00525800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00525900
&ZLC                  SETA  33             COMMMENT COLUMN              00526000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00526100
&ZLV                  SETC  '&ZLV'.' '                                  00526200
                      AGO   .ZL3                                        00526300
.*                                                                      00526400
.ZL4                  ANOP  ,                                           00526500
&ZLV                  SETC  '&ZLV'.' '                                  00526600
.ZL5                  ANOP  ,                                           00526700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00526800
.*                                                                      00526900
&ZLL     &ZLOP &ZLV                                                     00527000
.*                                                                      00527100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00527200
                      AGO   .ZL2           CONTINUE LOOPING             00527300
.*                                                                      00527400
.ZLX                  ANOP  ,                                           00527500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00527600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00527700
.*-----------BAL invocation code-------------                           00527800
         LA    0,2                ++ SET UCBSCAN FUNCTION CODE          00527900
         AIF    ('&XLINKAGE' NE 'BRANCH').LNKPC1                        00528000
.*********************************************************************  00528100
.*                                                                   *  00528200
.*  Get addressability to CVT                                        *  00528300
.*                                                                   *  00528400
.*********************************************************************  00528500
         L     15,16(0,0)                                               00528600
.*********************************************************************  00528700
.*                                                                   *  00528800
.*  Get addressability to IOCOM                                      *  00528900
.*                                                                   *  00529000
.*********************************************************************  00529100
         L     15,124(15,0)                                             00529200
.*********************************************************************  00529300
.*                                                                   *  00529400
.*  Get addressability to IOVT                                       *  00529500
.*                                                                   *  00529600
.*********************************************************************  00529700
         L     15,208(15,0)                                             00529800
         L     15,12(15,0)        ++ SET ADDR OF UCB SERVICES           00529900
         BALR  14,15              ++ BRANCH TO UCB SERVICES             00530000
         AGO   .DONE1                                                   00530100
.LNKPC1  ANOP                                                           00530200
         L     14,16(0,0)         ++ GET CVT ADDRESS                    00530300
         L     14,772(14,0)       ++ GET SFT ADDRESS                    00530400
.*********************************************************************  00530500
.*                                                                   *  00530600
.*  Get LX/EX for UCB Services                                       *  00530700
.*                                                                   *  00530800
.*  Note: LX/EX is calculated by multiplying the PC number (78/79)   *  00530900
.*        times the length of a PC/AUTH table entry (4) and          *  00531000
.*        then subtracting 4.                                        *  00531100
.*                                                                   *  00531200
.*        authorized functions (ADDRESS and EXIT) invoke PC 78       *  00531300
.*        unauthorized function (COPY) invokes PC 79                 *  00531400
.*                                                                   *  00531500
.*********************************************************************  00531600
         AIF    ('&XOUTVAR1' EQ 'ADDRESS').LNKPC2                       00531700
         AIF    ('&XOUTVAR1' EQ 'EXIT').LNKPC2                          00531800
         L     14,312(14,0)       ++ GET LX/EX FOR UCB SERVICES         00531900
         AGO   .DONE2                                                   00532000
.LNKPC2  ANOP                                                           00532100
         L     14,308(14,0)       ++ GET LX/EX FOR UCB SERVICES         00532200
.DONE2   ANOP                                                           00532300
         PC    0(14)              ++ PC TO UCB SERVICES                 00532400
.DONE1   ANOP                                                           00532500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00532600
.ZLINKX               ANOP  ,                                           00532700
.*******************************************************************    00532800
.***AP.X01  FORMAT GENERATED CODE                                       00532900
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00533000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00533100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00533200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00533300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00533400
                      AIF   (K'&ZLD LE 0).ZL51                          00533500
&ZLC                  SETA  35             COMMENT COLUMN               00533600
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00533700
&ZLC                  SETA  33             COMMENT COLUMN               00533800
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00533900
&ZLV                  SETC  '&ZLV'.' '                                  00534000
                      AGO   .ZL31                                       00534100
.*                                                                      00534200
.ZL41                 ANOP  ,                                           00534300
&ZLV                  SETC  '&ZLV'.' '                                  00534400
.ZL51                 ANOP  ,                                           00534500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00534600
.*                                                                      00534700
&ZLL     &ZLOP &ZLV                                                     00534800
.*                                                                      00534900
