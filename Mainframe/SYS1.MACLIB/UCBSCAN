&ZOPO(&ZOPN)          SETC  'LR'                                        00519700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00519800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00519900
.ZMRETX               ANOP  ,                                           00520000
.*******************************************************************    00520100
.***AO.X03  REASON CODE PROCESSING                                      00520200
.*******************************************************************    00520300
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00520400
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00520500
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00520600
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00520700
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00520800
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00520900
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00521000
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00521100
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00521200
&ZOPN                 SETA  &ZOPN+1                                     00521300
&ZOPL(&ZOPN)          SETC  ' '                                         00521400
&ZOPO(&ZOPN)          SETC  'ST'                                        00521500
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00521600
