.***AC.PLV01  PROCESS PLISTVER                                      *   00133600
.********************************************************************   00133700
&XPLISTVER      SETA  1                                                 00133800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00133900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00134000
&XPLISTVER      SETA  2                                                 00134100
.ZPLV0          ANOP  ,                                                 00134200
.*                                                                      00134300
                AIF   ('&UCBCXPTR' EQ '').ZPLV1E                        00134400
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00134500
&XPLISTVER      SETA  2                                                 00134600
.ZPLV1E         ANOP  ,                                                 00134700
.*                                                                      00134800
                AIF   ('&UCBPXPTR' EQ '').ZPLV2E                        00134900
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00135000
&XPLISTVER      SETA  2                                                 00135100
.ZPLV2E         ANOP  ,                                                 00135200
.*                                                                      00135300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00135400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00135500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00135600
.*-------------------------------------------------------------------   00135700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00135800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00135900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00136000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00136100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00136200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00136300
                AGO   .ZPVCK2                                           00136400
.ZPVCK3         ANOP  ,                                                 00136500
.*-------------------------------------------------------------------   00136600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00136700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00136800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00136900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00137000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00137100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00137200
                AGO   .ZPVCKE                                           00137300
.ZPVCK2         ANOP  ,                                                 00137400
.*-------------------------------------------------------------------   00137500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00137600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00137700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00137800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00137900
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00138000
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00138100
.ZPVCK6         ANOP  ,                                                 00138200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00138300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00138400
&ZMS            SETC  '&ZMS THE RANGE IS "1-2"'                         00138500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00138600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00138700
&XPLISTVER      SETA  2                                                 00138800
                AGO   .ZPVCKE                                           00138900
.*-------------------------------------------------------------------   00139000
.ZPVCK4A        ANOP  ,                                                 00139100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00139200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00139300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00139400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00139500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00139600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00139700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00139800
                AGO   .ZPVCK5                                           00139900
.ZPVCK4         ANOP  ,                                                 00140000
&XPLISTVER      SETA  &PLISTVER(1)                                      00140100
.ZPVCK5         ANOP  ,                                                 00140200
.ZPVCKE         ANOP  ,                                                 00140300
.********************************************************************   00140400
