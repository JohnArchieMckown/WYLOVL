&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00350400
                      AGO   .ZALVL6B                                    00350500
.ZALVL6A              ANOP                                              00350600
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00350700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00350800
.ZALVL6B              ANOP                                              00350900
&ZOPD(&ZOPN)          SETC  '++ UCBSCAN ARG PARMS'                      00351000
.*                                                                      00351100
&ZOPN                 SETA  &ZOPN+1                                     00351200
&ZOPL(&ZOPN)          SETC  ' '                                         00351300
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00351400
&ZOPV(&ZOPN)          SETC  '1'                                         00351500
&ZOPD(&ZOPN)          SETC  ' '                                         00351600
.*                                                                      00351700
