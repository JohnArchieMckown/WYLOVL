*DECLARE                                                                41600000
*   1 JBCLDHDR BASED ,              /* Job class section header      */ 41700000
*     3 JBCTEYE CHAR(4),            /* Eye catcher                   */ 41800000
*     3 JBCTOHDR FIXED(15),         /* Offset to first section       */ 41900000
*     3 *        FIXED(15),         /* Reserved for future use       */ 42000000
*     3 JBCTNEXT PTR(31),           /* Address of next CAT           */ 42100000
*     3 *        FIXED(31),         /* Reserved for future use       */ 42200000
*     3 *        FIXED(31);         /* Reserved for future use       */ 42300000
*   DCL JBCTEYEC  CHAR(4) CONSTANT('DCAT'); /* Eye catcher value     */ 42400000
*   DCL JBCLDHSZ FIXED(8) CONSTANT(LENGTH(JBCLDHDR));  /* Length     */ 42500000
*/*------------------------------------------------------------------*/ 42600000
*/*      Class Attribute Table Prefix section                        */ 42700000
*/*------------------------------------------------------------------*/ 42800000
*DECLARE                                                                42900000
*   1 JBCTPREF BASED ,              /* Prefix section                */ 43000000
*     3 JBCTHDLN FIXED(16),         /* Length of section             */ 43100000
*     3 JBCTHDTP FIXED(8),          /* Section type                  */ 43200000
*     3 JBCTHDMD FIXED(8);          /* Section modifier              */ 43300000
*/*------------------------------------------------------------------*/ 43400000
*/*      Class Attribute Table data section                          */ 43500000
*/*------------------------------------------------------------------*/ 43600000
*DECLARE                                                                43700000
*   1 JBCLDCAT BASED ,              /* Job class data                */ 43800000
*     3 JBCTLEN  FIXED(16),         /* Length of section             */ 43900000
*     3 JBCTTYPE FIXED(8),          /* Section type                  */ 44000000
*     3 JBCTMOD  FIXED(8),          /* Section modifier              */ 44100000
*     3 JBCJOBFL BIT(8),            /* Job flags                     */ 44200000
*       5 JBCBATCH BIT(1),          /* Batch job                     */ 44300000
*       5 JBCTSUJB BIT(1),          /* Time Sharing User             */ 44400000
*       5 JBCSTCJB BIT(1),          /* System task                   */ 44500000
*       5 JBCNOJNL BIT(1),          /* No journal option             */ 44600000
*       5 JBCNOUPT BIT(1),          /* No output option              */ 44700000
