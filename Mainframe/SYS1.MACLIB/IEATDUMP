.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00636900
&ZPLLEN               SETA 124                                          00637000
.*                                                                      00637100
.******************************************************************     00637200
.***AI.L01 IEATDUMP FMT=LIST FILL IN PARAMETER AREA                     00637300
.******************************************************************     00637400
                      ACTR  10000                                       00637500
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00637600
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00637700
&ZCVAL                SETC  'LAE'          XAX MODE                     00637800
.ZIMFXA               ANOP ,                                            00637900
.*                                                                      00638000
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00638100
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00638200
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00638300
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00638400
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00638500
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00638600
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00638700
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00638800
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00638900
&ZOPN                 SETA  &ZOPN+1                                     00639000
&ZOPL(&ZOPN)          SETC  ' '                                         00639100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00639200
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00639300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00639400
                      AGO   .ZIMFNX                                     00639500
.*                                                                      00639600
.ZIMFN2               ANOP ,                                            00639700
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00639800
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00639900
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00640000
&ZOPN                 SETA  &ZOPN+1                                     00640100
&ZOPL(&ZOPN)          SETC  ' '                                         00640200
&ZOPO(&ZOPN)          SETC  'LAE'                                       00640300
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00640400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00640500
                      AGO   .ZIMFNX                                     00640600
.ZIMFNX1              ANOP ,                                            00640700
&ZOPN                 SETA  &ZOPN+1                                     00640800
&ZOPL(&ZOPN)          SETC  ' '                                         00640900
