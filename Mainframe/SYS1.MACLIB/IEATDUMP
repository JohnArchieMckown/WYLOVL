                  AIF   ('&DSP_ORIGIN' NE '').ZG87B  KEYS?              00174400
                  AIF   ('&DSP_RECORDS@' NE '').ZG87B  KEYS?            00174500
                  AIF   ('&HDRAD' NE '').ZG87B  KEYS?                   00174600
                  AIF   ('&HDR' NE '').ZG87B    KEYS?                   00174700
                  AIF   ('&IDXAD' NE '').ZG87B  KEYS?                   00174800
                  AIF   ('&IDX' NE '').ZG87B    KEYS?                   00174900
