                  AIF   ('&PROBDESCAD' NE '').ZG87B  KEYS?              00175600
                  AIF   ('&PROBDESC' NE '').ZG87B  KEYS?                00175700
                  AIF   ('&LISTAD' NE '').ZG87B  KEYS?                  00175800
                  AIF   ('&LIST' NE '').ZG87B   KEYS?                   00175900
                  AIF   ('&SUBPLSTAD' NE '').ZG87B  KEYS?               00176000
                  AIF   ('&SUBPLST' NE '').ZG87B  KEYS?                 00176100
                  AIF   ('&DSPLISTAD' NE '').ZG87B  KEYS?               00176200
