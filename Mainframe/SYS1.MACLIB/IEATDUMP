.*******************************************************************    00571500
.***AE.01  START OF IEATDUMP MACRO CODE                                 00571600
.*******************************************************************    00571700
                  LCLC  &M0M0001                 NAME                   00571800
&M0M0001          SETC  '&XLABEL'                SET NAME               00571900
                  AIF   (K'&XLABEL NE 0).XLAB2                          00572000
&M0M0001          SETC  'M00M&SYSNDX'            DEFAULT NAME           00572100
.*                                                                      00572200
.XLAB2            ANOP  ,                                               00572300
                  AIF   ('&XMF' EQ 'L').XLAB3                           00572400
&M0M0001 DS    0H                             IEATDUMP-1                00572500
.XLAB3            ANOP  ,                                               00572600
.*******************************************************************    00572700
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00572800
.*******************************************************************    00572900
              PUSH  PRINT                                               00573000
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      00573100
                  PRINT OFF         ++ PR01                             00573200
                  AGO   .ZPRNTXX                                        00573300
.ZPRINTX          PRINT ON                                              00573400
.ZPRNTXX          ANOP  ,                                               00573500
.********************************************************************   00573600
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00573700
.********************************************************************   00573800
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00573900
                      LCLA  &ZOPN1         INVOCATION INDEX             00574000
                      LCLC  &ZOPL(4095)    LABEL                        00574100
                      LCLC  &ZOPO(4095)    OPERATION                    00574200
                      LCLC  &ZOPV(4095)    VALUE                        00574300
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00574400
.*                                                                      00574500
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00574600
.******************************************************************     00574700
.***AL.L10 IEATDUMP FMT=LIST MF=S|L PARAMETERS                          00574800
.******************************************************************     00574900
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00575000
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00575100
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00575200
&XMFATTR              SETC  '0D'                                        00575300
.*                                                                      00575400
&ZOPN                 SETA  &ZOPN+1                                     00575500
&ZOPL(&ZOPN)          SETC  ' '                                         00575600
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00575700
&ZOPO(&ZOPN)          SETC  'B'                                         00575800
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00575900
                      AGO   .ZALVL6B                                    00576000
.ZALVL6A              ANOP                                              00576100
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00576200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00576300
.ZALVL6B              ANOP                                              00576400
&ZOPD(&ZOPN)          SETC  '++ IEATDUMP ARG PARMS'                     00576500
.*                                                                      00576600
&ZOPN                 SETA  &ZOPN+1                                     00576700
&ZOPL(&ZOPN)          SETC  ' '                                         00576800
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00576900
&ZOPV(&ZOPN)          SETC  '1'                                         00577000
&ZOPD(&ZOPN)          SETC  ' '                                         00577100
.*                                                                      00577200
.ZLMFSL2              ANOP  ,                                           00577300
.*-------------------------------------------------------------------   00577400
