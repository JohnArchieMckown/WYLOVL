&ZJ               SETA  &ZJ+1       NEXT SUBLIST                        00368800
                  AIF   (&ZJ GT N'&SDATA).ZG51C  DONE                   00368900
                  AIF   (K'&SDATA(&ZJ) LE 0).ZG51A  NULL                00369000
&ZI               SETA  &ZI+1       NEXT ARRAY ENTRY                    00369100
&XSDATA(&ZI)   SETC  ''             NULL ENTRY                          00369200
&XSDATA(&ZI+1) SETC  ''             NULL AT ARRAY END                   00369300
.ZG52B  AIF   ('&SDATA(&ZJ)' NE 'DEFS').ZG53B                           00369400
&XSDATA(&ZI)   SETC  'DEFS'         SET KEYWORD                         00369500
.ZG53B  AIF   ('&SDATA(&ZJ)' NE 'ALLNUC').ZG54B                         00369600
&XSDATA(&ZI)   SETC  'ALLNUC'       SET KEYWORD                         00369700
.ZG54B  AIF   ('&SDATA(&ZJ)' NE 'CSA').ZG55B                            00369800
&XSDATA(&ZI)   SETC  'CSA'          SET KEYWORD                         00369900
.ZG55B  AIF   ('&SDATA(&ZJ)' NE 'GRSQ').ZG56B                           00370000
&XSDATA(&ZI)   SETC  'GRSQ'         SET KEYWORD                         00370100
.ZG56B  AIF   ('&SDATA(&ZJ)' NE 'LPA').ZG57B                            00370200
&XSDATA(&ZI)   SETC  'LPA'          SET KEYWORD                         00370300
.ZG57B  AIF   ('&SDATA(&ZJ)' NE 'LSQA').ZG58B                           00370400
&XSDATA(&ZI)   SETC  'LSQA'         SET KEYWORD                         00370500
.ZG58B  AIF   ('&SDATA(&ZJ)' NE 'NUC').ZG59B                            00370600
&XSDATA(&ZI)   SETC  'NUC'          SET KEYWORD                         00370700
.ZG59B  AIF   ('&SDATA(&ZJ)' NE 'RGN').ZG60B                            00370800
&XSDATA(&ZI)   SETC  'RGN'          SET KEYWORD                         00370900
.ZG60B  AIF   ('&SDATA(&ZJ)' NE 'SQA').ZG61B                            00371000
&XSDATA(&ZI)   SETC  'SQA'          SET KEYWORD                         00371100
.ZG61B  AIF   ('&SDATA(&ZJ)' NE 'SUM').ZG62B                            00371200
&XSDATA(&ZI)   SETC  'SUM'          SET KEYWORD                         00371300
.ZG62B  AIF   ('&SDATA(&ZJ)' NE 'SWA').ZG63B                            00371400
&XSDATA(&ZI)   SETC  'SWA'          SET KEYWORD                         00371500
.ZG63B  AIF   ('&SDATA(&ZJ)' NE 'TRT').ZG64B                            00371600
&XSDATA(&ZI)   SETC  'TRT'          SET KEYWORD                         00371700
.ZG64B  AIF   ('&SDATA(&ZJ)' NE 'PSA').ZG65B                            00371800
&XSDATA(&ZI)   SETC  'PSA'          SET KEYWORD                         00371900
.ZG65B            AIF   ('&XSDATA(&ZI)' NE '').ZG51A                    00372000
&ZI               SETA  &ZI-1       BACKUP ARRAY                        00372100
