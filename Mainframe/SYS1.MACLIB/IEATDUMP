.***AO.X02  RETURN CODE PROCESSING                                      00781000
.*******************************************************************    00781100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00781200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00781300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00781400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00781500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00781600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00781700
&ZOPN                 SETA  &ZOPN+1                                     00781800
&ZOPL(&ZOPN)          SETC  ' '                                         00781900
&ZOPO(&ZOPN)          SETC  'ST'                                        00782000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00782100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00782200
                      AGO   .ZMRETX                                     00782300
.*                                                                      00782400
.ZMRET2               ANOP  ,                                           00782500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00782600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00782700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00782800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00782900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00783000
&ZOPN                 SETA  &ZOPN+1                                     00783100
&ZOPL(&ZOPN)          SETC  ' '                                         00783200
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00783300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00783400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00783500
.ZMRETX               ANOP  ,                                           00783600
.*******************************************************************    00783700
.***AO.X03  REASON CODE PROCESSING                                      00783800
.*******************************************************************    00783900
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00784000
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00784100
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00784200
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00784300
