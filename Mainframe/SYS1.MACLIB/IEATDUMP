.***AC.PLV01  PROCESS PLISTVER                                      *   00164400
.********************************************************************   00164500
&XPLISTVER      SETA  1                                                 00164600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00164700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00164800
&XPLISTVER      SETA  1                                                 00164900
.ZPLV0          ANOP  ,                                                 00165000
.*                                                                      00165100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00165200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00165300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00165400
.*-------------------------------------------------------------------   00165500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00165600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00165700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00165800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00165900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00166000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00166100
                AGO   .ZPVCK2                                           00166200
.ZPVCK3         ANOP  ,                                                 00166300
.*-------------------------------------------------------------------   00166400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00166500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00166600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00166700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00166800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00166900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00167000
                AGO   .ZPVCKE                                           00167100
.ZPVCK2         ANOP  ,                                                 00167200
.*-------------------------------------------------------------------   00167300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00167400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00167500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00167600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00167700
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00167800
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00167900
.ZPVCK6         ANOP  ,                                                 00168000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00168100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00168200
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00168300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00168400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00168500
&XPLISTVER      SETA  1                                                 00168600
                AGO   .ZPVCKE                                           00168700
.*-------------------------------------------------------------------   00168800
.ZPVCK4A        ANOP  ,                                                 00168900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00169000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00169100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00169200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00169300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00169400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00169500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00169600
                AGO   .ZPVCK5                                           00169700
.ZPVCK4         ANOP  ,                                                 00169800
&XPLISTVER      SETA  &PLISTVER(1)                                      00169900
.ZPVCK5         ANOP  ,                                                 00170000
.ZPVCKE         ANOP  ,                                                 00170100
.********************************************************************   00170200
