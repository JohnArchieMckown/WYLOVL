.***AC.PLV01  PROCESS PLISTVER                                      *   32450000
.********************************************************************   32500000
&XPLISTVER      SETA  0                                                 32550000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       32600000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   32650000
&XPLISTVER      SETA  0                                                 32700000
.ZPLV0          ANOP  ,                                                 32750000
.*                                                                      32800000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      32850000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      32900000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  32950000
.*-------------------------------------------------------------------   33000000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 33050000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                33100000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 33150000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 33200000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               33250000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                33300000
                AGO   .ZPVCK2                                           33350000
.ZPVCK3         ANOP  ,                                                 33400000
.*-------------------------------------------------------------------   33450000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  33500000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                33550000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 33600000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         33650000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 33700000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                33750000
                AGO   .ZPVCKE                                           33800000
.ZPVCK2         ANOP  ,                                                 33850000
.*-------------------------------------------------------------------   33900000
&ZCVAL          SETC  '&PLISTVER(1)'                                    33950000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             34000000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             34050000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        34100000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      34150000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     34200000
.ZPVCK6         ANOP  ,                                                 34250000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                34300000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 34350000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         34400000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         34450000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                34500000
&XPLISTVER      SETA  0                                                 34550000
                AGO   .ZPVCKE                                           34600000
.*-------------------------------------------------------------------   34650000
.ZPVCK4A        ANOP  ,                                                 34700000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        34750000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                34800000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               34850000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          34900000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 34950000
                MNOTE 8,' &ZMS IS REQUIRED.'                            35000000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                35050000
                AGO   .ZPVCK5                                           35100000
.ZPVCK4         ANOP  ,                                                 35150000
&XPLISTVER      SETA  &PLISTVER(1)                                      35200000
.ZPVCK5         ANOP  ,                                                 35250000
.ZPVCKE         ANOP  ,                                                 35300000
.********************************************************************   35350000
