.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00627900
.*******************************************************************    00628000
                      ACTR  10000                                       00628100
.*                                                                      00628200
                      LCLA  &ZLC           COLUMN POSITION              00628300
                      LCLA  &ZOPX          LIST TABLE INDEX             00628400
                      LCLC  &ZLL           LABEL                        00628500
                      LCLC  &ZLOP          OPERATION                    00628600
                      LCLC  &ZLV           VALUE                        00628700
                      LCLC  &ZLD           DESCRIPTION                  00628800
.*                                                                      00628900
&ZOPX                 SETA  1                                           00629000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00629100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00629200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00629300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00629400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00629500
                      AIF   (K'&ZLD LE 0).ZL5                           00629600
&ZLC                  SETA  35             COMMENT COLUMN               00629700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00629800
&ZLC                  SETA  33             COMMMENT COLUMN              00629900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00630000
&ZLV                  SETC  '&ZLV'.' '                                  00630100
                      AGO   .ZL3                                        00630200
.*                                                                      00630300
.ZL4                  ANOP  ,                                           00630400
&ZLV                  SETC  '&ZLV'.' '                                  00630500
.ZL5                  ANOP  ,                                           00630600
