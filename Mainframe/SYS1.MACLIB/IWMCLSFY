.***AC.PLV01  PROCESS PLISTVER                                      *   00115100
.********************************************************************   00115200
&XPLISTVER      SETA  0                                                 00115300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00115400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00115500
&XPLISTVER      SETA  5                                                 00115600
.ZPLV0          ANOP  ,                                                 00115700
.*                                                                      00115800
                AIF   ('&PERFORM' EQ '').ZPLV1E                         00115900
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00116000
&XPLISTVER      SETA  1                                                 00116100
.ZPLV1E         ANOP  ,                                                 00116200
.*                                                                      00116300
                AIF   ('&PRCNAME' EQ '').ZPLV2E                         00116400
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00116500
&XPLISTVER      SETA  1                                                 00116600
.ZPLV2E         ANOP  ,                                                 00116700
.*                                                                      00116800
                AIF   ('&PRCNAME_LEN' EQ '').ZPLV3E                     00116900
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00117000
&XPLISTVER      SETA  1                                                 00117100
.ZPLV3E         ANOP  ,                                                 00117200
.*                                                                      00117300
                AIF   ('&PRIORITY' EQ '').ZPLV4E                        00117400
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00117500
&XPLISTVER      SETA  2                                                 00117600
.ZPLV4E         ANOP  ,                                                 00117700
.*                                                                      00117800
                AIF   ('&PROCESSNAME' EQ '').ZPLV5E                     00117900
                AIF   (&XPLISTVER GE 3).ZPLV5E                          00118000
&XPLISTVER      SETA  3                                                 00118100
.ZPLV5E         ANOP  ,                                                 00118200
.*                                                                      00118300
                AIF   ('&PROCESSNM_LEN' EQ '').ZPLV6E                   00118400
                AIF   (&XPLISTVER GE 3).ZPLV6E                          00118500
&XPLISTVER      SETA  3                                                 00118600
.ZPLV6E         ANOP  ,                                                 00118700
.*                                                                      00118800
                AIF   ('&SUBCOLN' EQ '').ZPLV7E                         00118900
                AIF   (&XPLISTVER GE 5).ZPLV7E                          00119000
&XPLISTVER      SETA  5                                                 00119100
.ZPLV7E         ANOP  ,                                                 00119200
.*                                                                      00119300
                AIF   ('&SCHEDENV' EQ '').ZPLV8E                        00119400
                AIF   (&XPLISTVER GE 5).ZPLV8E                          00119500
&XPLISTVER      SETA  5                                                 00119600
.ZPLV8E         ANOP  ,                                                 00119700
.*                                                                      00119800
                AIF   ('&SCHEDENV_LEN' EQ '').ZPLV9E                    00119900
                AIF   (&XPLISTVER GE 5).ZPLV9E                          00120000
&XPLISTVER      SETA  5                                                 00120100
.ZPLV9E         ANOP  ,                                                 00120200
.*                                                                      00120300
                AIF   ('&SRMTOKEN' EQ '').ZPLV10E                       00120400
                AIF   (&XPLISTVER GE 5).ZPLV10E                         00120500
&XPLISTVER      SETA  5                                                 00120600
.ZPLV10E        ANOP  ,                                                 00120700
.*                                                                      00120800
                AIF   ('&TTRACETOKEN' EQ '').ZPLV11E                    00120900
                AIF   (&XPLISTVER GE 4).ZPLV11E                         00121000
&XPLISTVER      SETA  4                                                 00121100
.ZPLV11E        ANOP  ,                                                 00121200
.*                                                                      00121300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00121400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00121500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00121600
.*-------------------------------------------------------------------   00121700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00121800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00121900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00122000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00122100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00122200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00122300
                AGO   .ZPVCK2                                           00122400
.ZPVCK3         ANOP  ,                                                 00122500
.*-------------------------------------------------------------------   00122600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00122700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00122800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00122900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00123000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00123100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00123200
                AGO   .ZPVCKE                                           00123300
.ZPVCK2         ANOP  ,                                                 00123400
.*-------------------------------------------------------------------   00123500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00123600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00123700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00123800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00123900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00124000
                AIF   (&ZN LE 5).ZPVCK4A   NOT HIGH                     00124100
.ZPVCK6         ANOP  ,                                                 00124200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00124300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00124400
&ZMS            SETC  '&ZMS THE RANGE IS "0-5"'                         00124500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00124600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00124700
&XPLISTVER      SETA  5                                                 00124800
                AGO   .ZPVCKE                                           00124900
.*-------------------------------------------------------------------   00125000
.ZPVCK4A        ANOP  ,                                                 00125100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00125200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00125300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00125400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00125500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00125600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00125700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00125800
                AGO   .ZPVCK5                                           00125900
.ZPVCK4         ANOP  ,                                                 00126000
&XPLISTVER      SETA  &PLISTVER(1)                                      00126100
.ZPVCK5         ANOP  ,                                                 00126200
.ZPVCKE         ANOP  ,                                                 00126300
.********************************************************************   00126400
