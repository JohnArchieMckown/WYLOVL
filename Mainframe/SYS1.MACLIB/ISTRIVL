RIV0CDSW EQU   X'10'                DSPLYWLD=YES                   @N1A 10877000
RIV0CFSP EQU   X'08'                FASTPASS=YES                   @N1A 10877900
RIV0CHAV EQU   X'04'                HAVAIL=YES                     @N1A 10878800
RIV0CPAR EQU   X'02'                PARSESS=YES                    @N1A 10879700
RIV0CPRS EQU   X'01'                PERSIST=MULTI                  @N1A 10880600
RIV0CFL2 DS    X                    MISCELLANEOUS FLAGS 2          @N1A 10881500
RIV0CSSL EQU   X'80'                SESSLIM=YES                    @N1A 10882400
RIV0CSON EQU   X'40'                SONSCIP=YES                    @N1A 10883300
