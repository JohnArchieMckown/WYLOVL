.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00561500
.*******************************************************************    00561600
                      ACTR  10000                                       00561700
.*                                                                      00561800
                      LCLA  &ZLC           COLUMN POSITION              00561900
                      LCLA  &ZOPX          LIST TABLE INDEX             00562000
                      LCLC  &ZLL           LABEL                        00562100
                      LCLC  &ZLOP          OPERATION                    00562200
                      LCLC  &ZLV           VALUE                        00562300
                      LCLC  &ZLD           DESCRIPTION                  00562400
.*                                                                      00562500
&ZOPX                 SETA  1                                           00562600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00562700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00562800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00562900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00563000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00563100
                      AIF   (K'&ZLD LE 0).ZL5                           00563200
&ZLC                  SETA  35             COMMENT COLUMN               00563300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00563400
&ZLC                  SETA  33             COMMMENT COLUMN              00563500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00563600
&ZLV                  SETC  '&ZLV'.' '                                  00563700
                      AGO   .ZL3                                        00563800
.*                                                                      00563900
.ZL4                  ANOP  ,                                           00564000
&ZLV                  SETC  '&ZLV'.' '                                  00564100
.ZL5                  ANOP  ,                                           00564200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00564300
.*                                                                      00564400
&ZLL     &ZLOP &ZLV                                                     00564500
.*                                                                      00564600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00564700
                      AGO   .ZL2           CONTINUE LOOPING             00564800
.*                                                                      00564900
.ZLX                  ANOP  ,                                           00565000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00565100
