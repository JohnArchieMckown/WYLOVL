.***AC.PLV01  PROCESS PLISTVER                                      *   00131200
.********************************************************************   00131300
&XPLISTVER      SETA  2                                                 00131400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00131500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00131600
&XPLISTVER      SETA  3                                                 00131700
.ZPLV0          ANOP  ,                                                 00131800
.*                                                                      00131900
                AIF   ('&GROUP' EQ '').ZPLV1E                           00132000
                AIF   (&XPLISTVER GE 3).ZPLV1E                          00132100
&XPLISTVER      SETA  3                                                 00132200
.ZPLV1E         ANOP  ,                                                 00132300
.*                                                                      00132400
                AIF   ('&PAVINFOSUM' EQ '').ZPLV2E                      00132500
                AIF   (&XPLISTVER GE 3).ZPLV2E                          00132600
&XPLISTVER      SETA  3                                                 00132700
.ZPLV2E         ANOP  ,                                                 00132800
.*                                                                      00132900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00133000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00133100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00133200
.*-------------------------------------------------------------------   00133300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00133400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00133500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00133600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00133700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00133800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00133900
                AGO   .ZPVCK2                                           00134000
.ZPVCK3         ANOP  ,                                                 00134100
.*-------------------------------------------------------------------   00134200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00134300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00134400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00134500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00134600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00134700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00134800
                AGO   .ZPVCKE                                           00134900
.ZPVCK2         ANOP  ,                                                 00135000
.*-------------------------------------------------------------------   00135100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00135200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00135300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00135400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00135500
                AIF   (&ZN LT 2).ZPVCK6    TOO LOW                      00135600
                AIF   (&ZN LE 3).ZPVCK4A   NOT HIGH                     00135700
.ZPVCK6         ANOP  ,                                                 00135800
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00135900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00136000
&ZMS            SETC  '&ZMS THE RANGE IS "2-3"'                         00136100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00136200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00136300
&XPLISTVER      SETA  3                                                 00136400
                AGO   .ZPVCKE                                           00136500
.*-------------------------------------------------------------------   00136600
.ZPVCK4A        ANOP  ,                                                 00136700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00136800
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00136900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00137000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00137100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00137200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00137300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00137400
                AGO   .ZPVCK5                                           00137500
.ZPVCK4         ANOP  ,                                                 00137600
&XPLISTVER      SETA  &PLISTVER(1)                                      00137700
.ZPVCK5         ANOP  ,                                                 00137800
.ZPVCKE         ANOP  ,                                                 00137900
.********************************************************************   00138000
