.*                                                                      00459700
.******************************************************************     00459800
.***AI.L01 UCBINFO FMT=LIST FILL IN PARAMETER AREA                      00459900
.******************************************************************     00460000
                      ACTR  10000                                       00460100
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00460200
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00460300
&ZCVAL                SETC  'LAE'          XAX MODE                     00460400
.ZIMFXA               ANOP ,                                            00460500
.*                                                                      00460600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00460700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00460800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00460900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00461000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00461100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00461200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00461300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00461400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00461500
&ZOPN                 SETA  &ZOPN+1                                     00461600
&ZOPL(&ZOPN)          SETC  ' '                                         00461700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00461800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00461900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00462000
                      AGO   .ZIMFNX                                     00462100
.*                                                                      00462200
.ZIMFN2               ANOP ,                                            00462300
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00462400
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00462500
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00462600
&ZOPN                 SETA  &ZOPN+1                                     00462700
&ZOPL(&ZOPN)          SETC  ' '                                         00462800
&ZOPO(&ZOPN)          SETC  'LAE'                                       00462900
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00463000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00463100
                      AGO   .ZIMFNX                                     00463200
.ZIMFNX1              ANOP ,                                            00463300
&ZOPN                 SETA  &ZOPN+1                                     00463400
&ZOPL(&ZOPN)          SETC  ' '                                         00463500
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00463600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00463700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00463800
.ZIMFNX               ANOP ,                                            00463900
