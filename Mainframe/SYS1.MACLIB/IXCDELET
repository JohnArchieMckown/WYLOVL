.***AG.MF01 PROCESS MF MACRO FORMS                                      00039400
.********************************************************************   00039500
              ACTR  10000                                               00039600
.*------------------------------------------------------------------    00039700
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00039800
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00039900
.*-------------------------------------------------------------------   00040000
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00040100
.ZG12         ANOP  ,                                                   00040200
&ZTLC         SETC 'S'                                                  00040300
              AIF   ('&MF(1)' NE '&ZTLC').ZG13                          00040400
&XMF          SETC  'S'         SET KEYWORD                             00040500
              AIF   (N'&MF LE 1).ZG12A          CK NUM                  00040600
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00040700
&ZMS          SETC  '&ZMS.&MF"'                                         00040800
&ZMS          SETC  '&ZMS "MF"'                                         00040900
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00041000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00041100
.ZG12A        ANOP  ,                                                   00041200
.*-------------------------------------------------------------------   00041300
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00041400
.ZG13         ANOP  ,                                                   00041500
&ZTLC         SETC 'L'                                                  00041600
              AIF   ('&MF(1)' NE '&ZTLC').ZG20                          00041700
&XMF          SETC  'L'         SET KEYWORD                             00041800
              AIF   (N'&MF LE 3).ZG13A          CK NUM                  00041900
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00042000
&ZMS          SETC  '&ZMS.&MF"'                                         00042100
&ZMS          SETC  '&ZMS "MF"'                                         00042200
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00042300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00042400
.ZG13A        ANOP  ,                                                   00042500
.*-------------------------------------------------------------------   00042600
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00042700
              AIF   (N'&SYSLIST NE 0).ZG13B     POSITIONALS?            00042800
              AIF   ('&TARGET' NE '').ZG13B     KEYS?                   00042900
              AIF   ('&RETCODE' NE '').ZG13B    KEYS?                   00043000
              AIF   ('&RSNCODE' NE '').ZG13B    KEYS?                   00043100
              AGO    .ZG13C     INVOCATION OKAY                         00043200
.ZG13B        ANOP  ,                                                   00043300
&ZMS          SETC  'THE "L" FORM OF "MF" CANNOT'  *MFLA* MNOTE         00043400
&ZMS          SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                   00043500
&ZMS          SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'              00043600
              MNOTE 8,' &ZMS ON THE INVOCATION.'                        00043700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00043800
.ZG13C        ANOP  ,                                                   00043900
.*-------------------------------------------------------------------   00044000
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00044100
&ZXMFCTRL     SETC  'INPUT'     SET IN/OUTPUT                           00044200
&XMFCTRL      SETC  '&MF(2,1)'  SET XVARIABLE                           00044300
              AIF   (N'&MF(2) GT 1).ZG15B       MULTI PARMS             00044400
&XMFCTRL      SETC  '&MF(2)'    SET XVARIABLE                           00044500
.ZG15B        AIF   (K'&MF(2) GT 0).ZG15C                               00044600
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00044700
&ZMS          SETC  'WHEN "L" IS SPECIFIED'     *KRQDA*                 00044800
&ZMS          SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'                 00044900
              MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                        00045000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00045100
.ZG15C        ANOP  ,                                                   00045200
.*-------------------------------------------------------------------   00045300
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00045400
              AIF   (K'&ZXMFCTRL LE 0).ZG15P    NOT I/O                 00045500
              AIF   ('&XMFCTRL'(1,1) NE '(').ZG15P                      00045600
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00045700
&ZMS          SETC  '"MF='      *KGPRA* MNOTE                           00045800
&ZMS          SETC  '&ZMS.&MF"'                                         00045900
&ZMS          SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'           00046000
              MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 2.'                   00046100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00046200
.ZG15P        ANOP  ,                                                   00046300
.*-------------------------------------------------------------------   00046400
.***AG.KXVAR13 VALUE, OPT INPUT, POS(3) -XVARIABLE                      00046500
&ZXMFATTR     SETC  'INPUT'     SET IN/OUTPUT                           00046600
&XMFATTR      SETC  '0D'                                                00046700
              AIF   (K'&MF(3) LE 0).ZG18B       PARM                    00046800
&XMFATTR      SETC  '&MF(3,1)'  SET XVARIABLE                           00046900
              AIF   (N'&MF(3) GT 1).ZG18B       MULTI PARMS             00047000
&XMFATTR      SETC  '&MF(3)'    SET XVARIABLE                           00047100
.ZG18B        ANOP  ,                                                   00047200
.*-------------------------------------------------------------------   00047300
.***AG.KCK01 VALUE, OPT INPUT, POS(3) -GPR NOT ALLOWED                  00047400
              AIF   (K'&ZXMFATTR LE 0).ZG18P    NOT I/O                 00047500
              AIF   ('&XMFATTR'(1,1) NE '(').ZG18P                      00047600
&ZXMFATTR     SETC  ''          RESET IN/OUTPUT                         00047700
&ZMS          SETC  '"MF='      *KGPRA* MNOTE                           00047800
