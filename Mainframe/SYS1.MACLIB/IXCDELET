.*                                                                      00087000
.******************************************************************     00087100
.***AI.L01 IXCDELET FMT=LIST FILL IN PARAMETER AREA                     00087200
.******************************************************************     00087300
                      ACTR  10000                                       00087400
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00087500
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00087600
&ZCVAL                SETC  'LAE'          XAX MODE                     00087700
.ZIMFXA               ANOP ,                                            00087800
.*                                                                      00087900
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00088000
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00088100
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00088200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00088300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00088400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00088500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00088600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00088700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00088800
&ZOPN                 SETA  &ZOPN+1                                     00088900
&ZOPL(&ZOPN)          SETC  ' '                                         00089000
