.*                                                                      00094600
.ZI4E                 ANOP  ,                                           00094700
.*-------------------------------------------------------------------   00094800
&ZOPN1                SETA &ZOPN                                        00094900
.*******************************************************************    00095000
.***  PROCESS IXCDELET OUTPUT PARAMETERS                                00095100
.*******************************************************************    00095200
                      ACTR  10000                                       00095300
.*******************************************************************    00095400
.***AO.X02  RETURN CODE PROCESSING                                      00095500
.*******************************************************************    00095600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00095700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00095800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00095900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00096000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00096100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00096200
&ZOPN                 SETA  &ZOPN+1                                     00096300
&ZOPL(&ZOPN)          SETC  ' '                                         00096400
&ZOPO(&ZOPN)          SETC  'ST'                                        00096500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00096600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00096700
                      AGO   .ZMRETX                                     00096800
.*                                                                      00096900
.ZMRET2               ANOP  ,                                           00097000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00097100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00097200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00097300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00097400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00097500
&ZOPN                 SETA  &ZOPN+1                                     00097600
&ZOPL(&ZOPN)          SETC  ' '                                         00097700
&ZOPO(&ZOPN)          SETC  'LR'                                        00097800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00097900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00098000
.ZMRETX               ANOP  ,                                           00098100
.*******************************************************************    00098200
.***AO.X03  REASON CODE PROCESSING                                      00098300
.*******************************************************************    00098400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00098500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00098600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00098700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00098800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00098900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00099000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00099100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00099200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00099300
&ZOPN                 SETA  &ZOPN+1                                     00099400
&ZOPL(&ZOPN)          SETC  ' '                                         00099500
&ZOPO(&ZOPN)          SETC  'ST'                                        00099600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00099700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00099800
                      AGO   .ZMRSNX                                     00099900
.*                                                                      00100000
.ZMRSN2               ANOP  ,                                           00100100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00100200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00100300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00100400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00100500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00100600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00100700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00100800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00100900
&ZOPN                 SETA  &ZOPN+1                                     00101000
&ZOPL(&ZOPN)          SETC  ' '                                         00101100
&ZOPO(&ZOPN)          SETC  'LR'                                        00101200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00101300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00101400
.ZMRSNX               ANOP  ,                                           00101500
.*******************************************************************    00101600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00101700
.*******************************************************************    00101800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00101900
.*******************************************************************    00102000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00102100
.*******************************************************************    00102200
                      ACTR  10000                                       00102300
.*                                                                      00102400
                      LCLA  &ZLC           COLUMN POSITION              00102500
                      LCLA  &ZOPX          LIST TABLE INDEX             00102600
                      LCLC  &ZLL           LABEL                        00102700
                      LCLC  &ZLOP          OPERATION                    00102800
                      LCLC  &ZLV           VALUE                        00102900
                      LCLC  &ZLD           DESCRIPTION                  00103000
.*                                                                      00103100
&ZOPX                 SETA  1                                           00103200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00103300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00103400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00103500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00103600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00103700
                      AIF   (K'&ZLD LE 0).ZL5                           00103800
&ZLC                  SETA  35             COMMENT COLUMN               00103900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00104000
&ZLC                  SETA  33             COMMMENT COLUMN              00104100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00104200
&ZLV                  SETC  '&ZLV'.' '                                  00104300
                      AGO   .ZL3                                        00104400
.*                                                                      00104500
.ZL4                  ANOP  ,                                           00104600
&ZLV                  SETC  '&ZLV'.' '                                  00104700
.ZL5                  ANOP  ,                                           00104800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00104900
.*                                                                      00105000
&ZLL     &ZLOP &ZLV                                                     00105100
.*                                                                      00105200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00105300
                      AGO   .ZL2           CONTINUE LOOPING             00105400
.*                                                                      00105500
.ZLX                  ANOP  ,                                           00105600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00105700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00105800
         L     14,16(0,0)              GET CVT ADDRESS                  00105900
         L     14,772(14,0)            GET SFT ADDRESS                  00106000
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00106100
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00106200
         OR    14,15                   GET PC NUMBER                    00106300
         PC    0(14)                                                    00106400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00106500
.ZLINKX               ANOP  ,                                           00106600
.*******************************************************************    00106700
.***AP.X01  FORMAT GENERATED CODE                                       00106800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00106900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00107000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00107100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00107200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00107300
                      AIF   (K'&ZLD LE 0).ZL51                          00107400
&ZLC                  SETA  35             COMMENT COLUMN               00107500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00107600
&ZLC                  SETA  33             COMMENT COLUMN               00107700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00107800
&ZLV                  SETC  '&ZLV'.' '                                  00107900
                      AGO   .ZL31                                       00108000
.*                                                                      00108100
.ZL41                 ANOP  ,                                           00108200
&ZLV                  SETC  '&ZLV'.' '                                  00108300
.ZL51                 ANOP  ,                                           00108400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00108500
.*                                                                      00108600
&ZLL     &ZLOP &ZLV                                                     00108700
.*                                                                      00108800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00108900
                      AGO   .ZL21          CONTINUE LOOPING             00109000
.*                                                                      00109100
.ZLX1                 ANOP  ,                                           00109200
