&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00090000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00090100
.ZIMFNX               ANOP ,                                            00090200
.********************************************************************   00090300
.***AI.L02 IXCDELET FMT=LIST INITIALIZE PARAMETER AREA                  00090400
