&ZOPD(&ZOPN)          SETC  '++ XL1 CONSTANT'                           00092100
.*-------------------------------------------------------------------   00092200
.***AI.L25  XTARGET XL8                                                 00092300
                      AIF   ('&ZXTARGET' EQ '').ZI4E  NOT USED          00092400
                      AIF   ('&ZXTARGET' EQ 'OUTPUT').ZI4E              00092500
&ZCGPR                SETC  '&XTARGET'                                  00092600
.*                                                                      00092700
                      AIF   ('&XTARGET'(1,1) NE '(').ZI4B               00092800
&ZCGPR   SETC  '0('.'&XTARGET'(2,K'&XTARGET-2).')'                      00092900
                      AGO   .ZI4D                                       00093000
.ZI4B                 ANOP  ,                                           00093100
.*                                                                      00093200
&ZC                   SETC  '&XTARGET'(1,1)                             00093300
                      AIF   (T'&ZC NE 'N').ZI4D                         00093400
&ZC                   SETC  '&XTARGET'                                  00093500
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI4D                00093600
