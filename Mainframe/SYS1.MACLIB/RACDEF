.* DEFINE LOCAL SET SYMBOLS                                        @L7A 02450000
         LCLA  &BIT0,&BIT1,&BIT2,&BIT3,&BIT4,&BIT5,&CLNGTH     @PC002PY 02500000
         LCLA  &BIT6,&BIT7                                       @L4A   02550000
         LCLA  &BITA0,&BITA1,&BITA2                                @L5A 02600000
         LCLA  &BITA3,&BITA4,&BITA5,&BITA6,&BITA7                  @L6A 02650000
         LCLB  &UACC0,&UACC1,&UACC2,&UACC3,&UACC4,&UACC5         @L4A   02700000
         LCLB  &UACC6,&UACC7                                     @L4A   02750000
         LCLA  &UACCVAL                                          @L4A   02800000
         LCLB  &AUDIT0,&AUDIT1,&AUDIT2,&AUDIT3,&AUDIT4,&AUDIT5   @L4A   02850000
         LCLB  &AUDIT6,&AUDIT7                                   @L4A   02900000
         LCLA  &AUDITVL                                          @L4A   02950000
