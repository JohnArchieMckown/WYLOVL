.***AC.PLV01  PROCESS PLISTVER                                      *   00080100
.********************************************************************   00080200
&XPLISTVER      SETA  0                                                 00080300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00080400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00080500
&XPLISTVER      SETA  2                                                 00080600
.ZPLV0          ANOP  ,                                                 00080700
.*                                                                      00080800
                AIF   ('&PROXYRESPONSE' EQ '').ZPLV1E                   00080900
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00081000
&XPLISTVER      SETA  1                                                 00081100
.ZPLV1E         ANOP  ,                                                 00081200
.*                                                                      00081300
                AIF   ('&PROXYRESPONSE' EQ '').ZPLV2E                   00081400
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00081500
&XPLISTVER      SETA  1                                                 00081600
.ZPLV2E         ANOP  ,                                                 00081700
.*                                                                      00081800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00081900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00082000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00082100
.*-------------------------------------------------------------------   00082200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00082300
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00082400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00082500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00082600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00082700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00082800
                AGO   .ZPVCK2                                           00082900
.ZPVCK3         ANOP  ,                                                 00083000
.*-------------------------------------------------------------------   00083100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00083200
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00083300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00083400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00083500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00083600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00083700
                AGO   .ZPVCKE                                           00083800
.ZPVCK2         ANOP  ,                                                 00083900
.*-------------------------------------------------------------------   00084000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00084100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00084200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00084300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00084400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00084500
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00084600
.ZPVCK6         ANOP  ,                                                 00084700
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00084800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00084900
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00085000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00085100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00085200
&XPLISTVER      SETA  2                                                 00085300
                AGO   .ZPVCKE                                           00085400
.*-------------------------------------------------------------------   00085500
.ZPVCK4A        ANOP  ,                                                 00085600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00085700
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00085800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00085900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00086000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00086100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00086200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00086300
                AGO   .ZPVCK5                                           00086400
.ZPVCK4         ANOP  ,                                                 00086500
&XPLISTVER      SETA  &PLISTVER(1)                                      00086600
.ZPVCK5         ANOP  ,                                                 00086700
.ZPVCKE         ANOP  ,                                                 00086800
.********************************************************************   00086900
