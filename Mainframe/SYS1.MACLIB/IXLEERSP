.*******************************************************************    00258900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00259000
.*******************************************************************    00259100
                      ACTR  10000                                       00259200
.*                                                                      00259300
                      LCLA  &ZLC           COLUMN POSITION              00259400
                      LCLA  &ZOPX          LIST TABLE INDEX             00259500
                      LCLC  &ZLL           LABEL                        00259600
                      LCLC  &ZLOP          OPERATION                    00259700
                      LCLC  &ZLV           VALUE                        00259800
                      LCLC  &ZLD           DESCRIPTION                  00259900
.*                                                                      00260000
&ZOPX                 SETA  1                                           00260100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00260200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00260300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00260400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00260500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00260600
                      AIF   (K'&ZLD LE 0).ZL5                           00260700
&ZLC                  SETA  35             COMMENT COLUMN               00260800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00260900
&ZLC                  SETA  33             COMMMENT COLUMN              00261000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00261100
&ZLV                  SETC  '&ZLV'.' '                                  00261200
                      AGO   .ZL3                                        00261300
.*                                                                      00261400
.ZL4                  ANOP  ,                                           00261500
&ZLV                  SETC  '&ZLV'.' '                                  00261600
.ZL5                  ANOP  ,                                           00261700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00261800
.*                                                                      00261900
&ZLL     &ZLOP &ZLV                                                     00262000
.*                                                                      00262100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00262200
                      AGO   .ZL2           CONTINUE LOOPING             00262300
.*                                                                      00262400
.ZLX                  ANOP  ,                                           00262500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00262600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00262700
         L     14,16(0,0)              GET CVT ADDRESS                  00262800
         L     14,772(14,0)            GET SFT ADDRESS                  00262900
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00263000
         LA    15,29                   GET EX FOR SERVICE ROUTINE       00263100
         OR    14,15                   GET PC NUMBER                    00263200
         PC    0(14)                                                    00263300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00263400
.ZLINKX               ANOP  ,                                           00263500
.*******************************************************************    00263600
.***AP.X01  FORMAT GENERATED CODE                                       00263700
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00263800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00263900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00264000
