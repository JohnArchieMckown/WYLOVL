.**  Start code to set macro version number                      @L2A** 00206900
.**------------------------------------------------------------------** 00207000
.*                                                                      00207100
         LCLA  &ERSPPLVERS                                         @L2A 00207200
.*                                                                      00207300
         AIF   (&XPLISTVER GE 2).ERSPPLVERGE2                      @L2A 00207400
         AIF   ('&XEVENT'  NE 'STRTEMPUNAVAIL').ERSPPLVERNOT2      @L2A 00207500
&ERSPPLVERS    SETA 2                                              @L2A 00207600
         AGO   .ERSPPLVERDONE                                      @L2A 00207700
.ERSPPLVERGE2  ANOP ,                                              @L2A 00207800
.ERSPPLVERNOT2 ANOP ,                                              @L2A 00207900
&ERSPPLVERS    SETA &XPLISTVER                                     @L2A 00208000
.*                                                                      00208100
.ERSPPLVERDONE ANOP ,                                              @L2A 00208200
         AIF   ('&PLISTVER(1)' EQ ''                     OR            *00208300
                '&PLISTVER(1)' EQ 'IMPLIED_VERSION'      OR            *00208400
                &XPLISTVER     GE &ERSPPLVERS).ERSPPLVERUPD        @L2A 00208500
         MNOTE 8,' PLISTVER=&PLISTVER(1), BUT PLISTVER OF &ERSPPLVERS  *00208600
               IS REQUIRED.'                                       @L2A 00208700
&ZMACRET SETA  8                                                   @L2A 00208800
         AGO   .ERSPPLVERCK                                        @L2A 00208900
.*                                                                      00209000
.ERSPPLVERUPD  ANOP ,                                              @L2A 00209100
&XPLISTVER  SETA  &ERSPPLVERS                                      @L2A 00209200
.*                                                                      00209300
