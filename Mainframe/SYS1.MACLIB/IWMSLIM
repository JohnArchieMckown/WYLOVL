.***AC.PLV01  PROCESS PLISTVER                                      *   00066800
.********************************************************************   00066900
&XPLISTVER      SETA  0                                                 00067000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00067100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00067200
&XPLISTVER      SETA  0                                                 00067300
.ZPLV0          ANOP  ,                                                 00067400
.*                                                                      00067500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00067600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00067700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00067800
.*-------------------------------------------------------------------   00067900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00068000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00068100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00068300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00068400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068500
                AGO   .ZPVCK2                                           00068600
.ZPVCK3         ANOP  ,                                                 00068700
.*-------------------------------------------------------------------   00068800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00068900
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00069000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00069100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00069200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00069300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069400
                AGO   .ZPVCKE                                           00069500
.ZPVCK2         ANOP  ,                                                 00069600
.*-------------------------------------------------------------------   00069700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00069800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00069900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00070000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00070100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00070200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00070300
.ZPVCK6         ANOP  ,                                                 00070400
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00070500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00070600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00070700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00070800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070900
&XPLISTVER      SETA  0                                                 00071000
                AGO   .ZPVCKE                                           00071100
.*-------------------------------------------------------------------   00071200
.ZPVCK4A        ANOP  ,                                                 00071300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00071400
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00071500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00071600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00071700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00071800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00071900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00072000
                AGO   .ZPVCK5                                           00072100
.ZPVCK4         ANOP  ,                                                 00072200
&XPLISTVER      SETA  &PLISTVER(1)                                      00072300
.ZPVCK5         ANOP  ,                                                 00072400
.ZPVCKE         ANOP  ,                                                 00072500
.********************************************************************   00072600
