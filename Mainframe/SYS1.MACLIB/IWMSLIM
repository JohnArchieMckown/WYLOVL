.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00158000
.*******************************************************************    00158100
                      ACTR  10000                                       00158200
.*                                                                      00158300
                      LCLA  &ZLC           COLUMN POSITION              00158400
                      LCLA  &ZOPX          LIST TABLE INDEX             00158500
                      LCLC  &ZLL           LABEL                        00158600
                      LCLC  &ZLOP          OPERATION                    00158700
                      LCLC  &ZLV           VALUE                        00158800
                      LCLC  &ZLD           DESCRIPTION                  00158900
.*                                                                      00159000
&ZOPX                 SETA  1                                           00159100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00159200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00159300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00159400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00159500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00159600
                      AIF   (K'&ZLD LE 0).ZL5                           00159700
&ZLC                  SETA  35             COMMENT COLUMN               00159800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00159900
&ZLC                  SETA  33             COMMMENT COLUMN              00160000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00160100
&ZLV                  SETC  '&ZLV'.' '                                  00160200
                      AGO   .ZL3                                        00160300
.*                                                                      00160400
.ZL4                  ANOP  ,                                           00160500
&ZLV                  SETC  '&ZLV'.' '                                  00160600
.ZL5                  ANOP  ,                                           00160700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00160800
.*                                                                      00160900
&ZLL     &ZLOP &ZLV                                                     00161000
.*                                                                      00161100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00161200
                      AGO   .ZL2           CONTINUE LOOPING             00161300
.*                                                                      00161400
.ZLX                  ANOP  ,                                           00161500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00161600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00161700
         L     14,16(0,0)              GET CVT ADDRESS                  00161800
         L     14,772(14,0)            GET SFT ADDRESS                  00161900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00162000
         LA    15,71                   GET EX FOR SERVICE ROUTINE       00162100
         OR    14,15                   GET PC NUMBER                    00162200
         PC    0(14)                                                    00162300
END&SYSNDX DS    0H                    END LABEL                        00162400
