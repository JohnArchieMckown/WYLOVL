                  AIF   (N'&MF LE 1).ZG23A      CK NUM                  00073900
&ZMS              SETC  '"MF='      *KPOSA* MNOTE                       00074000
&ZMS              SETC  '&ZMS.&MF"'                                     00074100
&ZMS              SETC  '&ZMS "MF"'                                     00074200
                  MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'             00074300
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00074400
.ZG23A            ANOP  ,                                               00074500
.*-------------------------------------------------------------------   00074600
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00074700
.ZG24             ANOP  ,                                               00074800
&ZTLC             SETC 'L'                                              00074900
                  AIF   ('&MF(1)' NE '&ZTLC').ZG31                      00075000
&XMF              SETC  'L'         SET KEYWORD                         00075100
                  AIF   (N'&MF LE 3).ZG24A      CK NUM                  00075200
&ZMS              SETC  '"MF='      *KPOSA* MNOTE                       00075300
&ZMS              SETC  '&ZMS.&MF"'                                     00075400
&ZMS              SETC  '&ZMS "MF"'                                     00075500
                  MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'            00075600
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00075700
.ZG24A            ANOP  ,                                               00075800
.*-------------------------------------------------------------------   00075900
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00076000
                  AIF   (N'&SYSLIST NE 0).ZG24B  POSITIONALS?           00076100
                  AIF   ('&AE_SERVERMAX' NE '').ZG24B  KEYS?            00076200
                  AIF   ('&AE_SERVERMIN' NE '').ZG24B  KEYS?            00076300
                  AIF   ('&AE_SPREADMIN' NE '').ZG24B  KEYS?            00076400
                  AIF   ('&RETCODE' NE '').ZG24B  KEYS?                 00076500
                  AIF   ('&RSNCODE' NE '').ZG24B  KEYS?                 00076600
                  AGO    .ZG24C     INVOCATION OKAY                     00076700
.ZG24B            ANOP  ,                                               00076800
&ZMS              SETC  'THE "L" FORM OF "MF"'  *MFLA* MNOTE            00076900
&ZMS              SETC  '&ZMS CAN ONLY HAVE'                            00077000
&ZMS              SETC  '&ZMS THE "PLISTVER" KEY'                       00077100
                  MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'          00077200
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00077300
.ZG24C            ANOP  ,                                               00077400
.*-------------------------------------------------------------------   00077500
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00077600
