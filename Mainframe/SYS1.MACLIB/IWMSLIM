.***AE.01  START OF IWMSLIM MACRO CODE                                  00120500
.*******************************************************************    00120600
                  LCLC  &M0M0001                 NAME                   00120700
&M0M0001          SETC  '&XLABEL'                SET NAME               00120800
                  AIF   (K'&XLABEL NE 0).XLAB2                          00120900
&M0M0001          SETC  'M00M&SYSNDX'            DEFAULT NAME           00121000
.*                                                                      00121100
.XLAB2            ANOP  ,                                               00121200
                  AIF   ('&XMF' EQ 'L').XLAB3                           00121300
&M0M0001 DS    0H                             IWMSLIM-0                 00121400
.XLAB3            ANOP  ,                                               00121500
.*******************************************************************    00121600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00121700
.*******************************************************************    00121800
              PUSH  PRINT                                               00121900
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      00122000
                  PRINT OFF         ++ PR01                             00122100
                  AGO   .ZPRNTXX                                        00122200
.ZPRINTX          PRINT ON                                              00122300
.ZPRNTXX          ANOP  ,                                               00122400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00122500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00122600
.********************************************************************   00122700
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00122800
.********************************************************************   00122900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00123000
                      LCLA  &ZOPN1         INVOCATION INDEX             00123100
                      LCLC  &ZOPL(4095)    LABEL                        00123200
                      LCLC  &ZOPO(4095)    OPERATION                    00123300
                      LCLC  &ZOPV(4095)    VALUE                        00123400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00123500
.*                                                                      00123600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00123700
.******************************************************************     00123800
.***AL.L10 IWMSLIM FMT=LIST MF=S|L PARAMETERS                           00123900
.******************************************************************     00124000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00124100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00124200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00124300
&XMFATTR              SETC  '0D'                                        00124400
.*                                                                      00124500
&ZOPN                 SETA  &ZOPN+1                                     00124600
&ZOPL(&ZOPN)          SETC  ' '                                         00124700
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00124800
&ZOPO(&ZOPN)          SETC  'B'                                         00124900
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00125000
                      AGO   .ZALVL6B                                    00125100
.ZALVL6A              ANOP                                              00125200
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00125300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00125400
.ZALVL6B              ANOP                                              00125500
&ZOPD(&ZOPN)          SETC  '++ IWMSLIM ARG PARMS'                      00125600
.*                                                                      00125700
&ZOPN                 SETA  &ZOPN+1                                     00125800
&ZOPL(&ZOPN)          SETC  ' '                                         00125900
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00126000
&ZOPV(&ZOPN)          SETC  '1'                                         00126100
&ZOPD(&ZOPN)          SETC  ' '                                         00126200
.*                                                                      00126300
.ZLMFSL2              ANOP  ,                                           00126400
.*-------------------------------------------------------------------   00126500
.***AM.L00  IWMSLIM FMT=LIST name                                       00126600
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00126700
&ZOPN                 SETA &ZOPN+1                                      00126800
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00126900
&ZOPO(&ZOPN)          SETC 'EQU'                                        00127000
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00127100
&ZOPD(&ZOPN)          SETC '++ IWMSLIM NAME'                            00127200
