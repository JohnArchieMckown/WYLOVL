              GBLC  &ZTEST      YES=TEST NO=NO TEST                     00040300
              GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX             00040400
.*                                                                      00040500
              GBLC  &ZIXCQUIES  YES=MACRO USED AT LEAST ONCE            00040600
.*******************************************************************    00040700
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00040800
.*******************************************************************    00040900
              LCLC  &ZMS        ERROR MESSAGE WORK AREA                 00041000
              LCLC  &ZCNDX      LAST THREE SYSNDX CHARACTERS            00041100
              LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED              00041200
              LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR             00041300
              LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE             00041400
              LCLA  &ZAWRK      USED AS A WORK AREA                     00041500
              LCLA  &ZASIZ      USED FOR SIZE OF VALUE                  00041600
              LCLA  &ZN         NUMERIC WORK VARIABLE                   00041700
              LCLC  &ZC         CHARACTER WORK VARIABLE                 00041800
              LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE             00041900
              LCLA  &ZTLA       VALUE TOO LONG VARIABLE                 00042000
              LCLA  &ZMACRET    MACRO RETURN CODE                       00042100
&ZMACRET      SETA  0           ASSUME NO ERRORS                        00042200
.*                                                                      00042300
              LCLC  &ZCSYSLIST(255)             NONXKEY, POS ARRAY      00042400
              LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()              00042500
              LCLA  &ZPLLEN     PARAMETER LIST LENGTH                   00042600
              LCLC  &ZENDBASEPL                 End of base PL          00042700
.********************************************************************   00042800
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00042900
.********************************************************************   00043000
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00043100
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00043200
.*                                                                      00043300
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00043400
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00043500
              SPLEVEL  TEST                                             00043600
.ZSYS2        ANOP  ,                                                   00043700
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00043800
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00043900
              SYSSTATE TEST                                             00044000
              AGO   .ZSYS4                                              00044100
.ZSYS3        ANOP  ,                                                   00044200
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00044300
.ZSYS4        ANOP  ,                                                   00044400
.*******************************************************************    00044500
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00044600
.*******************************************************************    00044700
              LCLC  &XMEMTOKEN  ++ NAME                                 00044800
              LCLC  &XUSTATE    ++ NAME                                 00044900
              LCLC  &XUSLEN     ++ NAME                                 00045000
              LCLC  &XRETCODE   ++ NAME                                 00045100
              LCLC  &XRSNCODE   ++ NAME                                 00045200
              LCLC  &XMF        ++ VALUE                                00045300
              LCLC  &XMFCTRL    ++ NAME                                 00045400
              LCLC  &XMFATTR    ++ VALUE                                00045500
              LCLC  &XMFSCK     ++ VALUE                                00045600
.*******************************************************************    00045700
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00045800
.*******************************************************************    00045900
              LCLC  &ZXMEMTOKEN                 ++ INPUT NAME           00046000
              LCLC  &ZXUSTATE   ++ INPUT NAME                           00046100
              LCLC  &ZXUSLEN    ++ INPUT NAME                           00046200
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00046300
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00046400
              LCLC  &ZXMF       ++ INPUT VALUE                          00046500
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00046600
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00046700
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00046800
.********************************************************************   00046900
.***AG.MF01 PROCESS MF MACRO FORMS                                      00047000
.********************************************************************   00047100
              ACTR  10000                                               00047200
.*------------------------------------------------------------------    00047300
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00047400
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00047500
.*-------------------------------------------------------------------   00047600
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00047700
.ZG18         ANOP  ,                                                   00047800
&ZTLC         SETC 'S'                                                  00047900
              AIF   ('&MF(1)' NE '&ZTLC').ZG19                          00048000
&XMF          SETC  'S'         SET KEYWORD                             00048100
              AIF   (N'&MF LE 1).ZG18A          CK NUM                  00048200
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00048300
&ZMS          SETC  '&ZMS.&MF"'                                         00048400
&ZMS          SETC  '&ZMS "MF"'                                         00048500
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00048600
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00048700
.ZG18A        ANOP  ,                                                   00048800
.*-------------------------------------------------------------------   00048900
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00049000
.ZG19         ANOP  ,                                                   00049100
&ZTLC         SETC 'L'                                                  00049200
              AIF   ('&MF(1)' NE '&ZTLC').ZG26                          00049300
&XMF          SETC  'L'         SET KEYWORD                             00049400
              AIF   (N'&MF LE 3).ZG19A          CK NUM                  00049500
&ZMS          SETC  '"MF='      *KPOSA* MNOTE                           00049600
&ZMS          SETC  '&ZMS.&MF"'                                         00049700
&ZMS          SETC  '&ZMS "MF"'                                         00049800
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00049900
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00050000
.ZG19A        ANOP  ,                                                   00050100
.*-------------------------------------------------------------------   00050200
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00050300
              AIF   (N'&SYSLIST NE 0).ZG19B     POSITIONALS?            00050400
