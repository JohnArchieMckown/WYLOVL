              AIF   ('&ZCGPR' LE '12').ZG11R                            00085200
.ZG11Q        ANOP  ,                                                   00085300
&ZMS          SETC  '"RETCODE='                 *KGPRA* MNOTE           00085400
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00085500
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00085600
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00085700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00085800
.ZG11R        ANOP  ,                                                   00085900
.*----------------------------------------------------------------***   00086000
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00086100
              AIF   (N'&RSNCODE LE 1).ZG14A     CK NUM                  00086200
&ZMS          SETC  '"RSNCODE='                 *KPOSA* MNOTE           00086300
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00086400
&ZMS          SETC  '&ZMS "RSNCODE"'                                    00086500
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00086600
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00086700
.ZG14A        ANOP  ,                                                   00086800
.*-------------------------------------------------------------------   00086900
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00087000
              AIF   (K'&RSNCODE(1) LE 0).ZG14C  NO PARM                 00087100
&ZXRSNCODE    SETC  'OUTPUT'    SET OUTPUT                              00087200
&XRSNCODE     SETC  '&RSNCODE(1)'               SET XVARIABLE           00087300
              AIF   (N'&RSNCODE GT 1).ZG14C                             00087400
&XRSNCODE     SETC  '&RSNCODE'  SET XVARIABLE                           00087500
.ZG14C        ANOP  ,                                                   00087600
.*-------------------------------------------------------------------   00087700
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00087800
              AIF   (K'&ZXRSNCODE LE 0).ZG14R   NOT I/O                 00087900
              AIF   ('&XRSNCODE'(1,1) NE '(').ZG14R                     00088000
&ZCGPR        SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                        00088100
              AIF   ('&ZCGPR'(1,1) NE '0').ZG14P                        00088200
              AIF   (K'&ZCGPR GT 2).ZG14Q                               00088300
              AIF   ('&ZCGPR' EQ '0').ZG14Q                             00088400
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00088500
              AIF   (T'&ZCGPR NE 'N').ZG14Q     INVALID REG NAME        00088600
.ZG14P        ANOP  ,                                                   00088700
              AIF   (T'&ZCGPR NE 'N').ZG14R     NON NUMERIC             00088800
              AIF   ('&ZCGPR' LT '2').ZG14Q                             00088900
              AIF   ('&ZCGPR' LE '12').ZG14R                            00089000
.ZG14Q        ANOP  ,                                                   00089100
&ZMS          SETC  '"RSNCODE='                 *KGPRA* MNOTE           00089200
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00089300
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00089400
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00089500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00089600
.ZG14R        ANOP  ,                                                   00089700
.******************************************************************     00089800
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00089900
.******************************************************************     00090000
              ACTR  10000                                               00090100
.*----------------------------------------------------------------***   00090200
.***AH.P02 EXTRA POSITIONALS                                            00090300
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00090400
&ZCVAL        SETC  ''          ERROR FIELD                             00090500
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00090600
.ZGLIST2      ANOP  ,                                                   00090700
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00090800
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00090900
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00091000
              AGO   .ZGLIST2                                            00091100
.*                                                                      00091200
.ZGLIST3      ANOP  ,                                                   00091300
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00091400
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00091500
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00091600
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00091700
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00091800
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00091900
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00092000
.ZGLISTX      ANOP  ,                                                   00092100
&ZASYSLNDX    SETA  0                                                   00092200
.*-------------------------------------------------------------------   00092300
.***AG.MF04 MF=(SLE)                                                    00092400
.ZGMFX        ANOP  ,           END OF FULL SYNTAX CHECK                00092500
.*******************************************************************    00092600
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00092700
.*******************************************************************    00092800
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          00092900
.*******************************************************************    00093000
.***AE.01  START OF IXCQUIES MACRO CODE                                 00093100
.*******************************************************************    00093200
              LCLC  &M0M0001                 NAME                       00093300
&M0M0001      SETC  '&XLABEL'                SET NAME                   00093400
              AIF   (K'&XLABEL NE 0).XLAB2                              00093500
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00093600
.*                                                                      00093700
.XLAB2        ANOP  ,                                                   00093800
&M0M0001 DS    0H                             IXCQUIES-0                00093900
.*******************************************************************    00094000
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00094100
.*******************************************************************    00094200
              PUSH  PRINT                                               00094300
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00094400
              PRINT OFF         ++ PR01                                 00094500
              AGO   .ZPRNTXX                                            00094600
.ZPRINTX      PRINT ON                                                  00094700
.ZPRNTXX      ANOP  ,                                                   00094800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00094900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00095000
.********************************************************************   00095100
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00095200
.********************************************************************   00095300
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00095400
                      LCLA  &ZOPN1         INVOCATION INDEX             00095500
                      LCLC  &ZOPL(4095)    LABEL                        00095600
                      LCLC  &ZOPO(4095)    OPERATION                    00095700
                      LCLC  &ZOPV(4095)    VALUE                        00095800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00095900
.*                                                                      00096000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00096100
.******************************************************************     00096200
.***AL.L10 IXCQUIES FMT=LIST MF=S|L PARAMETERS                          00096300
.******************************************************************     00096400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00096500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00096600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00096700
