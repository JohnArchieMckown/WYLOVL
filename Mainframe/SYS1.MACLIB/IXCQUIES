                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00107800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00107900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00108000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00108100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00108200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00108300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00108400
&ZOPN                 SETA  &ZOPN+1                                     00108500
