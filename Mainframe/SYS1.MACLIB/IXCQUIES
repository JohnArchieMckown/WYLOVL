.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00130700
.*******************************************************************    00130800
                      ACTR  10000                                       00130900
.*                                                                      00131000
                      LCLA  &ZLC           COLUMN POSITION              00131100
                      LCLA  &ZOPX          LIST TABLE INDEX             00131200
                      LCLC  &ZLL           LABEL                        00131300
                      LCLC  &ZLOP          OPERATION                    00131400
                      LCLC  &ZLV           VALUE                        00131500
                      LCLC  &ZLD           DESCRIPTION                  00131600
.*                                                                      00131700
&ZOPX                 SETA  1                                           00131800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00131900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00132000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00132100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00132200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00132300
                      AIF   (K'&ZLD LE 0).ZL5                           00132400
&ZLC                  SETA  35             COMMENT COLUMN               00132500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00132600
&ZLC                  SETA  33             COMMMENT COLUMN              00132700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00132800
&ZLV                  SETC  '&ZLV'.' '                                  00132900
                      AGO   .ZL3                                        00133000
.*                                                                      00133100
.ZL4                  ANOP  ,                                           00133200
&ZLV                  SETC  '&ZLV'.' '                                  00133300
.ZL5                  ANOP  ,                                           00133400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00133500
.*                                                                      00133600
&ZLL     &ZLOP &ZLV                                                     00133700
.*                                                                      00133800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00133900
                      AGO   .ZL2           CONTINUE LOOPING             00134000
.*                                                                      00134100
.ZLX                  ANOP  ,                                           00134200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00134300
