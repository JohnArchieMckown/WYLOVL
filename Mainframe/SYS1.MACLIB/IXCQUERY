.*******************************************************************    00595700
.***  PROCESS IXCQUERY OUTPUT PARAMETERS                                00595800
.*******************************************************************    00595900
                      ACTR  10000                                       00596000
.*******************************************************************    00596100
.***AO.X02  RETURN CODE PROCESSING                                      00596200
.*******************************************************************    00596300
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00596400
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00596500
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00596600
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00596700
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00596800
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00596900
&ZOPN                 SETA  &ZOPN+1                                     00597000
&ZOPL(&ZOPN)          SETC  ' '                                         00597100
&ZOPO(&ZOPN)          SETC  'ST'                                        00597200
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00597300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00597400
                      AGO   .ZMRETX                                     00597500
.*                                                                      00597600
.ZMRET2               ANOP  ,                                           00597700
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00597800
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00597900
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00598000
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00598100
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00598200
&ZOPN                 SETA  &ZOPN+1                                     00598300
&ZOPL(&ZOPN)          SETC  ' '                                         00598400
&ZOPO(&ZOPN)          SETC  'LR'                                        00598500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00598600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00598700
.ZMRETX               ANOP  ,                                           00598800
.*******************************************************************    00598900
.***AO.X03  REASON CODE PROCESSING                                      00599000
.*******************************************************************    00599100
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00599200
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00599300
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00599400
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00599500
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00599600
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00599700
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00599800
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00599900
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00600000
&ZOPN                 SETA  &ZOPN+1                                     00600100
&ZOPL(&ZOPN)          SETC  ' '                                         00600200
&ZOPO(&ZOPN)          SETC  'ST'                                        00600300
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00600400
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00600500
                      AGO   .ZMRSNX                                     00600600
.*                                                                      00600700
.ZMRSN2               ANOP  ,                                           00600800
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00600900
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00601000
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00601100
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00601200
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00601300
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00601400
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00601500
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00601600
&ZOPN                 SETA  &ZOPN+1                                     00601700
&ZOPL(&ZOPN)          SETC  ' '                                         00601800
&ZOPO(&ZOPN)          SETC  'LR'                                        00601900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00602000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00602100
.ZMRSNX               ANOP  ,                                           00602200
.*******************************************************************    00602300
