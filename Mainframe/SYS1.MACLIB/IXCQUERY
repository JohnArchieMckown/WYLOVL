.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00602800
.*******************************************************************    00602900
                      ACTR  10000                                       00603000
.*                                                                      00603100
                      LCLA  &ZLC           COLUMN POSITION              00603200
                      LCLA  &ZOPX          LIST TABLE INDEX             00603300
                      LCLC  &ZLL           LABEL                        00603400
                      LCLC  &ZLOP          OPERATION                    00603500
                      LCLC  &ZLV           VALUE                        00603600
                      LCLC  &ZLD           DESCRIPTION                  00603700
.*                                                                      00603800
&ZOPX                 SETA  1                                           00603900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00604000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00604100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00604200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00604300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00604400
                      AIF   (K'&ZLD LE 0).ZL5                           00604500
&ZLC                  SETA  35             COMMENT COLUMN               00604600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00604700
&ZLC                  SETA  33             COMMMENT COLUMN              00604800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00604900
&ZLV                  SETC  '&ZLV'.' '                                  00605000
                      AGO   .ZL3                                        00605100
.*                                                                      00605200
.ZL4                  ANOP  ,                                           00605300
&ZLV                  SETC  '&ZLV'.' '                                  00605400
.ZL5                  ANOP  ,                                           00605500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00605600
.*                                                                      00605700
&ZLL     &ZLOP &ZLV                                                     00605800
.*                                                                      00605900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00606000
                      AGO   .ZL2           CONTINUE LOOPING             00606100
.*                                                                      00606200
.ZLX                  ANOP  ,                                           00606300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00606400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00606500
         AIF   ('&XREQINFO' EQ 'CF').IXCLO                        @D1C  00606600
         AIF   ('&XREQINFO' EQ 'STR').IXCLO                       @D1A  00606700
         AIF   ('&XREQINFO' EQ 'CF_ALLDATA').IXCLO                @D1A  00606800
         AIF   ('&XREQINFO' EQ 'STR_ALLDATA').IXCLO               @D1A  00606900
         AGO   .NOTLO                                                   00607000
.IXCLO   ANOP                                                     @D1A  00607100
         MVI   1(1),4                  SET FUNCTION CODE                00607200
         L     14,16(0,0)              GET CVT ADDRESS                  00607300
         L     14,772(14,0)            GET SFT ADDRESS                  00607400
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00607500
         LA    15,37                   GET EX FOR SERVICE ROUTINE       00607600
         OR    14,15                   GET PC NUMBER                    00607700
         PC    0(14)                                                    00607800
         AGO   .LNKDONE                                                 00607900
.NOTLO   ANOP                                                     @D1C  00608000
         AIF   ('&XREQINFO' EQ 'ARMSTATUS').IXCARM                @L2A  00608100
         AIF   ('&XREQINFO' EQ 'ARMS_ALLDATA').IXCARM             @L2A  00608200
         AGO   .NOTARM                                            @L2A  00608300
.IXCARM  ANOP                                                     @L2A  00608400
         LR    14,1                    query parameter address    @L3A  00608500
         LA    1,68(,1)                load parameter address     @L3A  00608600
         MVI   0(1),1                  input version              @L3A  00608700
         MVI   1(1),135                input request              @L3A  00608800
         MVI   2(1),12                 input request type         @L3A  00608900
         ST    14,60(,1)               input parameter address    @L3A  00609000
         AIF   ('&SYSASCE' EQ 'P').ARMNOAR                        @PDA  00609100
         STAM  1,1,64(1)               input parameter list alet  @PDA  00609200
.ARMNOAR ANOP                                                     @PDA  00609300
         L     14,16(0,0)              load CVT address           @L3A  00609400
         L     14,772(14,0)            load SFT address           @L3A  00609500
         L     14,220(14,0)            get LX for service routine @L3A  00609600
         LA    15,48                   get EX for service routine @L3A  00609700
         OR    14,15                   form PC number             @L3A  00609800
         LA    0,1(0,0)                indicate type of call      @L3A  00609900
         PC    0(14)                   issue PC                   @L3A  00610000
         AGO   .LNKDONE                                           @L2A  00610100
.NOTARM  ANOP                                                     @L2A  00610200
         AIF   ('&XREQINFO' NE 'GROUP').NOTGRP                          00610300
         AIF   ('&XREQTYPE' EQ 'DEFER').MVIQRD                          00610400
         MVI   1(1),16                 SET FUNCTION CODE                00610500
         AGO   .GRPLNK                                                  00610600
.MVIQRD  ANOP                                                           00610700
         MVI   1(1),5                  SET FUNCTION CODE                00610800
         AGO   .GRPLNK                                                  00610900
.NOTGRP  ANOP                                                           00611000
         MVI   1(1),16                 SET FUNCTION CODE                00611100
.GRPLNK  ANOP                                                           00611200
         L     14,16(0,0)              GET CVT ADDRESS                  00611300
         L     14,772(14,0)            GET SFT ADDRESS                  00611400
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00611500
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00611600
         OR    14,15                   GET PC NUMBER                    00611700
         PC    0(14)                                                    00611800
.LNKDONE ANOP                                                           00611900
