.***AC.PLV01  PROCESS PLISTVER                                      *   00149800
.********************************************************************   00149900
&XPLISTVER      SETA  0                                                 00150000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00150100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00150200
&XPLISTVER      SETA  2                                                 00150300
.ZPLV0          ANOP  ,                                                 00150400
.*                                                                      00150500
                AIF   ('&QUAALEVEL' EQ '').ZPLV1E                       00150600
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00150700
&XPLISTVER      SETA  2                                                 00150800
.ZPLV1E         ANOP  ,                                                 00150900
.*                                                                      00151000
                AIF   ('&QUAALEVEL' EQ '').ZPLV2E                       00151100
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00151200
&XPLISTVER      SETA  2                                                 00151300
.ZPLV2E         ANOP  ,                                                 00151400
.*                                                                      00151500
                AIF   ('&QUAALEVEL' EQ '').ZPLV3E                       00151600
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00151700
&XPLISTVER      SETA  2                                                 00151800
.ZPLV3E         ANOP  ,                                                 00151900
.*                                                                      00152000
                AIF   ('&QUAALEVEL' EQ '').ZPLV4E                       00152100
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00152200
&XPLISTVER      SETA  2                                                 00152300
.ZPLV4E         ANOP  ,                                                 00152400
.*                                                                      00152500
                AIF   ('&QUAALEVEL' EQ '').ZPLV5E                       00152600
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00152700
&XPLISTVER      SETA  2                                                 00152800
.ZPLV5E         ANOP  ,                                                 00152900
.*                                                                      00153000
                AIF   ('&ELEMENT' EQ '').ZPLV6E                         00153100
                AIF   (&XPLISTVER GE 1).ZPLV6E                          00153200
&XPLISTVER      SETA  1                                                 00153300
.ZPLV6E         ANOP  ,                                                 00153400
.*                                                                      00153500
                AIF   ('&RESTARTGRP' EQ '').ZPLV7E                      00153600
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00153700
&XPLISTVER      SETA  1                                                 00153800
.ZPLV7E         ANOP  ,                                                 00153900
.*                                                                      00154000
                AIF   ('&LOCAL_ONLY' EQ '').ZPLV8E                      00154100
                AIF   (&XPLISTVER GE 1).ZPLV8E                          00154200
&XPLISTVER      SETA  1                                                 00154300
.ZPLV8E         ANOP  ,                                                 00154400
.*                                                                      00154500
                AIF   ('&JOBNAME' EQ '').ZPLV9E                         00154600
                AIF   (&XPLISTVER GE 1).ZPLV9E                          00154700
&XPLISTVER      SETA  1                                                 00154800
.ZPLV9E         ANOP  ,                                                 00154900
.*                                                                      00155000
                AIF   ('&LOCAL_ONLY' EQ '').ZPLV10E                     00155100
                AIF   (&XPLISTVER GE 1).ZPLV10E                         00155200
&XPLISTVER      SETA  1                                                 00155300
.ZPLV10E        ANOP  ,                                                 00155400
.*                                                                      00155500
                AIF   ('&SYSNAME' EQ '').ZPLV11E                        00155600
                AIF   (&XPLISTVER GE 1).ZPLV11E                         00155700
&XPLISTVER      SETA  1                                                 00155800
.ZPLV11E        ANOP  ,                                                 00155900
.*                                                                      00156000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00156100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00156200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00156300
.*-------------------------------------------------------------------   00156400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00156500
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00156600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00156700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00156800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00156900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00157000
                AGO   .ZPVCK2                                           00157100
.ZPVCK3         ANOP  ,                                                 00157200
.*-------------------------------------------------------------------   00157300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00157400
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00157500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00157600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00157700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00157800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00157900
                AGO   .ZPVCKE                                           00158000
.ZPVCK2         ANOP  ,                                                 00158100
.*-------------------------------------------------------------------   00158200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00158300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00158400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00158500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00158600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00158700
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00158800
.ZPVCK6         ANOP  ,                                                 00158900
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00159000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00159100
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00159200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00159300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00159400
&XPLISTVER      SETA  2                                                 00159500
                AGO   .ZPVCKE                                           00159600
.*-------------------------------------------------------------------   00159700
.ZPVCK4A        ANOP  ,                                                 00159800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00159900
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00160000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00160100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00160200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00160300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00160400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00160500
                AGO   .ZPVCK5                                           00160600
.ZPVCK4         ANOP  ,                                                 00160700
&XPLISTVER      SETA  &PLISTVER(1)                                      00160800
.ZPVCK5         ANOP  ,                                                 00160900
.ZPVCKE         ANOP  ,                                                 00161000
.********************************************************************   00161100
