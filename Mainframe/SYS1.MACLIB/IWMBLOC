                      AGO   .ZO8C                                       00152400
.*                                                                      00152500
.ZO8B                 ANOP  ,                                           00152600
&ZAWRK   SETA  K'&XSYSNAME                                              00152700
         AIF   ('&XSYSNAME'(&ZAWRK,1) NE ')').ZO8G                      00152800
&ZN      SETA  2                                                        00152900
.ZO8H    ANOP                                                           00153000
         AIF   (&ZN GE &ZAWRK).ZO8G                                     00153100
         AIF   ('&XSYSNAME'(&ZN,1) NE '(').ZO8I                         00153200
&ZC      SETC  '&XSYSNAME'(1,&ZN).'8'.','                               00153300
&ZC      SETC  '&ZC'.'&XSYSNAME'(&ZN+1,&ZAWRK-&ZN).',36(1)'             00153400
         AGO   .ZO8J                                                    00153500
