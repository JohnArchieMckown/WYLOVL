              AIF   (N'&SYSLIST NE 0).ZG28B     POSITIONALS?            00070000
              AIF   ('&QTOKEN' NE '').ZG28B     KEYS?                   00070100
              AIF   ('&SRVCLSNM' NE '').ZG28B   KEYS?                   00070200
