.*----------------------------------------------------------------***   00117500
.***AH.P02 EXTRA POSITIONALS                                            00117600
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00117700
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00117800
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00117900
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00118000
.ZGLISTX1     ANOP  ,                                                   00118100
&ZCVAL        SETC  ''          ERROR FIELD                             00118200
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00118300
.ZGLIST2      ANOP  ,                                                   00118400
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00118500
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00118600
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00118700
              AGO   .ZGLIST2                                            00118800
.*                                                                      00118900
.ZGLIST3      ANOP  ,                                                   00119000
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00119100
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00119200
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00119300
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00119400
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00119500
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00119600
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00119700
.ZGLISTX      ANOP  ,                                                   00119800
&ZASYSLNDX    SETA  0                                                   00119900
