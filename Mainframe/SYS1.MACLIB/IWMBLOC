.***AC.PLV01  PROCESS PLISTVER                                      *   00060700
.********************************************************************   00060800
&XPLISTVER      SETA  0                                                 00060900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00061000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00061100
&XPLISTVER      SETA  0                                                 00061200
.ZPLV0          ANOP  ,                                                 00061300
.*                                                                      00061400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00061500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00061600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00061700
.*-------------------------------------------------------------------   00061800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00061900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00062000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00062100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00062200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00062300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062400
                AGO   .ZPVCK2                                           00062500
.ZPVCK3         ANOP  ,                                                 00062600
.*-------------------------------------------------------------------   00062700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00062800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00062900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00063100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00063200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063300
                AGO   .ZPVCKE                                           00063400
.ZPVCK2         ANOP  ,                                                 00063500
.*-------------------------------------------------------------------   00063600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00063700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00063800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00063900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00064000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00064100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00064200
.ZPVCK6         ANOP  ,                                                 00064300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00064400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00064600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00064700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064800
&XPLISTVER      SETA  0                                                 00064900
                AGO   .ZPVCKE                                           00065000
.*-------------------------------------------------------------------   00065100
.ZPVCK4A        ANOP  ,                                                 00065200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00065300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00065400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00065500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00065600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00065700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00065800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065900
                AGO   .ZPVCK5                                           00066000
.ZPVCK4         ANOP  ,                                                 00066100
&XPLISTVER      SETA  &PLISTVER(1)                                      00066200
.ZPVCK5         ANOP  ,                                                 00066300
.ZPVCKE         ANOP  ,                                                 00066400
.********************************************************************   00066500
