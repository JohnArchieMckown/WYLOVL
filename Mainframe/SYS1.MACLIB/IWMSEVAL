.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00049900
.********************************************************************   00050000
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00050100
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00050200
.*                                                                      00050300
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00050400
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00050500
              SPLEVEL  TEST                                             00050600
.ZSYS2        ANOP  ,                                                   00050700
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00050800
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00050900
              SYSSTATE TEST                                             00051000
              AGO   .ZSYS4                                              00051100
.ZSYS3        ANOP  ,                                                   00051200
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00051300
.ZSYS4        ANOP  ,                                                   00051400
.*******************************************************************    00051500
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00051600
.*******************************************************************    00051700
              LCLC  &XSCHENV    ++ NAME                                 00051800
              LCLC  &XRETCODE   ++ NAME                                 00051900
              LCLC  &XRSNCODE   ++ NAME                                 00052000
              LCLA  &XPLISTVER  ++ VALUE                                00052100
              LCLC  &XMF        ++ VALUE                                00052200
              LCLC  &XMFCTRL    ++ NAME                                 00052300
              LCLC  &XMFATTR    ++ VALUE                                00052400
              LCLC  &XMFSCK     ++ VALUE                                00052500
.*******************************************************************    00052600
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00052700
.*******************************************************************    00052800
              LCLC  &ZXSCHENV   ++ INPUT NAME                           00052900
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00053000
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00053100
              LCLC  &ZXPLISTVER                 ++ INPUT VALUE          00053200
              LCLC  &ZXMF       ++ INPUT VALUE                          00053300
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00053400
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00053500
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00053600
.********************************************************************   00053700
.***AC.PLV01  PROCESS PLISTVER                                      *   00053800
.********************************************************************   00053900
&XPLISTVER      SETA  0                                                 00054000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00054100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00054200
&XPLISTVER      SETA  0                                                 00054300
.ZPLV0          ANOP  ,                                                 00054400
.*                                                                      00054500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00054600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00054700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00054800
.*-------------------------------------------------------------------   00054900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00055000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00055100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00055300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00055400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00055500
                AGO   .ZPVCK2                                           00055600
.ZPVCK3         ANOP  ,                                                 00055700
.*-------------------------------------------------------------------   00055800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00055900
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00056000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00056100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00056200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00056300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056400
                AGO   .ZPVCKE                                           00056500
.ZPVCK2         ANOP  ,                                                 00056600
.*-------------------------------------------------------------------   00056700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00056800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00056900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00057000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00057100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00057200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00057300
.ZPVCK6         ANOP  ,                                                 00057400
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00057500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00057700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00057800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057900
&XPLISTVER      SETA  0                                                 00058000
                AGO   .ZPVCKE                                           00058100
.*-------------------------------------------------------------------   00058200
.ZPVCK4A        ANOP  ,                                                 00058300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00058400
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00058500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00058600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00058700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00058800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00058900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059000
                AGO   .ZPVCK5                                           00059100
.ZPVCK4         ANOP  ,                                                 00059200
&XPLISTVER      SETA  &PLISTVER(1)                                      00059300
.ZPVCK5         ANOP  ,                                                 00059400
.ZPVCKE         ANOP  ,                                                 00059500
.********************************************************************   00059600
