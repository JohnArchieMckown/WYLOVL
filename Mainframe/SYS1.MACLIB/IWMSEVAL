.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00123700
.*******************************************************************    00123800
                      ACTR  10000                                       00123900
.*                                                                      00124000
                      LCLA  &ZLC           COLUMN POSITION              00124100
                      LCLA  &ZOPX          LIST TABLE INDEX             00124200
                      LCLC  &ZLL           LABEL                        00124300
                      LCLC  &ZLOP          OPERATION                    00124400
                      LCLC  &ZLV           VALUE                        00124500
                      LCLC  &ZLD           DESCRIPTION                  00124600
.*                                                                      00124700
&ZOPX                 SETA  1                                           00124800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00124900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00125000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00125100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00125200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00125300
                      AIF   (K'&ZLD LE 0).ZL5                           00125400
&ZLC                  SETA  35             COMMENT COLUMN               00125500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00125600
&ZLC                  SETA  33             COMMMENT COLUMN              00125700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00125800
&ZLV                  SETC  '&ZLV'.' '                                  00125900
                      AGO   .ZL3                                        00126000
.*                                                                      00126100
.ZL4                  ANOP  ,                                           00126200
&ZLV                  SETC  '&ZLV'.' '                                  00126300
.ZL5                  ANOP  ,                                           00126400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00126500
.*                                                                      00126600
&ZLL     &ZLOP &ZLV                                                     00126700
.*                                                                      00126800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00126900
                      AGO   .ZL2           CONTINUE LOOPING             00127000
.*                                                                      00127100
.ZLX                  ANOP  ,                                           00127200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00127300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00127400
         L     14,16(0,0)              GET CVT ADDRESS                  00127500
         TM    CVTOSLV2-CVT(14),X'80'  TEST FOR OS/390 RELEASE IV       00127600
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00127700
         LA    0,IWMRSNCODENOSCHENV    SET REASON CODE = NO WLM SVCS    00127800
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00127900
         B     END&SYSNDX              BRANCH TO END LABEL              00128000
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00128100
         L     14,16(0,0)              GET CVT ADDRESS                  00128200
         L     14,772(14,0)            GET SFT ADDRESS                  00128300
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00128400
         LA    15,57                   GET EX FOR SERVICE ROUTINE       00128500
         OR    14,15                   GET PC NUMBER                    00128600
         PC    0(14)                                                    00128700
