                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00120400
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00120500
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00120600
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00120700
