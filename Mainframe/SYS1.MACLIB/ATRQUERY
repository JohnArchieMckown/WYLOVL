&ZASIZ           SETA  1                                                00390700
                 AGO   .ZG39TT                                          00390800
.ZG39S           ANOP ,                                                 00390900
&ZCLNG           SETC  '&XURTYPE'(1,K'&XURTYPE-1)                       00391000
                 AIF   (T'&ZCLNG EQ 'N').ZG39SS  NUMERIC                00391100
&ZCGPR           SETC  '&XURTYPE'(1,1)          FIRST CHAR              00391200
                 AIF   (T'&ZCGPR EQ 'N').ZG39U  NUMERIC ERR             00391300
                 AGO   .ZG39V                                           00391400
.*                                                                      00391500
.ZG39SS          ANOP ,                                                 00391600
&ZAWRK           SETA  &ZCLNG      CONVERT                              00391700
&ZCGPR           SETC  '&XURTYPE'(K'&XURTYPE,1)                         00391800
&ZASIZ           SETA  1024                                             00391900
                 AIF   ('&ZCGPR' EQ 'K').ZG39T  K FACTOR                00392000
&ZASIZ           SETA  1024*1024                                        00392100
                 AIF   ('&ZCGPR' EQ 'M').ZG39T  M FACTOR                00392200
&ZASIZ           SETA  1024*1024*1024                                   00392300
                 AIF   ('&ZCGPR' NE 'G').ZG39U  NOT G FACTOR            00392400
.ZG39T           ANOP  ,                                                00392500
&ZCVAL           SETC  '&ZAWRK*&ZASIZ'          EQUIVALENT NUMBER       00392600
.ZG39TT          ANOP  ,                                                00392700
&ZN              SETA  2147483647/&ZASIZ        CONVERT                 00392800
                 AIF   (&ZN LT 0).ZG39U         TOO LOW                 00392900
                 AIF   (&ZAWRK GT &ZN).ZG39U    TOO HIGH                00393000
&XURTYPE         SETC '&ZCVAL'     SET XVARIABLE NUMBER                 00393100
                 AGO   .ZG39V                                           00393200
.*                                                                      00393300
.ZG39U           ANOP  ,                                                00393400
