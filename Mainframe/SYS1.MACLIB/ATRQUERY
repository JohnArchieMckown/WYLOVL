.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               01047300
.*******************************************************************    01047400
                      ACTR  10000                                       01047500
.*                                                                      01047600
                      LCLA  &ZLC           COLUMN POSITION              01047700
                      LCLA  &ZOPX          LIST TABLE INDEX             01047800
                      LCLC  &ZLL           LABEL                        01047900
                      LCLC  &ZLOP          OPERATION                    01048000
                      LCLC  &ZLV           VALUE                        01048100
                      LCLC  &ZLD           DESCRIPTION                  01048200
.*                                                                      01048300
&ZOPX                 SETA  1                                           01048400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            01048500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01048600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01048700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01048800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01048900
                      AIF   (K'&ZLD LE 0).ZL5                           01049000
&ZLC                  SETA  35             COMMENT COLUMN               01049100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    01049200
&ZLC                  SETA  33             COMMMENT COLUMN              01049300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   01049400
&ZLV                  SETC  '&ZLV'.' '                                  01049500
                      AGO   .ZL3                                        01049600
.*                                                                      01049700
.ZL4                  ANOP  ,                                           01049800
&ZLV                  SETC  '&ZLV'.' '                                  01049900
.ZL5                  ANOP  ,                                           01050000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01050100
.*                                                                      01050200
&ZLL     &ZLOP &ZLV                                                     01050300
.*                                                                      01050400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01050500
                      AGO   .ZL2           CONTINUE LOOPING             01050600
.*                                                                      01050700
.ZLX                  ANOP  ,                                           01050800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01050900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01051000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01051100
         L     14,16(0,0)                                               01051200
