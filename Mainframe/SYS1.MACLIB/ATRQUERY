.*******************************************************************    01031800
.***  PROCESS ATRQUERY OUTPUT PARAMETERS                                01031900
.*******************************************************************    01032000
                      ACTR  10000                                       01032100
.*------------------------------------------------------------------    01032200
.***AO.L02 XCOUNT F                                                     01032300
         AIF   ('&ZXCOUNT' EQ 'OUTPUT').ZO25A  OUTPUT                   01032400
         AIF   ('&ZXCOUNT' NE 'INOUT').ZO25C  INOUT                     01032500
.ZO25A    AIF   ('&XCOUNT'(1,1) NE '(').ZO25B  ADDR IN GPR              01032600
&ZCGPR   SETC  '&XCOUNT'(2,K'&XCOUNT-2)                                 01032700
&ZOPN                 SETA  &ZOPN+1                                     01032800
&ZOPL(&ZOPN)          SETC  ' '                                         01032900
&ZOPO(&ZOPN)          SETC  'MVC'                                       01033000
&ZOPV(&ZOPN)          SETC  '0(4,&ZCGPR),24(1)'                         01033100
&ZOPD(&ZOPN)          SETC  '++ XCOUNT F'                               01033200
                      AGO   .ZO25C                                      01033300
.*                                                                      01033400
.ZO25B                ANOP  ,                                           01033500
&ZAWRK   SETA  K'&XCOUNT                                                01033600
         AIF   ('&XCOUNT'(&ZAWRK,1) NE ')').ZO25G                       01033700
&ZN      SETA  2                                                        01033800
.ZO25H    ANOP                                                          01033900
         AIF   (&ZN GE &ZAWRK).ZO25G                                    01034000
         AIF   ('&XCOUNT'(&ZN,1) NE '(').ZO25I                          01034100
&ZC      SETC  '&XCOUNT'(1,&ZN).'4'.','                                 01034200
&ZC      SETC  '&ZC'.'&XCOUNT'(&ZN+1,&ZAWRK-&ZN).',24(1)'               01034300
         AGO   .ZO25J                                                   01034400
.ZO25I    ANOP                                                          01034500
&ZN      SETA  &ZN+1                                                    01034600
         AGO   .ZO25H                                                   01034700
.ZO25G    ANOP                                                          01034800
&ZC      SETC  '&XCOUNT.(4),24(1)'                                      01034900
.ZO25J    ANOP                                                          01035000
&ZOPN                 SETA  &ZOPN+1                                     01035100
&ZOPL(&ZOPN)          SETC  ' '                                         01035200
&ZOPO(&ZOPN)          SETC  'MVC'                                       01035300
&ZOPV(&ZOPN)          SETC  '&ZC'                                       01035400
&ZOPD(&ZOPN)          SETC  '++ XCOUNT F'                               01035500
.ZO25C                ANOP  ,                                           01035600
.*------------------------------------------------------------------    01035700
.***AO.LVER01                                                           01035800
                      AIF  (&XPLISTVER LT 4).ZO54Z  PLVER               01035900
