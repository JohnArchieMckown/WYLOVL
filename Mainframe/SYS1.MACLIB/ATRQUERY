.***AC.PLV01  PROCESS PLISTVER                                      *   00224600
.********************************************************************   00224700
&XPLISTVER      SETA  1                                                 00224800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00224900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00225000
&XPLISTVER      SETA  6                                                 00225100
.ZPLV0          ANOP  ,                                                 00225200
.*                                                                      00225300
                AIF   ('&GNAME' EQ '').ZPLV1E                           00225400
                AIF   (&XPLISTVER GE 4).ZPLV1E                          00225500
&XPLISTVER      SETA  4                                                 00225600
.ZPLV1E         ANOP  ,                                                 00225700
.*                                                                      00225800
                AIF   ('&SYSNAME' EQ '').ZPLV2E                         00225900
                AIF   (&XPLISTVER GE 4).ZPLV2E                          00226000
&XPLISTVER      SETA  4                                                 00226100
.ZPLV2E         ANOP  ,                                                 00226200
.*                                                                      00226300
                AIF   ('&WMNAME' EQ '').ZPLV3E                          00226400
                AIF   (&XPLISTVER GE 3).ZPLV3E                          00226500
&XPLISTVER      SETA  3                                                 00226600
.ZPLV3E         ANOP  ,                                                 00226700
.*                                                                      00226800
                AIF   ('&GNAME' EQ '').ZPLV4E                           00226900
                AIF   (&XPLISTVER GE 4).ZPLV4E                          00227000
&XPLISTVER      SETA  4                                                 00227100
.ZPLV4E         ANOP  ,                                                 00227200
.*                                                                      00227300
                AIF   ('&SYSNAME' EQ '').ZPLV5E                         00227400
                AIF   (&XPLISTVER GE 3).ZPLV5E                          00227500
&XPLISTVER      SETA  3                                                 00227600
.ZPLV5E         ANOP  ,                                                 00227700
.*                                                                      00227800
                AIF   ('&GNAME' EQ '').ZPLV6E                           00227900
                AIF   (&XPLISTVER GE 4).ZPLV6E                          00228000
&XPLISTVER      SETA  4                                                 00228100
.ZPLV6E         ANOP  ,                                                 00228200
.*                                                                      00228300
                AIF   ('&SYSNAME' EQ '').ZPLV7E                         00228400
                AIF   (&XPLISTVER GE 4).ZPLV7E                          00228500
&XPLISTVER      SETA  4                                                 00228600
.ZPLV7E         ANOP  ,                                                 00228700
.*                                                                      00228800
                AIF   ('&DEFURONLY' EQ '').ZPLV8E                       00228900
                AIF   (&XPLISTVER GE 2).ZPLV8E                          00229000
&XPLISTVER      SETA  2                                                 00229100
.ZPLV8E         ANOP  ,                                                 00229200
