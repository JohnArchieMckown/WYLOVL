.***AE.KCK06 VALUE, OPT INPUT, KEYS -KMG RANGE OR EQUATE                00639500
                 AIF   (K'&ZXURTYPE LE 0).ZE39W  NOT I/O                00639600
&ZCVAL           SETC  '&XURTYPE'                                       00639700
                 AIF   ('&ZCVAL'(1,1) EQ '(').ZE39W  IN GPR             00639800
                 AIF   (T'&ZCVAL EQ 'N').ZE39U  NUMBER                  00639900
&ZCLNG           SETC  '&ZCVAL'(1,K'&ZCVAL-1)   GET PREFIX              00640000
                 AIF   (T'&ZCLNG EQ 'N').ZE39S  NUMERIC                 00640100
&ZCGPR           SETC  '&ZCVAL'(1,1)            FIRST CHAR              00640200
                 AIF   (T'&ZCGPR EQ 'N').ZE39V  NUMERIC ERR             00640300
                 AGO               .ZE39W                               00640400
.*                                                                      00640500
.ZE39S           ANOP  ,                                                00640600
&ZAWRK           SETA  &ZCLNG      CONVERT                              00640700
&ZCGPR           SETC  '&ZCVAL'(K'&ZCVAL,1)     GET FACTOR              00640800
&ZASIZ           SETA  1024                                             00640900
                 AIF   ('&ZCGPR' EQ 'K').ZE39T  K FACTOR                00641000
&ZASIZ           SETA  1024*1024                                        00641100
                 AIF   ('&ZCGPR' EQ 'M').ZE39T  M FACTOR                00641200
&ZASIZ           SETA  1024*1024*1024                                   00641300
                 AIF   ('&ZCGPR' NE 'G').ZE39V  NOT G FACTOR            00641400
                 AIF   (&ZAWRK GT 2).ZE39V      NOT MAX G FACTOR        00641500
.ZE39T           ANOP  ,                                                00641600
&ZCVAL           SETC  '&ZASIZ*&ZAWRK'          EQUIVALENT NUMBER       00641700
.ZE39U           ANOP  ,                                                00641800
&ZASIZ           SETA  &ZCVAL      CONVERT                              00641900
                 AIF   (&ZASIZ LT 0).ZE39V      TOO LOW                 00642000
                 AIF   (&ZASIZ GT 2147483647).ZE39V  TOO HIGH           00642100
&XURTYPE         SETC '&ZASIZ'     SET XVARIABLE NUMBER                 00642200
                 AGO   .ZE39W                                           00642300
.*                                                                      00642400
.ZE39V           ANOP  ,                                                00642500
&ZXURTYPE        SETC  ''          RESET IN/OUTPUT                      00642600
&XURTYPE         SETC 'ATR_NOFILTER'            SET DEFAULT             00642700
&ZMS             SETC  '"URTYPE='  *KRNGA* MNOTE                        00642800
&ZMS             SETC  '&ZMS.&URTYPE"'                                  00642900
&ZMS             SETC  '&ZMS THE RANGE IS "0-2147483647"'               00643000
                 MNOTE 8,' &ZMS FOR ARGUMENT 1.'                        00643100
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00643200
