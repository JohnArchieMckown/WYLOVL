.***AE.01  START OF ATRQUERY MACRO CODE                                 00789200
.*******************************************************************    00789300
                 LCLC  &M0M0001                 NAME                    00789400
&M0M0001         SETC  '&XLABEL'                SET NAME                00789500
                 AIF   (K'&XLABEL NE 0).XLAB2                           00789600
&M0M0001         SETC  'M00M&SYSNDX'            DEFAULT NAME            00789700
.*                                                                      00789800
.XLAB2           ANOP  ,                                                00789900
                 AIF   ('&XMF' EQ 'L').XLAB3                            00790000
&M0M0001 DS    0H                             ATRQUERY-6                00790100
.XLAB3           ANOP  ,                                                00790200
.*******************************************************************    00790300
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00790400
.*******************************************************************    00790500
              PUSH  PRINT                                               00790600
                 AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON       00790700
                 PRINT OFF         ++ PR01                              00790800
                 AGO   .ZPRNTXX                                         00790900
.ZPRINTX         PRINT ON                                               00791000
.ZPRNTXX         ANOP  ,                                                00791100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00791200
              AIF   ('&TID' NE '').QRYA                                 00791300
              AGO   .QRYD INVOCATION OKAY                               00791400
.QRYA         ANOP  ,                                                   00791500
              AIF   ('&TIDHIGH' NE '' OR '&TIDLOW' NE '').QRYC          00791600
              AGO   .QRYD INVOCATION OKAY                               00791700
.QRYC         ANOP  ,                                                   00791800
&ZMS          SETC  '"TID" MAY NOT BE SPECIFIED '                       00791900
&ZMS          SETC  '&ZMS IF EITHER "TIDHIGH" OR "TIDLOW" '             00792000
              MNOTE 8,'&ZMS IS SPECIFIED'                               00792100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00792200
.QRYD         ANOP  ,                                                   00792300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00792400
.********************************************************************   00792500
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00792600
.********************************************************************   00792700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00792800
                      LCLA  &ZOPN1         INVOCATION INDEX             00792900
                      LCLC  &ZOPL(4095)    LABEL                        00793000
                      LCLC  &ZOPO(4095)    OPERATION                    00793100
                      LCLC  &ZOPV(4095)    VALUE                        00793200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00793300
.*                                                                      00793400
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00793500
.******************************************************************     00793600
.***AL.L10 ATRQUERY FMT=LIST MF=S|L PARAMETERS                          00793700
.******************************************************************     00793800
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00793900
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00794000
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00794100
&XMFATTR              SETC  '0D'                                        00794200
.*                                                                      00794300
&ZOPN                 SETA  &ZOPN+1                                     00794400
&ZOPL(&ZOPN)          SETC  ' '                                         00794500
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00794600
&ZOPO(&ZOPN)          SETC  'B'                                         00794700
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00794800
                      AGO   .ZALVL6B                                    00794900
.ZALVL6A              ANOP                                              00795000
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00795100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00795200
.ZALVL6B              ANOP                                              00795300
&ZOPD(&ZOPN)          SETC  '++ ATRQUERY ARG PARMS'                     00795400
.*                                                                      00795500
&ZOPN                 SETA  &ZOPN+1                                     00795600
&ZOPL(&ZOPN)          SETC  ' '                                         00795700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00795800
&ZOPV(&ZOPN)          SETC  '1'                                         00795900
&ZOPD(&ZOPN)          SETC  ' '                                         00796000
.*                                                                      00796100
.ZLMFSL2              ANOP  ,                                           00796200
.*-------------------------------------------------------------------   00796300
