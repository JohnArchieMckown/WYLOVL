                 AGO               .ZE42W                               00652500
.*                                                                      00652600
.ZE42S           ANOP  ,                                                00652700
&ZAWRK           SETA  &ZCLNG      CONVERT                              00652800
&ZCGPR           SETC  '&ZCVAL'(K'&ZCVAL,1)     GET FACTOR              00652900
&ZASIZ           SETA  1024                                             00653000
                 AIF   ('&ZCGPR' EQ 'K').ZE42T  K FACTOR                00653100
&ZASIZ           SETA  1024*1024                                        00653200
                 AIF   ('&ZCGPR' EQ 'M').ZE42T  M FACTOR                00653300
&ZASIZ           SETA  1024*1024*1024                                   00653400
                 AIF   ('&ZCGPR' NE 'G').ZE42V  NOT G FACTOR            00653500
                 AIF   (&ZAWRK GT 2).ZE42V      NOT MAX G FACTOR        00653600
.ZE42T           ANOP  ,                                                00653700
&ZCVAL           SETC  '&ZASIZ*&ZAWRK'          EQUIVALENT NUMBER       00653800
.ZE42U           ANOP  ,                                                00653900
&ZASIZ           SETA  &ZCVAL      CONVERT                              00654000
                 AIF   (&ZASIZ LT 0).ZE42V      TOO LOW                 00654100
                 AIF   (&ZASIZ GT 2147483647).ZE42V  TOO HIGH           00654200
&XURSTATE        SETC '&ZASIZ'     SET XVARIABLE NUMBER                 00654300
                 AGO   .ZE42W                                           00654400
.*                                                                      00654500
.ZE42V           ANOP  ,                                                00654600
