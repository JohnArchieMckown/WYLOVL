.*******************************************************************    00653600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00653700
.*******************************************************************    00653800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00653900
.*******************************************************************    00654000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00654100
.*******************************************************************    00654200
                      ACTR  10000                                       00654300
.*                                                                      00654400
                      LCLA  &ZLC           COLUMN POSITION              00654500
                      LCLA  &ZOPX          LIST TABLE INDEX             00654600
                      LCLC  &ZLL           LABEL                        00654700
                      LCLC  &ZLOP          OPERATION                    00654800
                      LCLC  &ZLV           VALUE                        00654900
                      LCLC  &ZLD           DESCRIPTION                  00655000
.*                                                                      00655100
&ZOPX                 SETA  1                                           00655200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00655300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00655400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00655500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00655600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00655700
                      AIF   (K'&ZLD LE 0).ZL5                           00655800
&ZLC                  SETA  35             COMMENT COLUMN               00655900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00656000
&ZLC                  SETA  33             COMMMENT COLUMN              00656100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00656200
&ZLV                  SETC  '&ZLV'.' '                                  00656300
                      AGO   .ZL3                                        00656400
.*                                                                      00656500
.ZL4                  ANOP  ,                                           00656600
&ZLV                  SETC  '&ZLV'.' '                                  00656700
.ZL5                  ANOP  ,                                           00656800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00656900
.*                                                                      00657000
&ZLL     &ZLOP &ZLV                                                     00657100
.*                                                                      00657200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00657300
                      AGO   .ZL2           CONTINUE LOOPING             00657400
.*                                                                      00657500
.ZLX                  ANOP  ,                                           00657600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00657700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00657800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00657900
         L     15,16(0,0)              GET CVT ADDRESS             @L1A 00658000
         L     15,772(15,0)            GET SFT ADDRESS             @L1A 00658100
.*********************************************************************  00658200
.*                                                                   *  00658300
.*  Get LX/EX for Logical Parmlib Service                            *  00658400
.*                                                                   *  00658500
.*  Note: LX/EX is calculated by multiplying the PC number (115)     *  00658600
.*        times the length of a PC/AUTH table entry (4) and          *  00658700
.*        then subtracting 4.                                        *  00658800
.*                                                                   *  00658900
.*        (115*4)-4 = 456                                            *  00659000
.*                                                                   *  00659100
.*********************************************************************  00659200
         L     15,456(15,0)       ++ Get PC number                 @L1A 00659300
         PC    0(15)              ++ PC to Logical Parmlib Service @L1A 00659400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00659500
.ZLINKX               ANOP  ,                                           00659600
.*******************************************************************    00659700
.***AP.X01  FORMAT GENERATED CODE                                       00659800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00659900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00660000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00660100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00660200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00660300
                      AIF   (K'&ZLD LE 0).ZL51                          00660400
&ZLC                  SETA  35             COMMENT COLUMN               00660500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00660600
&ZLC                  SETA  33             COMMENT COLUMN               00660700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00660800
&ZLV                  SETC  '&ZLV'.' '                                  00660900
                      AGO   .ZL31                                       00661000
.*                                                                      00661100
.ZL41                 ANOP  ,                                           00661200
&ZLV                  SETC  '&ZLV'.' '                                  00661300
.ZL51                 ANOP  ,                                           00661400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00661500
.*                                                                      00661600
&ZLL     &ZLOP &ZLV                                                     00661700
.*                                                                      00661800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00661900
                      AGO   .ZL21          CONTINUE LOOPING             00662000
.*                                                                      00662100
.ZLX1                 ANOP  ,                                           00662200
.*******************************************************************    00662300
.***AP.X02  END OF IEFPRMLB MACRO                                       00662400
&ZIEFPRMLB            SETC  'YES'          SET IEFPRMLB USED            00662500
         POP   PRINT                                                    00662600
*                                            IEFPRMLB-1                 00662700
         SPACE 1                                                        00662800
.ZLX2                 ANOP  ,                                           00662900
         MEND                                                           00663000
**********************************************************************/ 00663100
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00663200
*%IEFPRMLB: MACRO KEYS(REQUEST,S99RB,WAITDSN,MOUNT,RETMSG,CONSOLID,     00663300
*                 CART,MSGBUF,S99RBPTR,ALLOCDDNAME,READ,MEMNAME,        00663400
*                 READBUF,BLANK72,MEMNOTFOUND,FREECLOSE,DDNAME,MSG,     00663500
*                 BUFFER,CALLERNAME,RETCODE,RSNCODE,PLISTVER,MF);       00663600
*  ANS('?'||MACLABEL||' '||'IEFPRMLP '||MACLIST||                       00663700
*         ' '||MACKEYS||';') COL(MACLMAR);                              00663800
*%END IEFPRMLB;                                                         00663900
