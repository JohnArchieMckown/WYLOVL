.***AC.PLV01  PROCESS PLISTVER                                      *   00172500
.********************************************************************   00172600
&XPLISTVER      SETA  0                                                 00172700
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00172800
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00172900
&XPLISTVER      SETA  1                                                 00173000
.ZPLV0          ANOP  ,                                                 00173100
.*                                                                      00173200
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00173300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00173400
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00173500
.*-------------------------------------------------------------------   00173600
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00173700
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00173800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00173900
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00174000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00174100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00174200
                AGO   .ZPVCK2                                           00174300
.ZPVCK3         ANOP  ,                                                 00174400
.*-------------------------------------------------------------------   00174500
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00174600
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00174700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00174800
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00174900
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00175000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00175100
                AGO   .ZPVCKE                                           00175200
.ZPVCK2         ANOP  ,                                                 00175300
.*-------------------------------------------------------------------   00175400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00175500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00175600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00175700
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00175800
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00175900
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00176000
.ZPVCK6         ANOP  ,                                                 00176100
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00176200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00176300
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00176400
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00176500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00176600
&XPLISTVER      SETA  1                                                 00176700
                AGO   .ZPVCKE                                           00176800
.*-------------------------------------------------------------------   00176900
.ZPVCK4A        ANOP  ,                                                 00177000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00177100
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00177200
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00177300
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00177400
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00177500
                MNOTE 8,' &ZMS IS REQUIRED.'                            00177600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00177700
                AGO   .ZPVCK5                                           00177800
.ZPVCK4         ANOP  ,                                                 00177900
&XPLISTVER      SETA  &PLISTVER(1)                                      00178000
.ZPVCK5         ANOP  ,                                                 00178100
.ZPVCKE         ANOP  ,                                                 00178200
.********************************************************************   00178300
