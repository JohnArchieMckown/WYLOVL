                 AIF   ('&CART' NE '').ZG142B   KEYS?                   00182600
                 AIF   ('&MSGBUF' NE '').ZG142B  KEYS?                  00182700
                 AIF   ('&S99RBPTR' NE '').ZG142B  KEYS?                00182800
                 AIF   ('&ALLOCDDNAME' NE '').ZG142B  KEYS?             00182900
                 AIF   ('&READ' NE '').ZG142B   KEYS?                   00183000
                 AIF   ('&MEMNAME' NE '').ZG142B  KEYS?                 00183100
                 AIF   ('&READBUF' NE '').ZG142B  KEYS?                 00183200
                 AIF   ('&BLANK72' NE '').ZG142B  KEYS?                 00183300
                 AIF   ('&MEMNOTFOUND' NE '').ZG142B  KEYS?             00183400
                 AIF   ('&FREECLOSE' NE '').ZG142B  KEYS?               00183500
                 AIF   ('&DDNAME' NE '').ZG142B  KEYS?                  00183600
                 AIF   ('&MSG' NE '').ZG142B    KEYS?                   00183700
                 AIF   ('&BUFFER' NE '').ZG142B  KEYS?                  00183800
                 AIF   ('&CALLERNAME' NE '').ZG142B  KEYS?              00183900
                 AIF   ('&RETCODE' NE '').ZG142B  KEYS?                 00184000
                 AIF   ('&RSNCODE' NE '').ZG142B  KEYS?                 00184100
                 AGO    .ZG142C    INVOCATION OKAY                      00184200
.ZG142B          ANOP  ,                                                00184300
&ZMS             SETC  'THE "L" FORM OF "MF"'   *MFLA* MNOTE            00184400
&ZMS             SETC  '&ZMS CAN ONLY HAVE'                             00184500
