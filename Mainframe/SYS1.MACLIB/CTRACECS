&ZMS          SETC  '&ZMS "&ZCVAL"'                                     23250000
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              23300000
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      23350000
              MNOTE 8,' &ZMS ARE ALLOWED.'                              23400000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             23450000
.ZGLISTX      ANOP  ,                                                   23500000
&ZASYSLNDX    SETA  0                                                   23550000
.*******************************************************************    23600000
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       23650000
.*******************************************************************    23700000
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          23750000
.*******************************************************************    23800000
.***AE.01  START OF CTRACECS MACRO CODE                                 23850000
.*******************************************************************    23900000
              LCLC  &M0M0001                 NAME                       23950000
&M0M0001      SETC  '&XLABEL'                SET NAME                   24000000
              AIF   (K'&XLABEL NE 0).XLAB2                              24050000
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               24100000
.*                                                                      24150000
.XLAB2        ANOP  ,                                                   24200000
&M0M0001 DS    0H                             CTRACECS-0                24250000
.*******************************************************************    24300000
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              24350000
.*******************************************************************    24400000
              PUSH  PRINT                                               24450000
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          24500000
              PRINT OFF         ++ PR01                                 24550000
              AGO   .ZPRNTXX                                            24600000
.ZPRINTX      PRINT ON                                                  24650000
.ZPRNTXX      ANOP  ,                                                   24700000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  24750000
.*====================================================================  24800000
.*     More ERROR checking...                                           24801500
.*====================================================================  24803000
.*  CSLABEL is required with TESTMODE or TESTSEQ# except when the       24805500
.*  defaults are specified.                                        @P2A 24808000
              AIF   (T'&CSLABEL  NE 'O').CS11E     SPECIFIED       @L1A 24810500
              AIF   (T'&TESTSEQ# EQ 'O').CS11A     OMITTED         @P2C 24810700
              AIF   ('&TESTSEQ#' NE 'TBWCSEQ').CS11B  DEFAULT      @P2A 24810900
.CS11A        ANOP  ,                                              @P2A 24811100
              AIF   (T'&TESTMODE EQ 'O').CS11E     OMITTED         @L1A 24811300
              AIF   ('&TESTMODE' EQ 'CURRENT').CS11E  DEFAULT      @P2A 24811500
.CS11B        ANOP  ,                                              @L1A 24811700
&ZMS          SETC  '"CSLABEL'              *KDEPA* MNOTE          @L1A 24812000
&ZMS          SETC  '&ZMS.=&CSLABEL"'                              @L1A 24813500
&ZMS          SETC  '&ZMS IS REQUIRED WHEN '                       @L1A 24815000
&ZMS          SETC  '&ZMS "TESTSEQ#" OR "TESTMODE"'                @L1A 24816500
              MNOTE 8,' &ZMS IS ALSO SPECIFIED.'                   @L1A 24818000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE        @L1A 24819500
              AGO   .CS99E                                         @L1A 24821000
.CS11E        ANOP  ,                                              @L1A 24822500
.*====================================================================  24824000
.*  CSLABEL=RETRY is only valid with MODE, TBWC, TESTMODE=CURRENT       24825500
.*  or TESTSEQ#=TBWCSEQ                                                 24826000
              AIF   (T'&CSLABEL  EQ 'O').CS12E      OMITTED        @L1A 24826500
              AIF   ('&CSLABEL' NE 'RETRY').CS12E                  @L1A 24827000
              AIF   (T'&TESTSEQ# EQ 'O').CS12A      OMITTED        @P2C 24827800
              AIF   ('&TESTSEQ#' NE 'TBWCSEQ').CS12B  DEFAULT      @P2A 24828600
.CS12A        ANOP  ,                                              @P2A 24829400
              AIF   (T'&TESTMODE EQ 'O').CS12E      OMITTED        @P2A 24830200
              AIF   ('&TESTMODE' EQ 'AVAIL').CS12B                 @L1A 24831000
              AIF   ('&TESTMODE' EQ 'FULL').CS12B                  @L1A 24831800
              AIF   ('&TESTMODE' NE 'FILLING').CS12E  CURRENT      @L1A 24833000
.CS12B        ANOP  ,                                              @L1A 24834500
&ZMS          SETC  '"CSLABEL'              *KDEPA* MNOTE          @L1A 24836000
&ZMS          SETC  '&ZMS.=&CSLABEL"'                              @L1A 24837500
&ZMS          SETC  '&ZMS IS NOT VALID WITH "TESTSEQ#'             @L1A 24839000
&ZMS          SETC  '&ZMS.=&TESTSEQ#" OR'                          @P2A 24840500
              MNOTE 8,' &ZMS "TESTMODE=AVAIL / FULL / FILLING".'   @L1A 24842000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE        @L1A 24843500
              AGO   .CS99E                                         @L1A 24845000
.CS12E        ANOP  ,                                              @L1A 24846500
.********************************************************************   24850000
.*         INVOKE MVS MACROS TO CONTROL CODE GENERATION                 24900000
.********************************************************************   24950000
         GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                      25000000
         GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE               25050000
.*                                                                      25100000
         AIF   (K'&SYSASCE GT 0).CS04      SYS STATE DEFINED            25150000
         AIF   (K'&SYSSPLV GT 0).CS02      SP LEVEL DEFINED             25200000
         SPLEVEL  TEST                                                  25250000
.CS02    ANOP  ,                                                        25300000
         AIF   ('&SYSSPLV' EQ '1').CS03    PRE SP 3.1                   25350000
         AIF   ('&SYSSPLV' EQ '2').CS03    PRE SP 3.1                   25400000
         SYSSTATE TEST                                                  25450000
         AGO   .CS04                                                    25500000
.CS03    ANOP  ,                                                        25550000
&SYSASCE SETC  'P'         ASSUME SYSSTATE PRIMARY                      25600000
.CS04    ANOP  ,                                                        25650000
.*====================================================================  25700000
         LCLC  &CSL0001                LABEL NAME                       25741600
         LCLC  &CSTBWC0,&CSTBWC1       TBWC + 0 - FLAGS            @01C 25783200
         LCLC  &CSTBWC4                TBWC + 4 - SEQUENCE NUMBER       25824800
         LCLC  &CSBSEQ                 BUFFSEQ#                         25866400
&CSL0001 SETC  '#CS&SYSNDX'            LABEL FOR CDS                    25908000
.*====================================================================  25950000
.*  SET (R1) to the SEQUENCE NUMBER                                     25992800
         AIF   (T'&TESTSEQ# EQ 'O').CS21A        OMITTED           @P2C 26018400
         AIF   ('&TESTSEQ#' NE 'TBWCSEQ').CS21C  NOT DEFAULT       @P2A 26044000
.CS21A   ANOP  ,                                                   @L1A 26069600
.*                               Assume defaults are in Register        26095200
&CSTBWC4 SETC  '&XTBWC+4'                        - SQUENCE NUMBER (R1)  26121200
         AIF   ('&XTBWC'(1,1) NE '(').CS21B      NOT REGISTER           26164000
&CSTBWC4 SETC  '4(,'.'&XTBWC'(2,K'&XTBWC-2).')'  TBWC=(REGISTER)   @L1C 26207000
.CS21B   ANOP  ,                                                        26250000
         L     1,&CSTBWC4         ++ SAVE SEQUENCE NUMBER               26252700
         AGO   .CS21E                                              @L1A 26255400
.CS21C   ANOP  ,                                                   @L1A 26268900
&CSTBWC4 SETC  '&XTESTSEQ#'                  - OLD SQUENCE NUMBER  @L1A 26269400
         AIF   ('&XTESTSEQ#'(1,1) NE '(').CS21D      NOT REGISTER  @L1A 26269900
&CSTBWC4 SETC  '0(,'.'&XTESTSEQ#'(2,K'&XTBWC-2).')'                @01A 26270400
.CS21D   ANOP  ,                                                   @L1A 26270900
         L     1,&CSTBWC4         ++ TEST OLD SEQUENCE NUMBER      @L1A 26271600
.CS21E   ANOP  ,                                                   @L1A 26274300
.*====================================================================  26277000
.*  SET (R0) to the TESTMODE value (Filling, Available, Full, Current)  26279700
.*                               Assume defaults are in Register        26282400
&CSTBWC0 SETC  '&XTBWC'                          - FLAGS (R0)      @L1M 26285100
&CSTBWC1 SETC  '&XTBWC'                          - FLAGS (R0)      @01A 26286400
         AIF   ('&XTBWC'(1,1) NE '(').CS24B      NOT REGISTER      @L1A 26287800
&CSTBWC0 SETC  '0('.'&XTBWC'(2,K'&XTBWC-2).')'   TBWC=(REGISTER)        26290500
&CSTBWC1 SETC  '0(,'.'&XTBWC'(2,K'&XTBWC-2).')'  TBWC=(REGISTER)   @01A 26291800
.CS24B   ANOP  ,                                                   @L1A 26293200
         L     0,&CSTBWC1         ++ SAVE CURRENT FLAGS            @01C 26295900
.*--------------- TESTMODE VALUES ---------------------------           26298600
         AIF   ('&XTESTMODE' EQ 'CURRENT').CS29E   -- DEFAULT      @D1C 26301300
         AIF   ('&XTESTMODE' EQ 'AVAIL').CS25A                     @L1A 26304000
         AIF   ('&XTESTMODE' EQ 'FILLING').CS26A                   @L1A 26306700
         AIF   ('&XTESTMODE' EQ 'FULL').CS27A                      @L1A 26309400
.*--------------- AVAILABLE                                             26312100
.CS25A   ANOP  ,                                                   @L1A 26314800
         O     0,=X'00400000'     ++ TEST FOR AVAILABLE            @L1A 26317500
         AGO   .CS29E                                              @L1A 26320200
.*--------------- FILLING                                               26322900
.CS26A   ANOP  ,                                                   @L1A 26325600
         O     0,=X'80000000'     ++ TEST FOR FILLING              @L1A 26328300
         AGO   .CS29E                                              @L1A 26331000
.*--------------- FULL                                                  26333700
.CS27A   ANOP  ,                                                   @L1A 26336400
         O     0,=X'40000000'     ++ TEST FOR FULL                 @L1A 26339100
.CS29E   ANOP  ,                  END OF TESTMODE                  @L1A 26341800
.*====================================================================  26344500
.* Setup BUFSEQ# for use with MODE(FILLING)                             26347200
         AIF   ('&ZXBUFFSEQ#' EQ '').CS31E       NOT USED               26350000
&CSBSEQ  SETC  '&XBUFFSEQ#'                      ASSUME NOT REGISTER    26400000
         AIF   ('&XBUFFSEQ#'(1,1) NE '(').CS31B  NOT GPR                26450000
&CSBSEQ  SETC  '0(,'.'&XBUFFSEQ#'(2,K'&XBUFFSEQ#-2).')'  REGISTER  @L1C 26500000
         AGO   .CS31E                                                   26550000
.CS31B   ANOP  ,                                                        26600000
&ZC      SETC  '&XBUFFSEQ#'(1,1)                 GET FIRST CHAR         26650000
         AIF   (T'&ZC NE 'N').CS31E              NOT SELF-DEFINING      26700000
&CSBSEQ  SETC  '=FL4''&XBUFFSEQ#'''                                     26750000
.CS31E   ANOP  ,                                                        26850000
.*====================================================================  26900000
.* Set R14 to the MODE value  (Filling, Available, Full) and            26937500
.* Set R15 to the desired SEQUENCE number                               26975000
.*--------------- MODE(FILLING)                                         27012500
         AIF   ('&XMODE' NE 'FILLING').CS41B    NOT FILLING             27050000
         L     14,=X'80000000'    ++ SET TO FILLING                     27100000
         L     15,&CSBSEQ         ++ NEW SEQUENCE NUMBER                27150000
&CSL0001 DS    0H                                                       27200000
         AGO   .CS49E                                                   27233300
.*--------------- GENERATE LABEL                                        27266600
.CS41B   ANOP  ,                                                        27300000
&CSL0001 DS    0H                                                       27350000
.*--------------- MODE(AVAILABLE)                                       27356200
         AIF   ('&XMODE' NE 'AVAIL').CS43B    NOT AVAILABLE        @L1A 27362400
         L     14,=X'00400000'    ++ SET TO AVAILABLE              @L1A 27368600
         LR    15,1               ++ SAME SEQUENCE NUMBER          @L1A 27374800
         AGO   .CS49E                                              @L1A 27381000
.CS43B   ANOP  ,                                                   @L1A 27387200
.*--------------- MODE(FULL)                                            27393400
         LR    14,0               ++ COPY FLAGS TO CHANGE               27400000
         N     14,=X'7FFFFFFF'    ++ CLEAR FILLING FLAG                 27450000
         O     14,=X'40000000'    ++ SET FULL FLAG                      27500000
         LR    15,1               ++ SAME SEQUENCE NUMBER               27550000
.CS49E   ANOP  ,                                                        27575000
.*-------------------------------------------------------------------   27600000
.*  Perform Compare and Swap and branch according to CSLABEL       @L1A 27625000
         CDS   0,14,&CSTBWC0      ++ SET NEW MODE, SEQ NUM              27650000
         AIF   (T'&CSLABEL EQ 'O').CS51A     OMITTED               @L1A 27658300
         AIF   ('&XCSLABEL' EQ 'RETRY').CS51A                      @L1A 27666600
         BC    7,&XCSLABEL        ++ BRANCH ON FAILURE             @L1A 27674900
         AGO   .CS51B                                              @L1A 27683200
.CS51A   ANOP  ,                                                   @L1A 27691500
         BC    7,&CSL0001         ++ REPEAT ON FAILURE                  27700000
.CS51B   ANOP  ,                                                   @L1A 27716600
.CS99E   ANOP  ,                                                   @L1A 27733200
.*====================================================================  27750000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  27800000
.********************************************************************   27800900
.***AL.A01  INTERNAL ARRAY TO PRODUCE FORMATTED CODE                    27801800
.********************************************************************   27802700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       27803600
                      LCLA  &ZOPN1         INVOCATION INDEX             27804500
                      LCLC  &ZOPL(4095)    LABEL                        27805400
                      LCLC  &ZOPO(4095)    OPERATION                    27806300
                      LCLC  &ZOPV(4095)    VALUE                        27807200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  27808100
.*                                                                      27809000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     27809900
.*******************************************************************    27810800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               27811700
.*******************************************************************    27812600
                      ACTR  10000                                       27813500
.*                                                                      27814400
                      LCLA  &ZLC           COLUMN POSITION              27815300
                      LCLA  &ZOPX          LIST TABLE INDEX             27816200
                      LCLC  &ZLL           LABEL                        27817100
                      LCLC  &ZLOP          OPERATION                    27818000
                      LCLC  &ZLV           VALUE                        27818900
                      LCLC  &ZLD           DESCRIPTION                  27819800
.*                                                                      27820700
&ZOPX                 SETA  1                                           27821600
.*******************************************************************    27822500
.***AP.X01  FORMAT GENERATED CODE                                       27823400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            27824300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              27825200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              27826100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              27827000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              27827900
                      AIF   (K'&ZLD LE 0).ZL51                          27828800
&ZLC                  SETA  35             COMMENT COLUMN               27829700
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   27830600
&ZLC                  SETA  33             COMMENT COLUMN               27831500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  27832400
&ZLV                  SETC  '&ZLV'.' '                                  27833300
                      AGO   .ZL31                                       27834200
.*                                                                      27835100
.ZL41                 ANOP  ,                                           27836000
&ZLV                  SETC  '&ZLV'.' '                                  27836900
.ZL51                 ANOP  ,                                           27837800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               27838700
.*                                                                      27839600
&ZLL     &ZLOP &ZLV                                                     27840500
.*                                                                      27841400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               27842300
                      AGO   .ZL21          CONTINUE LOOPING             27843200
.*                                                                      27844100
.ZLX1                 ANOP  ,                                           27845000
.*******************************************************************    27850000
.***AP.X02  END OF CTRACECS MACRO                                       27900000
&ZCTRACECS            SETC  'YES'          SET CTRACECS USED            27950000
         POP   PRINT                                                    28000000
