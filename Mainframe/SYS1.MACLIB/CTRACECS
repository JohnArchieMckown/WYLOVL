.***AG.KXVAR05 NAME, RQD INPUT, KEYS -XVARIABLE KEY                     11750000
              AIF   (N'&TBWC LE 1).ZG2A         CK NUM                  11800000
&ZMS          SETC  '"TBWC='    *KPOSA* MNOTE                           11850000
&ZMS          SETC  '&ZMS.&TBWC"'                                       11900000
&ZMS          SETC  '&ZMS "TBWC"'                                       11950000
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 12000000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             12050000
.ZG2A         ANOP  ,                                                   12100000
.*-------------------------------------------------------------------   12150000
.***AG.KXVAR06 NAME, RQD INPUT, KEYS -XVARIABLE                         12200000
&ZXTBWC       SETC  'INPUT'     SET IN/OUTPUT                           12250000
&XTBWC        SETC  '&TBWC(1)'  SET XVARIABLE                           12300000
              AIF   (N'&TBWC GT 1).ZG2B         MULTI PARMS             12350000
&XTBWC        SETC  '&TBWC'     SET XVARIABLE                           12400000
.ZG2B         AIF   (K'&TBWC(1) GT 0).ZG2C                              12450000
&ZXTBWC       SETC  ''          RESET IN/OUTPUT                         12500000
&ZMS          SETC  ' '         *KRQDA*                                 12550000
&ZMS          SETC  '&ZMS "TBWC" KEY AND ITS'                           12600000
              MNOTE 8,' &ZMS ARGUMENT ARE REQUIRED.'                    12650000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             12700000
.ZG2C         ANOP  ,                                                   12750000
.*-------------------------------------------------------------------   12800000
.***AG.KCK02 NAME, RQD INPUT, KEYS -GPR OUT OF RANGE                    12850000
              AIF   (K'&ZXTBWC LE 0).ZG2R       NOT I/O                 12900000
              AIF   ('&XTBWC'(1,1) NE '(').ZG2R                         12950000
&ZCGPR        SETC  '&XTBWC'(2,K'&XTBWC-2)                              13000000
              AIF   ('&ZCGPR'(1,1) NE '0').ZG2P                         13050000
              AIF   (K'&ZCGPR GT 2).ZG2Q                                13100000
              AIF   ('&ZCGPR' EQ '0').ZG2Q                              13150000
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       13200000
              AIF   (T'&ZCGPR NE 'N').ZG2Q      INVALID REG NAME        13250000
.ZG2P         ANOP  ,                                                   13300000
              AIF   (T'&ZCGPR NE 'N').ZG2R      NON NUMERIC             13350000
              AIF   ('&ZCGPR' LT '2').ZG2Q                              13400000
              AIF   ('&ZCGPR' LE '12').ZG2R                             13450000
.ZG2Q         ANOP  ,                                                   13500000
&ZMS          SETC  '"TBWC='    *KGPRA* MNOTE                           13550000
&ZMS          SETC  '&ZMS.&TBWC"'                                       13600000
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           13650000
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    13700000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             13750000
.ZG2R         ANOP  ,                                                   13800000
